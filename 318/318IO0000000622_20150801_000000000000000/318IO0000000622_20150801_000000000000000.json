    "@Num": "622",
    "@Year": "18",
    "LawNum": "昭和十八年勅令第六百二十二号",
    "LawBody": {
      "LawTitle": "昭和十八年勅令第六百二十二号（占領地軍政官憲ノ為シタル行為ノ法律上ノ効力等ニ関スル法律ノ施行ニ関スル件）",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "○１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "戸籍、船舶又ハ船員ニ関スル事項ニ付占領地軍政官憲ノ為シタル行為ニシテ戸籍法、民法、船舶法又ハ船員法ニ依リ領事官ノ為ス行為ニ相当スルモノハ之ヲ当該法律ニ依リ領事官ガ為シタルモノト看做ス"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "○２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "戸籍、船舶又ハ船員ニ関スル事項ニ付在留臣民其ノ他ノ者ガ占領地軍政官憲ニ対シ為シタル行為ニシテ戸籍法、民法、船舶法又ハ船員法ニ依リ領事官ニ対シテ為ス行為ニ相当スルモノハ之ヲ当該法律ニ依リ領事官ニ対シテ為シタルモノト看做ス"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphNum": "○３",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "前二項ノ占領地軍政官憲ノ範囲其ノ他本令施行ニ関シ必要ナル事項ハ主務大臣之ヲ定ム"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "本令ハ昭和十八年法律第六十一号施行ノ日ヨリ之ヲ施行ス"
            }
          }
        }
      }
    }
  }
}
