{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "11",
    "@Year": "21",
    "LawNum": "平成二十一年防衛省令第十一号",
    "LawBody": {
      "LawTitle": "防衛会議の組織及び運営に関する省令",
      "EnactStatement": "防衛省設置法（昭和二十九年法律第百六十四号）第十九条の二第六項の規定に基づき、防衛会議の組織及び運営に関する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（議長）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "防衛会議（以下「会議」という。）の議長は、会務を総理する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "#text": "会議の庶務は、防衛省大臣官房企画評価課において総括し、及び処理する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "#text": "ただし、防衛省組織令（昭和二十九年政令第百七十八号）第六条第一号から第八号までに掲げる事務に係るものについては、防衛省防衛政策局防衛政策課において処理する。"
                  }
                ]
              }
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令に定めるもののほか、議事の手続その他会議の運営に関し必要な事項は、防衛大臣が定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成二十一年八月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年一〇月一日防衛省令第一七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成二十七年十月一日から施行する。"
            }
          }
        }
      ]
    }
  }
}
