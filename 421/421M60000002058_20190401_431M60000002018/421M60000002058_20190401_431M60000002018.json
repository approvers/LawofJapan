            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ArticleCaption": "（管理室及び広報室並びに訟務対策官及びサイバーセキュリティ・情報化企画官）",
                "ArticleTitle": "第一条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "総務課に、管理室及び広報室並びに訟務対策官（検察官をもって充てるものとする。）及びサイバーセキュリティ・情報化企画官それぞれ一人を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "管理室は、次に掲げる事務をつかさどる。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁の職員の教養及び訓練に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "栄典の推薦及び伝達の実施並びに表彰及び儀式に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "長官の官印及び庁印の保管に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "公文書類の接受、発送、編集及び保存に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "5",
                        "ItemTitle": "五",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁の所掌に係る会計及び会計の監査に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "6",
                        "ItemTitle": "六",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁所属の行政財産及び物品の管理に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "7",
                        "ItemTitle": "七",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "東日本大震災復興特別会計の経理のうち消費者庁の所掌に係るものに関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "8",
                        "ItemTitle": "八",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "東日本大震災復興特別会計に属する物品の管理のうち消費者庁の所掌に係るものに関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "9",
                        "ItemTitle": "九",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "庁内の管理に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "10",
                        "ItemTitle": "十",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁所属の建築物の営繕に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "11",
                        "ItemTitle": "十一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁の職員の衛生、医療その他の福利厚生に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "12",
                        "ItemTitle": "十二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁の職員に貸与する宿舎に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "13",
                        "ItemTitle": "十三",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "課徴金の徴収に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "14",
                        "ItemTitle": "十四",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "国立国会図書館支部消費者庁図書館に関すること。"
                          }
                        }
                      }
                    ]
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "管理室に、室長を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "4",
                    "ParagraphNum": "４",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "広報室は、消費者庁の所掌事務に関して行う広報に関する事務をつかさどる。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "5",
                    "ParagraphNum": "５",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "広報室に、室長を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "6",
                    "ParagraphNum": "６",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "訟務対策官は、命を受けて、消費者庁の所掌事務に関する不服申立て及び訴訟に関する事務を処理する。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "7",
                    "ParagraphNum": "７",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "サイバーセキュリティ・情報化企画官は、命を受けて、消費者庁の所掌事務に関するサイバーセキュリティ（サイバーセキュリティ基本法（平成二十六年法律第百四号）第二条に規定するサイバーセキュリティをいう。）の確保並びに情報システムの整備及び管理並びにこれらと併せて行われる事務の運営の改善及び効率化に関する調査、企画及び立案を行う。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "2",
                "ArticleCaption": "（財産被害対策室及び国際室並びに企画官及び企画調整官）",
                "ArticleTitle": "第二条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "消費者政策課に、財産被害対策室及び国際室並びに企画官及び企画調整官それぞれ一人を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "財産被害対策室は、消費者安全法（平成二十一年法律第五十号）の規定による消費者安全の確保に関する事務をつかさどる（同法第二条第五項第三号に規定する消費者事故等に該当するものに係るものに限る。）。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "財産被害対策室に、室長を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "4",
                    "ParagraphNum": "４",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国際室は、次に掲げる事務をつかさどる。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁の所掌事務に係る国際機関、国際会議その他の国際的な枠組み並びに外国の行政機関及び団体に関する事務の総括に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁の所掌事務に係る国際協力に関する事務の連絡調整に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "前二号に掲げるもののほか、消費者政策課の所掌事務のうち国際関係に係るものに関すること（財産被害対策室の所掌に属するものを除く。）。"
                          }
                        }
                      }
                    ]
                  },
                  {
                    "@Hide": "false",
                    "@Num": "5",
                    "ParagraphNum": "５",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国際室に、室長を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "6",
                    "ParagraphNum": "６",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "企画官は、命を受けて、消費者政策課の所掌事務のうち特定事項の調査、企画及び立案を行う。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "7",
                    "ParagraphNum": "７",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "企画調整官は、命を受けて、消費者政策課の所掌事務に関する重要事項についての企画及び立案並びに調整に関する事務を処理する。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "3",
                "ArticleCaption": "（企画官）",
                "ArticleTitle": "第三条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "消費者制度課に、企画官一人を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "企画官は、命を受けて、消費者制度課の所掌事務のうち特定事項の調査、企画及び立案を行う。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "4",
                "ArticleCaption": "（消費者教育推進室）",
                "ArticleTitle": "第四条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "消費者教育・地方協力課に、消費者教育推進室を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "消費者教育推進室は、次に掲げる事務をつかさどる。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者の利益の擁護及び増進に関する基本的な政策のうち消費者教育に関するものの企画及び立案並びに推進に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者の利益の擁護及び増進を図る上で必要な環境の整備に関する基本的な政策のうち消費者教育に関するものの企画及び立案並びに推進に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者教育の推進に関する法律（平成二十四年法律第六十一号）第九条第一項に規定する消費者教育の推進に関する基本的な方針の策定及び推進に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者庁の所掌事務に係る消費者の利益の擁護及び増進に資する情報の消費者に対する提供に関する企画及び立案並びに推進に関すること。"
                          }
                        }
                      }
                    ]
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "消費者教育推進室に、室長を置く。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "5",
                "ArticleCaption": "（事故調査室及び企画官）",
                "ArticleTitle": "第五条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "消費者安全課に、事故調査室及び企画官一人を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "事故調査室は、次に掲げる事務をつかさどる。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者安全調査委員会の庶務に関すること。"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "消費者安全調査委員会の行う消費者安全法第二十七条に規定する調査に対する援助に関すること。"
                          }
                        }
                      }
                    ]
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "事故調査室に、室長を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "4",
                    "ParagraphNum": "４",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "企画官は、命を受けて、消費者安全課の所掌事務のうち特定事項の調査、企画及び立案を行う。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "6",
                "ArticleCaption": "（統括消費者取引対策官）",
                "ArticleTitle": "第六条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "取引対策課に、統括消費者取引対策官一人を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "統括消費者取引対策官は、命を受けて、特定商取引に関する法律（昭和五十一年法律第五十七号）の規定による購入者等（同法第一条第一項に規定するものをいう。）の利益の保護に関する事務のうち命令等に関する事務を行う。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
