                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "公益的施設の整備計画"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "特定業務施設の整備計画"
                    }
                  },
                  {
                    "@Num": "7",
                    "ItemTitle": "七",
                    "ItemSentence": {
                      "Sentence": "附帯事業の概要"
                    }
