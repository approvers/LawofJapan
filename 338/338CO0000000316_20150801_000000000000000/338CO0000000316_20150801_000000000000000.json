{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "316",
    "@Year": "38",
    "LawNum": "昭和三十八年政令第三百十六号",
    "LawBody": {
      "LawTitle": "日本消防検定協会に対する出資の目的とする土地等の評価に関する政令",
      "EnactStatement": "内閣は、消防法の一部を改正する法律（昭和三十八年法律第八十八号）附則第六条第三項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（評価委員の任命）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "消防法の一部を改正する法律（以下「法」という。）附則第六条第二項の評価委員は、必要のつど、自治大臣が次の各号に掲げる者のうちからそれぞれ一人ずつ任命する。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "大蔵省の職員"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "自治省の職員"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "日本消防検定協会の役員（日本消防検定協会が成立するまでの間は、法附則第三条第一項の設立委員）"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "学識経験がある者"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（評価額の決定）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "評価額は、評価委員の過半数の一致によつて定める。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（自治省令への委任）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前二条に定めるもののほか、評価委員その他評価に関し必要な事項は、自治省令で定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、公布の日から施行する。"
            }
          }
        }
      }
    }
  }
}
