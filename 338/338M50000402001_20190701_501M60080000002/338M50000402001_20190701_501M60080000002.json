            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この命令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四三年七月二〇日総理府・通商産業省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
