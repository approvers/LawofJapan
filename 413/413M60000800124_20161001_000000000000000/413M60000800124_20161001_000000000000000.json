{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "124",
    "@Year": "13",
    "LawNum": "平成十三年国土交通省令第百二十四号",
    "LawBody": {
      "LawTitle": "運輸安全委員会設置法施行規則",
      "EnactStatement": "航空・鉄道事故調査委員会設置法（昭和四十八年法律第百十三号）第二条の二第四項及び第五項の規定に基づき、航空・鉄道事故調査委員会設置法第二条の二第四項の国土交通省令で定める重大な事故及び同条第五項の国土交通省令で定める事態を定める省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（法第二条第三項の国土交通省令で定める重大な事故）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "運輸安全委員会設置法（昭和四十八年法律第百十三号。以下「法」という。）第二条第三項の国土交通省令で定める重大な事故は、次に掲げる事故とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "鉄道事故等報告規則（昭和六十二年運輸省令第八号。以下「規則」という。）第三条第一項第一号から第三号までに掲げる事故（同項第二号に掲げる事故にあっては、作業中の除雪車に係るものを除く。）"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "規則第三条第一項第四号から第六号までに掲げる事故であって、次に掲げるもの"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "乗客、乗務員等に死亡者を生じたもの"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "五人以上の死傷者を生じたもの（死亡者を生じたものに限る。）"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "踏切遮断機が設置されていない踏切道において発生したものであって、死亡者を生じたもの"
                      }
                    },
                    {
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": "鉄道係員の取扱い誤り又は車両若しくは鉄道施設の故障、損傷、破壊等に原因があるおそれがあると認められるものであって、死亡者を生じたもの"
                      }
                    }
                  ]
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "規則第三条第一項第二号及び第四号から第七号までに掲げる事故であって、特に異例と認められるもの"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "専用鉄道において発生した規則第三条第一項第一号から第七号までに掲げる事故に準ずるものであって、特に異例と認められるもの"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "軌道において発生した第一号から第三号までに掲げる事故に準ずるものとして運輸安全委員会が告示で定めるもの"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（法第二条第四項第二号の国土交通省令で定める事態）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二条第四項第二号の国土交通省令で定める事態は、次に掲げる事態とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "規則第四条第一項第一号に掲げる事態であって、同号に規定する区間に他の列車又は車両が存在したもの"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "規則第四条第一項第二号に掲げる事態であって、同号に規定する進路に列車が進入したもの"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "規則第四条第一項第三号に掲げる事態であって、同号に規定する進路の区間を防護する信号機の防護区域に他の列車又は車両が進入したもの"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "規則第四条第一項第七号に掲げる事態であって、列車の衝突、脱線又は火災が発生する危険性が特に著しい故障、損傷、破壊等が生じたもの"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "規則第四条第一項第八号に掲げる事態であって、列車の衝突、脱線又は火災が発生する危険性が特に著しい故障、損傷、破壊等が生じたもの"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "規則第四条第一項第一号から第十号までに掲げる事態であって、特に異例と認められるもの"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "軌道において発生した前各号に掲げる事態に準ずるものとして運輸安全委員会が告示で定めるもの"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（法第二条第六項第二号の国土交通省令で定める事態）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二条第六項第二号の国土交通省令で定める事態は、次に掲げる事態とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "次に掲げる事由により、船舶が運航不能となった事態"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "航行に必要な設備の故障"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "船体の傾斜"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "機関の運転に必要な燃料又は清水の不足"
                      }
                    }
                  ]
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "船舶が乗り揚げたもののその船体に損傷を生じなかった事態"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "前二号に掲げるもののほか、船舶の安全又は運航が阻害された事態"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十三年十月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年九月一日国土交通省令第七七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令は、平成二十年十月一日から施行する。"
                }
              }
            },
            {
              "@Num": "2",
              "ArticleCaption": "（海難審判庁事務章程等の廃止）",
              "ArticleTitle": "第二条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "次に掲げる省令は、廃止する。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "海難審判庁事務章程（昭和二十三年運輸省令第九号）"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "海難審判庁の裁決書の謄本等交付手数料に関する規則（昭和二十三年／総理庁令／運輸省令／第九号）"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "海事補佐人登録規則（昭和二十三年／総理庁令／運輸省令／第十二号）"
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成二六年三月二八日国土交通省令第三五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成二十六年四月一日から施行し、第一条の規定による改正後の鉄道事故等報告規則の規定は、同日以後に発生した同令第一条に規定する事故に関する報告について適用する。"
            }
          }
        }
      ]
    }
  }
}
