{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "41",
    "@Year": "13",
    "LawNum": "平成十三年政令第四十一号",
    "LawBody": {
      "LawTitle": "電子署名及び認証業務に関する法律施行令",
      "EnactStatement": "内閣は、電子署名及び認証業務に関する法律（平成十二年法律第百二号）第七条第一項（同法第十五条第二項において準用する場合を含む。）、第二十二条第一項（同法第三十一条第六項において準用する場合を含む。）並びに第三十六条第一項及び第二項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（特定認証業務に係る認定の有効期間）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "電子署名及び認証業務に関する法律（以下「法」という。）第七条第一項（法第十五条第二項において準用する場合を含む。）の政令で定める期間は、一年とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（指定調査機関の指定等の有効期間）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第二十二条第一項（法第三十一条第六項において準用する場合を含む。）の政令で定める期間は、五年とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（認定等の申請に係る手数料の額）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第三十六条第一項各号に掲げる者が同項の規定により国に納めなければならない手数料の額は、次の各号に掲げる場合に応じ、それぞれ当該各号に定める額とする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "主務大臣が法第十七条第一項の指定調査機関に同項の規定による調査の全部を行わせる場合"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "イ又はロに掲げる者の区分に応じ、それぞれイ又はロに定める額"
                          }
                        }
                      ]
                    },
                    "Subitem1": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "Subitem1Title": "イ",
                        "Subitem1Sentence": {
                          "Column": [
                            {
                              "@LineBreak": "false",
                              "@Num": "1",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "法第七条第一項（法第十五条第二項において準用する場合を含む。）の認定の更新を受けようとする者"
                              }
                            },
                            {
                              "@LineBreak": "false",
                              "@Num": "2",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "一万三百円"
                              }
                            }
                          ]
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "Subitem1Title": "ロ",
                        "Subitem1Sentence": {
                          "Column": [
                            {
                              "@LineBreak": "false",
                              "@Num": "1",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "法第九条第一項（法第十五条第二項において準用する場合を含む。）の変更の認定を受けようとする者"
                              }
                            },
                            {
                              "@LineBreak": "false",
                              "@Num": "2",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "五千六百円"
                              }
                            }
                          ]
                        }
                      }
                    ]
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "主務大臣が法第十七条第一項の指定調査機関に同項の規定による調査の全部を行わせない場合"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "別に政令で定める額"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "行政手続等における情報通信の技術の利用に関する法律（平成十四年法律第百五十一号）第三条第一項の規定により同項に規定する電子情報処理組織を使用して認定又はその更新の申請を行う場合における前項の規定の適用については、同項第一号中「一万三百円」とあるのは「九千九百円」と、「五千六百円」とあるのは「五千二百円」とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（指定調査機関が行う調査に係る手数料の額の認可）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "法第三十六条第二項の規定による認可を受けようとする指定調査機関は、認可を受けようとする手数料の額及び調査の業務の実施に要する費用の額に関し主務省令で定める事項を記載した申請書を主務大臣に提出しなければならない。"
                    },
                    {
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "手数料の額の変更の認可を受けようとするときも、同様とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "主務大臣は、次の各号のいずれにも適合すると認めるときでなければ、前項の認可をしてはならない。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "手数料の額が当該調査の業務の適正な実施に要する費用の額を超えないこと。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "特定の者に対して不当な差別的取扱いをするものでないこと。"
                      }
                    }
                  }
                ]
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十三年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年一月三〇日政令第一一号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十六年三月二十九日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一八年三月三一日政令第一二八号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十八年四月一日から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
