    "LawNum": "平成十三年法務省令第五号",
    "LawBody": {
      "LawTitle": "婦人補導院組織規則",
      "EnactStatement": "法務省設置法（平成十一年法律第九十三号）第十二条第三項の規定に基づき、及び同法を実施するため、婦人補導院組織規程の全部を改正する命令を次のように定める。",
      "Preamble": {
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "婦人補導院組織規程の全部を改正する命令"
            }
          },
          {
            "@Num": "2",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "婦人補導院組織規程（昭和三十三年法務省令第三十三号）の全部を次のように改正する。"
            }
          }
        ]
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（名称及び位置）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "婦人補導院の名称及び位置は、別表のとおりとする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（院長）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "婦人補導院に、院長を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "院長は、婦人補導院の事務を掌理する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（婦人補導院に置く課）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "婦人補導院に、補導課を置く。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（補導課の所掌事務）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "補導課は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "公文書類の接受、発送、編集及び保存に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "人事に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "経理に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "統計に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "入院、退院及び仮退院に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "資質及び環境の調査並びに分類に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "処遇に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "補導に関すること。"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "給養に関すること。"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "保健、衛生、防疫、医療及び薬剤に関すること。"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "面会及び通信に関すること。"
                  }
                },
                {
                  "@Num": "12",
                  "ItemTitle": "十二",
                  "ItemSentence": {
                    "Sentence": "保安に関すること。"
                  }
                },
                {
                  "@Num": "13",
                  "ItemTitle": "十三",
                  "ItemSentence": {
                    "Sentence": "領置に関すること。"
                  }
                },
                {
                  "@Num": "14",
                  "ItemTitle": "十四",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、婦人補導院の所掌に属するものに関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令に定めるもののほか、婦人補導院に関し必要な事項は、院長が定める。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "院長は、前項の規定に基づき、事務分掌その他組織の細目を定めようとするときは、法務大臣の承認を受けなければならない。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この中央省庁等改革推進本部令（次項において「本部令」という。）は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          },
          {
            "@Num": "2",
            "ParagraphCaption": "（この本部令の効力）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": "この本部令は、その施行の日に、婦人補導院組織規則（平成十三年法務省令第五号）となるものとする。"
            }
          }
        ]
      },
