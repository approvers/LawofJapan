{
  "Law": {
    "@DataInfo": "190416e04k03m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "297",
    "@Year": "13",
    "LawNum": "平成十三年政令第二百九十七号",
    "LawBody": {
      "LawTitle": "自動車検査独立行政法人の設立に伴う関係政令の整備及び経過措置に関する政令　抄",
      "EnactStatement": "内閣は、自動車検査独立行政法人法（平成十一年法律第二百十八号）第十五条第四項並びに附則第二条第一項及び第二項、第五条第一項、第二項及び第四項並びに第六条並びに関係法律の規定に基づき、この政令を制定する。",
      "TOC": {
        "TOCLabel": "目次",
        "TOCChapter": [
          {
            "@Delete": "false",
            "@Num": "1",
            "ChapterTitle": "第一章　関係政令の整備",
            "ArticleRange": "（第一条―第十二条）"
          },
          {
            "@Delete": "false",
            "@Num": "2",
            "ChapterTitle": "第二章　経過措置",
            "ArticleRange": "（第十三条―第十七条）"
          }
        ],
        "TOCSupplProvision": {
          "SupplProvisionLabel": "附則"
        }
      },
      "MainProvision": {
        "Chapter": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "2",
          "ChapterTitle": "第二章　経過措置",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "13",
              "ArticleCaption": "（職員の引継ぎに係る政令で定める部局又は機関）",
              "ArticleTitle": "第十三条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "自動車検査独立行政法人法（以下「法」という。）附則第二条第一項の政令で定める国土交通省の部局又は機関は、次に掲げる部局又は機関とする。"
                    }
                  },
                  "Item": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "ItemTitle": "一",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "自動車交通局総務課及び技術安全部技術企画課"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "地方運輸局総務部及び整備部"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "ItemTitle": "三",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "陸運支局及びその事務所"
                        }
                      }
                    }
                  ]
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第二条第二項の政令で定める内閣府の部局又は機関は、次に掲げる部局又は機関とする。"
                    }
                  },
                  "Item": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "ItemTitle": "一",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "沖縄総合事務局運輸部"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "沖縄総合事務局の事務所（陸運支局において所掌することとされている事務を分掌するものに限る。）及びその支所"
                        }
                      }
                    }
                  ]
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "14",
              "ArticleCaption": "（自動車検査独立行政法人の成立の時において承継される権利及び義務）",
              "ArticleTitle": "第十四条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第五条第一項の政令で定める権利及び義務は、次に掲げる権利及び義務とする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国土交通大臣の所管に属する土地、建物及び工作物（その土地に定着する物及びその建物に附属する工作物を含む。次条第一号において「土地等」という。）のうち国土交通大臣が財務大臣に協議して指定するものに関する権利及び義務"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国土交通大臣の所管に属する物品のうち国土交通大臣が指定するものに関する権利及び義務"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "法第十一条に規定する業務に関し国が有する権利及び義務のうち前二号に掲げるもの以外のものであって、国土交通大臣が指定するもの"
                      }
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "15",
              "ArticleCaption": "（権利及び義務の承継の際出資があったものとされる財産）",
              "ArticleTitle": "第十五条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第五条第二項の政令で定める財産は、次に掲げる財産とする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "前条第一号の規定により指定された土地等"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "前条第三号の規定により指定された権利に係る財産のうち国土交通大臣が指定するもの"
                      }
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "16",
              "ArticleCaption": "（出資があったものとされる財産に係る評価委員の任命等）",
              "ArticleTitle": "第十六条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第五条第三項の評価委員は、次に掲げる者につき国土交通大臣が任命する。"
                    }
                  },
                  "Item": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "ItemTitle": "一",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "財務省の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "国土交通省の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "ItemTitle": "三",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "自動車検査独立行政法人（以下「検査法人」という。）の役員（検査法人が成立するまでの間は、検査法人に係る独立行政法人通則法（平成十一年法律第百三号。以下「通則法」という。）第十五条第一項の設立委員）"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "ItemTitle": "四",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "学識経験のある者"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "二人"
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第五条第三項の規定による評価は、同項の評価委員の過半数の一致によるものとする。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "3",
                  "ParagraphNum": "３",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第五条第三項の規定による評価に関する庶務は、国土交通省自動車交通局技術安全部技術企画課において処理する。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "17",
              "ArticleCaption": "（国有財産の無償使用）",
              "ArticleTitle": "第十七条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第六条に規定する政令で定める国有財産は、検査法人の成立の際現に専ら第十三条第一項第三号及び同条第二項第二号に規定する部局又は機関に使用されている庁舎等（国の庁舎等の使用調整等に関する特別措置法（昭和三十二年法律第百十五号）第二条第二項に規定する庁舎等をいう。）とする。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前項の国有財産については、通則法第十四条第一項の規定により指名を受けた検査法人の長となるべき者が検査法人の成立前に申請したときに限り、検査法人に対し、無償で使用させることができる。"
                    }
                  }
                }
              ]
            }
          ]
        }
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": [
              {
                "@Function": "main",
                "@Num": "1",
                "@WritingMode": "vertical",
                "#text": "この政令は、法附則第一条ただし書に規定する規定の施行の日（平成十四年七月一日）から施行する。"
              },
              {
                "@Function": "proviso",
                "@Num": "2",
                "@WritingMode": "vertical",
                "#text": "ただし、第十一条及び第十三条から第十七条までの規定は、公布の日から施行する。"
              }
            ]
          }
        }
      }
    }
  }
}
