            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、平成十三年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年六月三〇日厚生労働省令第一〇七号",
