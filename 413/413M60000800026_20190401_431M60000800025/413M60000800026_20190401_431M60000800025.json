                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "第三項に規定するもののほか、航空交通管理管制官のうちから国土交通大臣が指名する者を次席航空交通管理管制官とする。"
                  }
                }
              },
