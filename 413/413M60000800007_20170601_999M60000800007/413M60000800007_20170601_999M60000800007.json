{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "7",
    "@Year": "13",
    "LawNum": "平成十三年国土交通省令第七号",
    "LawBody": {
      "LawTitle": "国土交通政策研究所組織規則",
      "EnactStatement": "国土交通省組織令（平成十二年政令第二百五十五号）第百九十二条第二項の規定に基づき、及び同令を実施するため、国土交通政策研究所組織規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（国土交通政策研究所の位置）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国土交通政策研究所は、東京都に置く。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（所長及び副所長）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国土交通政策研究所に、所長及び副所長一人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "所長は、国土交通政策研究所の事務を掌理する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "副所長は、所長を助け、国土交通政策研究所の事務を整理する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（研究調整官）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国土交通政策研究所に、研究調整官二人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "研究調整官は、命を受けて、国土交通政策研究所の所掌事務に関する重要な調査及び研究の企画及び立案並びに調整に関する事務をつかさどる。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（総括主任研究官）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国土交通政策研究所に、総括主任研究官二人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "総括主任研究官は、命を受けて、国土交通省の所掌事務に係る政策に関する基礎的な調査及び研究に関する事務を行い、並びに主任研究官の行う事務を統括する。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": "（主任研究官及び研究官）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国土交通政策研究所に、主任研究官六人以内及び研究官十二人以内を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "主任研究官は、命を受けて、国土交通省の所掌事務に係る政策に関する基礎的な調査及び研究に関する事務を行い、並びに研究官の行う事務を整理する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "研究官は、命を受けて、国土交通省の所掌事務に係る政策に関する基礎的な調査及び研究に関する事務を行う。"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（国土交通政策研究所に置く課）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国土交通政策研究所に、総務課を置く。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（総務課の所掌事務）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務課は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "職員の任免、給与、懲戒、服務その他の人事に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "所長の官印及び所印の保管に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "公文書類の接受、発送、編集及び保存に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "経費及び収入の予算、決算及び会計に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "職員の衛生、医療その他の福利厚生に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "情報システムの整備及び管理に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、国土交通政策研究所の所掌事務で他の所掌に属しないものに関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "8",
            "ArticleCaption": "（客員研究官）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国土交通政策研究所に、客員研究官を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "客員研究官は、命を受けて、国土交通省の所掌事務に係る政策に関する基礎的な調査及び研究に参画する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "客員研究官は、非常勤とする。"
                }
              }
            ]
          },
          {
            "@Num": "9",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令に定めるもののほか、国土交通政策研究所に関し必要な事項は、所長が定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この中央省庁等改革推進本部令（次項において「本部令」という。）は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（この本部令の効力）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この本部令は、その施行の日に、国土交通政策研究所組織規則（平成十三年国土交通省令第七号）となるものとする。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一四年四月一日国土交通省令第四〇号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二一年三月三〇日国土交通省令第一六号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、国家公務員法等の一部を改正する法律附則第一条第三号に掲げる規定の施行の日（平成二十一年四月一日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
