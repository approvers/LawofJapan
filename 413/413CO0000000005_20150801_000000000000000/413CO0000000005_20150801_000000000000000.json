{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "5",
    "@Year": "13",
    "LawNum": "平成十三年政令第五号",
    "LawBody": {
      "LawTitle": "下請代金支払遅延等防止法施行令",
      "EnactStatement": "内閣は、下請代金支払遅延等防止法（昭和三十一年法律第百二十号）第三条第二項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（法第二条第七項第一号の政令で定める情報成果物及び役務）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "下請代金支払遅延等防止法（以下「法」という。）第二条第七項第一号の政令で定める情報成果物は、プログラムとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二条第七項第一号の政令で定める役務は、次に掲げるものとする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "運送"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "物品の倉庫における保管"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "情報処理"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（情報通信の技術を利用する方法）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "親事業者は、法第三条第二項の規定により同項に規定する事項を提供しようとするときは、公正取引委員会規則で定めるところにより、あらかじめ、当該下請事業者に対し、その用いる同項前段に規定する方法（以下「電磁的方法」という。）の種類及び内容を示し、書面又は電磁的方法による承諾を得なければならない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "前項の規定による承諾を得た親事業者は、当該下請事業者から書面又は電磁的方法により電磁的方法による提供を受けない旨の申出があったときは、当該下請事業者に対し、法第三条第二項に規定する事項の提供を電磁的方法によってしてはならない。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、当該下請事業者が再び前項の規定による承諾をした場合は、この限りでない。"
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、書面の交付等に関する情報通信の技術の利用のための関係法律の整備に関する法律（平成十二年法律第百二十六号）の施行の日（平成十三年四月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年一〇月三日政令第四五二号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、下請代金支払遅延等防止法の一部を改正する法律の施行の日（平成十六年四月一日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
