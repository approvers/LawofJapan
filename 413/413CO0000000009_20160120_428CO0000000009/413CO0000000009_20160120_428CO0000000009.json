{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "9",
    "@Year": "13",
    "LawNum": "平成十三年政令第九号",
    "LawBody": {
      "LawTitle": "財政融資資金の長期運用に対する特別措置に関する法律第五条第二項第三号に規定する法人を定める政令",
      "EnactStatement": "内閣は、財政融資資金の長期運用に対する特別措置に関する法律（昭和四十八年法律第七号）第六条第二項第三号の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "財政融資資金の長期運用に対する特別措置に関する法律第五条第二項第三号の政令で定める法人は、次に掲げるものとする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "民間都市開発の推進に関する特別措置法（昭和六十二年法律第六十二号）第三条第一項に規定する民間都市開発推進機構"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "中部国際空港の設置及び管理に関する法律（平成十年法律第三十六号）第四条第二項に規定する指定会社"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "株式会社日本政策投資銀行"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "4",
              "ItemTitle": "四",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "株式会社民間資金等活用事業推進機構"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "5",
              "ItemTitle": "五",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "株式会社海外交通・都市開発事業支援機構"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "6",
              "ItemTitle": "六",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "株式会社海外通信・放送・郵便事業支援機構"
                }
              }
            }
          ]
        }
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令は、平成十三年四月一日から施行する。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ArticleCaption": "（経過措置）",
              "ArticleTitle": "第二条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "財政融資資金の長期運用に対する特別措置に関する法律第五条第二項第三号の政令で定める法人は、本則各号に掲げるもののほか、当分の間、成田国際空港株式会社及び高速道路株式会社法（平成十六年法律第九十九号）第一条に規定する会社とする。"
                  }
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一五年九月二五日政令第四四三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法第三条の規定の施行の日（平成十五年十月二日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年三月一九日政令第五〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この政令は、公布の日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、附則第九条から第四十四条までの規定は、平成十六年四月一日から施行する。"
                  }
                ]
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年六月一日政令第二〇三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
