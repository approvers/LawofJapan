                    "ItemSentence": {
                      "Sentence": "受入移送犯罪の名称、犯数、共助刑の種類及び刑期"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "共助刑の執行の状況"
                    }
                  },
                  {
                    "@Num": "5",
