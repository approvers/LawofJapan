                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "学識経験のある者"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
