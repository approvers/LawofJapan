          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、平成十五年四月一日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（経過措置）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令の施行の際現に交付されている児童扶養手当証書は、この省令による児童扶養手当証書とみなす。"
              }
            }
          ]
