{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "011",
    "@Year": "15",
    "LawNum": "平成十五年内閣府令第十一号",
    "LawBody": {
      "LawTitle": "構造改革特別区域法施行規則",
      "EnactStatement": "構造改革特別区域法（平成十四年法律第百八十九号）第四条第一項、第六条第一項及び第三十八条の規定に基づき、構造改革特別区域法施行規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（構造改革特別区域計画の認定の申請）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "構造改革特別区域法（以下「法」という。）第四条第一項の規定により認定の申請をしようとする地方公共団体は、別記様式第一による申請書に次に掲げる図書を添えて、これらを内閣総理大臣に提出しなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "構造改革特別区域に含まれる行政区画を表示した図面又は縮尺、方位、目標となる地物及び構造改革特別区域を表示した付近見取図"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "規制の特例措置の適用を受ける主体の特定の状況を明らかにすることができる書類"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "構造改革特別区域計画の工程表及びその内容を説明した文書"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "法第四条第四項の規定により聴いた意見の概要"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "法第四条第五項の規定による提案を踏まえた認定の申請をする場合にあっては、当該提案の概要"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、内閣総理大臣が必要と認める事項を記載した書類"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（構造改革特別区域計画の変更の認定の申請）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六条第一項の規定により構造改革特別区域計画の変更の認定を受けようとする地方公共団体は、別記様式第二による申請書に前条各号に掲げる図書のうち当該計画の変更に伴いその内容が変更されるものであってその変更後のものを添えて、これらを内閣総理大臣に提出しなければならない。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（法第六条第一項の内閣府令で定める軽微な変更）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六条第一項の内閣府令で定める軽微な変更は、次に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "地域の名称の変更又は地番の変更に伴う範囲の変更"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "規制の特例措置の適用の開始の日の変更であってその変更が六月以内のもの"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "前二号に掲げるもののほか、構造改革特別区域計画の実施に支障がないと内閣総理大臣が認める変更"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（訓令又は通達に関する措置）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法附則第五条に規定する措置に基づき、構造改革特別区域基本方針（法第三条第一項に規定する構造改革特別区域基本方針をいう。）に定める訓令又は通達の特例に関する措置の適用を受けようとする場合に法第四条第一項及び法第六条第一項の規定に準じて行う手続は、前三条の規定に準ずるものとする。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、平成十五年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年三月三一日内閣府令第三三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二三年一一月二九日内閣府令第六七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、地域の自主性及び自立性を高めるための改革の推進を図るための関係法律の整備に関する法律附則第一条第一号に掲げる規定の施行の日（平成二十三年十一月三十日）から施行する。"
            }
          }
        }
      ],
