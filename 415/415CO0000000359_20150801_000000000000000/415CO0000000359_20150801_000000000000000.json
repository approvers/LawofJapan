{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "359",
    "@Year": "15",
    "LawNum": "平成十五年政令第三百五十九号",
    "LawBody": {
      "LawTitle": "独立行政法人北方領土問題対策協会法施行令",
      "EnactStatement": "内閣は、独立行政法人北方領土問題対策協会法（平成十四年法律第百三十二号）第十条第六項及び第十八条並びに附則第二条第六項及び第九項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（評議員の任期等）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "評議員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、補欠の評議員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "評議員は、再任されることができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "次の各号に掲げる者のうちから任命される評議員は、それぞれ当該各号に定める数とする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "独立行政法人北方領土問題対策協会法（以下「法」という。）第十条第五項に規定する学識経験を有する者"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "七人以内"
                        }
                      }
                    ]
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "北方地域旧漁業権者等"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "八人以内"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（評議員会の会議）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "評議員会は、理事長が招集する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "理事長は、評議員の総数の三分の一以上の評議員が審議すべき事項を示して評議員会の招集を請求したときは、その請求のあった日から三十日以内に、評議員会を招集しなければならない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "評議員会に議長を置き、評議員の互選によってこれを定める。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "議長は、会務を総理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "評議員会は、評議員の過半数が出席しなければ、議事を開き、議決をすることができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "評議員会の議事は、出席した評議員の過半数をもって決し、可否同数のときは、議長の決するところによる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "7",
                "ParagraphNum": "７",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前各項に定めるもののほか、評議員会の会議に関し必要な事項は、評議員会が定める。"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "この政令は、公布の日から施行する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、附則第四条から第十条までの規定は、平成十五年十月一日から施行する。"
                    }
                  ]
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ArticleCaption": "（協会が承継する資産に係る評価委員の任命等）",
              "ArticleTitle": "第二条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第二条第五項の評価委員は、次に掲げる者につき内閣総理大臣が任命する。"
                    }
                  },
                  "Item": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "ItemTitle": "一",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "内閣府の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "財務省の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "ItemTitle": "三",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "農林水産省の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "ItemTitle": "四",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "協会の役員（協会が成立するまでの間は、協会に係る独立行政法人通則法（平成十一年法律第百三号）第十五条第一項の設立委員）"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "5",
                      "ItemTitle": "五",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "学識経験のある者"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第二条第五項の規定による評価は、同項の評価委員の過半数の一致によるものとする。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "3",
                  "ParagraphNum": "３",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第二条第五項の規定による評価に関する庶務は、内閣府北方対策本部において処理する。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ArticleCaption": "（北方領土問題対策協会の解散の登記の嘱託等）",
              "ArticleTitle": "第三条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第二条第一項の規定により北方領土問題対策協会が解散したときは、内閣総理大臣及び農林水産大臣は、遅滞なく、その解散の登記を登記所に嘱託しなければならない。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "登記官は、前項の規定による嘱託に係る解散の登記をしたときは、その登記用紙を閉鎖しなければならない。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "4",
              "ArticleCaption": "（北方領土問題対策協会法施行令の廃止）",
              "ArticleTitle": "第四条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "北方領土問題対策協会法施行令（昭和四十四年政令第二百四十六号）は、廃止する。"
                  }
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一七年四月一日政令第一一八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、公布の日から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
