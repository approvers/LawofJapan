{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "19",
    "@Year": "15",
    "LawNum": "平成十五年政令第十九号",
    "LawBody": {
      "LawTitle": "貨幣回収準備資金に関する法律施行令",
      "EnactStatement": "内閣は、貨幣回収準備資金に関する法律（平成十四年法律第四十二号）第三条第二項、第六条及び第十三条第一項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（資金の管理事務の委任）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "財務大臣は、貨幣回収準備資金（以下「資金」という。）の管理に関する事務の一部を、独立行政法人造幣局の事務所の所在地を管轄する財務局長に委任することができる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（一般会計からの資金への繰入額の算定基準）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "貨幣回収準備資金に関する法律（以下「法」という。）第六条に規定する政令で定める額は、毎会計年度末における貨幣の流通額の百分の五に相当する金額、日本銀行の保管に係る貨幣の額面額に相当する金額及び資金に属する地金（政府において引き換え、又は回収した貨幣を含む。）の価額に相当する金額の合計額とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（資金の増減及び現在額計算書の作成）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十三条第一項に規定する資金の増減及び現在額の計算書は、翌年度の七月三十一日までに作成するものとする。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、平成十五年四月一日から施行する。"
            }
          }
        }
      }
    }
  }
}
