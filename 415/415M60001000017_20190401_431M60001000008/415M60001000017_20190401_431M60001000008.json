                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "水俣病に関する医学的調査及び研究に必要な情報の収集及び整理に関すること。"
                    }
                  }
                }
              ]
            }
