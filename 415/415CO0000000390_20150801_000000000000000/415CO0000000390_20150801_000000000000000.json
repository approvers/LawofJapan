{
  "Law": {
    "@DataInfo": "190620e04k03m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "390",
    "@Year": "15",
    "LawNum": "平成十五年政令第三百九十号",
    "LawBody": {
      "LawTitle": "独立行政法人農業技術研究機構法の一部を改正する法律の施行に伴う関係政令の整備及び経過措置に関する政令　抄",
      "EnactStatement": "独立行政法人農業技術研究機構法の一部を改正する法律の施行に伴う関係政令の整備及び経過措置に関する政令内閣は、独立行政法人農業技術研究機構法の一部を改正する法律（平成十四年法律第百二十九号）附則第四条第十一項及び第十四項並びに第十二条並びに関係法律の規定に基づき、この政令を制定する。",
      "TOC": {
        "TOCLabel": "目次",
        "TOCChapter": [
          {
            "@Delete": "false",
            "@Num": "1",
            "ChapterTitle": "第一章　関係政令の整備",
            "ArticleRange": "（第一条―第十七条）"
          },
          {
            "@Delete": "false",
            "@Num": "2",
            "ChapterTitle": "第二章　経過措置",
            "ArticleRange": "（第十八条―第二十条）"
          }
        ],
        "TOCSupplProvision": {
          "SupplProvisionLabel": "附則"
        }
      },
      "MainProvision": {
        "Chapter": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ChapterTitle": "第一章　関係政令の整備",
            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ArticleCaption": "（生物系特定産業技術研究推進機構法第二条第三号の業種を定める政令の廃止）",
                "ArticleTitle": "第一条",
                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "生物系特定産業技術研究推進機構法第二条第三号の業種を定める政令（昭和六十一年政令第二百八十号）は、廃止する。"
                    }
                  }
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "8",
                "ArticleCaption": "（特許法施行令及び商標法施行令の一部改正に伴う経過措置）",
                "ArticleTitle": "第八条",
                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "独立行政法人農業・生物系特定産業技術研究機構（以下「研究機構」という。）は、次の各号に掲げる特許料、割増特許料、手数料、登録料及び割増登録料の納付については、それぞれ当該各号に定める規定の政令で定める独立行政法人とみなす。"
                    }
                  },
                  "Item": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "ItemTitle": "一",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "第六条の規定の施行前に独立行政法人農業技術研究機構（次号及び第三号において「農研機構」という。）がした特許出願又は国際出願（特許協力条約に基づく国際出願等に関する法律（昭和五十三年法律第三十号）第二条に規定する国際出願をいう。）に係る特許料、割増特許料及び手数料"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "特許法（昭和三十四年法律第百二十一号）第百七条第二項"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "第六条の規定の施行前に農研機構がした実用新案登録出願に係る登録料、割増登録料及び手数料"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "実用新案法（昭和三十四年法律第百二十三号）第三十一条第二項"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "ItemTitle": "三",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "前条の規定の施行前に農研機構がした商標登録出願及び商標権の存続期間の更新登録の申請に係る登録料、割増登録料及び手数料"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "商標法（昭和三十四年法律第百二十七号）第四十条第三項（同法第四十一条の二第五項において準用する場合を含む。）"
                            }
                          }
                        ]
                      }
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ChapterTitle": "第二章　経過措置",
            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "18",
                "ArticleCaption": "（評価委員の任命等）",
                "ArticleTitle": "第十八条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "独立行政法人農業技術研究機構法の一部を改正する法律（以下「改正法」という。）附則第四条第十項の評価委員は、次に掲げる者につき農林水産大臣が任命する。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@LineBreak": "false",
                              "@Num": "1",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "財務省の職員"
                              }
                            },
                            {
                              "@LineBreak": "false",
                              "@Num": "2",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "二人"
                              }
                            }
                          ]
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@LineBreak": "false",
                              "@Num": "1",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "農林水産省の職員"
                              }
                            },
                            {
                              "@LineBreak": "false",
                              "@Num": "2",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "一人"
                              }
                            }
                          ]
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@LineBreak": "false",
                              "@Num": "1",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "研究機構の役員"
                              }
                            },
                            {
                              "@LineBreak": "false",
                              "@Num": "2",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "一人"
                              }
                            }
                          ]
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@LineBreak": "false",
                              "@Num": "1",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "学識経験のある者"
                              }
                            },
                            {
                              "@LineBreak": "false",
                              "@Num": "2",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "二人"
                              }
                            }
                          ]
                        }
                      }
                    ]
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "改正法附則第四条第十項の規定による評価は、同項の評価委員の過半数の一致によるものとする。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "改正法附則第四条第十項の規定による評価に関する庶務は、農林水産技術会議の事務局において処理する。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "19",
                "ArticleCaption": "（生物系特定産業技術研究推進機構の解散の登記の嘱託等）",
                "ArticleTitle": "第十九条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "改正法附則第四条第一項の規定により生物系特定産業技術研究推進機構（次条において「推進機構」という。）が解散したときは、農林水産大臣は、遅滞なく、その解散の登記を登記所に嘱託しなければならない。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "登記官は、前項の規定による嘱託に係る解散の登記をしたときは、その登記用紙を閉鎖しなければならない。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "20",
                "ArticleCaption": "（独立行政法人等の保有する情報の公開に関する法律の適用に関する経過措置）",
                "ArticleTitle": "第二十条",
                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "改正法の施行前に独立行政法人等の保有する情報の公開に関する法律（平成十三年法律第百四十号）の規定に基づき推進機構がした行為及び推進機構に対してされた行為は、同法第二条第二項に規定する法人文書の開示に関する同法の規定の適用については、研究機構がした行為及び研究機構に対してされた行為とみなす。"
                    }
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、平成十五年十月一日から施行する。"
            }
          }
        }
      }
    }
  }
}
