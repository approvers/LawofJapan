{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "355",
    "@Year": "15",
    "LawNum": "平成十五年政令第三百五十五号",
    "LawBody": {
      "LawTitle": "特殊開錠用具の所持の禁止等に関する法律施行令",
      "EnactStatement": "内閣は、特殊開錠用具の所持の禁止等に関する法律（平成十五年法律第六十五号）第二条第二号及び第三号並びに第七条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（特殊開錠用具）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "特殊開錠用具の所持の禁止等に関する法律（以下「法」という。）第二条第二号の政令で定める器具は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "ピッキング用具（法第二条第二号に規定するピッキング用具をいう。）"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "破壊用シリンダー回し（特定の型式の建物錠のシリンダーに挿入して強制的に回転させることによりこれを破壊するための器具をいう。）"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "ホールソー（ドリルに取り付けて用いる筒状ののこぎりをいう。）のシリンダー用軸（特定の型式の建物錠のシリンダーに挿入して用いるための軸をいう。）"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "サムターン回し（建物錠が設けられている戸の外側から挿入して当該建物錠のサムターン（かんぬきの開閉を行うためのつまみをいう。以下同じ。）を回転させるための器具をいう。）"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（指定侵入工具）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第二条第三号の政令で定める工具は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "次のいずれにも該当するドライバー"
                    }
                  },
                  "Subitem1": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "先端部が平らで、その幅が〇・五センチメートル以上であること。"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "長さ（専用の柄を取り付けることができるものにあっては、柄を取り付けたときの長さ）が十五センチメートル以上であること。"
                        }
                      }
                    }
                  ]
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "次のいずれにも該当するバール"
                    }
                  },
                  "Subitem1": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "作用する部分のいずれかの幅が二センチメートル以上であること。"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "長さが二十四センチメートル以上であること。"
                        }
                      }
                    }
                  ]
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "ドリル（直径一センチメートル以上の刃が附属するものに限る。）"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（指定建物錠）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第七条の政令で定める建物錠は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "シリンダー錠"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "シリンダー"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "サムターン"
                    }
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "true",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Article": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "1",
          "ArticleCaption": "（施行期日）",
          "ArticleTitle": "第一条",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法の施行の日（平成十五年九月一日）から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、第三条の規定は、法第七条の規定の施行の日から施行する。"
                }
              ]
            }
          }
        }
      }
    }
  }
}
