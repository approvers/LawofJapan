              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成二十年十月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年九月一八日政令第三三五号",
          "@Type": "Amend",
