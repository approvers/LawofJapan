            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十五年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年七月二日政令第二一四号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
