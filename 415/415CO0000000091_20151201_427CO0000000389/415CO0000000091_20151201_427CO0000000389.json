            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成二十年七月四日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年一一月二〇日政令第三八九号",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
