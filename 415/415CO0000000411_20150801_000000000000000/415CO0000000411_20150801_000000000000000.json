            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年七月二八日政令第二四七号",
