{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "273",
    "@Year": "15",
    "LawNum": "平成十五年政令第二百七十三号",
    "LawBody": {
      "LawTitle": "食品安全委員会令",
      "EnactStatement": "内閣は、食品安全基本法（平成十五年法律第四十八号）第二十四条第一項第十三号及び第三十八条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（関係各大臣が食品安全委員会の意見を聴かなければならないとき）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "食品安全基本法（以下「法」という。）第二十四条第一項第十四号の政令で定めるときは、同項第一号から第十三号までに掲げる法律に基づく命令（政令を除き、告示を含む。）の規定に基づき食品の安全性の確保に関する施策を策定しようとする場合であって、法第十一条第一項に規定する食品健康影響評価が行われなければならないときとして内閣府令で定めるときとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "内閣総理大臣は、前項の内閣府令を制定し、又は改廃しようとするときは、関係各大臣の意見を聴かなければならない。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（事務局次長）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "食品安全委員会（以下「委員会」という。）の事務局に、事務局次長一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "事務局次長は、事務局長を助け、局務を整理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（事務局の内部組織）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員会の事務局に、課を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項に定めるもののほか、委員会の事務局に、命を受けて局務に関する重要事項に係るものに参画する職を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "第一項の規定に基づき置かれる課の数は、四以内とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前三項に定めるもののほか、委員会の事務局の内部組織の細目は、内閣府令で定める。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（委員会の運営）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、議事の手続その他委員会の運営に関し必要な事項は、委員長が委員会に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法の施行の日（平成十五年七月一日）から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、第一条第二項の規定は、公布の日から施行する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年八月一日政令第三五〇号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、食品衛生法等の一部を改正する法律の施行の日（平成十五年八月二十九日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年一二月一〇日政令第五〇五号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、食品衛生法等の一部を改正する法律（以下「改正法」という。）附則第一条第三号に掲げる規定の施行の日（平成十六年二月二十七日）から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
