      "MainProvision": {
        "Chapter": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ChapterTitle": "第一章　総則",
            "Article": {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（趣旨）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この法律は、情報公開・個人情報保護審査会の設置及び組織並びに調査審議の手続等について定めるものとする。"
                  }
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ChapterTitle": "第二章　設置及び組織",
            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "2",
                "ArticleCaption": "（設置）",
                "ArticleTitle": "第二条",
                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "次に掲げる法律の規定による諮問に応じ審査請求について調査審議するため、総務省に、情報公開・個人情報保護審査会（以下「審査会」という。）を置く。"
                    }
                  },
                  "Item": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "ItemTitle": "一",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "行政機関の保有する情報の公開に関する法律（平成十一年法律第四十二号）第十九条第一項"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "独立行政法人等の保有する情報の公開に関する法律（平成十三年法律第百四十号）第十九条第一項"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "ItemTitle": "三",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "行政機関の保有する個人情報の保護に関する法律（平成十五年法律第五十八号）第四十三条第一項"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "ItemTitle": "四",
                      "ItemSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "独立行政法人等の保有する個人情報の保護に関する法律（平成十五年法律第五十九号）第四十三条第一項"
                        }
                      }
                    }
                  ]
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "3",
                "ArticleCaption": "（組織）",
                "ArticleTitle": "第三条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "審査会は、委員十五人をもって組織する。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": [
                        {
                          "@Function": "main",
                          "@Num": "1",
                          "@WritingMode": "vertical",
                          "#text": "委員は、非常勤とする。"
                        },
                        {
                          "@Function": "proviso",
                          "@Num": "2",
                          "@WritingMode": "vertical",
                          "#text": "ただし、そのうち五人以内は、常勤とすることができる。"
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "4",
                "ArticleCaption": "（委員）",
                "ArticleTitle": "第四条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "委員は、優れた識見を有する者のうちから、両議院の同意を得て、内閣総理大臣が任命する。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "委員の任期が満了し、又は欠員を生じた場合において、国会の閉会又は衆議院の解散のために両議院の同意を得ることができないときは、内閣総理大臣は、前項の規定にかかわらず、同項に定める資格を有する者のうちから、委員を任命することができる。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": [
                        {
                          "@Num": "1",
                          "@WritingMode": "vertical",
                          "#text": "前項の場合においては、任命後最初の国会で両議院の事後の承認を得なければならない。"
                        },
                        {
                          "@Num": "2",
                          "@WritingMode": "vertical",
                          "#text": "この場合において、両議院の事後の承認が得られないときは、内閣総理大臣は、直ちにその委員を罷免しなければならない。"
                        }
                      ]
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "4",
                    "ParagraphNum": "４",
                    "ParagraphSentence": {
                      "Sentence": [
                        {
                          "@Function": "main",
                          "@Num": "1",
                          "@WritingMode": "vertical",
                          "#text": "委員の任期は、三年とする。"
                        },
                        {
                          "@Function": "proviso",
                          "@Num": "2",
                          "@WritingMode": "vertical",
                          "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                        }
                      ]
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "5",
                    "ParagraphNum": "５",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "委員は、再任されることができる。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "6",
                    "ParagraphNum": "６",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "委員の任期が満了したときは、当該委員は、後任者が任命されるまで引き続きその職務を行うものとする。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "7",
                    "ParagraphNum": "７",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "内閣総理大臣は、委員が心身の故障のため職務の執行ができないと認めるとき、又は委員に職務上の義務違反その他委員たるに適しない非行があると認めるときは、両議院の同意を得て、その委員を罷免することができる。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "8",
                    "ParagraphNum": "８",
                    "ParagraphSentence": {
                      "Sentence": [
                        {
                          "@Num": "1",
                          "@WritingMode": "vertical",
                          "#text": "委員は、職務上知ることができた秘密を漏らしてはならない。"
                        },
                        {
                          "@Num": "2",
                          "@WritingMode": "vertical",
                          "#text": "その職を退いた後も同様とする。"
                        }
                      ]
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "9",
                    "ParagraphNum": "９",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "委員は、在任中、政党その他の政治的団体の役員となり、又は積極的に政治運動をしてはならない。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "10",
                    "ParagraphNum": "１０",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "常勤の委員は、在任中、内閣総理大臣の許可がある場合を除き、報酬を得て他の職務に従事し、又は営利事業を営み、その他金銭上の利益を目的とする業務を行ってはならない。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "11",
                    "ParagraphNum": "１１",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "委員の給与は、別に法律で定める。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "5",
                "ArticleCaption": "（会長）",
                "ArticleTitle": "第五条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "審査会に、会長を置き、委員の互選によりこれを定める。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "会長は、会務を総理し、審査会を代表する。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "会長に事故があるときは、あらかじめその指名する委員が、その職務を代理する。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "6",
                "ArticleCaption": "（合議体）",
                "ArticleTitle": "第六条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "審査会は、その指名する委員三人をもって構成する合議体で、審査請求に係る事件について調査審議する。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "前項の規定にかかわらず、審査会が定める場合においては、委員の全員をもって構成する合議体で、審査請求に係る事件について調査審議する。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "7",
                "ArticleCaption": "（事務局）",
                "ArticleTitle": "第七条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "審査会の事務を処理させるため、審査会に事務局を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "事務局に、事務局長のほか、所要の職員を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "事務局長は、会長の命を受けて、局務を掌理する。"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ChapterTitle": "第三章　審査会の調査審議の手続",
            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "8",
                "ArticleCaption": "（定義）",
                "ArticleTitle": "第八条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "この章において「諮問庁」とは、次に掲げる者をいう。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "行政機関の保有する情報の公開に関する法律第十九条第一項の規定により審査会に諮問をした行政機関の長"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "独立行政法人等の保有する情報の公開に関する法律第十九条第一項の規定により審査会に諮問をした独立行政法人等"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "行政機関の保有する個人情報の保護に関する法律第四十三条第一項の規定により審査会に諮問をした行政機関の長"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "独立行政法人等の保有する個人情報の保護に関する法律第四十三条第一項の規定により審査会に諮問をした独立行政法人等"
                          }
                        }
                      }
                    ]
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "この章において「行政文書等」とは、次に掲げるものをいう。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "行政機関の保有する情報の公開に関する法律第十条第一項に規定する開示決定等に係る行政文書（同法第二条第二項に規定する行政文書をいう。以下この項において同じ。）（独立行政法人等の保有する情報の公開に関する法律第十三条第二項の規定により行政文書とみなされる法人文書（同法第二条第二項に規定する法人文書をいう。次号において同じ。）を含む。）"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "独立行政法人等の保有する情報の公開に関する法律第十条第一項に規定する開示決定等に係る法人文書（行政機関の保有する情報の公開に関する法律第十二条の二第二項の規定により法人文書とみなされる行政文書を含む。）"
                          }
                        }
                      }
                    ]
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "この章において「保有個人情報」とは、次に掲げるものをいう。"
                      }
                    },
                    "Item": [
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "行政機関の保有する個人情報の保護に関する法律第十九条第一項、第三十一条第一項又は第四十条第一項に規定する開示決定等、訂正決定等又は利用停止決定等に係る行政保有個人情報（同法第二条第五項に規定する保有個人情報をいう。以下この項において同じ。）（独立行政法人等の保有する個人情報の保護に関する法律第二十二条第二項又は第三十四条第二項の規定により行政保有個人情報とみなされる法人保有個人情報（同法第二条第五項に規定する保有個人情報をいう。次号において同じ。）を含む。）"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "独立行政法人等の保有する個人情報の保護に関する法律第十九条第一項、第三十一条第一項又は第四十条第一項に規定する開示決定等、訂正決定等又は利用停止決定等に係る法人保有個人情報（行政機関の保有する個人情報の保護に関する法律第二十二条第二項又は第三十四条第二項の規定により法人保有個人情報とみなされる行政保有個人情報を含む。）"
                          }
                        }
                      }
                    ]
                  }
                ]
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "9",
                "ArticleCaption": "（審査会の調査権限）",
                "ArticleTitle": "第九条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": [
                        {
                          "@Num": "1",
                          "@WritingMode": "vertical",
                          "#text": "審査会は、必要があると認めるときは、諮問庁に対し、行政文書等又は保有個人情報の提示を求めることができる。"
                        },
                        {
                          "@Num": "2",
                          "@WritingMode": "vertical",
                          "#text": "この場合においては、何人も、審査会に対し、その提示された行政文書等又は保有個人情報の開示を求めることができない。"
                        }
                      ]
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "諮問庁は、審査会から前項の規定による求めがあったときは、これを拒んではならない。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "審査会は、必要があると認めるときは、諮問庁に対し、行政文書等に記録されている情報又は保有個人情報に含まれている情報の内容を審査会の指定する方法により分類又は整理した資料を作成し、審査会に提出するよう求めることができる。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "4",
