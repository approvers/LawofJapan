{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "328",
    "@Year": "15",
    "LawNum": "平成十五年政令第三百二十八号",
    "LawBody": {
      "LawTitle": "独立行政法人日本万国博覧会記念機構の設立に伴う関係政令の整備及び経過措置に関する政令　抄",
      "EnactStatement": "内閣は、独立行政法人日本万国博覧会記念機構法（平成十四年法律第百二十五号）の施行に伴い、並びに同法附則第二条第九項及び第十一項並びに関係法律の規定に基づき、この政令を制定する。",
      "TOC": {
        "TOCLabel": "目次",
        "TOCChapter": [
          {
            "@Num": "1",
            "ChapterTitle": "第一章　関係政令の整備",
            "ArticleRange": "（第一条―第十二条）"
          },
          {
            "@Num": "2",
            "ChapterTitle": "第二章　経過措置",
            "ArticleRange": "（第十三条・第十四条）"
          }
        ],
        "TOCSupplProvision": {
          "SupplProvisionLabel": "附則"
        }
      },
      "MainProvision": {
        "Chapter": [
          {
            "@Num": "1",
            "ChapterTitle": "第一章　関係政令の整備",
            "Article": {
              "@Num": "1",
              "ArticleCaption": "（日本万国博覧会記念協会法第四条第一項及び第二十四条の地方公共団体を定める政令等の廃止）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "次に掲げる政令は、廃止する。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "日本万国博覧会記念協会法第四条第一項及び第二十四条の地方公共団体を定める政令（昭和四十六年政令第二百十五号）"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "日本万国博覧会記念協会法施行令（昭和四十六年政令第二百十六号）"
                    }
                  }
                ]
              }
            }
          },
          {
            "@Num": "2",
            "ChapterTitle": "第二章　経過措置",
            "Article": [
              {
                "@Num": "13",
                "ArticleCaption": "（独立行政法人日本万国博覧会記念機構が承継する資産に係る評価委員の任命等）",
                "ArticleTitle": "第十三条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "独立行政法人日本万国博覧会記念機構法（以下この条及び次条第一項において「法」という。）附則第二条第八項の評価委員は、次に掲げる者につき財務大臣が任命する。"
                    },
                    "Item": [
                      {
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@Num": "1",
                              "Sentence": "財務省の職員"
                            },
                            {
                              "@Num": "2",
                              "Sentence": "一人"
                            }
                          ]
                        }
                      },
                      {
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@Num": "1",
                              "Sentence": "法附則第二条第六項に規定する地方公共団体の職員"
                            },
                            {
                              "@Num": "2",
                              "Sentence": "一人"
                            }
                          ]
                        }
                      },
                      {
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@Num": "1",
                              "Sentence": "独立行政法人日本万国博覧会記念機構（以下この号において「機構」という。）の役員（機構が成立するまでの間は、機構に係る独立行政法人通則法（平成十一年法律第百三号）第十五条第一項の設立委員）"
                            },
                            {
                              "@Num": "2",
                              "Sentence": "一人"
                            }
                          ]
                        }
                      },
                      {
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Column": [
                            {
                              "@Num": "1",
                              "Sentence": "学識経験のある者"
                            },
                            {
                              "@Num": "2",
                              "Sentence": "二人"
                            }
                          ]
                        }
                      }
                    ]
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "法附則第二条第八項の規定による評価は、同項の評価委員の過半数の一致によるものとする。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": "法附則第二条第八項の規定による評価に関する庶務は、財務省理財局国有財産業務課において処理する。"
                    }
                  }
                ]
              },
              {
                "@Num": "14",
                "ArticleCaption": "（日本万国博覧会記念協会の解散の登記の嘱託等）",
                "ArticleTitle": "第十四条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "法附則第二条第一項の規定により日本万国博覧会記念協会が解散したときは、財務大臣は、遅滞なく、その解散の登記を登記所に嘱託しなければならない。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "登記官は、前項の規定による嘱託に係る解散の登記をしたときは、その登記用紙を閉鎖しなければならない。"
                    }
                  }
                ]
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": [
              {
                "@Function": "main",
                "@Num": "1",
                "#text": "この政令は、平成十五年十月一日から施行する。"
              },
              {
                "@Function": "proviso",
                "@Num": "2",
                "#text": "ただし、第八条（財務省組織令第七条第二十三号及び第五十二条第十四号の改正規定を除く。）、第九条及び第十三条の規定は、公布の日から施行する。"
              }
            ]
          }
        }
      }
    }
  }
}
