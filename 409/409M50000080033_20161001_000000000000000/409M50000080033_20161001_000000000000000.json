            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年三月三一日文部科学省令第一五号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
