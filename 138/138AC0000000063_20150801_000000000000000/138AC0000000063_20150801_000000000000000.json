{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "63",
    "@Year": "38",
    "LawNum": "明治三十八年法律第六十三号",
    "LawBody": {
      "LawTitle": "外国裁判所ノ嘱託ニ因ル共助法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "裁判所ハ外国裁判所ノ嘱託ニ因リ民事及刑事ノ訴訟事件ニ関スル書類ノ送達及証拠調ニ付法律上ノ輔助ヲ為ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法律上ノ輔助ハ所要ノ事務ヲ取扱フヘキ地ヲ管轄スル区裁判所ニ於テ之ヲ為ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1_2",
            "ArticleTitle": "第一条ノ二",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法律上ノ輔助ハ左ノ条件ヲ具備スル場合ニ於テ之ヲ為ス"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "嘱託カ外交機関ヲ経由シタルモノナルコト"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "書類送達ノ嘱託ハ送達ヲ受クヘキ者並其ノ国籍及住所又ハ居所ヲ記載シタル書面ヲ以テ為シタルモノナルコト"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "証拠調ノ嘱託ハ訴訟事件ノ当事者、証拠方法ノ種類、取調ヲ受クヘキ者ノ氏名国籍及住所又ハ居所並取調ヲ要スル事項ヲ記載シタル書面ヲ以テ為シ仍刑事ニ付テハ其ノ事件ノ要旨ヲ記載シタル書面ヲ添附シタルモノナルコト"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "日本語ヲ以テ作成セサル嘱託書及其ノ関係書類ニハ日本語ノ翻訳文ヲ添附スルコト"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "嘱託裁判所所属国カ受託事項施行ノ為要スル費用ノ弁償ヲ保証シタルコト"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "嘱託裁判所所属国カ同一又ハ類似ノ事項ニ付日本ノ裁判所ノ嘱託ニ因リ法律上ノ輔助ヲ為シ得ヘキ旨ノ保証ヲ為シタルコト"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "条約又ハ之ニ準スヘキモノニ前項ノ規定ト異ル規定アルトキハ其ノ規定ニ従フ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "受託事項カ他ノ裁判所ノ管轄ニ属スルトキハ受託裁判所ハ嘱託ヲ管轄裁判所ニ移送スヘシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "受託事項ハ日本ノ法律ニ依リ之ヲ施行スヘシ"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@AmendLawNum": "昭和一三年三月二二日法律第一七号",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "本法ハ公布ノ日ヨリ之ヲ施行ス"
            }
          }
        }
      }
    }
  }
}
