{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "1",
    "@Year": "08",
    "LawNum": "平成八年総理府・運輸省令第一号",
    "LawBody": {
      "LawTitle": "海洋汚染等及び海上災害の防止に関する法律第六十五条第二項第一号に規定する担保金の提供等に関する命令",
      "EnactStatement": "海洋汚染及び海上災害の防止に関する法律（昭和四十五年法律第百三十六号）第六十八条の規定に基づき、海洋汚染及び海上災害の防止に関する法律第六十五条第二項第一号に規定する担保金の提供等に関する命令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（趣旨）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "海洋汚染等及び海上災害の防止に関する法律（以下「法」という。）第六十五条から第六十七条までの規定の実施のため必要な手続その他の事項については、他の法令に定めるもののほか、この命令の定めるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（告知）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六十五条第一項の規定による告知は、告知を受ける者に対し、次に掲げる事項を記載した書面を交付することにより行うものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "事件に係る船舶の名称"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "違反者の氏名"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "逮捕又は押収（以下「逮捕等」という。）の年月日"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "違反の類型"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "法第六十五条第二項各号に掲げる事項"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "担保金（担保金の提供を保証する書面（以下「保証書」という。）に記載されているところに従って提供されるものを除く。）又は保証書（以下「担保金等」という。）の提供期限"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "担保金等の提供場所及び提供先"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "告知の年月日"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "告知をする取締官の氏名及びその者が法第六十五条第一項の取締官である旨"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "その他必要な事項"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（担保金等の提供期間の延長）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "担保金等の提出期間の延長を求める者は、次に掲げる事項を記載した書面を取締官に提出しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を求める者の氏名又は名称、住所及び違反者との関係"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "事件に係る船舶の名称"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "違反者の氏名"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "逮捕等の年月日"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "告知の年月日"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "希望する延長期間"
                    }
                  },
                  {
                    "@Num": "7",
                    "ItemTitle": "七",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を求める理由"
                    }
                  },
                  {
                    "@Num": "8",
                    "ItemTitle": "八",
                    "ItemSentence": {
                      "Sentence": "その他必要な事項"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "取締官は、前項の書面の提出があったときは、当該書面に係る担保金等の提供期間の延長を求めた者に対し、次に掲げる事項を通知するものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を求めた者の氏名又は名称及び住所"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "事件に係る船舶の名称"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "違反者の氏名"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "逮捕等の年月日"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を認める旨又は認めない旨及び延長を認める場合は、延長後の担保金等の提供期限"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "通知をする取締官の氏名及びその者が法第六十五条第一項の取締官である旨"
                    }
                  },
                  {
                    "@Num": "7",
                    "ItemTitle": "七",
                    "ItemSentence": {
                      "Sentence": "その他必要な事項"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（担保金提供書）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Num": "1",
                    "#text": "担保金を提供する者は、主務大臣に対し、次に掲げる事項を記載した担保金提供書を併せて提出しなければならない。"
                  },
                  {
                    "@Num": "2",
                    "#text": "この場合においては、保管金取扱規程（大正十一年大蔵省令第五号）第五条第一項又は第二項の保管金提出書を省略することができる。"
                  }
                ]
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "担保金を提供する者の氏名又は名称、住所及び違反者との関係"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "事件に係る船舶の名称"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "違反者の氏名"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "逮捕等の年月日"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "提供する担保金の額"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "担保金の提供の年月日"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "その他必要な事項"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（保証書）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "保証書には、次に掲げる事項を記載しなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "担保金の提供を保証する者の氏名又は名称、住所及び担保金を提供する者との関係"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "事件に係る船舶の名称"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "違反者の氏名"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "逮捕等の年月日"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "提供される担保金の額"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "担保金を提供する者の氏名又は名称、住所及び違反者との関係"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "第五号の額の担保金が海洋汚染等及び海上災害の防止に関する法律施行令（昭和四十六年政令第二百一号）第二十一条第一項第二号イに規定する期間内に本邦通貨で提供されることを保証する旨"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "保証書の提供の年月日"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "その他必要な事項"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（出頭期日等の変更の申出）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六十七条第二項ただし書の規定による申出は、次に掲げる事項を記載した書面により行わなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "申出を行う者の氏名又は名称及び住所"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "事件に係る船舶の名称"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "逮捕等の年月日"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "違反者が出頭する年月日又は押収物を提出する年月日"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "前号の日に出頭する違反者の氏名又は提出する押収物"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "その他必要な事項"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この命令は、海洋汚染及び海上災害の防止に関する法律の一部を改正する法律（平成八年法律第七十九号）の施行の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年一二月二日内閣府・国土交通省令第四号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この命令は、海洋汚染及び海上災害の防止に関する法律等の一部を改正する法律（平成十六年法律第三十六号）の施行の日から施行する。"
            }
          }
        }
      ]
    }
  }
}
