{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "152",
    "@Year": "08",
    "LawNum": "平成八年政令第百五十二号",
    "LawBody": {
      "LawTitle": "消費経済審議会令",
      "EnactStatement": "内閣は、通商産業省設置法（昭和二十七年法律第二百七十五号）第七条第二項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "消費経済審議会（以下「審議会」という。）は、委員二十人以内で組織する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に、特別の事項を調査審議させるため必要があるときは、臨時委員を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に、専門の事項を調査させるため必要があるときは、専門委員を置くことができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（委員等の任命）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員及び臨時委員は、学識経験のある者のうちから、経済産業大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、当該専門の事項に関し学識経験のある者のうちから、経済産業大臣が任命する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（委員の任期等）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、再任されることができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "臨時委員は、その者の任命に係る当該特別の事項に関する調査審議が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、その者の任命に係る当該専門の事項に関する調査が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員、臨時委員及び専門委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に会長を置き、委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長は、会務を総理し、審議会を代表する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長に事故があるときは、あらかじめその指名する委員が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（部会）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、その定めるところにより、部会を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に属すべき委員、臨時委員及び専門委員は、会長が指名する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に部会長を置き、当該部会に属する委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長は、当該部会の事務を掌理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長に事故があるときは、当該部会に属する委員のうちから部会長があらかじめ指名する者が、その職務を代理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、その定めるところにより、部会の議決をもって審議会の議決とすることができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、委員及び議事に関係のある臨時委員の過半数が出席しなければ、会議を開き、議決することができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会の議事は、委員及び議事に関係のある臨時委員で会議に出席したものの過半数で決し、可否同数のときは、会長の決するところによる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前二項の規定は、部会の議事に準用する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（資料の提出等の要求）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会は、その所掌事務を遂行するため必要があると認めるときは、関係行政機関の長に対し、資料の提出、意見の表明、説明その他必要な協力を求めることができる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会の庶務は、経済産業省商務情報政策局商取引・消費経済政策課において処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "9",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、議事の手続その他審議会の運営に関し必要な事項は、会長が審議会に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、公布の日から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphCaption": "（製品安全及び家庭用品品質表示審議会令の廃止）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "製品安全及び家庭用品品質表示審議会令（昭和四十八年政令第百四十九号）は、廃止する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一一年一〇月八日政令第三一八号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、訪問販売等に関する法律及び割賦販売法の一部を改正する法律の施行の日（平成十一年十月二十二日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三一一号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、附則第三条の規定は、公布の日から施行する。"
                    }
                  ]
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ArticleCaption": "（委員等の任期に関する経過措置）",
              "ArticleTitle": "第三条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令の施行の日の前日において次に掲げる従前の審議会の会長、委員その他の職員である者（任期の定めのない者を除く。）の任期は、当該会長、委員その他の職員の任期を定めたそれぞれの政令の規定にかかわらず、その日に満了する。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1:3",
                    "ItemTitle": "一から三まで",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "略"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "消費経済審議会"
                      }
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一九年四月一日政令第一三四号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、公布の日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二一年八月一四日政令第二一七号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、消費者庁及び消費者委員会設置法の施行の日（平成二十一年九月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二三年六月二九日政令第一八五号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成二十三年七月一日から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
