            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "担保金等の提供期間の延長を求める者は、次に掲げる事項を記載した書面を取締官に提出しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を求める者の氏名又は名称、住所及び違反者との関係"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "Ruby": {
                          "Rt": "だ",
                          "#text": "拿"
                        },
                        "#text": "捕に係る船舶の名称"
                      }
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "違反者の氏名"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "Ruby": {
                          "Rt": "だ",
                          "#text": "拿"
                        },
                        "#text": "捕の年月日"
                      }
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "告知の年月日"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "希望する延長期間"
                    }
                  },
                  {
                    "@Num": "7",
                    "ItemTitle": "七",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を求める理由"
                    }
                  },
                  {
                    "@Num": "8",
                    "ItemTitle": "八",
                    "ItemSentence": {
                      "Sentence": "その他必要な事項"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "取締官は、前項の書面の提出があったときは、当該書面に係る担保金等の提供期間の延長を求めた者に対し、次に掲げる事項を通知するものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を求めた者の氏名又は名称及び住所"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "Ruby": {
                          "Rt": "だ",
                          "#text": "拿"
                        },
                        "#text": "捕に係る船舶の名称"
                      }
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "違反者の氏名"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "Ruby": {
                          "Rt": "だ",
                          "#text": "拿"
                        },
                        "#text": "捕の年月日"
                      }
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "提供期間の延長を認める旨又は認めない旨及び延長を認める場合は、延長後の担保金等の提供期限"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "通知をする取締官の氏名及びその者が法第二十四条第一項の取締官である旨"
                    }
                  },
                  {
                    "@Num": "7",
                    "ItemTitle": "七",
                    "ItemSentence": {
                      "Sentence": "その他必要な事項"
                    }
                  }
                ]
              }
            ]
          },
