            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年四月二六日厚生省・建設省令第三号",
          "SupplProvisionLabel": "附　則",
