{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "323",
    "@Year": "08",
    "LawNum": "平成八年政令第三百二十三号",
    "LawBody": {
      "LawTitle": "社会保障研究所の解散に関する法律の施行に伴う関係政令の整理等に関する政令　抄",
      "EnactStatement": "内閣は、社会保障研究所の解散に関する法律（平成八年法律第四十号）の施行に伴い、並びに同法第三項及び関係法律の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（社会保障研究所の解散の登記の嘱託等）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "社会保障研究所の解散に関する法律第一項の規定により社会保障研究所が解散したときは、厚生大臣は、遅滞なく、その解散の登記を登記所に嘱託しなければならない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "登記官は、前項の規定による嘱託に係る解散の登記をしたときは、その登記用紙を閉鎖しなければならない。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（社会保障研究所法第十二条第二号の教育公務員の範囲を定める政令の廃止）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "社会保障研究所法第十二条第二号の教育公務員の範囲を定める政令（昭和三十九年政令第三百二十二号）は、廃止する。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、平成八年十二月一日から施行する。"
            }
          }
        }
      }
    }
  }
}
