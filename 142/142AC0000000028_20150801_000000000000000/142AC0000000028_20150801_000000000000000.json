{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "28",
    "@Year": "42",
    "LawNum": "明治四十二年法律第二十八号",
    "LawBody": {
      "LawTitle": "明治四十二年法律第二十八号（軌道ノ抵当ニ関スル法律）",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "軌道ノ抵当ニ関シテハ本法ニ別段ノ規定アルモノヲ除クノ外鉄道抵当法ヲ準用ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "軌道財団ハ左ニ掲クルモノニシテ軌道財団ノ所有者ニ属スルモノヲ以テ之ヲ組成ス"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "軌道線路、其ノ他ノ軌道用地及其ノ上ニ存スル工作物並之ニ属スル器具機械"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "工場、倉庫、厩舎、発電所、変電所、配電所、事務所、舎宅其ノ他工事又ハ運輸ニ要スル建物及其ノ敷地並之ニ属スル器具機械"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "用水ニ関スル工作物及其ノ敷地並之ニ属スル器具機械"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "軌道用通信、信号又ハ送電ニ要スル工作物及其ノ敷地並之ニ属スル器具機械"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前四号ニ掲ケタル工作物ヲ所有シ又ハ使用スル為他人ノ不動産ノ上ニ存スル地上権、登記シタル賃借権及前四号ニ掲ケタル土地ノ為ニ存スル地役権"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "車両及馬匹並之ニ属スル器具機械"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "保線其ノ他ノ修繕ニ要スル材料及器具機械"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "特許ニ附シタル条件ニ依リ軌道財団ニ属スルモノヲ無償ニテ国又ハ公共団体ニ引渡スヘキトキハ其ノ財団ヲ目的トスル抵当権ハ消滅ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "軌道営業者カ株式会社ニ非サル場合ニ於ケル軌道ノ抵当ニ関シテハ別ニ定ムル所ニ依ル"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本法施行ノ期日ハ勅令ヲ以テ之ヲ定ム"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三一年四月二日法律第六三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
