{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "231",
    "@Year": "03",
    "LawNum": "平成三年政令第二百三十一号",
    "LawBody": {
      "LawTitle": "国民健康保険法の一部を改正する法律の一部の施行に伴う経過措置に関する政令",
      "EnactStatement": "内閣は、国民健康保険法の一部を改正する法律（平成二年法律第三十一号）附則第六条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "国民健康保険法の一部を改正する法律（以下「改正法」という。）による改正後の国民健康保険法（以下「新法」という。）第三十九条の規定の施行の際現に改正法による改正前の国民健康保険法（以下「旧法」という。）第三十九条第一項の登録を受けている者は、新法第三十九条第一項又は第二項の登録を受けている者とみなす。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "新法第三十九条の規定の施行の際現に旧法第三十九条第一項の規定によりされている申請は、新法第三十九条第一項の規定によりされている申請とみなす。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、平成四年四月一日から施行する。"
            }
          }
        }
      }
    }
  }
}
