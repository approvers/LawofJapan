              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三三三号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令（第一条を除く。）は、平成十三年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年八月八日政令第三六八号",
          "@Extract": "true",
          "@Type": "Amend",
