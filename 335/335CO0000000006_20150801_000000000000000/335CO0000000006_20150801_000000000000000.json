    "@Year": "35",
    "LawNum": "昭和三十五年政令第六号",
    "LawBody": {
      "LawTitle": "核燃料物質の加工の請負に伴う外国人等の責任の免除等に関する法律施行令",
      "EnactStatement": "内閣は、核燃料物質の加工の請負に伴う外国人等の責任の免除等に関する法律（昭和三十四年法律第百九十四号）の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "核燃料物質の加工の請負に伴う外国人等の責任の免除等に関する法律本則の政令で定める加工は、次のとおりとする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "臨界未満実験装置に使用できる形状又は組成とするための加工"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "速中性子を発生させるための媒介体として使用できる形状又は組成とするための加工"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前二号に掲げるもののほか、試験研究に使用するための形状又は組成とするための加工"
                }
              }
            }
          ]
        }
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三六年九月一日政令第三〇一号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、核原料物質、核燃料物質及び原子炉の規制に関する法律の一部を改正する法律（昭和三十六年法律第五十号）の施行の日（昭和三十六年九月三十日）から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
