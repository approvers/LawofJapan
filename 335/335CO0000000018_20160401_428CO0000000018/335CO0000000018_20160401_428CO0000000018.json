{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "18",
    "@Year": "35",
    "LawNum": "昭和三十五年政令第十八号",
    "LawBody": {
      "LawTitle": "意匠法施行令",
      "EnactStatement": "内閣は、意匠法（昭和三十四年法律第百二十五号）の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "特許法施行令（昭和三十五年政令第十六号）第一条（第二号及び第三号を除く。）（在外者の手続の特例）の規定は、意匠登録出願、請求その他意匠登録に関する手続に準用する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "特許法施行令第四条から第六条まで（審査官、審判官及び審判書記官の資格）の規定は、審査官、審判官及び審判書記官の資格に準用する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphNum": "３",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "特許法施行令第七条（工業所有権審議会）の規定は、登録意匠又はこれに類似する意匠についての裁定の手続に準用する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、意匠法の施行の日（昭和三十五年四月一日）から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "意匠に関する審判其の他の手続の費用及び登録に関する件（大正十年勅令第四百六十三号）は、廃止する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一一年一二月二七日政令第四三〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十二年一月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三一一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三三三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令（第一条を除く。）は、平成十三年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年九月一〇日政令第三九八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、特許法等の一部を改正する法律附則第一条第二号に掲げる規定の施行の日（平成十六年四月一日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年一月二八日政令第二六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成二十六年改正法の施行の日（平成二十七年四月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年一月二二日政令第一八号",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、特許法等の一部を改正する法律の施行の日（平成二十八年四月一日）から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
