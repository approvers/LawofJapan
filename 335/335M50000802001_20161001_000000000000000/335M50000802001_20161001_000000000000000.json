{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "001",
    "@Year": "35",
    "LawNum": "昭和三十五年総理府・運輸省令第一号",
    "LawBody": {
      "LawTitle": "故障車両の整備確認の手続等に関する命令",
      "EnactStatement": "道路交通法第六十三条第六項から第八項までの規定に基づき、及び同法を実施するため、故障車両の整備確認の手続等に関する命令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（故障車両運転許可証の様式）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "道路交通法（昭和三十五年法律第百五号。以下「法」という。）第六十三条第三項の規定により交付する許可証の様式は、別記様式第一のとおりとする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（整備通告書の様式）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六十三条第四項の規定により故障車両（法第六十三条第二項の故障車両をいう。）の運転者に対し交付する文書（以下「整備通告書」という。）の様式は、別記様式第二のとおりとする。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（標章の様式）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六十三条第四項の規定によりはりつける標章の様式は、別記様式第三のとおりとする。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（通知事項）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六十三条第六項の内閣府令・国土交通省令で定める事項は、次の各号に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "整備を要する事項を認めた年月日"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "車両の使用者の氏名又は名称及び住所又は所在地"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "番号標に表示されている番号"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "整備を要する事項"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（確認の手続等）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第六十三条第七項の規定による確認を受けようとするときは、当該車両及び交付された整備通告書を、もよりの警察署の警察署長又は車両の整備に係る事項について権限を有する行政庁（以下「警察署長又は行政庁」という。）に提示するものとする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "警察署長又は行政庁は、当該車両が必要な整備をされていることを確認したときは、当該整備通告書が交付された場所を管轄する警察署長及び当該車両の使用の本拠の位置を管轄する地方運輸局長に対し、その旨を通知するものとする。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この命令は、法施行の日（昭和三十五年十二月二十日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "道路を通行する諸車若しくは軌道車の構造及び装置の調整又は警告書の交付等に関する命令（昭和二十四年総理府・運輸省令第一号）は、廃止する。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和三八年七月八日総理府・運輸省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この命令は、道路交通法の一部を改正する法律（昭和三十八年法律第九十号）の施行の日（昭和三十八年七月十四日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和四六年一二月一日総理府・運輸省令第二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この命令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和五九年六月二二日総理府・運輸省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この命令は、昭和五十九年七月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和六〇年二月五日総理府・運輸省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この命令は、道路運送法等の一部を改正する法律の施行の日（昭和六十年四月一日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（経過措置）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この命令の施行前に道路交通法の規定により交付された従前の様式による整備通告書は、この命令による改正後の様式によるものとみなす。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一二年八月一四日総理府・運輸省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この命令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（経過措置）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "整備通告書の様式については、改正後の故障車両の整備確認の手続等に関する命令別記様式第二の様式にかかわらず、当分の間、なお従前の例によることができる。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一四年六月二八日内閣府・国土交通省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この命令は、平成十四年七月一日から施行する。"
            }
          }
        }
      ],
