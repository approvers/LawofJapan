                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "第四条第一号に掲げる事項については、船舶運航事業を営む者又は水産業協同組合の長"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "第四条第二号に掲げる事項については、船舶運航事業を営む者"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "第四条第三号に掲げる事項については、港湾運送業若しくは船舶運航事業を営む者又は水産業協同組合の長"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "第四条第四号に掲げる事項については、港湾運送業を営む者"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "第四条第五号に掲げる事項については、その管理者"
