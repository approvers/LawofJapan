    "LawNum": "昭和二十六年法律第百八十八号",
    "LawBody": {
      "LawTitle": "自動車抵当法施行法　抄",
      "MainProvision": {
        "Article": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "7",
          "ArticleTitle": "第七条",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法施行の際、現に存する質権で法第二条の自動車を目的とするものは、法施行の日から二箇月以内に自動車登録原簿にその旨の登録を受けたときは、法第二十条の規定にかかわらず、なおその効力を有する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前項の規定により、なお効力を有する質権は、法の規定による抵当権に優先する。"
                }
              }
            }
          ]
        }
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この法律は、法施行の日から施行する。"
            }
          }
        }
      }
    }
  }
}
