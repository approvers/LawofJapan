            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行し、昭和二十六年十月二日から適用する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二七年四月二八日法律第一〇九号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
