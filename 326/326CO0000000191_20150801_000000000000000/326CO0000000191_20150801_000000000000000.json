              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十三年一月六日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年九月一九日政令第二九七号",
          "@Extract": "true",
