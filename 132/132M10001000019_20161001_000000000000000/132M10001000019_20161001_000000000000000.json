            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "属具目録ハ別表書式ニ依リ之ヲ調製スベシ"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "前項ノ書類ハ書式ニ示ス順序ニ依リ之ヲ編綴シ且各頁ニ頁数ヲ附スヘシ但其紙数ハ適宜トス"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "属具目録ニハ書式ニ定メサル事項ヲ記載スル為メ欄ヲ設クルコトヲ得"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "前項ノ場合ニ於テハ船籍港ヲ管轄スル海運局ノ認可ヲ受クヘシ"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "属具目録ニハ各事項ニ付英訳ヲ附シ又ハ記載心得等ヲ掲クルコトヲ得"
              }
            }
          }
        ]
      },
