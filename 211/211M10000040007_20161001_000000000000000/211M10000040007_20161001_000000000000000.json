{
  "Law": {
    "@Era": "Taisho",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "007",
    "@Year": "11",
    "LawNum": "大正十一年大蔵省令第七号",
    "LawBody": {
      "LawTitle": "政府所有有価証券取扱規程",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "政府所有有価証券取扱規程左ノ通定ム"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁ニ於ケル政府所有有価証券ハ別段ノ定アル場合ヲ除クノ外本令ノ定ムル所ニ依リ之カ受払保管ヲ為スヘシ"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁ハ特殊ノ事由アルモノヲ除クノ外政府所有有価証券ヲ其ノ所在地日本銀行（本店、支店又ハ代理店ヲ謂フ以下同シ）ニ寄託スヘシ但シ其ノ地ニ日本銀行ナキトキハ最寄ノ日本銀行ニ之ヲ寄託スルモノトス"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁前条ノ寄託ヲ為サムトスルトキハ第一号書式ノ政府所有有価証券寄託書ヲ添ヘ有価証券ヲ日本銀行ニ送付シ政府所有有価証券受託証書ノ交付ヲ受クヘシ"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "各官庁日本銀行ニ寄託セル有価証券ノ払渡ヲ請求セムトスルトキハ第二号書式ノ政府所有有価証券払渡請求書ヲ日本銀行ニ提出シ之カ交付ヲ受クヘシ"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "各官庁日本銀行ニ寄託セル有価証券ノ一部払渡ヲ請求セムトスルトキハ政府所有有価証券受託証書ヲ添ヘ第二号ノ二書式ノ政府所有有価証券一部払渡請求書ヲ日本銀行ニ提出シ之ガ交付ヲ受クベシ"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁日本銀行ニ寄託セル有価証券附属利札又ハ有価証券附属賦札ノ交付ヲ請求セムトスルトキハ第三号書式ノ政府所有有価証券利札・賦札請求書ヲ提出シ之カ交付ヲ受クヘシ但シ各官庁日本銀行ニ対シ最後ノ政府所有有価証券附属賦札ノ交付ヲ請求セムトスルトキハ前条第一項ノ例ニ従ヒ有価証券ノ交付ヲ受クヘシ"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "各官庁日本銀行統轄店ヨリ政府所有有価証券払渡請求書ノ番号ヲ記載シタル書類ヲ添ヘ政府所有有価証券月計突合表ノ送付ヲ受ケタルトキハ之ヲ調査シ適正デアルト認メタルトキハ当該突合表ニ記名押印スベシ但シ相違アル点ニ付テハ其ノ事由ヲ付記スルモノトス"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "各官庁前項ノ規定ニ依リ送付ヲ受ケタル政府所有有価証券月計突合表ニ誤リガアルコトヲ発見シタルトキハ当該突合表ノ送付ヲ受ケタル月ノ第十二営業日（「営業日」トハ日本銀行ノ休日ヲ除ク日ヲ謂フ）迄ニ統轄店ニ通知スベシ"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": "前項ノ規定ニ依リ統轄店ニ通知スル場合ニ於テハ其ノ所属店ヲ経由スベシ"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "○４",
                "ParagraphSentence": {
                  "Sentence": "第一項ノ規定ハ各官庁ガ第二項ノ通知ヲシタル後統轄店ヨリ再度政府所有有価証券月計突合表ノ送付ヲ受ケタル場合ニ於テ之ヲ準用ス"
                }
              }
            ]
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁第三条ノ政府所有有価証券寄託書ノ記載事項ニ誤謬アルコトヲ発見シタルトキ又ハ其ノ変更ヲ要スルトキハ之カ訂正ヲ為ス為訂正請求書ヲ日本銀行ニ送付スヘシ"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁政府所有有価証券受託証書ヲ亡失又ハ毀損シタルトキハ証明請求書ヲ日本銀行ニ提出シ之カ証明ヲ請求スルコトヲ得"
              }
            }
          },
          {
            "@Num": "9",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "削除"
              }
            }
          },
          {
            "@Num": "10",
            "ArticleTitle": "第十条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "各官庁ハ取扱主任官ヲ新設シタル場合、取扱主任官ニ異動アリタル場合又ハ取扱主任官ヲ廃止シタル場合ハ直チニ第四号書式ノ取引関係通知書ヲ作成シ之ヲ日本銀行ニ送付スベシ"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "前項ノ規定ハ取扱主任官ヲ廃止シタル場合ニ於テ当該取扱主任官ノ残務ヲ引継グベキ取扱主任官ヲ定メタルトキニ之ヲ準用ス"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": "前二項ノ取扱主任官ハ照較ノ用ニ供スルタメ其ノ印鑑ヲ日本銀行ニ提出スベシ但シ廃止サレタル取扱主任官ニ付テハ此ノ限ニ在ラズ"
                }
              }
            ]
          },
          {
            "@Num": "11",
            "ArticleTitle": "第十一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁日本銀行政府有価証券取扱規程第四十一条ノ二ノ規定ニ依リ日本銀行ヨリ政府所有有価証券現在額報告表ノ送付ヲ受ケタルトキハ一月内ニ之ガ副本ヲ財務省ニ提出スベシ"
              }
            }
          },
          {
            "@Num": "12",
            "ArticleTitle": "第十二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各官庁本省令ニ規定スル書式ノ記載ニ付其ノ記載ニ係ル政府所有有価証券ガ外貨表示ノモノナルトキハ支出官事務規程（昭和二十二年大蔵省令第九十四号）第十一条第二項第四号ノ規定ニ基キ定メラレタル外国貨幣換算率ニ依リ換算シタル邦貨額及当該換算率ヲ附記スベシ"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "本令ハ大正十一年四月一日ヨリ之ヲ施行ス"
            }
          }
        },
        {
          "@AmendLawNum": "大正一五年三月二九日大蔵省令第一〇号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "本令ハ大正十五年四月一日ヨリ之ヲ施行ス"
            }
          }
        },
        {
          "@AmendLawNum": "昭和一九年一一月三〇日大蔵省令第一一〇号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "本令ハ昭和十九年十二月一日ヨリ之ヲ施行ス"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二五年三月三〇日大蔵省令第一一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二九年三月六日大蔵省令第一〇号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、昭和二十九年四月一日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令施行の際改正前の政府所有有価証券取扱規程の書式により作製された用紙で現に存するものは、当分の間これを取り繕い使用することができる。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和四八年一月一九日大蔵省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、昭和四十八年四月一日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令施行の際、改正前の政府所有有価証券取扱規程、政府保管有価証券取扱規程、供託有価証券取扱規程及び日本銀行政府有価証券取扱規程の書式により作成された用紙で現に存するものは、当分の間、これを取りつくろい使用することができる。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成六年三月二四日大蔵省令第一四号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、平成六年四月一日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令施行の際、現に存するこの省令による改正前の書式による用紙は、当分の間、これを取り繕い使用することができる。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一二年八月二一日大蔵省令第六九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十三年一月六日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年三月四日財務省令第一〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十六年三月二十二日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年三月三〇日財務省令第二二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令は、予算決算及び会計令等の一部を改正する政令の施行の日（平成十七年四月一日）から施行する。"
                }
              }
            },
            {
              "@Num": "5",
              "ArticleTitle": "第五条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令の施行前に行ったこの省令の規定による改正前の各省令の規定による歳入の徴収及び支出に関する事務の取扱いについては、なお従前の例による。"
                }
              }
            }
          ]
        }
      ],
