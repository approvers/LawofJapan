                    "@Num": "1",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "医療法第五十二条第一項の規定による届出の義務"
                    }
                  },
                  {
                    "@LineBreak": "false",
                    "@Num": "2",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "平成二十三年九月三十日"
                    }
                  }
