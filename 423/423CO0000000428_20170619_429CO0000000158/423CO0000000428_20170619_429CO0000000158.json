{
  "Law": {
    "@DataInfo": "190425e04k03m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "428",
    "@Year": "23",
    "LawNum": "平成二十三年政令第四百二十八号",
    "LawBody": {
      "LawTitle": "水防法施行令",
      "EnactStatement": "内閣は、水防法（昭和二十四年法律第百九十三号）第三十二条第一項第二号の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（通常の管理行為、軽易な行為その他の行為）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "水防法（以下「法」という。）第十五条の八第一項ただし書の政令で定める行為は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "浸水被害軽減地区内の土地の維持管理のために行う行為"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "仮設の建築物の建築その他の浸水被害軽減地区内の土地を一時的な利用に供する目的で行う行為（当該利用に供された後に当該浸水被害軽減地区が有する浸水の拡大を抑制する効用が当該行為前の状態に回復されることが確実な場合に限る。）"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（特定緊急水防活動）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第三十二条第一項第二号の政令で定める水防活動は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "氾濫により浸水した区域及びその周辺の状況のビデオカメラその他の撮影機器及び通信機器を用いた監視又は上空からの監視"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "氾濫による浸水の量のビデオカメラその他の撮影機器及び通信機器を用いた観測又は上空からの観測"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前二号の監視又は観測の結果に基づく氾濫により浸水する区域及び時期又は氾濫による浸水の量の予測"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "人工衛星局の中継により行う無線通信による通信の確保"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "堤防その他の施設が決壊した場所において行う氾濫による被害の拡大を防止するための仮締切の作業その他国土交通省令で定める作業"
                    }
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、津波防災地域づくりに関する法律の施行に伴う関係法律の整備等に関する法律（平成二十三年法律第百二十四号）の施行の日（平成二十三年十二月二十七日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二九年六月一四日政令第一五八号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、水防法等の一部を改正する法律の施行の日（平成二十九年六月十九日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
