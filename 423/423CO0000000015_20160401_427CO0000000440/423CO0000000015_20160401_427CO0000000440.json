                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "出願料の軽減を受けようとする旨"
                      }
                    }
                  }
                ]
