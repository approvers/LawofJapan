      "MainProvision": {
        "Chapter": [
          {
            "@Num": "1",
            "ChapterTitle": "第一章　総則",
            "Article": [
              {
                "@Num": "1",
                "ArticleCaption": "（目的）",
                "ArticleTitle": "第一条",
                "Paragraph": {
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": "この法律は、独立行政法人農林水産消費安全技術センターの名称、目的、業務の範囲等に関する事項を定めることを目的とする。"
                  }
                }
              },
              {
                "@Num": "2",
                "ArticleCaption": "（名称）",
                "ArticleTitle": "第二条",
                "Paragraph": {
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": "この法律及び独立行政法人通則法（平成十一年法律第百三号。以下「通則法」という。）の定めるところにより設立される通則法第二条第一項に規定する独立行政法人の名称は、独立行政法人農林水産消費安全技術センターとする。"
                  }
                }
              },
              {
                "@Num": "3",
                "ArticleCaption": "（センターの目的）",
                "ArticleTitle": "第三条",
                "Paragraph": {
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": "独立行政法人農林水産消費安全技術センター（以下「センター」という。）は、一般消費者の利益の保護に資するため、農林水産物、飲食料品及び油脂の品質及び表示に関する調査及び分析、農林物資等の検査等を行うことにより、これらの物資の品質及び表示の適正化を図るとともに、肥料、農薬、飼料及び飼料添加物並びに土壌改良資材の検査等を行うことにより、これらの資材の品質の適正化及び安全性の確保を図ることを目的とする。"
                  }
                }
              },
              {
                "@Num": "4",
                "ArticleCaption": "（行政執行法人）",
                "ArticleTitle": "第四条",
                "Paragraph": {
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": "センターは、通則法第二条第四項に規定する行政執行法人とする。"
                  }
                }
              },
              {
                "@Num": "5",
                "ArticleCaption": "（事務所）",
                "ArticleTitle": "第五条",
                "Paragraph": {
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": "センターは、主たる事務所を埼玉県に置く。"
                  }
                }
              },
              {
                "@Num": "6",
                "ArticleCaption": "（資本金）",
                "ArticleTitle": "第六条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "センターの資本金は、附則第五条第二項の規定により政府から出資があったものとされた金額とする。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "政府は、必要があると認めるときは、予算で定める金額の範囲内において、センターに追加して出資することができる。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": "センターは、前項の規定による政府の出資があったときは、その出資額により資本金を増加するものとする。"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "2",
            "ChapterTitle": "第二章　役員",
            "Article": [
              {
                "@Num": "7",
                "ArticleCaption": "（役員）",
                "ArticleTitle": "第七条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "センターに、役員として、その長である理事長及び監事二人を置く。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "センターに、役員として、理事四人以内を置くことができる。"
                    }
                  }
                ]
              },
              {
                "@Num": "8",
                "ArticleCaption": "（理事の職務及び権限等）",
                "ArticleTitle": "第八条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "理事は、理事長の定めるところにより、理事長を補佐してセンターの業務を掌理する。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": [
                        {
                          "@Function": "main",
                          "@Num": "1",
                          "#text": "通則法第十九条第二項の個別法で定める役員は、理事とする。"
                        },
                        {
                          "@Function": "proviso",
                          "@Num": "2",
                          "#text": "ただし、理事が置かれていないときは、監事とする。"
                        }
                      ]
                    }
                  },
                  {
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": "前項ただし書の場合において、通則法第十九条第二項の規定により理事長の職務を代理し又はその職務を行う監事は、その間、監事の職務を行ってはならない。"
                    }
                  }
                ]
              },
              {
                "@Num": "9",
                "ArticleCaption": "（理事長及び理事の任期等）",
                "ArticleTitle": "第九条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "通則法第二十一条の三第一項の個別法で定める期間は、四年とする。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "理事の任期は、二年とする。"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "3",
            "ChapterTitle": "第三章　業務等",
            "Article": [
              {
                "@Num": "10",
                "ArticleCaption": "（業務の範囲）",
                "ArticleTitle": "第十条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "センターは、第三条の目的を達成するため、次の業務を行う。"
                    },
                    "Item": [
                      {
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": "農林水産物、飲食料品（酒類を除く。以下同じ。）及び油脂の品質及び表示に関する調査及び分析並びにこれらに関する情報の提供を行うこと。"
                        }
                      },
                      {
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": "前号に掲げるもののほか、農林水産物、飲食料品及び油脂の消費の改善に関する技術上の情報の収集、整理及び提供を行うこと。"
                        }
                      },
                      {
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Sentence": "日本農林規格又は飲食料品以外の農林物資の品質に関する表示の基準が定められた農林物資及び食品表示法（平成二十五年法律第七十号）第四条第六項に規定する食品表示基準が定められた同法第二条第一項に規定する食品（酒類を除く。）の検査を行うこと。"
                        }
                      },
                      {
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Sentence": "日本農林規格その他の農林水産分野における規格に関する認証又は試験等（日本農林規格等に関する法律（昭和二十五年法律第百七十五号）第二条第二項第三号に規定する試験等をいう。）その他これらに類する事業を行う者の技術的能力その他のこれらの事業の適正な実施に必要な能力に関する評価及び指導を行うこと。"
                        }
                      },
                      {
                        "@Num": "5",
                        "ItemTitle": "五",
                        "ItemSentence": {
                          "Sentence": "第三号に規定する農林物資及び食品（次号において「農林物資等」という。）の品質管理及び表示に関する技術上の調査及び指導を行うこと。"
                        }
                      },
                      {
                        "@Num": "6",
                        "ItemTitle": "六",
                        "ItemSentence": {
                          "Sentence": "前二号に掲げるもののほか、農林物資等の検査技術に関する調査及び研究並びに講習を行うこと。"
                        }
                      },
                      {
                        "@Num": "7",
                        "ItemTitle": "七",
                        "ItemSentence": {
                          "Sentence": "肥料、農薬、飼料及び飼料添加物並びに土壌改良資材の検査を行うこと。"
                        }
                      },
                      {
                        "@Num": "8",
                        "ItemTitle": "八",
                        "ItemSentence": {
                          "Sentence": "飼料及び飼料添加物の検定及び表示に関する業務を行うこと。"
                        }
                      },
                      {
                        "@Num": "9",
                        "ItemTitle": "九",
                        "ItemSentence": {
                          "Sentence": "飼料及び飼料添加物について登録検定機関が行う検定に関する技術上の調査及び指導を行うこと。"
                        }
                      },
                      {
                        "@Num": "10",
                        "ItemTitle": "十",
                        "ItemSentence": {
                          "Sentence": "飼料及び飼料添加物の製造設備、製造管理の方法等に関する調査を行うこと。"
                        }
                      },
                      {
                        "@Num": "11",
                        "ItemTitle": "十一",
                        "ItemSentence": {
                          "Sentence": "前各号の業務に附帯する業務を行うこと。"
                        }
                      }
                    ]
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "センターは、前項の業務のほか、次の業務を行う。"
                    },
                    "Item": [
                      {
                        "@Num": "1",
                        "ItemTitle": "一",
                        "ItemSentence": {
                          "Sentence": "日本農林規格等に関する法律第三十五条第二項第六号及び第五十五条第一項第五号の規定による検査及び質問並びに同法第六十六条第一項から第五項までの規定による立入検査及び質問"
                        }
                      },
                      {
                        "@Num": "2",
                        "ItemTitle": "二",
                        "ItemSentence": {
                          "Sentence": "食品表示法第九条第一項の規定による立入検査及び質問"
                        }
                      },
                      {
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Sentence": "肥料取締法（昭和二十五年法律第百二十七号）第三十条の二第一項の規定による立入検査、質問及び収去並びに同法第三十三条の三第二項の規定による立入検査及び質問"
                        }
                      },
                      {
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Sentence": "農薬取締法（昭和二十三年法律第八十二号）第三十条第一項の規定による集取及び立入検査並びに同法第三十五条第二項の規定による立入検査"
                        }
                      },
                      {
                        "@Num": "5",
                        "ItemTitle": "五",
                        "ItemSentence": {
                          "Sentence": "飼料の安全性の確保及び品質の改善に関する法律（昭和二十八年法律第三十五号）第五十七条第一項の規定による立入検査、質問及び収去"
                        }
                      },
                      {
                        "@Num": "6",
                        "ItemTitle": "六",
                        "ItemSentence": {
                          "Sentence": "愛がん動物用飼料の安全性の確保に関する法律（平成二十年法律第八十三号）第十三条第一項の規定による立入検査、質問及び集取"
                        }
                      },
                      {
                        "@Num": "7",
                        "ItemTitle": "七",
                        "ItemSentence": {
                          "Sentence": "地力増進法（昭和五十九年法律第三十四号）第十七条第一項の規定による立入検査"
                        }
                      },
                      {
                        "@Num": "8",
                        "ItemTitle": "八",
                        "ItemSentence": {
                          "Sentence": "遺伝子組換え生物等の使用等の規制による生物の多様性の確保に関する法律（平成十五年法律第九十七号）第三十二条第一項の規定による立入り、質問、検査及び収去"
                        }
                      }
                    ]
                  }
                ]
              },
              {
                "@Num": "11",
                "ArticleCaption": "（積立金の処分）",
                "ArticleTitle": "第十一条",
                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "センターは、毎事業年度に係る通則法第四十四条第一項又は第二項の規定による整理を行った後、同条第一項の規定による積立金があるときは、その額に相当する金額のうち農林水産大臣の承認を受けた金額を、翌事業年度に係る通則法第三十五条の十第一項の認可を受けた事業計画（同項後段の規定による変更の認可を受けたときは、その変更後のもの）の定めるところにより、翌事業年度における前条に規定する業務の財源に充てることができる。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "農林水産大臣は、前項の規定による承認をしようとするときは、財務大臣に協議しなければならない。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": "センターは、第一項に規定する積立金の額に相当する金額から同項の規定による承認を受けた金額を控除してなお残余があるときは、その残余の額を国庫に納付しなければならない。"
                    }
                  },
                  {
                    "@Num": "4",
                    "ParagraphNum": "４",
                    "ParagraphSentence": {
                      "Sentence": "前三項に定めるもののほか、納付金の納付の手続その他積立金の処分に関し必要な事項は、政令で定める。"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "4",
            "ChapterTitle": "第四章　雑則",
            "Article": [
              {
                "@Num": "12",
                "ArticleCaption": "（緊急時の命令）",
                "ArticleTitle": "第十二条",
                "Paragraph": {
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": "農林水産大臣は、農林水産物、飲食料品又は油脂について、その品質又は表示が適正でないものが販売され、又は販売されるおそれがあり、これを放置しては一般消費者の利益を著しく害すると認められる場合において、一般消費者の利益を保護するため緊急の必要があるときは、センターに対し、第十条第一項第一号及び第三号に掲げる業務のうち必要な調査、分析又は検査を実施すべきことを命ずることができる。"
                  }
                }
              },
              {
                "@Num": "13",
                "ArticleCaption": "（主務大臣等）",
                "ArticleTitle": "第十三条",
                "Paragraph": {
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": "センターに係る通則法における主務大臣及び主務省令は、それぞれ農林水産大臣及び農林水産省令とする。"
                  }
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ChapterTitle": "第五章　罰則",
            "Article": {
              "@Num": "14",
              "ArticleTitle": "第十四条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "次の各号のいずれかに該当する場合には、その違反行為をしたセンターの役員は、二十万円以下の過料に処する。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "第十条に規定する業務以外の業務を行ったとき。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "第十一条第一項の規定により農林水産大臣の承認を受けなければならない場合において、その承認を受けなかったとき。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "第十二条の規定による農林水産大臣の命令に違反したとき。"
                    }
                  }
                ]
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "この法律は、平成十三年一月六日から施行する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、第十条第二項及び附則第八条から第十一条までの規定は、同日から起算して六月を超えない範囲内において政令で定める日から施行する。"
                    }
                  ]
                }
              }
            },
            {
              "@Num": "2",
              "ArticleCaption": "（職員の引継ぎ等）",
              "ArticleTitle": "第二条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "センターの成立の際現に農林水産省の部局又は機関で政令で定めるものの職員である者は、別に辞令を発せられない限り、センターの成立の日において、センターの相当の職員となるものとする。"
                }
              }
            },
            {
              "@Num": "3",
              "ArticleTitle": "第三条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
