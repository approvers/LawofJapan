                  "@Num": "4",
                  "ParagraphNum": "４",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前項の評価委員その他評価に関し必要な事項は、政令で定める。"
                    }
                  }
                }
              ]
            },
