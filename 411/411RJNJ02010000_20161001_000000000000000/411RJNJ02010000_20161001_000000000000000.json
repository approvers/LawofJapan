{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "Rule",
    "@Num": "",
    "@Year": "11",
    "LawNum": "平成十一年人事院規則二―一〇",
    "LawBody": {
      "LawTitle": "人事院規則二―一〇（国家公務員倫理審査会事務局の組織）",
      "EnactStatement": "人事院は、国家公務員法（昭和二十二年法律第百二十号）及び国家公務員倫理法（平成十一年法律第百二十九号）に基づき、国家公務員倫理審査会事務局の組織に関し次の人事院規則を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（趣旨）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国家公務員倫理審査会事務局（以下単に「審査会事務局」という。）の組織については、法、倫理法又は規則に別段の定めのある場合を除き、この規則の定めるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（事務局長）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審査会事務局の長は、事務局長とする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "事務局長は、会長の命を受けて、審査会事務局の事務を総括し、当該事務に関し審査会事務局の職員を指揮監督する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（参事官）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審査会事務局に、参事官二を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "参事官は、命を受けて、審査会事務局の所掌事務を分掌する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "人事院の定めるところにより、参事官のうち一を首席参事官と称する。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（参事官補佐等）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "参事官の下に、人事院の定めるところにより、参事官補佐、倫理企画官、倫理企画専門官、倫理審査官及び倫理審査専門官を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "参事官補佐は、参事官を補佐し、その命を受けて、審査会事務局の事務に従事する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "倫理企画官、倫理企画専門官、倫理審査官及び倫理審査専門官は、命を受けて、審査会事務局における専門の事務に従事する。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": "（組織の細目）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この規則に定めるもののほか、事務分掌その他の組織の細目に関し必要な事項は、人事院が定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この規則は、公布の日から施行する。"
          }
        }
      }
    }
  }
}
