{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "417",
    "@Year": "11",
    "LawNum": "平成十一年政令第四百十七号",
    "LawBody": {
      "LawTitle": "家畜保健衛生所法施行令",
      "EnactStatement": "内閣は、家畜保健衛生所法（昭和二十五年法律第十二号）第三条第二項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "家畜保健衛生所法第三条第二項の政令で定める基準は、次のとおりとする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "家畜の伝染病の予防に関する事務を迅速かつ的確に行うために必要な専用の検査室及び専用の病理解剖室を有するものであること。"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "光学顕微鏡（細菌の形態を識別することができるものに限る。）、遠心分離機その他家畜の伝染病の予防に関する事務を迅速かつ的確に行うために必要なものとして農林水産大臣が定める設備及び器具並びに家畜の死体その他の汚物の処理のための焼却施設を有するものであること。"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "所長は、獣医師であって、家畜保健衛生所の事務に関し相当の経験を有するものであること。"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "4",
              "ItemTitle": "四",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "所長のほか、獣医師である常勤の職員を置くものであること。"
                }
              }
            }
          ]
        }
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、平成十二年四月一日から施行する。"
            }
          }
        }
      }
    }
  }
}
