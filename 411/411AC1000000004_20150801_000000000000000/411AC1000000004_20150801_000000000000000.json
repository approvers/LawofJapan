            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から起算して一年を超えない範囲内において政令で定める日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年八月一日法律第一三六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この法律は、公布の日から起算して六月を超えない範囲内において政令で定める日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                  }
                ]
              },
              "Item": {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
