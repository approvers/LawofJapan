{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "270",
    "@Year": "11",
    "LawNum": "平成十一年政令第二百七十号",
    "LawBody": {
      "LawTitle": "国民金融公庫法の一部を改正する法律の施行に伴う関係政令の整備等に関する政令　抄",
      "EnactStatement": "内閣は、国民金融公庫法の一部を改正する法律（平成十一年法律第五十六号）の施行に伴い、及び関係法律の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（環境衛生金融公庫の解散の登記の嘱託等）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国民金融公庫法の一部を改正する法律附則第三条第一項の規定により環境衛生金融公庫が解散したときは、厚生大臣及び大蔵大臣は、遅滞なく、その解散の登記を登記所に嘱託しなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "登記官は、前項の規定による嘱託に係る解散の登記をしたときは、その登記用紙を閉鎖しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（環境衛生金融公庫法施行令等の廃止）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "次に掲げる政令は、廃止する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "環境衛生金融公庫法施行令（昭和四十二年政令第二百七十三号）"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "環境衛生金融公庫が国民金融公庫から承継する債権の範囲等を定める政令（昭和四十二年政令第三百二十七号）"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この政令は、平成十一年十月一日から施行する。"
          }
        }
      }
    }
  }
}
