                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二十四条第五項の規定による公示は、次に掲げる方法により行わなければならない。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
