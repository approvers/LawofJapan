{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "046",
    "@Year": "31",
    "LawNum": "平成三十一年政令第四十六号",
    "LawBody": {
      "LawTitle": "海洋再生可能エネルギー発電設備の整備に係る海域の利用の促進に関する法律施行令",
      "EnactStatement": "内閣は、海洋再生可能エネルギー発電設備の整備に係る海域の利用の促進に関する法律（平成三十年法律第八十九号）第二条第三項、第十条第一項及び第四項並びに第三十条並びに附則第二条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（海洋再生可能エネルギー源）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "海洋再生可能エネルギー発電設備の整備に係る海域の利用の促進に関する法律（以下「法」という。）第二条第三項の政令で定めるものは、海域における風力とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（促進区域内海域の占用等に係る許可を要する海域の上空及び海底の区域）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十条第一項の政令で定める区域は、海域の上空三百十五メートルまでの区域及び海底下百メートルまでの区域とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（促進区域内海域における制限行為で許可を要しない行為）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十条第一項ただし書の政令で定める行為は、海洋再生可能エネルギー発電設備の維持管理のために行う行為とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（促進区域内海域の利用又は保全に支障を与えるおそれのある行為）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十条第一項第四号の政令で定める行為は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "海底の掘削又は切土その他海底の形状を変更する行為"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "海洋再生可能エネルギー発電設備整備促進区域ごとに国土交通大臣が指定する廃物の投棄"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（占用の期間）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十条第四項の政令で定める期間は、次の各号に掲げる占用の区分に応じ、当該各号に定める期間とする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "容易に移転し、又は撤去することができる構造の施設又は工作物による占用"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "五年"
                        }
                      }
                    ]
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "法第十九条第一項に規定する認定公募占用計画に係る海洋再生可能エネルギー発電設備による占用"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "三十年"
                        }
                      }
                    ]
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "前二号に掲げるもの以外の占用"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "十年"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（権限の委任）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "法第十条第一項（同条第三項の規定により読み替えて適用する場合を含む。）及び第五項から第七項まで、第二十三条第一項、第二十四条第一項から第七項まで、第二十五条第一項及び第二項並びに第二十六条第一項から第三項までの規定による国土交通大臣の権限は、地方整備局長又は北海道開発局長に委任する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、法第二十四条第一項から第七項まで並びに第二十五条第一項及び第二項の規定による権限にあっては、国土交通大臣が自らその権限を行うことを妨げない。"
                  }
                ]
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法の施行の日（平成三十一年四月一日）から施行する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphCaption": "（法附則第二条の政令で定める日）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "法附則第二条の政令で定める日は、法の施行後最初に法第十六条の規定により経済産業大臣が選定事業者における海洋再生可能エネルギー発電設備に係る調達価格及び調達期間を告示した日又は平成三十二年十二月六日のいずれか早い日とする。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphCaption": "（国土交通省組織令の一部改正）",
            "ParagraphNum": "３",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "国土交通省組織令（平成十二年政令第二百五十五号）の一部を次のように改正する。"
              }
            },
            "AmendProvision": {
              "AmendProvisionSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "第百六十二条中第十三号を第十四号とし、第六号から第十二号までを一号ずつ繰り下げ、第五号の次に次の一号を加える。"
                }
              },
              "NewProvision": {
                "Item": {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "海洋再生可能エネルギー発電設備の整備に係る海域の利用の促進に関する法律（平成三十年法律第八十九号）の施行に関する事務で国土交通省の所掌に属するものに関すること。"
                    }
                  }
                }
              }
            }
          }
        ]
      }
    }
  }
}
