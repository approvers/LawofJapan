    "@LawType": "Act",
    "@Num": "148",
    "@Year": "22",
    "LawNum": "昭和二十二年法律第百四十八号",
    "LawBody": {
      "LawTitle": "貨幣損傷等取締法",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "○１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "貨幣は、これを損傷し又は鋳つぶしてはならない。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "○２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "貨幣は、これを損傷し又は鋳つぶす目的で集めてはならない。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphNum": "○３",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "第一項又は前項の規定に違反した者は、これを一年以下の懲役又は二十万円以下の罰金に処する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "○１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、公布の日から、これを施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "○２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "昭和十五年大蔵省令第四十号（補助貨幣のしゆう集、鋳つぶし又は損傷の取締に関する省令）は、これを廃止する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和六二年六月一日法律第四二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この法律は、昭和六十三年四月一日から施行する。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "16",
              "ArticleCaption": "（罰則の適用に関する経過措置）",
              "ArticleTitle": "第十六条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この法律の施行前にした行為に対する罰則の適用については、なお従前の例による。"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
