{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "13",
    "@Year": "22",
    "LawNum": "昭和二十二年法律第十三号",
    "LawBody": {
      "LawTitle": "請願法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "請願については、別に法律の定める場合を除いては、この法律の定めるところによる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "請願は、請願者の氏名（法人の場合はその名称）及び住所（住所のない場合は居所）を記載し、文書でこれをしなければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "請願書は、請願の事項を所管する官公署にこれを提出しなければならない。"
                    },
                    {
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "天皇に対する請願書は、内閣にこれを提出しなければならない。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "請願の事項を所管する官公署が明らかでないときは、請願書は、これを内閣に提出することができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "請願書が誤つて前条に規定する官公署以外の官公署に提出されたときは、その官公署は、請願者に正当な官公署を指示し、又は正当な官公署にその請願書を送付しなければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律に適合する請願は、官公署において、これを受理し誠実に処理しなければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "何人も、請願をしたためにいかなる差別待遇も受けない。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この法律は、日本国憲法施行の日から、これを施行する。"
            }
          }
        }
      }
    }
  }
}
