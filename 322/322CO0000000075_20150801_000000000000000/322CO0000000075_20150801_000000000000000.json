              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から、これを施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二三年四月九日政令第八一号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
