            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から、これを施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二六年六月三〇日政令第二四八号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
