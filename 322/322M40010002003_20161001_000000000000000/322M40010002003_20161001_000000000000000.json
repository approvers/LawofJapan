{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "003",
    "@Year": "22",
    "LawNum": "昭和二十二年総理庁・商工省令第三号",
    "LawBody": {
      "LawTitle": "経済産業省職員受託出張規則",
      "EnactStatement": "商工部内職員受託出張規則を次のように制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "経済産業省又はその所属庁の所管事項について調査、試験、分析、鑑定、講習、講話等のための職員出張の申請は、別に規定する場合を除く外、本則の定むるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "申請者は出張事項、出張地名及び出張期間その他必要な事項を記載した申請書を当該官庁に差し出さなければならない。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "申請者は、職員の出張について左に掲げる費用を負担しなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "国家公務員等の旅費に関する法律（昭和二十五年法律第百十四号）に基づき算定する旅費"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "職員の出張期間に応ずる給与"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "機械器具の損料、通信費その他必要な費用"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "当該官庁は、申請者が次の各号に掲げる者であつて、かつ、必要があると認めるときは、前条第二号及び第三号に掲げる費用の全部又は一部の負担を免除することができる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "国又は地方公共団体"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "特殊法人、公益社団法人若しくは公益財団法人又はこれに準ずる団体"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "前二号に掲げる者から委託を受けた業務に関し申請した者"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "別に規定するところにより手数料を納付すべき業務に関し申請した者"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "出張の申請を許可したときは、申請者は第三条の費用の概算額を納めなければならない。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "但し、当該官庁においてその必要がないと認める場合はこの限りではない。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "前項の規定によつて納めた概算額につき、精算をなした場合において、不足額があるときは申請者にこれを納めしめ、過剰額があるときは申請者に払戻請求書を差し出させる。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "○１",
              "ParagraphSentence": {
                "Sentence": "この命令は、公布の日から、これを施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "○２",
              "ParagraphSentence": {
                "Sentence": "軍需部内職員受託出張規則は、これを廃止する。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和二四年五月二五日通商産業省令第三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "○１",
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和四七年九月一八日通商産業省令第一〇三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年一一月二九日通商産業省令第三六七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十三年一月六日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年一二月一日経済産業省令第八二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、一般社団法人及び一般財団法人に関する法律の施行の日（平成二十年十二月一日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
