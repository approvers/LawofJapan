    "LawNum": "昭和二十二年司法省令第二十九号",
    "LawBody": {
      "LawTitle": "閉鎖機関に関する登記取扱手続",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "閉鎖機関に関する登記取扱手続を、次のように定める。"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "閉鎖機関令第二十三条第一項の登記を嘱託する場合において、当該閉鎖機関の本店又は支店の所在地に支配人の登記があるときは、嘱託書にその支配人の氏名及び住所を記載しなければならない。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "閉鎖機関令第二十三条第五項の登記をした場合において、当該閉鎖機関につき会社経理応急措置法の規定による特別経理会社の特別管理人に関する登記があるときは、その登記を抹消する記号を記録しなければならない。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "閉鎖機関令第二十三条第八項の登記は、同条第一項の登記と同時に、これをしなければならない。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から、これを施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二八年一一月五日法務省令第七六号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和三九年三月三一日法務省令第四五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、昭和三十九年四月一日から施行する。"
            }
          }
        },
        {
