{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "82",
    "@Year": "22",
    "LawNum": "昭和二十二年法律第八十二号",
    "LawBody": {
      "LawTitle": "昭和二十二年法律第八十二号（国会予備金に関する法律）",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "各議院の予備金は、その院の議長がこれを管理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "各議院の予備金を支出するには、事前に、時宜によつては事後に、その院の議院運営委員会の承認を経なければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "各議院の予備金の支出については、これを議院運営委員会の委員長が、次の常会の会期の初めにおいて、その院に報告して承諾を求めなければならない。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この法律は、国会法施行の日から、これを施行する。"
            }
          }
        }
      }
    }
  }
}
