{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "005",
    "@Year": "22",
    "LawNum": "昭和二十二年大蔵省・司法省令第五号",
    "LawBody": {
      "LawTitle": "昭和二十二年大蔵・司法省令第五号（金融債券の登録の変更等に関する省令）",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "金融機関経理応急措置法第二十条及び第三十三条並びに社債等登録法施行令第六条の規定により、金融債券の登録の変更等に関し、次のように定める。"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "登録機関は、指定時（金融機関経理応急措置法に定める指定時をいう。）において金融債券の債務者たる金融機関の新勘定の負債に属する金融債券（以下新勘定金融債券という。）につき、この省令施行の日から一箇月以内に、社債登録簿の適当な箇所に、その債務者たる金融機関の新勘定の負債に属することを明らかにする表示をしなければならない。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Num": "1",
                    "#text": "新勘定金融債券で株式会社資金統合銀行の有するものについては、左の各号の一に該当する場合を除くの外、この省令施行の日以後、登録の変更を請求することができる。"
                  },
                  {
                    "@Num": "2",
                    "#text": "担保権の登録の抹消の請求についても、また同様とする。"
                  }
                ]
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "日本銀行又は金融機関以外の者に対する社債の移転登録又は担保権の移転登録"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "前号の者を担保権者とする担保権の設定の登録"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "第一号の者を受託者とする信託の登録"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "前条の規定により登録の変更又は担保権の登録の抹消の請求をする場合においては、その請求書に、同条の規定により請求する旨を明記した書類を添附しなければならない。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "登録機関は、同一社債権者の新勘定金融債券について、社債等登録法施行規則第九条の規定にかかわらず、別用紙を備えることができる。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この省令は、公布の日から、これを施行する。"
          }
        }
      }
    }
  }
}
