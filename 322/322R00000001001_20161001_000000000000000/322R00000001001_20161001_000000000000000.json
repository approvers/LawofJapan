    "@LawType": "Rule",
    "@Num": "1",
    "@Year": "22",
    "LawNum": "昭和二十二年会計検査院規則第一号",
    "LawBody": {
      "LawTitle": "会計検査院規則の公布に関する規則",
      "EnactStatement": "会計検査院規則の公布に関する規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "会計検査院規則には、会計検査院長が年月日を記入して、これに署名する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "会計検査院規則は、官報で、これを公布する。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "会計検査院規則は、特に施行期日を定めない場合には、公布の日から起算し二十日を経て、これを施行する。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この規則は、昭和二十二年五月三日から、これを施行する。"
          }
        }
      }
    }
  }
}
