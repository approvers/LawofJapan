{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "140",
    "@Year": "29",
    "LawNum": "平成二十九年政令第百四十号",
    "LawBody": {
      "LawTitle": "休眠預金等活用審議会令",
      "EnactStatement": "内閣は、民間公益活動を促進するための休眠預金等に係る資金の活用に関する法律（平成二十八年法律第百一号）第四十一条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（部会）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "休眠預金等活用審議会（以下「審議会」という。）は、その定めるところにより、部会を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "部会に属すべき委員及び専門委員は、会長が指名する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "部会に部会長を置き、当該部会に属する委員のうちから、会長が指名する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "部会長は、当該部会の事務を掌理する。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": "部会長に事故があるときは、当該部会に属する委員のうちから部会長があらかじめ指名する者が、その職務を代理する。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会は、委員の過半数が出席しなければ、会議を開き、議決することができない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "審議会の議事は、委員で会議に出席したものの過半数で決し、可否同数のときは、会長の決するところによる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "前二項の規定は、部会の議事に準用する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "審議会の庶務は、内閣府本府に置かれる政策統括官が処理する。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（審議会の運営）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令に定めるもののほか、議事の手続その他審議会の運営に関し必要な事項は、会長が審議会に諮って定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphCaption": "（施行期日）",
          "ParagraphNum": "１",
          "ParagraphSentence": {
            "Sentence": "この政令は、民間公益活動を促進するための休眠預金等に係る資金の活用に関する法律附則第一条第三号に掲げる規定の施行の日（平成二十九年四月二十四日）から施行する。"
          }
        }
      }
    }
  }
}
