                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "令第二条第九号に掲げる廃棄物の破砕施設"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
