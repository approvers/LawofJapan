        },
        {
          "@AmendLawNum": "昭和四二年三月六日労働省令第三号",
          "@Extract": "false",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
