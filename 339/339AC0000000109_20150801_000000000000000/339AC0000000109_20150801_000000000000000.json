              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、条約が日本国について効力を生ずる日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四四年八月一日法律第六八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
