            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法の施行の日（昭和四十四年七月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四九年六月二六日政令第二二五号",
          "@Extract": "true",
          "@Type": "Amend",
