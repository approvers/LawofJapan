            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和四十九年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五三年七月五日政令第二八二号",
          "@Extract": "true",
