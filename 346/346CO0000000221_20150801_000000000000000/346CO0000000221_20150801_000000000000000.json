            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "16",
              "ArticleCaption": "（卸売市場法施行令の一部改正に伴う経過措置）",
              "ArticleTitle": "第十六条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
