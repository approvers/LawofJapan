            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、昭和四十六年七月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四七年一〇月一七日政令第三七五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
