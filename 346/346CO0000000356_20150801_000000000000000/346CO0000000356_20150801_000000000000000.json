            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十二年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一八年一一月二二日政令第三六一号",
