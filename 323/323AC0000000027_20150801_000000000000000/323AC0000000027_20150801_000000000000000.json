                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "この法律は、公社法の施行の日から施行する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                    }
                  ]
                },
                "Item": {
