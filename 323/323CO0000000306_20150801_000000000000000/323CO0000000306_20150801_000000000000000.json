{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "306",
    "@Year": "23",
    "LawNum": "昭和二十三年政令第三百六号",
    "LawBody": {
      "LawTitle": "沖縄関係事務整理に伴う恩給の特別措置に関する政令",
      "EnactStatement": "内閣は、ポツダム宣言の受諾に伴い発する命令に関する件（昭和二十年勅令第五百四十二号）に基き、ここに沖縄関係事務整理に伴う戸籍、恩給等の特別措置に関する政令を制定する。",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "都道府県の負担すべき恩給で沖縄県に係るものは、恩給法（大正十二年法律第四十八号）第十六条の規定にかかわらず、国庫が、負担する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "都道府県知事の裁定すべき恩給で沖縄県に係るものは、恩給法第十二条の規定にかかわらず、総理庁恩給局長が、裁定する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和二十三年十月一日から、施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二四年五月三一日法律第一三七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、昭和二十四年六月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二四年一二月二一日政令第三九六号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和二十五年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二六年一〇月六日政令第三二二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二七年一月一九日政令第五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行し、昭和二十六年十二月五日から適用する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二七年四月二八日法律第一〇六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、法施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二七年七月三一日法律第二六八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、昭和二十七年八月一日から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "3",
              "ParagraphNum": "３",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "従前の機関及び職員は、この法律に基く相当の機関及び職員となり、同一性をもつて存続するものとする。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和四二年七月二五日法律第八一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、公布の日から起算して六月をこえない範囲内において政令で定める日（以下「施行日」という。）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四三年六月一日法律第八三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この法律は、南方諸島及びその他の諸島に関する日本国とアメリカ合衆国との間の協定の効力発生の日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、附則第三条第二項の規定は、政令で定める日から施行する。"
                  }
                ]
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四六年一二月三一日法律第一三〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、琉球諸島及び大東諸島に関する日本国とアメリカ合衆国との間の協定の効力発生の日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
