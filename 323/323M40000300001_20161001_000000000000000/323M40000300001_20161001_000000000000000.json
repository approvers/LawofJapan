{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "1",
    "@Year": "23",
    "LawNum": "昭和二十三年厚生省・農林省令第一号",
    "LawBody": {
      "LawTitle": "大麻取締法施行規則",
      "EnactStatement": "大麻取締法施行規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "大麻取締法（以下「法」という。）第四条第一項第一号に規定する大麻の輸入又は輸出の許可を受けようとする大麻研究者が、同条第二項の規定によつて提出する申請書に記載すべき事項は、次のとおりとし、その様式は、別記第一号様式とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "申請者の氏名及び住所"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "免許証の番号及び免許年月日"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "輸入し、又は輸出しようとする大麻の品名及び数量"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "輸出者又は輸入者の氏名又は住所（法人にあつては、その名称及び主たる事務所所在地）"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "輸入又は輸出の期間"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "輸送の方法"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "輸入港名又は輸出港名"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第五条の規定による大麻取扱者免許を受けようとする者は、次に掲げる事項を記載した申請書を都道府県知事に提出しなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "申請者の住所、氏名若しくは名称及び生年月日（法人については生年月日を除く。）"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "栽培地の数、位置及び面積"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "大麻研究者にあつては研究目的及び履歴書"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六条の規定による大麻取扱者名簿に登録すべき事項は、左の通りである。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "登録番号及び登録年月日"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "住所地、氏名若しくは名称及び生年月日（法人については生年月日を除く。）"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "大麻栽培者又は大麻研究者の別"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "栽培地の数、位置及び面積又は研究目的"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "免許証の再交付の事由及び年月日"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "登録のまつ消の事由及び年月日"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第十条第一項に該当する場合においては、大麻取扱者は、免許証を添え、事由を書き申請しなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "法第十条第二項に該当する場合においては、同項に規定する者は、免許証を添え一月以内に届け出なければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "法第十条第二項に規定する者が当該大麻を栽培し又は所持しようとするときは、大麻取扱者免許の申請をしなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第十六条第一項に規定する大麻の譲渡しの許可を受けようとする大麻研究者が、同条第二項の規定によつて提出する申請書に記載すべき事項は、次のとおりとし、その様式は、別記第二号様式とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "申請者の氏名及び住所"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "免許証の番号及び免許年月日"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "譲り渡そうとする大麻の品名及び数量"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "譲渡先"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "譲渡しの理由"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十一条第一項の規定により麻薬取締官又は麻薬取締員その他の職員が大麻を収去しようとするときは、収去証（別記第三号様式）を交付しなければならない。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十一条第二項の規定により、携帯すべき身分を示す証票は、別記第四号様式による。"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十二条の三第四項に規定する厚生労働省令で定める事項は、次のとおりとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "交付を受けた大麻の品名及び数量並びにその年月日"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "交付を受けた大麻につき、滅失その他の事故を生じたときは、当該事故に係る大麻の品名及び数量、その年月日その他事故の状況を明らかにするため必要な事項"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から、これを施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二八年四月九日厚生省・農林省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行し、昭和二十八年四月一日から適用する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二九年六月三日厚生省・農林省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成元年三月二四日厚生省・農林水産省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、公布の日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令の施行の際この省令による改正前の様式により使用されている収去証及び証票は、この省令による改正後の様式によるものとみなす。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成二年八月一日厚生省・農林水産省令第二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、麻薬取締法等の一部を改正する法律（平成二年法律第三十三号）（同法附則第一条ただし書に規定する部分を除く。）の施行の日（平成二年八月二十五日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成四年五月一三日厚生省・農林水産省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、麻薬及び向精神薬取締法等の一部を改正する法律（平成三年法律第九十三号）の施行の日（平成四年七月一日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令の施行の際この省令による改正前の様式により使用されている証票は、この省令による改正後の様式によるものとみなす。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成六年三月二五日厚生省・農林水産省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、平成六年四月一日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令の施行の際現にあるこの省令による改正前の様式による用紙については、当分の間、これを使用することができる。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一二年三月二四日厚生省・農林水産省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十二年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年一一月二二日厚生省・農林水産省令第四号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          }
        }
      ],
