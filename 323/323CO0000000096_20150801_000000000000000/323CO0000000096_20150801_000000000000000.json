            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二六年五月一九日政令第一五六号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
