    "LawBody": {
      "LawTitle": "海上保安庁法施行令",
      "MainProvision": {
        "Chapter": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ChapterTitle": "第一章　総則",
            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ArticleCaption": "（法の施行期日）",
                "ArticleTitle": "第一条",
                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "海上保安庁法（昭和二十三年法律第二十八号、以下法という。）第三十四条の規定による法の施行期日は、昭和二十三年五月一日とする。"
                    }
                  }
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "2",
                "ArticleCaption": "（船舶の番号及び標識並びに海上保安庁の旗）",
                "ArticleTitle": "第二条",
                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法第四条第三項の規定による海上保安庁の船舶の番号及び標識並びに海上保安庁の旗は、長官がこれを定める。"
                    }
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ChapterTitle": "第二章　職員",
            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "3:5",
                "ArticleTitle": "第三条から第五条まで",
                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "削除"
                    }
                  }
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "6",
                "ArticleCaption": "（首席監察官）",
                "ArticleTitle": "第六条",
                "Paragraph": [
                  {
                    "@Hide": "false",
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "海上保安庁に、法第三十三条の規定に基づき、首席監察官一人を置く。"
                      }
                    }
                  },
                  {
                    "@Hide": "false",
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "首席監察官は、上官の命を受け、海上保安庁の職員の非違及び所管行政の実況を監察する。"
                      }
                    }
                  }
                ]
              },
              {
                "@Delete": "false",
