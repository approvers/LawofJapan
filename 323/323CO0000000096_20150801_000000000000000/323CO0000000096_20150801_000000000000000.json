                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "巡視警戒に任ずる船舶の乗組員（海上保安官を命ぜられた者を除く。）は、海上保安官の職務を助ける。"
                    }
