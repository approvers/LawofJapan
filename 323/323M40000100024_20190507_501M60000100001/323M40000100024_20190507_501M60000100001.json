            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、公布の日から、これを施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二五年四月一日厚生省令第一三号",
