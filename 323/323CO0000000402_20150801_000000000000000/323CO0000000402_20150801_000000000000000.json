{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "402",
    "@Year": "23",
    "LawNum": "昭和二十三年政令第四百二号",
    "LawBody": {
      "LawTitle": "会社等臨時措置法等を廃止する政令　抄",
      "EnactStatement": "内閣は、ポツダム宣言の受諾に伴い発する命令に関する件（昭和二十年勅令第五百四十二号）に基き、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "会社等臨時措置法（昭和十九年法律第三号）及び会社等臨時措置法施行令（昭和十九年勅令第百四十二号）は、廃止する。"
            }
          }
        }
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令は、公布の日から起算して十日を経過した日から施行する。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "5",
              "ArticleTitle": "第五条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "昭和二十四年十二月三十一日までになした社債（債券を含む。以下同じ。）の登記については、旧法第五条及び第八条並びに旧令第四条及び第十七条の規定は、その社債の総額の償還があつたことの登記が完了するときまで、なおその効力を有する。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "7",
              "ArticleTitle": "第七条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令の施行前にした行為に対する罰則の適用については、旧法第九条の規定は、なおその効力を有する。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "9",
              "ArticleTitle": "第九条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令の施行前、改正前の社債等登録法第三条第一項第二号（同法第十四条において準用する場合を含む。）の規定に基き登録した社債については、改正前の同法の規定は、なおその効力を有する。"
                  }
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和二四年四月三〇日法律第四七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
