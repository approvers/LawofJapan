{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "178",
    "@Year": "23",
    "LawNum": "昭和二十三年法律第百七十八号",
    "LawBody": {
      "LawTitle": "国民の祝日に関する法律",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "自由と平和を求めてやまない日本国民は、美しい風習を育てつつ、よりよき社会、より豊かな生活を築きあげるために、ここに国民こぞつて祝い、感謝し、又は記念する日を定め、これを「国民の祝日」と名づける。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "「国民の祝日」を次のように定める。"
                }
              },
              "List": [
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "元日　一月一日　年のはじめを祝う。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "成人の日　一月の第二月曜日　おとなになつたことを自覚し、みずから生き抜こうとする青年を祝いはげます。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "建国記念の日　政令で定める日　建国をしのび、国を愛する心を養う。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "天皇誕生日　二月二十三日　天皇の誕生日を祝う。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "春分の日　春分日　自然をたたえ、生物をいつくしむ。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "昭和の日　四月二十九日　激動の日々を経て、復興を遂げた昭和の時代を顧み、国の将来に思いをいたす。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "憲法記念日　五月三日　日本国憲法の施行を記念し、国の成長を期する。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "みどりの日　五月四日　自然に親しむとともにその恩恵に感謝し、豊かな心をはぐくむ。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "こどもの日　五月五日　こどもの人格を重んじ、こどもの幸福をはかるとともに、母に感謝する。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "海の日　七月の第三月曜日　海の恩恵に感謝するとともに、海洋国日本の繁栄を願う。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "山の日　八月十一日　山に親しむ機会を得て、山の恩恵に感謝する。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "敬老の日　九月の第三月曜日　多年にわたり社会につくしてきた老人を敬愛し、長寿を祝う。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "秋分の日　秋分日　祖先をうやまい、なくなつた人々をしのぶ。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "体育の日　十月の第二月曜日　スポーツにしたしみ、健康な心身をつちかう。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "文化の日　十一月三日　自由と平和を愛し、文化をすすめる。"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "勤労感謝の日　十一月二十三日　勤労をたつとび、生産を祝い、国民たがいに感謝しあう。"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "「国民の祝日」は、休日とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "「国民の祝日」が日曜日に当たるときは、その日後においてその日に最も近い「国民の祝日」でない日を休日とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "その前日及び翌日が「国民の祝日」である日（「国民の祝日」でない日に限る。）は、休日とする。"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、公布の日からこれを施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "昭和二年勅令第二十五号は、これを廃止する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和四一年六月二五日法律第八六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、公布の日から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphCaption": "（建国記念の日となる日を定める政令の制定）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "改正後の第二条に規定する建国記念の日となる日を定める政令は、この法律の公布の日から起算して六月以内に制定するものとする。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "3",
              "ParagraphNum": "３",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "内閣総理大臣は、改正後の第二条に規定する建国記念の日となる日を定める政令の制定の立案をしようとするときは、建国記念日審議会に諮問し、その答申を尊重してしなければならない。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和四八年四月一二日法律第一〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和六〇年一二月二七日法律第一〇三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成元年二月一七日法律第五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成七年三月八日法律第二二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、平成八年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一〇年一〇月二一日法律第一四一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、平成十二年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一三年六月二二日法律第五九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、平成十五年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年五月二〇日法律第四三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、平成十九年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二六年五月三〇日法律第四三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、平成二十八年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二九年六月一六日法律第六三号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": [
                      {
                        "@Function": "main",
                        "@Num": "1",
                        "@WritingMode": "vertical",
                        "#text": "この法律は、公布の日から起算して三年を超えない範囲内において政令で定める日から施行する。"
                      },
                      {
                        "@Function": "proviso",
                        "@Num": "2",
                        "@WritingMode": "vertical",
                        "#text": "ただし、第一条並びに次項、次条、附則第八条及び附則第九条の規定は公布の日から、附則第十条及び第十一条の規定はこの法律の施行の日の翌日から施行する。"
                      }
                    ]
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前項の政令を定めるに当たっては、内閣総理大臣は、あらかじめ、皇室会議の意見を聴かなければならない。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ArticleCaption": "（この法律の失効）",
              "ArticleTitle": "第二条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この法律は、この法律の施行の日以前に皇室典範第四条の規定による皇位の継承があったときは、その効力を失う。"
                  }
                }
              }
            }
          ]
        }
      ]
    }
  }
}
