              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成二十七年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年一一月二六日政令第三九二号",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
