            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十八年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年三月一八日政令第七四号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
