{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "67",
    "@Year": "05",
    "LawNum": "平成五年通商産業省令第六十七号",
    "LawBody": {
      "LawTitle": "計量法施行令附則第四条、第五条及び附則別表第四の規定に基づく質量計に係る経過措置に関する省令",
      "EnactStatement": "計量法施行令（平成五年政令第三百二十九号）附則第四条第二項、第五条第一項から第三項まで及び附則別表第四第二号ロの規定に基づき、並びにこれらの規定及び同令附則第四条第三項の規定を実施するため、計量法施行令附則第四条、第五条及び附則別表第四の規定に基づく質量計に係る経過措置に関する省令を次のように制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（追加非自動はかりに係る届出事項等）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "計量法施行令（平成五年政令第三百二十九号。以下「令」という。）附則第四条第二項に規定する届出をしようとする者は、次の事項を記載した様式第一による届出書を提出しなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "氏名又は名称及び住所並びに法人にあっては、その代表者の氏名"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "当該非自動はかりを使用する場所"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "当該非自動はかりに係る次に掲げる事項"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "種類"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "製造事業者の氏名又は名称"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "型式又は製造番号（器物番号を含む。以下同じ。）"
                      }
                    },
                    {
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": "ひょう量"
                      }
                    },
                    {
                      "@Num": "5",
                      "Subitem1Title": "ホ",
                      "Subitem1Sentence": {
                        "Sentence": "目量又は感量（以下「目量等」という。）"
                      }
                    }
                  ]
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（届出済証）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "令附則第四条第三項の届出済証は、次に掲げる形状、方法及び大きさにより付するものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "届出済証の形状は、次のとおりとする。"
                    },
                    "FigStruct": {
                      "Fig": {
                        "@src": "./pict/H05F03801000067-001.jpg"
                      }
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "届出済証は、はり付け印とする。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "届出済証の大きさは直径が三十五ミリメートルの円形とする。"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "届出済証を付する非自動はかりの部分は、当該非自動はかりの見やすい箇所とする。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（令附則第五条の質量計）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "令附則第五条第一項の経済産業省令で定める非自動はかり、分銅及びおもり（定量おもり及び定量増おもりをいう。以下同じ。）は、次のとおりとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "令第二条第二号イに掲げる非自動はかり（令第五条第一号及び第二号に掲げるものを除く。）であって、次に掲げるもの"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "検出部が電気式のものであって、次に掲げるもの"
                      },
                      "Subitem2": [
                        {
                          "@Num": "1",
                          "Subitem2Title": "（１）",
                          "Subitem2Sentence": {
                            "Sentence": "電気抵抗線式はかりであって、懸垂式のもの"
                          }
                        },
                        {
                          "@Num": "2",
                          "Subitem2Title": "（２）",
                          "Subitem2Sentence": {
                            "Sentence": "電気抵抗線式はかり（（１）に掲げるものを除く。）であって、ひょう量が三トン以下のもの"
                          }
                        },
                        {
                          "@Num": "3",
                          "Subitem2Title": "（３）",
                          "Subitem2Sentence": {
                            "Sentence": "（１）又は（２）に掲げるもの以外のものであって、ひょう量が百五十キログラム以下のもの"
                          }
                        }
                      ]
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "イに掲げるもの以外のものであって、次に掲げるもの"
                      },
                      "Subitem2": [
                        {
                          "@Num": "1",
                          "Subitem2Title": "（１）",
                          "Subitem2Sentence": {
                            "Sentence": "ばね式指示はかりであって、懸垂式のもの"
                          }
                        },
                        {
                          "@Num": "2",
                          "Subitem2Title": "（２）",
                          "Subitem2Sentence": {
                            "Sentence": "ばね式指示はかり（（１）に掲げるものを除く。）であって、ひょう量が五百キログラム以下のもの"
                          }
                        },
                        {
                          "@Num": "3",
                          "Subitem2Title": "（３）",
                          "Subitem2Sentence": {
                            "Sentence": "台手動はかりであって、ひょう量が三トン以下のもの"
                          }
                        },
                        {
                          "@Num": "4",
                          "Subitem2Title": "（４）",
                          "Subitem2Sentence": {
                            "Sentence": "棒はかりであって、ひょう量が二百五十キログラム以下のもの"
                          }
                        },
                        {
                          "@Num": "5",
                          "Subitem2Title": "（５）",
                          "Subitem2Sentence": {
                            "Sentence": "（１）から（４）までに掲げるもの以外のものであって、ひょう量が百五十キログラム以下のもの"
                          }
                        }
                      ]
                    }
                  ]
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "表す質量が十ミリグラム以上の分銅"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "おもり"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（検定済み質量計の所持の届出）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "令附則第五条第二項に規定する届出をしようとする計量法（平成四年法律第五十一号。以下「法」という。）第四十一条の届出製造事業者、法第四十六条第二項の届出修理事業者又は法第五十二条の販売事業者は、次の事項を記載した様式第二による届出書を提出しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "氏名又は名称及び住所並びに法人にあっては、その代表者の氏名"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "当該質量計に係る次に掲げる事項"
                    },
                    "Subitem1": [
                      {
                        "@Num": "1",
                        "Subitem1Title": "イ",
                        "Subitem1Sentence": {
                          "Sentence": "非自動はかりにあっては、次に掲げる事項"
                        },
                        "Subitem2": [
                          {
                            "@Num": "1",
                            "Subitem2Title": "（１）",
                            "Subitem2Sentence": {
                              "Sentence": "種類"
                            }
                          },
                          {
                            "@Num": "2",
                            "Subitem2Title": "（２）",
                            "Subitem2Sentence": {
                              "Sentence": "製造事業者の氏名又は名称"
                            }
                          },
                          {
                            "@Num": "3",
                            "Subitem2Title": "（３）",
                            "Subitem2Sentence": {
                              "Sentence": "型式又は製造番号"
                            }
                          },
                          {
                            "@Num": "4",
                            "Subitem2Title": "（４）",
                            "Subitem2Sentence": {
                              "Sentence": "ひょう量"
                            }
                          },
                          {
                            "@Num": "5",
                            "Subitem2Title": "（５）",
                            "Subitem2Sentence": {
                              "Sentence": "目量等"
                            }
                          }
                        ]
                      },
                      {
                        "@Num": "2",
                        "Subitem1Title": "ロ",
                        "Subitem1Sentence": {
                          "Sentence": "分銅及びおもりにあっては、次に掲げる事項"
                        },
                        "Subitem2": [
                          {
                            "@Num": "1",
                            "Subitem2Title": "（１）",
                            "Subitem2Sentence": {
                              "Sentence": "種類"
                            }
                          },
                          {
                            "@Num": "2",
                            "Subitem2Title": "（２）",
                            "Subitem2Sentence": {
                              "Sentence": "製造事業者の氏名又は名称"
                            }
                          },
                          {
                            "@Num": "3",
                            "Subitem2Title": "（３）",
                            "Subitem2Sentence": {
                              "Sentence": "分銅にあっては表す質量、おもりにあっては掛量"
                            }
                          }
                        ]
                      }
                    ]
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の届出書は、平成六年十月三十一日までに提出するものとする。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": "（証票）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "令附則第五条第三項の通商産業省令で定める証票の形状は、次のとおりとする。"
                },
                "FigStruct": {
                  "Fig": {
                    "@src": "./pict/H05F03801000067-002.jpg"
                  }
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "第二条（第一項第一号を除く。）の規定は、前項の証票に準用する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、特定計量器検定検査規則（平成五年通商産業省令第七十号）第四十八条第三項に規定するものにあっては、同項の経済産業大臣が別に定める方法及び箇所とすることを妨げない。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（型式外質量計と構造上一体となった器具等）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "令附則別表第四第二号ロの経済産業省令で定める器具、機械又は装置は、次のとおりとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "自動搬出入装置"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "選別機械"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "印字機構"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "自動包装機械又は自動荷造機械"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "金銭登録機械又はその類似の機械"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "医療用又は医療関連器具、機械又は装置"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "前各号に類する器具、機械又は装置"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、法の施行の日（平成五年十一月一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年一〇月一三日通商産業省令第二四五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十三年一月六日から施行する。"
            }
          }
        }
      ],
      "AppdxStyle": [
        {
          "AppdxStyleTitle": "様式第１（第１条関係）",
          "StyleStruct": {
            "Style": {
              "Fig": {
                "@src": "./pict/H05F03801000067-001.pdf"
              }
            }
          }
        },
        {
          "AppdxStyleTitle": "様式第２（第４条関係）",
          "StyleStruct": {
            "Style": {
              "Fig": {
                "@src": "./pict/H05F03801000067-002.pdf"
              }
            }
          }
        }
      ]
    }
  }
}
