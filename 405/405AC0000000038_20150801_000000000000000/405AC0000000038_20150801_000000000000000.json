              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一四年一二月一一日法律第一四五号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "この法律は、公布の日から施行する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
