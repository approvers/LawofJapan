            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この法律（第二条及び第三条を除く。）は、平成十三年一月六日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
