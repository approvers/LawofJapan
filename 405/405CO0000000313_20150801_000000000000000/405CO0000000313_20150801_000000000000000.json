            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法の施行の日（平成五年十月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成七年九月二九日政令第三四七号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
