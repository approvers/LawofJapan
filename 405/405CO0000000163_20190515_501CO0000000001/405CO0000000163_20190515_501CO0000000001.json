              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前条に規定する貨幣の発行枚数は、二百万枚とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
