            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十二年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年三月二九日政令第一三二号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
