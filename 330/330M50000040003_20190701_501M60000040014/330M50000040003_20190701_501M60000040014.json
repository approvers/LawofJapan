                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "企業の主な業務"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "給与所得者用調査票の枚数及び人員数"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "5",
                      "Subitem1Title": "ホ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "組織及び資本金"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "6",
                      "Subitem1Title": "ヘ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "給与所得者数"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "7",
                      "Subitem1Title": "ト",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "年間給与支給総額"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "8",
                      "Subitem1Title": "チ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "給与支給総額に対する年間源泉徴収税額"
                        }
                      }
                    }
                  ]
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "給与所得者に関する事項"
                    }
                  },
                  "Subitem1": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "給与所得者の氏名又は記号等、性別、年齢、勤続年数及び職務"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
