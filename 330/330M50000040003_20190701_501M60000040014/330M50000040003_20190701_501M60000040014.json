                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "年末調整の有無"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "扶養親族の内訳"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "5",
                      "Subitem1Title": "ホ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
