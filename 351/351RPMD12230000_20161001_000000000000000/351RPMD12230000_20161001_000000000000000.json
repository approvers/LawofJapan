    "@Year": "51",
    "LawNum": "昭和五十一年十二月二十三日内閣総理大臣決定",
    "LawBody": {
      "LawTitle": "内閣情報調査室組織規則",
      "EnactStatement": "内閣官房組織令（昭和三十二年政令第二百十九号）第九条の規定に基づき、内閣調査室組織規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（この規則の趣旨）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この規則は、内閣官房組織令第四条に規定する内閣情報調査室の所掌事務に関し、法令に従い能率的にその任務を遂行するに足る内閣情報調査室の組織を定めるものとする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（次長）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣情報調査室に、次長一人を置き、内閣審議官のうちから命ずる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "次長は、内閣情報官を助け、内閣情報調査室の事務（内閣衛星情報センターの所掌に属するものを除く。）を整理する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（事務の処理区分）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "内閣情報調査室の事務は、内閣衛星情報センターの所掌に属するものを除き、次の四部門及び一センターに区分して処理する。"
              },
              "List": [
                {
                  "ListSentence": {
                    "Sentence": "総務部門"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "国内部門"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "国際部門"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "経済部門"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "内閣情報集約センター"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（総務部門）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務部門においては、内閣情報調査室に関し次に掲げる事務を担当する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "職員の人事、厚生及び教養訓練に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "予算、決算及び会計に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "公印の保管に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "公文書類の接受、発送及び保存に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "広報に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策に関する図書その他の資料の収集、整理、保存及び利用に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "電子計算機及び関連機器による情報の処理に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "各部門及び内閣情報集約センターの連絡調整に関すること。"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策に関する重要な情報の総合的な分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策に関する学識経験者の研究、提言等の取りまとめに関すること。"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "各行政機関の行う情報の収集及び分析その他の調査であつて、内閣の重要政策一般に係るものの連絡調整に関すること。"
                  }
                },
                {
                  "@Num": "12",
                  "ItemTitle": "十二",
                  "ItemSentence": {
                    "Sentence": "内閣情報会議に関すること。"
                  }
                },
                {
                  "@Num": "13",
                  "ItemTitle": "十三",
                  "ItemSentence": {
                    "Sentence": "内閣衛星情報センターとの連絡調整に関すること。"
                  }
                },
                {
                  "@Num": "14",
                  "ItemTitle": "十四",
                  "ItemSentence": {
                    "Sentence": "情報の保全に関すること。"
                  }
                },
                {
                  "@Num": "15",
                  "ItemTitle": "十五",
                  "ItemSentence": {
                    "Sentence": "次に掲げる事務のうち特定秘密の保護に関すること"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "内閣の重要政策に関する基本的な方針に関する企画及び立案並びに総合調整に関する事務"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "閣議に係る重要事項に関する企画及び立案並びに総合調整に関する事務"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "行政各部の施策の統一を図るために必要となる企画及び立案並びに総合調整に関する事務"
                      }
                    },
                    {
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": "イからハまでに掲げるもののほか、行政各部の施策に関するその統一保持上必要な企画及び立案並びに総合調整に関する事務"
                      }
                    }
                  ]
                },
                {
                  "@Num": "16",
                  "ItemTitle": "十六",
                  "ItemSentence": {
                    "Sentence": "内閣情報官の事務の整理に関すること。"
                  }
                },
                {
                  "@Num": "17",
                  "ItemTitle": "十七",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、他の部門及び内閣情報集約センターの担当に属しない事務に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（国内部門）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国内部門においては、経済部門の担当に属するものを除き、次に掲げる事務を担当する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策に関する国内の新聞、放送、雑誌等の論調の収集及び分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策に関する国民の意見の収集及び分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "前二号に掲げるもののほか、内閣の重要政策に関する国内の情報の収集及び分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "国内政策に関連して各行政機関が行う情報の収集及び分析その他の調査であつて、内閣の重要政策に係るものの連絡調整に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（国際部門）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国際部門においては、経済部門の担当に属するものを除き、次に掲げる事務を担当する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策の策定に当つて参考となる外国政府の政策に関する情報の収集及び分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策に関する外国の新聞、放送、雑誌等の論調の収集及び分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "前二号に掲げるもののほか、内閣の重要政策に関する国外の情報の収集及び分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "対外政策に関連して各行政機関が行う情報の収集及び分析その他の調査であつて、内閣の重要政策に係るものの連絡調整に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（経済部門）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "経済部門においては、次に掲げる事務を担当する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "内閣の重要政策に関する内外の経済情報の収集及び分析その他の調査に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "経済政策に関連して各行政機関が行う情報の収集及び分析その他の調査であつて、内閣の重要政策に係るものの連絡調整に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "8",
            "ArticleCaption": "（内閣情報集約センター）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "内閣情報集約センターにおいては、緊急な情報の集約及び連絡を一括して処理するほか、次に掲げる事務を担当する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "大規模災害その他の緊急事態における情報集約体制の整備に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "大規模災害その他の緊急事態に関連して各行政機関が行う情報の収集及び分析その他の調査であって、内閣の重要政策に係るものの連絡調整に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "通信社等によるニュースの受信及び連絡に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "9",
            "ArticleCaption": "（部門等における事務の整理）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "第四条から前条までの規定により各部門及び内閣情報集約センターにおいて担当することとされた事務は、内閣参事官のうちから指名された者が、部門又は内閣情報集約センターごとに、その全部又は一部を整理する。"
              }
            }
          },
          {
            "@Num": "10",
            "ArticleCaption": "（調査官）",
            "ArticleTitle": "第十条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣情報調査室に、併任の者を除き、調査官十人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "調査官は、命を受けて内閣情報調査室の事務に従事する。"
                }
              }
            ]
          },
          {
            "@Num": "10_2",
            "ArticleCaption": "（内閣衛星情報センター）",
            "ArticleTitle": "第十条の二",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "内閣衛星情報センターの内部組織については、内閣衛星情報センター組織規則（平成十三年三月二十九日内閣総理大臣決定）の定めるところによる。"
              }
            }
          },
          {
            "@Num": "11",
            "ArticleCaption": "（補則）",
            "ArticleTitle": "第十一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この規則に定めるもののほか、内閣情報調査室の内部組織に関し必要な細目は、内閣官房長官が定める。"
              }
            }
          }
        ]
      },
