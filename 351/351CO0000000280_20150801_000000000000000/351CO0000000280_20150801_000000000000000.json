                  "Subitem1Title": "イ",
                  "Subitem1Sentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "液圧プレス（矯正プレスを除く。）"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "Subitem1Title": "ロ",
                  "Subitem1Sentence": {
