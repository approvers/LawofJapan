            "ChapterTitle": "第一章　総則",
            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ArticleCaption": "（目的）",
                "ArticleTitle": "第一条",
                "Paragraph": {
