                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法第九条第一項の規定による補償に係る物件の所在する土地"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
