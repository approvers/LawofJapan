{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "187",
    "@Year": "53",
    "LawNum": "昭和五十三年政令第百八十七号",
    "LawBody": {
      "LawTitle": "農漁業保険審査会令",
      "EnactStatement": "内閣は、農林省設置法（昭和二十四年法律第百五十三号）第三十四条第二項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "農漁業保険審査会（以下「審査会」という。）は、委員十五人で組織する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、学識経験のある者のうちから、農林水産大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、これに欠員が生じた場合における補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、再任されることができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "農林水産大臣は、委員が次の各号のいずれかに該当する場合には、任期中でも、これを解任することができる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "故意に職務を怠つた場合"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "心身の故障のため、職務の遂行に支障があり、又はこれに堪えない場合"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "刑事事件に関し起訴された場合"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会に会長を置き、委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長は、会務を総理し、審査会を代表する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長に事故があるときは、あらかじめその指名する委員が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（会議）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会の会議は、委員の過半数が出席しなければ、開くことができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会の議事は、出席した委員の過半数で決し、可否同数のときは、会長の決するところによる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（部会）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会は、その定めるところにより、部会を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に属すべき委員は、会長が指名する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に部会長を置き、当該部会に属する委員の互選によつてこれを定める。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長は、当該部会の事務を掌理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長に事故があるときは、当該部会に属する委員のうちから部会長があらかじめ指名する者が、その職務を代理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "前条の規定は、部会に準用する。"
                    },
                    {
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "この場合において、同条第二項中「会長」とあるのは、「部会長」と読み替えるものとする。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審査会の庶務は、農林水産省経営局保険課において水産庁漁政部漁業保険管理官の協力を得て処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、審査会の運営に関し必要な事項は、会長が審査会に諮つて定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、公布の日から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "農業共済再保険審査会令（昭和二十四年政令第三十五号）、森林保険審査会令（昭和二十四年政令第四十九号）及び漁業共済保険審査会令（昭和四十二年政令第三百三十七号）は、廃止する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和五三年七月五日政令第二八二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、公布の日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五六年九月一一日政令第二七六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、漁船損害補償法の一部を改正する法律の施行の日（昭和五十六年十月一日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五九年六月二一日政令第二〇七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和五十九年七月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和六三年四月一二日政令第一二三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三一〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、次条の規定は、公布の日から施行する。"
                    }
                  ]
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ArticleCaption": "（委員の任期に関する経過措置）",
              "ArticleTitle": "第二条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令の施行の日の前日において次に掲げる従前の審議会その他の機関の委員である者の任期は、当該委員の任期を定めたそれぞれの政令の規定にかかわらず、その日に満了する。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "獣医事審議会"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "果樹農業振興審議会"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "林政審議会"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "農林漁業保険審査会"
                      }
                    }
                  }
                ]
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一七年九月二二日政令第三〇〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十七年十月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二五年三月二七日政令第八四号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成二十五年四月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年二月一二日政令第四二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成二十七年四月一日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
