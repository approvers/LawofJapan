    "@Year": "49",
    "LawNum": "昭和四十九年人事院規則二―八",
    "LawBody": {
      "LawTitle": "人事院規則二―八（人事院の顧問及び参与）",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "人事院は、国家公務員法に基づき、人事院規則二―八（人事院の参与）の全部を次のように改正する。"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（顧問）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "人事院に、顧問一人を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "顧問は、人事院の所掌する事務のうち、人事行政上の重要事項について、人事院の諮問に答える。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "顧問は、人事行政に関し学識経験のある者のうちから、総裁が委嘱する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "顧問の任期は、二年とする。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": "顧問は、非常勤とする。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（参与）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "人事院に、参与十二人以内を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "参与は、人事院の所掌する事務のうち、重要な事項について、人事院に意見を述べる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "参与は、学識経験のある者のうちから、総裁が委嘱する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "参与の任期は、二年とする。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": "参与は、非常勤とする。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "@AmendLawNum": "昭和六〇年二月二七日人事院規則二―八―一",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和六一年四月五日人事院規則二―八―二",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、公布の日から施行する。"
            }
          }
        }
      ]
    }
  }
}
