              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十二年四月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三一一号",
          "@Extract": "true",
