{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "285",
    "@Year": "49",
    "LawNum": "昭和四十九年政令第二百八十五号",
    "LawBody": {
      "LawTitle": "生産緑地法施行令",
      "EnactStatement": "内閣は、生産緑地法（昭和四十九年法律第六十八号）第二条第二号及び第四号、第六条第六項（第十二条第四項において準用する場合を含む。）並びに第八条第九項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（公共施設等）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "生産緑地法（以下「法」という。）第二条第二号の政令で定める公共の用に供する施設又は公益性が高いと認められる施設で政令で定めるものは、次に掲げる施設とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "都市計画法（昭和四十三年法律第百号）第四条第六項に規定する都市計画施設"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "土地収用法（昭和二十六年法律第二百十九号）第三条各号（第二十九号及び第二十九号の二を除く。）に掲げる施設"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "土地収用法第三条第二十九号に掲げる公園事業に係る施設"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（地方公共団体等）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二条第四号の政令で定める法人は、地方公共団体、港務局、土地開発公社、地方住宅供給公社、地方道路公社及び独立行政法人都市再生機構とする。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（条例で農地等の区域の規模に関する条件を定める場合の基準）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第三条第二項の政令で定める基準は、三百平方メートル以上五百平方メートル未満の一定の規模以上の区域であることとする。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（収用委員会の裁決の申請手続）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第六条第六項（法第十二条第四項において準用する場合を含む。）の規定により土地収用法第九十四条第二項の規定による裁決を申請しようとする者は、国土交通省令で定める様式に従い、次に掲げる事項を記載した裁決申請書を収用委員会に提出しなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "裁決申請者の氏名及び住所"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "相手方の氏名及び住所"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "損失の事実並びに損失の補償の見積り及びその内訳（生産緑地の買取りの申出に係る場合にあつては、当該生産緑地の価額の見積り及びその内訳）"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "協議の経過"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（法第八条第二項第三号の政令で定める施設）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第八条第二項第三号の政令で定める施設は、次に掲げる施設であつて、主として都市の住民の利用に供される農地で、相当数の者を対象として定型的な条件で、レクリエーションその他の営利以外の目的で継続して行われる農作業の用に供されるものに設置される当該農地の保全又は利用上必要なものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "農作業の講習の用に供する施設"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "管理事務所その他の管理施設"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（法第八条第九項の政令で定める行為）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第八条第九項の政令で定める行為は、次に掲げる行為とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "建築物以外の工作物で次に掲げるものの新設、改築又は増設"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "仮設の工作物"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "Ruby": {
                            "Rt": "きよ",
                            "#text": "渠"
                          },
                          "#text": "水道管、下水道管その他これらに類する工作物で地下に設けるもの"
                        }
                      }
                    }
                  ]
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "法令又はこれに基づく処分による義務の履行として行う行為"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "当該生産緑地において農林漁業を営むために行う法第八条第二項第一号又は第二号に規定する施設（畜舎を除く。）の設置又は管理に係る行為で次に掲げるもの"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "建築物その他の工作物の新築、改築又は増築で、その新築、改築又は増築に係る部分の床面積の合計又は築造面積が九十平方メートル以下であるもの"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "幅員が二メートル以下の用排水路又は幅員が二メートル以下の農道若しくは林道の設置又は管理"
                      }
                    }
                  ]
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "農地等とするための土地の形質の変更、水面の埋立て又は干拓"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この政令は、法の施行の日（昭和四十九年八月三十一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和五〇年八月五日政令第二四八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五〇年一〇月二四日政令第三〇六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、法の施行の日（昭和五十年十一月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五六年八月三日政令第二六八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、昭和五十六年十月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三年九月六日政令第二八二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この政令は、生産緑地法の一部を改正する法律（平成三年法律第三十九号）の施行の日（平成三年九月十日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一一年八月一八日政令第二五六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、都市基盤整備公団法（以下「公団法」という。）の一部の施行の日（平成十一年十月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三一二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年四月九日政令第一六〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、平成十六年七月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年五月二六日政令第一八一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この政令は、機構の成立の時から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二九年六月一四日政令第一五六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、都市緑地法等の一部を改正する法律の施行の日(平成二十九年六月十五日)から施行する。"
              }
            }
          }
        }
      ]
    }
  }
