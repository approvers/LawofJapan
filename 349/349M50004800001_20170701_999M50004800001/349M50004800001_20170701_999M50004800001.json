            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二条第三項第五号の国土交通省令で定める図書は、次に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "個人にあつては、戸籍抄本又は本籍の記載のある住民票の写し"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "法人（公共団体を除く。次号において同じ。）を設立しようとするものにあつては、次に掲げる書類"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "定款又は寄附行為の謄本"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "発起人、社員又は設立者（以下「発起人等」という。）の名簿"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "株式の引受け、出資又は財産の寄附の状況又は見込みを記載した書類"
                      }
                    }
                  ]
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "既存の法人にあつては、次に掲げる書類"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "定款又は寄附行為の謄本及び登記事項証明書"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "最近の事業年度における財産目録、貸借対照表及び損益計算書"
                      }
                    }
                  ]
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "直前三月以内に撮影した埋立区域等の写真"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "埋立てに用いる土砂等の採取場所及び採取量を記載した図書"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "埋立てに関する工事に要する費用に充てる資金の調達方法を証する書類"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "埋立地の用途及び利用計画の概要を表示した図面"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "環境保全に関し講じる措置を記載した図書"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "公共施設の配置及び規模について説明した図書"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "公有水面埋立法施行令（以下「令」という。）第七条に規定する法人にあつては、同条第二号に適合することを証する書類"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "法第四条第三項の権利を有する者がある場合にあつては、その者の同意を得たことを証する書類又は同意が得られない旨及びその事由を記載した書類"
                  }
                },
                {
                  "@Num": "12",
                  "ItemTitle": "十二",
                  "ItemSentence": {
                    "Sentence": "公有水面の利用に関して設置した施設で埋立てのためにその効用が妨げられるものがある場合にあつては、当該施設の種類及び設置者を記載した書類"
                  }
                }
              ]
            }
          },
