    "LawNum": "昭和三年勅令第百八十八号",
    "LawBody": {
      "LawTitle": "昭和三年勅令第百八十八号（大礼記念章制定ノ件）",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "昭和三年十一月行ハルル大礼記念ノ表章トシテ記念章ヲ設ク"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "記念章ノ図式左ノ如シ"
                },
                "List": [
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "章"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "銀円形、径一寸トス"
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": "表面ニハ輪廓内中央ニ高御座、其ノ中心ニ金菊御紋、周辺ニ桜橘文様、左右空間ニ雲文ヲ置キ下方ニ万歳ノ文字ヲ識ス"
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": "裏面ニハ輪廓ニ菊花形、中央ニ旗形、其ノ左右ニ雲文ヲ置キ旗形ノ内ニ大礼記念章、下方ニ昭和三年十一月ノ文字ヲ識ス"
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "環"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "銀円形トス"
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "綬"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "織地、幅一寸二分、青黄赤白紫色ノ等分トス"
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "記念章ハ綬ヲ以テ左肋ニ佩ブ"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "記念章ハ左ニ掲グル者ニ之ヲ授与ス"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "践祚ノ式ニ召サレタル者"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "即位礼及大嘗祭ノ式ニ召サレタル者"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "各所在地ニ於テ饗饌ヲ賜リタル者"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "大礼ノ事務及大礼ニ伴フ要務ニ関与シタル者"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "記念章ハ本人ニ限リ終身之ヲ佩用シ子孫ヲシテ之ヲ保存セシム"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "記念章ヲ授与セラルベキ者其ノ授与前死亡シタルトキハ之ヲ其ノ家督相続人又ハ戸主ニ交付シテ保存セシム"
              }
            }
          }
        ]
      },
      "AppdxStyle": {
        "AppdxStyleTitle": "大礼記念章ノ図",
