              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "紀元二千六百年祝典記念ノ表章トシテ記念章ヲ設ク"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "@OldStyle": "false",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "記念章ノ図式左ノ如シ"
                  }
                },
                "List": [
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "章"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "アルミニウム青銅円形径三糎トス"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "表面ニハ輪廓内ニ菊御紋、賢所、皇霊殿、神殿及宮城ノ図ヲ表シ裏面ニハ輪廓内中央ニ紀元二千六百年祝典記念章ノ文字、下部ニ昭和十五年ノ文字ヲ識ス"
                      }
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "環"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "アルミニウム青銅円形トス"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "綬"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "織地幅三糎六粍トス"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "空色ニ八条ノ紅線トス"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "@OldStyle": "false",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "記念章ハ綬ヲ用ヒテ左肋ニ佩ブ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "@OldStyle": "false",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "記念章ハ左ニ掲グル者ニ之ヲ授与ス"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "昭和十五年紀元節祭ニ召サレタル者"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "紀元二千六百年式典ニ招カレタル者"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "祝典ノ事務及祝典ニ伴フ要務ニ関与シタル者"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "@OldStyle": "false",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ニ規定スル者以外ノ者ニモ特ニ記念章ヲ授与スルコトアルベシ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "@OldStyle": "false",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "記念章ハ本人ニ限リ終身之ヲ佩用シ子孫ヲシテ之ヲ保存セシム"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "@OldStyle": "false",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "記念章ヲ授与セラルベキ者其ノ授与前死亡シタルトキハ之ヲ其ノ家督相続人又ハ戸主ニ交付シテ保存セシム"
                }
              },
              "List": {
                "ListSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "紀元二千六百年祝典記念章ノ図"
                  }
                }
              }
            }
          }
        ]
      }
    }
  }
