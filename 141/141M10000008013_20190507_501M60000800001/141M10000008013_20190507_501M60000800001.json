{
  "Law": {
    "@DataInfo": "190624e03k11m01",
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "013",
    "@Year": "41",
    "LawNum": "明治四十一年内務省令第十三号",
    "LawBody": {
      "LawTitle": "明治四十一年内務省令第十三号（水害予防組合法ニ依ル予算調製ノ式及費目流用其ノ他財務ニ関スル件）",
      "Preamble": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "水利組合法第七十条ニ依リ予算調製ノ式及費目流用其ノ他財務ニ関スル件左ノ通定ム"
            }
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "組合費其ノ他一切ノ収入ヲ歳入トシ一切ノ経費ヲ歳出トシ歳入歳出ハ予算ニ編入スヘシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "各年度ニ於テ決定シタル歳入ヲ以テ他ノ年度ニ属スヘキ歳出ニ充ツルコトヲ得ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "各年度ニ於テ歳計ニ剰余アルトキハ翌年度ノ歳入ニ編入スヘシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "歳入ノ誤納過納ト為リタル金額ノ払戻ハ各之ヲ収入シタル歳入ヨリ支払フヘシ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "歳出ノ誤払過渡ト為リタル金額現金前渡前金払概算払繰替払ノ返納ハ各之ヲ支払ヒタル経費ノ定額ニ戻入スヘシ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "出納閉鎖後ノ収入支出ハ之ヲ現年度ノ歳入歳出ト為スヘシ第四条ノ払戻金戻入金ノ出納閉鎖後ニ係ルモノ亦同シ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "継続費ハ毎年度ノ支払残額ヲ継続年度ノ終リ迄逓次繰越使用スルコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "歳入歳出予算ハ之ヲ款項ニ区分スベシ但シ必要アルトキハ経常臨時ノ二部ニ別チ各部ヲ更ニ款項ニ区分スルコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "歳入歳出予算ニハ予算説明ヲ附スベシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "9",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "歳入歳出予算ハ別記第一号様式ニ依リ継続費ノ年期及支出方法ハ別記第二号様式ニ依リ之ヲ調製スベシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "10",
            "ArticleTitle": "第十条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "予算ハ会計年度経過後ニ於テ更正又ハ追加ヲ為スコトヲ得ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "11",
            "ArticleTitle": "第十一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "予算ニ定メタル各款ノ金額ハ彼是流用スルコトヲ得ス予算各項ノ金額ハ組合会ノ議決ヲ経テ之ヲ流用スルコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "12",
            "ArticleTitle": "第十二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "決算ハ予算ト同一ノ区分ニ依リ之ヲ調製シ予算ニ対スル過不足ノ説明ヲ付スヘシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "13",
            "ArticleTitle": "第十三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "会計年度経過後ニ至リ歳入ニ不足ヲ生シ歳出ニ充ツルニ足ラサルトキハ翌年度ノ歳入ヲ繰上ケ之ニ充用スルコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "14",
            "ArticleTitle": "第十四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "組合ノ出納ハ毎月例日ヲ定メテ検査シ且毎会計年度少クトモ二回臨時検査ヲ為スヘシ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "検査ハ管理者之ヲ為シ臨時検査ニハ組合会ニ於テ選挙シタル議員二人以上立会ハシムヘシ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "15",
            "ArticleTitle": "第十五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "組合ニ属スル現金ノ出納及保管ノ為組合会ノ議決ヲ経テ組合金庫ヲ置クコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "16",
            "ArticleTitle": "第十六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "金庫事務ノ取扱ヲ為サシムヘキ銀行ハ組合会ノ議決ヲ経テ之ヲ定ム"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "17",
            "ArticleTitle": "第十七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "金庫事務ノ取扱ヲ為ス者ハ現金ノ出納保管ニ付責任ヲ有ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "18",
            "ArticleTitle": "第十八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "金庫事務ノ取扱ヲ為ス者ハ担保ヲ組合ニ提出スヘシ其ノ担保ニ関シテハ組合会ノ議決ヲ経テ管理者之ヲ定ム"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "19",
            "ArticleTitle": "第十九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "管理者ハ組合金庫ヲ監督シ定期及臨時ニ現金帳簿ヲ検査シ又必要ト認ムルトキハ臨機ノ処分ヲ為スコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "20",
            "ArticleTitle": "第二十条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本令ニ規定スルモノノ外組合ハ府県知事ノ許可ヲ得テ必要ナル規定ヲ設クルコトヲ得"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@AmendLawNum": "昭和一九年一月七日内務省令第二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本令ハ公布ノ日ヨリ之ヲ施行ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成元年三月二七日建設省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "令和元年五月七日国土交通省令第一号",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
