                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "6",
              "ItemTitle": "六",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "独立行政法人造幣局"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "7",
              "ItemTitle": "七",
              "ItemSentence": {
