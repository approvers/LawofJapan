            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphCaption": "（関係政令の廃止）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "次に掲げる政令は、廃止する。"
              }
            },
