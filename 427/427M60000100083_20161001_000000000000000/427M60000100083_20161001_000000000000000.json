{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "083",
    "@Year": "27",
    "LawNum": "平成二十七年厚生労働省令第八十三号",
    "LawBody": {
      "LawTitle": "地方年金記録訂正審議会規則",
      "EnactStatement": "厚生労働省組織令（平成十二年政令第二百五十二号）第百五十三条の二第三項の規定に基づき、地方年金記録訂正審議会規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（所掌事務）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "地方年金記録訂正審議会（以下「審議会」という。）は、厚生年金保険法（昭和二十九年法律第百十五号）第百条の九第三項の規定により読み替えられた同法第二十八条の四第三項及び国民年金法（昭和三十四年法律第百四十一号）第百九条の九第三項の規定により読み替えられた同法第十四条の四第三項の規定によりその権限に属させられた事項の処理に関する事務をつかさどる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会は、委員三十人以内で組織する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "審議会に、特別の事項を調査審議させるため必要があるときは、臨時委員を置くことができる。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（委員等の任命）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "委員及び臨時委員（以下「委員等」という。）は、学識経験のある者のうちから、地方厚生局長が任命する。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（委員の任期等）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "委員の任期は、二年とし、一年ごとに、その半数を任命する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "委員は、再任されることができる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "臨時委員は、第二条第二項の規定による特別の事項の調査審議が終了したときは、解任されるものとする。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "委員等は、非常勤とする。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "#text": "委員等は、職務上知ることのできた秘密を漏らしてはならない。"
                    },
                    {
                      "@Num": "2",
                      "#text": "その職を退いた後も同様とする。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会に、会長を置き、委員の互選により選任する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "会長は、会務を総理し、審議会を代表する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "会長に事故があるとき、又は会長が欠けたときは、あらかじめその指名する委員が、その職務を行う。"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（部会）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会は、その定めるところにより、部会を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "部会に属すべき委員等は、会長が指名する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "部会に、部会長を置き、当該部会に属する委員等のうちから、会長が指名する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "部会長は、当該部会の事務を掌理する。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": "部会長に事故があるときは、当該部会に属する委員等のうちから部会長があらかじめ指名する者が、その職務を代理する。"
                }
              },
              {
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": "審議会は、その定めるところにより、部会の議決をもって審議会の議決とすることができる。"
                }
              }
            ]
          },
          {
            "@Num": "7",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会は、委員及び議事に関係のある臨時委員の過半数が出席しなければ、会議を開き、議決することができない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "審議会の議事は、委員及び議事に関係のある臨時委員で会議に出席したものの過半数で決し、可否同数のときは、会長の決するところによる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "前二項の規定は、部会の議事について準用する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "委員等は、自己の利害に関係する議事に参与することができない。"
                }
              }
            ]
          },
          {
            "@Num": "8",
            "ArticleCaption": "（資料の提出等の要求）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会は、その所掌事務を遂行するため必要があると認めるときは、地方厚生局長又は地方厚生支局長に対し、資料の提出、意見の表明、説明その他必要な協力を求め、又は国民年金法第十四条の二第一項（同条第二項において準用する場合を含む。）又は厚生年金保険法第二十八条の二第一項（同条第二項及び第三項において準用する場合を含む。）の規定により訂正の請求をした者、事業主その他の関係者の意見を聴くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の規定は、部会について準用する。"
                }
              }
            ]
          },
          {
            "@Num": "9",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "審議会の庶務は、地方厚生局年金審査課において処理する。"
              }
            }
          },
          {
            "@Num": "10",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第十条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令に定めるもののほか、議事の手続その他審議会の運営に関し必要な事項は、会長が審議会に諮って定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          },
          {
            "@Num": "2",
            "ParagraphCaption": "（経過措置）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": "この省令の施行に伴い新たに任命される委員のうち、地方厚生局長が任命の際に指名する者の任期は、第四条第一項の規定にかかわらず、一年とする。"
            }
          }
        ]
      }
    }
  }
}
