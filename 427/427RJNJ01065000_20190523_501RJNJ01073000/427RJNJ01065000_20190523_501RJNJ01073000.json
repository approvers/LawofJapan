                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "派遣職員が法第七十八条第二号又は第三号に該当することとなった場合"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
