                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "派遣予定職員の組織委員会における特定業務の従事の状況の連絡に関する事項"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
