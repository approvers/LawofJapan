{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "309",
    "@Year": "27",
    "LawNum": "平成二十七年政令第三百九号",
    "LawBody": {
      "LawTitle": "電力・ガス取引監視等委員会令",
      "EnactStatement": "内閣は、電気事業法（昭和三十九年法律第百七十号）第六十六条の九第四項及び第六十六条の十六の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（特別委員）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "電力・ガス取引監視等委員会（以下「委員会」という。）に、あっせん又は仲裁に参与させるため、特別委員を置くことができる。"
                  }
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特別委員は、学識経験のある者のうちから、経済産業大臣が任命する。"
                  }
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特別委員の任期は、二年とする。"
                  }
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特別委員は、再任されることができる。"
                  }
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特別委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（専門委員）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員会に、専門の事項を調査審議させるため、専門委員を置くことができる。"
                  }
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、当該専門の事項に関し学識経験のある者のうちから、経済産業大臣が任命する。"
                  }
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、当該専門の事項に関する調査審議が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（事務局の内部組織）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員会の事務局に、課を置く。"
                  }
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項の規定に基づき置かれる課の数は、三以内とする。"
                  }
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前二項に定めるもののほか、委員会の事務局の内部組織の細目は、経済産業省令で定める。"
                  }
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（委員会の運営）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、議事の手続その他委員会の運営に関し必要な事項は、委員長が委員会に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、電気事業法等の一部を改正する等の法律（平成二十七年法律第四十七号）附則第一条第二号に掲げる規定の施行の日（平成二十七年九月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年二月二四日政令第四八号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、電気事業法等の一部を改正する等の法律（以下「改正法」という。）附則第一条第三号に掲げる規定の施行の日（平成二十八年四月一日）から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
