{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "2",
    "@Year": "06",
    "LawNum": "平成六年文部省令第二号",
    "LawBody": {
      "LawTitle": "オリンピック競技大会及びパラリンピック競技大会優秀者顕彰規程",
      "EnactStatement": "スポーツ振興法（昭和三十六年法律第百四十一号）第十五条の規定を実施するため、オリンピック競技大会優秀者顕彰規程を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（趣旨）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この規程は、国がオリンピック競技大会又はパラリンピック競技大会において特に優秀な成績を収めた者に対し顕彰を行うのに必要な事項を定めるものとする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（顕彰を受ける者）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国は、次の各号に掲げる者を特に優秀な成績を収めた者として顕彰する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "オリンピック競技大会において第一位から第三位までに入賞した者"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "パラリンピック競技大会において第一位から第三位までに入賞した者"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（顕彰状の授与）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "顕彰は、国が、顕彰状を授与することにより行う。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（奨励）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国は、次の各号に掲げる団体が当該各号に定める者を表彰（報奨金の交付を含む。）することを奨励するものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "公益財団法人日本オリンピック委員会（平成元年八月七日に公益財団法人日本オリンピック委員会という名称で設立された法人をいう。）及びその加盟競技団体（オリンピック競技大会において実施される競技に係る国際団体に加盟している団体に限る。）"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "第二条第一号に規定する者"
                      }
                    ]
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "財団法人日本障害者スポーツ協会（昭和四十年五月二十四日に財団法人日本身体障害者スポーツ協会という名称で設立された法人をいう。）"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "第二条第二号に規定する者"
                      }
                    ]
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行し、第十七回オリンピック冬季競技大会から適用する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年一〇月三一日文部省令第五三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年一二月一日文部科学省令第三六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、平成二十年十二月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二一年三月一二日文部科学省・厚生労働省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "#text": "この省令は、公布の日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "#text": "ただし、この省令による改正後のオリンピック競技大会及びパラリンピック競技大会優秀者顕彰規程の規定は、パラリンピック競技大会については、二〇〇八北京パラリンピック競技大会から適用する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "平成二二年三月三一日文部科学省・厚生労働省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "#text": "この省令は、公布の日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "#text": "ただし、改正後のオリンピック競技大会及びパラリンピック競技大会優秀者顕彰規程の規定は、第二十一回オリンピック冬季競技大会から適用する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "平成二三年七月二七日文部科学省令第二七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、スポーツ基本法の施行の日（平成二十三年八月二十四日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
