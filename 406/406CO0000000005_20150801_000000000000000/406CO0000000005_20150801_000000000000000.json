    "@Year": "06",
    "LawNum": "平成六年政令第五号",
    "LawBody": {
      "LawTitle": "労働基準法第三十七条第一項の時間外及び休日の割増賃金に係る率の最低限度を定める政令",
      "EnactStatement": "内閣は、労働基準法（昭和二十二年法律第四十九号）第三十七条第一項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "労働基準法第三十七条第一項の政令で定める率は、同法第三十三条又は第三十六条第一項の規定により延長した労働時間の労働については二割五分とし、これらの規定により労働させた休日の労働については三割五分とする。"
            }
          }
        }
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成六年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一一年一月二九日政令第一六号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十一年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三〇九号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
