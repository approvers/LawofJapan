                      }
                    },
                    {
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Sentence": "受領先及び受領の年月日（分離した場合にあっては、分離した年月日及び分離したときの記録）"
                      }
                    },
                    {
                      "@Num": "3",
                      "ItemTitle": "三",
                      "ItemSentence": {
                        "Sentence": "継代数、継代年月日及び継代方法"
                      }
                    },
                    {
                      "@Num": "4",
                      "ItemTitle": "四",
                      "ItemSentence": {
                        "Sentence": "継代中に生じた変化及びこれに対してとった処置"
                      }
                    },
                    {
                      "@Num": "5",
                      "ItemTitle": "五",
                      "ItemSentence": {
                        "Sentence": "生物学的性状及び毒力並びにこれらを検査した年月日"
                      }
                    },
                    {
                      "@Num": "6",
                      "ItemTitle": "六",
                      "ItemSentence": {
                        "Sentence": "保存方法"
                      }
                    },
                    {
                      "@Num": "7",
                      "ItemTitle": "七",
                      "ItemSentence": {
                        "Sentence": "製造に使用したときは、その年月日並びに製造品名及び製造番号"
                      }
                    },
                    {
                      "@Num": "8",
                      "ItemTitle": "八",
                      "ItemSentence": {
                        "Sentence": "その他参考となる事項"
                      }
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "3",
