{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "26",
    "@Year": "06",
    "LawNum": "平成六年農林水産省令第二十六号",
    "LawBody": {
      "LawTitle": "臘虎膃肭獣猟獲取締法施行規則",
      "EnactStatement": "臘虎膃肭獣猟獲取締法（明治四十五年法律第二十一号）第一条第一項及び第二項の規定に基づき、臘虎膃肭獣猟獲取締法施行規則（昭和十七年農林省令第四十六号）の全部を改正する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（猟獲の禁止等）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "何人も、らっこの猟獲又はおっとせいの陸上猟獲をしてはならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "何人も、北緯三十度の線以北の太平洋の海域（ベーリング海、オホーツク海及び日本海の海域を含む。）においては、当分の間、おっとせいの海上猟獲をしてはならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "前二項の規定は、試験研究その他の特別の事由により農林水産大臣の許可を受けた者がする猟獲については、適用しない。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "農林水産大臣は、前項の許可をしたときは、許可証を交付する。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（制限又は条件）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "前条第三項の許可には、制限又は条件を付し、及びこれを変更することができる。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（所持の禁止）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "第一条第一項若しくは第二項の規定又は前条の制限若しくは条件に違反して猟獲されたらっこ又はおっとせいは、所持してはならない。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          },
          {
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": "この省令の施行前にした行為に対する罰則の適用については、なお従前の例による。"
            }
          }
        ]
      }
    }
  }
}
