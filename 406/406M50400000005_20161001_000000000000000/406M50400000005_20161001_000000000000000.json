                        {
                          "@Num": "1",
                          "Sentence": "定款"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "第二条第二項"
                        }
                      ]
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "役員の氏名、住所及び略歴を記載した書面"
