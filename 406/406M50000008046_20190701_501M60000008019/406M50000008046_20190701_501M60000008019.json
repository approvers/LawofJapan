              }
            }
          }
        },
        {
          "@AmendLawNum": "令和元年五月三一日総務省令第九号",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "令和元年六月二八日総務省令第一九号",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
