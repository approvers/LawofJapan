                        "@Num": "2",
                        "Subitem1Title": "ロ",
                        "Subitem1Sentence": {
                          "Sentence": "市町村名、大字名、字名及びその境界線"
                        }
                      },
                      {
                        "@Num": "3",
                        "Subitem1Title": "ハ",
                        "Subitem1Sentence": {
                          "Sentence": "地形及び地目（記号をもつて表示すること。）"
                        }
                      },
                      {
                        "@Num": "4",
                        "Subitem1Title": "ニ",
