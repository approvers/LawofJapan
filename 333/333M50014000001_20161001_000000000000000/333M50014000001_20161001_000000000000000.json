                        }
                      },
                      {
                        "@Num": "7",
                        "Subitem1Title": "ト",
                        "Subitem1Sentence": {
                          "Sentence": "方位"
                        }
                      },
                      {
                        "@Num": "8",
                        "Subitem1Title": "チ",
                        "Subitem1Sentence": {
                          "Sentence": "縮尺"
                        }
                      },
                      {
                        "@Num": "9",
