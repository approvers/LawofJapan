            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から起算して九十日を経過した日から施行し、この法律の施行後における証人又は参考人の供述又は出頭に係る被害について適用する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三四年四月一日法律第八七号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
