{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "192",
    "@Year": "07",
    "LawNum": "平成七年政令第百九十二号",
    "LawBody": {
      "LawTitle": "化学兵器の禁止及び特定物質の規制等に関する法律施行令",
      "EnactStatement": "内閣は、化学兵器の禁止及び特定物質の規制等に関する法律（平成七年法律第六十五号）第二条第一項から第五項まで及び第三十四条第一項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（毒性物質）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "化学兵器の禁止及び特定物質の規制等に関する法律（以下「法」という。）第二条第一項の毒性物質は、別表の第三欄に掲げる物質とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（化学兵器）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第二条第二項の政令で定める兵器は、次に掲げる兵器とする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "砲弾又はその弾体"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "ロケット弾又はその弾体"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "地雷又はその外殻"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "爆弾又はその弾体"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（特定物質及び指定物質）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二条第三項の特定物質は、別表一の項の第三欄又は第四欄に掲げる物質とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二条第四項の指定物質は、別表二の項又は三の項の第三欄又は第四欄に掲げる物質とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二条第五項の第一種指定物質は、別表二の項の第三欄又は第四欄に掲げる物質とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3_2",
            "ArticleCaption": "（運搬証明書の書換え）",
            "ArticleTitle": "第三条の二",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "運搬証明書の交付を受けた者は、当該運搬証明書の記載事項に変更を生じたときは、国家公安委員会規則で定めるところにより、遅滞なく交付を受けた都道府県公安委員会に届け出て、その書換えを受けなければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3_3",
            "ArticleCaption": "（運搬証明書の再交付）",
            "ArticleTitle": "第三条の三",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "運搬証明書の交付を受けた者は、当該運搬証明書を喪失し、汚損し、又は盗取されたときは、国家公安委員会規則で定めるところにより、その事由を付して交付を受けた都道府県公安委員会にその再交付を文書で申請しなければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3_4",
            "ArticleCaption": "（不要となった運搬証明書の返納）",
            "ArticleTitle": "第三条の四",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "運搬証明書の交付を受けた者は、次の各号のいずれかに該当することとなったときは、速やかに当該運搬証明書（第三号の場合にあっては、発見し、又は回復した運搬証明書）を交付を受けた都道府県公安委員会に返納するようにしなければならない。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "運搬を終了したとき。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "運搬をしないこととなったとき。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "運搬証明書の再交付を受けた場合において、喪失し、又は盗取された運搬証明書を発見し、又は回復したとき。"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3_5",
            "ArticleCaption": "（都道府県公安委員会の間の連絡）",
            "ArticleTitle": "第三条の五",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "運搬が二以上の都道府県にわたることとなる場合には、関係都道府県公安委員会（以下この条において「関係公安委員会」という。）は、次に掲げる措置をとるものとする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "出発地を管轄する都道府県公安委員会（以下この号において「出発地公安委員会」という。）以外の関係公安委員会にあっては、出発地公安委員会を通じて、法第十七条第一項の届出の受理及び運搬証明書の交付並びに同条第二項の指示を行うこと。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "法第十七条第二項の指示を行おうとするときは、あらかじめ、当該指示の内容を他の関係公安委員会に通知すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "前二号に定めるもののほか、当該運搬において特定物質が盗取され、又は所在不明となることを防ぐため、他の関係公安委員会と緊密な連絡を保つこと。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "前項に規定するもののほか、運搬が二以上の都道府県にわたることとなる場合には、関係公安委員会は、一の関係公安委員会を通じて、第三条の二の規定による届出、第三条の三の規定による申請及び前条の規定による返納の受理を行うことができるものとする。"
                    },
                    {
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "この場合において、他の関係公安委員会は、当該一の関係公安委員会を通じて、運搬証明書の書換え又は再交付を行うものとする。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（有機化学物質及び特定有機化学物質）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二十九条第一項の有機化学物質は、次のとおりとする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "関税定率法（明治四十三年法律第五十四号）別表第二八類及び第二九類に該当する物品（単一の構造式を有する炭素化合物に限るものとし、炭素の酸化物及び硫化物並びに金属炭酸塩を除く。）"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "関税定率法別表第三二・〇四項に該当する物品（単一の構造式を有する炭素化合物に限るものとし、炭素の酸化物及び硫化物並びに金属炭酸塩を除く。）"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "エチルアルコール"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "メタン"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "プロパン"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "尿素"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二十九条第一項の政令で定める製造は、その製造工程における化学反応に合成反応（発酵に係るものを除く。）を含まないものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第二十九条第二項の特定有機化学物質は、第一項第一号及び第二号に掲げる有機化学物質であって、りん原子、硫黄原子又はふっ素原子を含むものとする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（国際機関の指定する者の検査等への立会い）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第三十条第一項の政令で定める場合は、化学兵器の開発、生産、貯蔵及び使用の禁止並びに廃棄に関する条約実施及び検証に関する附属書第一部３に規定する申立てによる査察が行われる場合とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（特定施設）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第三十四条第一項の政令で指定する施設は、陸上自衛隊化学学校とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法第三十四条第一項の政令で定める数量は、年間十キログラムとする。"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法の施行の日（平成七年五月五日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成九年二月一九日政令第一九号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、化学兵器の禁止及び特定物質の規制等に関する法律附則第一条第一号に掲げる規定の施行の日（平成九年三月十九日）から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、第三条の次に二条を加える改正規定（第五条に係る部分に限る。）は、同法附則第一条第二号に掲げる規定の施行の日（平成九年四月二十九日）から施行する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "平成一一年一〇月一四日政令第三二一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、地方分権の推進を図るための関係法律の整備等に関する法律の施行の日（平成十二年四月一日）から施行する。"
              }
            }
          }
        }
      ],
