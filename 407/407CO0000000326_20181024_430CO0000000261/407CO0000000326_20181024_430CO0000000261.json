{
  "Law": {
    "@DataInfo": "181102e01k01m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "326",
    "@Year": "07",
    "LawNum": "平成七年政令第三百二十六号",
    "LawBody": {
      "LawTitle": "古物営業法施行令",
      "EnactStatement": "内閣は、古物営業法（昭和二十四年法律第百八号）第二条第一項、第二十六条及び第二十八条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（法の規制に係る証票その他の物）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "古物営業法（以下「法」という。）第二条第一項の政令で定める証票その他の物は、次に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "航空券"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "興行場又は美術館、遊園地、動物園、博覧会の会場その他不特定かつ多数の者が入場する施設若しくは場所でこれらに類するものの入場券"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "収入印紙"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "金額（金額を度その他の単位により換算して表示していると認められる場合の当該単位数を含む。）が記載され、又は電磁的方法（電子的方法、磁気的方法その他の人の知覚によって認識することができない方法をいう。）により記録されている証票その他の物であって、次に掲げるもの"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "乗車券の交付を受けることができるもの"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "電話の料金の支払のために使用することができるもの"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "タクシーの運賃又は料金の支払のために使用することができるもの"
                      }
                    },
                    {
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": "有料の道路の料金の支払のために使用することができるもの"
                      }
                    }
                  ]
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（法の規制の除外に係る大型機械類）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二条第一項の政令で定める大型機械類は、次に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "船舶（総トン数二十トン未満の船舶及び端舟その他ろかいのみをもって運転し、又は主としてろかいをもって運転する舟を除く。）"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "航空機"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "鉄道車両"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "コンクリートによる埋め込み、溶接、アンカーボルトを用いた接合その他これらと同等以上の強度を有する接合方法により、容易に取り外すことができない状態で土地又は建造物に固定して用いられる機械であって、重量が一トンを超えるもの"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、重量が五トンを超える機械（船舶を除く。）であって、自走することができるもの及びけん引されるための装置が設けられているもの以外のもの"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（電子情報処理組織及び競りの方法）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第二条第二項第三号の政令で定める電子情報処理組織は、古物の売買をしようとする者の使用に係る電子計算機（入出力装置を含む。以下同じ。）と、その者から送信された古物に関する事項及びその買受けの申出に係る金額を電気通信回線に接続して行う自動公衆送信により公衆の閲覧に供して競りを行う機能を有する電子計算機とを電気通信回線で接続した電子情報処理組織とする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "法第二条第二項第三号の政令で定める方法は、前項に規定する電子情報処理組織を使用する競りの方法とする。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（方面公安委員会への権限の委任）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法又は法に基づく政令の規定により道公安委員会の権限に属する事務は、次に掲げるものを除き、道警察本部の所在地を包括する方面を除く方面については、当該方面公安委員会が行う。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "法第三条の規定による許可に関する事務"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "法第六条第一項若しくは第二項又は第二十四条の規定による許可の取消しに関する事務"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の規定により方面公安委員会が行う処分に係る聴聞を行うに当たっては、道公安委員会が定める手続に従うものとする。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この政令は、古物営業法の一部を改正する法律（平成七年法律第六十六号）の施行の日（平成七年十月十八日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（古物営業法施行令等の廃止）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "次に掲げる政令は、廃止する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "古物営業法施行令（昭和二十八年政令第二百二十八号）"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "古物営業法関係の手数料の額の基準を定める政令（平成七年政令第二百八十七号）"
                  }
                }
              ]
            }
          ]
        },
        {
          "@AmendLawNum": "平成一一年三月二五日政令第四九号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "#text": "この政令は、平成十一年四月一日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "#text": "ただし、第二条の規定は、平成十一年八月一日から施行する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "平成一一年一〇月一四日政令第三二一号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この政令は、地方分権の推進を図るための関係法律の整備等に関する法律の施行の日（平成十二年四月一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年二月一七日政令第四一号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、古物営業法の一部を改正する法律（平成十四年法律第百十五号）の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三〇年九月一四日政令第二六一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この政令は、古物営業法の一部を改正する法律（平成三十年法律第二十一号）附則第一条ただし書に規定する規定の施行の日（平成三十年十月二十四日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
