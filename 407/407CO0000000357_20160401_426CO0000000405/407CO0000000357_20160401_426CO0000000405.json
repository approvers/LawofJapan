            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "農産物検査法（以下「法」という。）第二条第二項の政令で定める農産物は、大豆、小豆、いんげん、かんしょ生切干及びそばとする。"
                  }
