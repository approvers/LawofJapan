    "LawBody": {
      "LawTitle": "警察大学校警察情報通信研究センターの内部組織に関する規則",
      "EnactStatement": "警察法施行規則（昭和二十九年総理府令第四十四号）第七条の十七第八項の規定に基づき、警察大学校警察通信研究センターの内部組織に関する規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（研究室の設置）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "警察大学校警察情報通信研究センターに、次の三研究室を置く。"
              },
              "List": [
                {
                  "ListSentence": {
                    "Sentence": "基礎研究室"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "応用第一研究室"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "応用第二研究室"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（基礎研究室）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "基礎研究室においては、次の事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "警察に関する情報通信に関する研究に関する事務のうち、技術に関する基礎的な研究に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "警察情報通信研究センターにおいて行う研究の計画の策定に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "前二号に掲げるもののほか、他の研究室の所掌に属しないこと。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（応用第一研究室）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "応用第一研究室においては、警察に関する情報通信に関する研究に関する事務のうち、技術の応用及び開発に関する研究に関する事務（応用第二研究室の所掌に属するものを除く。）をつかさどる。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（応用第二研究室）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "応用第二研究室においては、警察に関する情報通信に関する研究に関する事務のうち、情報の管理に関する技術の応用及び開発に関する研究に関する事務をつかさどる。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（研究室長）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "研究室に、研究室長を置き、教授をもって充てる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "研究室長は、命を受け、研究室の事務を掌理する。"
                }
              }
            ]
          }
        ]
      },
