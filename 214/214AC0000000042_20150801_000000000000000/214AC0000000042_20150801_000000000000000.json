{
  "Law": {
    "@Era": "Taisho",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "42",
    "@Year": "14",
    "LawNum": "大正十四年法律第四十二号",
    "LawBody": {
      "LawTitle": "外国人土地法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "帝国臣民又ハ帝国法人ニ対シ土地ニ関スル権利ノ享有ニ付禁止ヲ為シ又ハ条件若ハ制限ヲ附スル国ニ属スル外国人又ハ外国法人ニ対シテハ勅令ヲ以テ帝国ニ於ケル土地ニ関スル権利ノ享有ニ付同一若ハ類似ノ禁止ヲ為シ又ハ同一若ハ類似ノ条件若ハ制限ヲ附スルコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "帝国法人又ハ外国法人ニシテ社員、株主若ハ業務ヲ執行スル役員ノ半数以上又ハ資本ノ半額以上若ハ議決権ノ過半数カ前条ノ外国人又ハ外国法人ニ属スルモノニ対シテハ勅令ノ定ムル所ニ依リ之ヲ其ノ外国人又ハ外国法人ト同一ノ国ニ属スルモノト看做シ前条ノ規定ヲ適用ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ資本ノ額又ハ議決権ノ数ノ計算ハ勅令ノ定ムル所ニ依ル"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "外国ノ一部ニシテ土地ニ関シ特別ノ立法権ヲ有スルモノハ本法ノ適用ニ付テハ之ヲ国ト看做ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "国防上必要ナル地区ニ於テハ勅令ヲ以テ外国人又ハ外国法人ノ土地ニ関スル権利ノ取得ニ付禁止ヲ為シ又ハ条件若ハ制限ヲ附スルコトヲ得"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ地区ハ勅令ヲ以テ之ヲ指定ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "帝国法人ニシテ社員、株主若ハ業務ヲ執行スル役員ノ半数以上又ハ資本ノ半額以上若ハ議決権ノ過半数カ外国人又ハ外国法人ニ属スルモノニ対シテハ前条ノ規定ヲ適用ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ資本ノ額又ハ議決権ノ数ノ計算ニ付テハ第二条第二項ノ規定ヲ準用ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "土地ニ関スル権利ヲ有スル者カ本法ニ依リ其ノ権利ヲ享有スルコトヲ得サルニ至リタル場合ニ於テハ一年内ニ之ヲ譲渡スコトヲ要ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ規定ニ依ル権利ノ譲渡ナカリシ場合ニ於テ其ノ権利ノ処分ニ関シ必要ナル事項ハ勅令ヲ以テ之ヲ定ム"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前二項ノ規定ハ土地ニ関スル権利ヲ有スル者ノ相続人其ノ他ノ包括承継人カ本法ニ依リ其ノ権利ヲ取得スルコトヲ得サル場合ニ之ヲ準用ス但シ第一項ニ規定スル期間ハ之ヲ三年トス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "○４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "第一項及前項ニ規定スル期間ハ通シテ三年ヲ超ユルコトヲ得ス"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本法施行ノ期日ハ勅令ヲ以テ之ヲ定ム"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本法ノ施行ニ伴フ不動産登記法ニ関スル特例ハ勅令ヲ以テ之ヲ定ム"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "9",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "明治六年第十八号布告及明治四十三年法律第五十一号ハ之ヲ廃止ス"
                }
              }
            }
          }
        ]
      }
    }
  }
}
