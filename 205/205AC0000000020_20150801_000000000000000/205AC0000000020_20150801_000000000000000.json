{
  "Law": {
    "@Era": "Taisho",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "20",
    "@Year": "05",
    "LawNum": "大正五年法律第二十号",
    "LawBody": {
      "LawTitle": "海底電信線保護万国連合条約罰則",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "海底電信線保護万国連合条約ニ依ル海底電信線ヲ損壊シテ通信ヲ障碍シ又ハ障碍スヘキ危険ヲ生セシメタル者ハ五年以下ノ懲役又ハ五十万円以下ノ罰金ニ処ス但シ海底電信線ヲ布設又ハ修繕スルニ付已ムコトヲ得サルニ出テタル者ハ此ノ限ニ在ラス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ未遂罪ハ之ヲ罰ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "過失ニ因リ第一項ノ行為ヲ為シタル者ハ五十万円以下ノ罰金ニ処ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "自己ノ生命若ハ船舶ヲ保護スル為又ハ海底電信線ヲ布設若ハ修繕スルニ付已ムコトヲ得スシテ海底電信線ヲ損壊シタル者ハ直ニ無線電信ニ依リ電信官署又ハ帝国領事館ニ届出ツヘシ無線電信ニ依ルコトヲ得サルトキハ最初ニ著船シタル時ヨリ二十四時間内ニ其ノ地ノ電信官署又ハ帝国領事館ニ届出ツヘシ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ規定ニ違反シタル者ハ五千円以下ノ罰金ニ処ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "海底電信線保護万国連合条約第五条第一項乃至第三項又ハ第六条ノ規定ニ違反シタル者ハ一万円以下ノ罰金ニ処ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "海底電信線保護万国連合条約第十条第二項ノ場合ニ於テ公書ノ呈示ヲ拒ミタル者ハ一万円以下ノ罰金ニ処ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "暴行又ハ脅迫ヲ以テ前項ノ呈示ヲ拒ミタル者ハ三年以下ノ懲役ニ処ス"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本法施行ノ期日ハ勅令ヲ以テ之ヲ定ム"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二八年七月三一日法律第九八号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、昭和二十八年八月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四三年六月一九日法律第一〇二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公海に関する条約が日本国について効力を生ずる日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
