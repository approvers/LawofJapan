            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二九年三月三一日政令第四五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
