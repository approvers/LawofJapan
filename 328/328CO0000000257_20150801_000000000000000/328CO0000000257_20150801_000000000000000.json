                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "道路上におけるこう配をゆるやかにし、又は千分の三十三まで急にする工事"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
