              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法附則第七条の規定の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三九年三月二三日政令第二九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
