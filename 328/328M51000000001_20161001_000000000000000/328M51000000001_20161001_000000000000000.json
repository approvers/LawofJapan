{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Rule",
    "@Num": "001",
    "@Year": "28",
    "LawNum": "昭和二十八年公安審査委員会規則第一号",
    "LawBody": {
      "LawTitle": "公安審査委員会審査規則",
      "EnactStatement": "破壊活動防止法（昭和二十七年法律第二百四十号）第二十六条の規定に基き、公安審査委員会審査規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（用語の略称）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この規則においては、それぞれ左の各号に定める略称を用いる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "「法」とは、破壊活動防止法をいう。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "「委員会」とは、公安審査委員会をいう。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "「委員長」及び「委員」とは、公安審査委員会の委員長及び委員をいう。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "「職員」とは、公安審査委員会の委員補佐及び事務局におかれる職員をいう。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "「被請求団体」とは、公安調査庁長官から処分の請求をされた団体をいう。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（処分請求書の記載要件）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "処分請求書には、法第二十条第一項に規定する事項の外、左に掲げる事項を記載しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "被請求団体の名称"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "被請求団体の主たる事務所の所在地"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "被請求団体の代表者又は主幹者の氏名、年齢、職業及び住所又は居所"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項第一号に掲げる事項が明らかでないときは、その団体を特定するに足りる事項を記載しなければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "第一項第二号及び第三号に掲げる事項が明らかでないときは、その旨を記載しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（添附書類）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十条第二項に規定する証拠及び調書については、目録を作成し、且つ、請求の原因たる事実を証すべき証拠については、証拠と証明すべき事実との関係を明らかにした書面を、被請求団体の提出した証拠については、証拠とそれによつて被請求団体が証明すべく主張した事実との関係を明らかにした書面を添附しなければならない。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（意見書）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "被請求団体の提出する意見書には、左の事項を記載し、代表者又は主幹者が署名押印しなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "被請求団体の名称、主たる事務所の所在地並びに代表者又は主幹者の氏名及び住所又は居所"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "意見の趣旨及び理由"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "意見書提出の年月日"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（調書の作成）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第二十二条第二項各号に掲げる処分をした場合においては、調書を作成するものとする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の調書には、左の事項を記載しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "被請求団体の名称"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "処分の要領"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "処分をした年月日及び場所"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "処分に関与した者の官職氏名"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（処分の結果に対する意見）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "委員会は、法第二十二条第二項各号に掲げる処分をした場合において、その結果につき、必要があると認めるときは、日時及び場所を定めて公安調査庁長官又はその指定する公安調査庁の職員及び被請求団体の役職員（代理人を含む。）双方よりそれぞれ三人以内に限り出頭して意見を述べる機会を与えることができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "被請求団体の役職員が前項に規定する期日に代理人を出頭させようとするときは、役職員と代理人との連署による選任書を提出しなければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "第一項に規定する期日は、公安調査庁及び被請求団体に、郵便その他適宜の方法によつて通知しなければならない。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "但し、被請求団体の事務所が知れないときは、この限りでない。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "前条第一項に規定する期日の経過については、調書を作成するものとする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の調書には、左の事項を記載しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "被請求団体の名称"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "出頭した者の氏名、年齢、職業及び住所又は居所並びに資格"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "期日の経過の要領"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "年月日及び場所"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "関与した者の官職氏名"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "8",
            "ArticleCaption": "（書類の作成）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "委員会の審査手続に関する書類は、特別の定のある場合を除いては委員会の事務局におかれる職員が作成し、作成者及び委員長が署名押印しなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "法第二十二条第三項の規定により、委員又は職員が処分をした場合には、その委員又は職員は、その調書に署名押印するものとする。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "書類には、毎葉に契印し、且つ、文字を加え、削り又は欄外に記入したときは、作成者がこれに認印し、その字数を記載しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "9",
            "ArticleCaption": "（決定書）",
            "ArticleTitle": "第九条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "決定書は、決定に関与した委員長及び委員が作成する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "決定書には、左の事項を記載し、決定に関与した委員長及び委員が署名押印しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "主文"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "理由"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "被請求団体の名称、主たる事務所の所在地並びに代表者又は主幹者の氏名、年齢、職業及び住所又は居所"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "委員会の表示"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "年月日"
                    }
                  }
                ]
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "第二条第二項及び第三項の規定は、前項第三号の事項について準用する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "決定書に委員長が署名押印することができないときは、決定に関与した委員の一人がその事由を附記して署名押印し、委員が署名押印することができないときは、委員長がその事由を附記して署名押印しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "10",
            "ArticleCaption": "（証票）",
            "ArticleTitle": "第十条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十二条第四項に規定する証票は、別記様式のとおりとする。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この規則は、公布の日から施行する。"
          }
        }
      },
      "AppdxStyle": {
        "AppdxStyleTitle": "別記様式",
        "StyleStruct": {
          "Style": {
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "証票様式"
              },
              "Item": {
                "@Num": "1",
                "ItemTitle": null,
                "ItemSentence": {
                  "Sentence": null
                },
                "Subitem1": [
                  {
                    "@Num": "1",
                    "Subitem1Title": "一",
                    "Subitem1Sentence": {
                      "Sentence": "証票の表紙は、チヨコレート色革製品で、大きさは縦一二〇ミリ横七〇ミリとし、表紙のわくと上部五三の桐模様を金色とする。"
                    }
                  },
                  {
                    "@Num": "2",
                    "Subitem1Title": "二",
                    "Subitem1Sentence": {
                      "Sentence": "桐模様の下部に公安審査委員会の文字を金色で表示する。"
                    }
                  },
                  {
                    "@Num": "3",
                    "Subitem1Title": "三",
                    "Subitem1Sentence": {
                      "Sentence": "表紙の内側に名刺入をつける。"
                    }
                  },
                  {
                    "@Num": "4",
                    "Subitem1Title": "四",
                    "Subitem1Sentence": {
                      "Sentence": "用紙の表扉に脱帽正面上半身の大きさ縦三五ミリ横三〇ミリの写真をはりつけ、証票番号、官職名、氏名及び貸与年月日を記し、委員会の公印を押し、貸与年月日の下には委員長の職印を押すものとする。"
                    }
                  },
                  {
                    "@Num": "5",
                    "Subitem1Title": "五",
                    "Subitem1Sentence": {
                      "Sentence": "用紙は、差換式縦一一四ミリ横六〇ミリとする。"
                    },
                    "StyleStruct": {
                      "Style": {
                        "Fig": {
                          "@src": "./pict/S28F31101000001-001.pdf"
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        }
      }
    }
  }
}
