            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和二十八年九月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二九年七月一五日政令第二〇五号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
