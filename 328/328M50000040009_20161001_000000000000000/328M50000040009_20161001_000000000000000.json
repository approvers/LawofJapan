          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、昭和三十七年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年八月二一日大蔵省令第六九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十三年一月六日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一八年三月三一日財務省令第二五号",
          "@Extract": "true",
