{
  "Law": {
    "@DataInfo": "190319e04k03m01",
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "199",
    "@Year": "34",
    "LawNum": "昭和三十四年政令第百九十九号",
    "LawBody": {
      "LawTitle": "消防審議会令",
      "EnactStatement": "内閣は、消防組織法（昭和二十二年法律第二百二十六号）第四条の四第三項の規定に基き、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "消防審議会（以下「審議会」という。）は、委員十三人以内で組織する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、学識経験のある者のうちから、消防庁長官が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（委員の任期）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、再任されることができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に会長を置き、委員の互選によつてこれを定める。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長は、会務を総理し、審議会を代表する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長に事故があるときは、あらかじめその指名する委員がその職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（幹事）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に幹事を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、学識経験のある者及び関係行政機関の職員のうちから、消防庁長官が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、審議会の所掌事務について、委員を補佐する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、委員の過半数が出席しなければ、会議を開き、議決することができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会の議事は、委員で会議に出席したものの過半数で決し、可否同数のときは、会長の決するところによる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（資料の提出等の要求）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会は、その所掌事務を遂行するため必要があると認めるときは、関係行政機関の長に対し、資料の提出、意見の開陳、説明その他必要な協力を求めることができる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会の庶務は、消防庁総務課において処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、議事の手続その他審議会の運営に関し必要な事項は、会長が審議会に諮つて定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三五年六月三〇日政令第一八五号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、自治庁設置法の一部を改正する法律の施行の日（昭和三十五年七月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四〇年三月二五日政令第四四号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和四十年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五三年五月二三日政令第一九九号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五九年六月二一日政令第二一〇号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和五十九年七月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三〇四号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、次項の規定は、公布の日から施行する。"
                  }
                ]
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令の施行の日の前日において次に掲げる従前の審議会等の委員その他の職員である者（任期の定めのない者を除く。）の任期は、当該委員その他の職員の任期を定めたそれぞれの政令の規定にかかわらず、その日に満了する。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1:3",
                  "ItemTitle": "一～三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "略"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "消防審議会"
                    }
                  }
                }
              ]
            }
          ]
        }
      ]
    }
  }
}
