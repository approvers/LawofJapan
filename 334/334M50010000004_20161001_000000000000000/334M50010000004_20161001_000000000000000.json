{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "4",
    "@Year": "34",
    "LawNum": "昭和三十四年農林省令第四号",
    "LawBody": {
      "LawTitle": "いるか猟獲取締規則",
      "EnactStatement": "漁業法（昭和二十四年法律第二百六十七号）第六十五条第一項の規定に基き、いるか猟獲取締規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "日本海を除く北緯三十六度以北の太平洋においては、毎年二月二十日から六月二十日までの間は、もりづつ以外の銃砲を使用しているかを猟獲してはならない。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "前条の規定に違反した者は、一年以下の懲役若しくは五十万円以下の罰金に処し、又はこれを併科する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "前項の場合においては、犯罪行為に供した銃砲又は犯罪行為により得た漁獲物及びその製品であつて、犯人が所有し、又は所持するものは、没収することができる。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、犯人が所有していたこれらの物件の全部又は一部を没収することができないときは、その価格を追徴することができる。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法人の代表者又は法人若しくは人の代理人、使用人その他の従業者が、その法人又は人の業務又は財産に関して、前条第一項の違反行為をしたときは、行為者を罰するほか、その法人又は人に対し、同項の罰金刑を科する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和五八年六月一一日農林水産省令第一七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、漁業法及び水産資源保護法の一部を改正する法律（昭和五十八年法律第六十二号）の施行の日（昭和五十八年七月一日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
