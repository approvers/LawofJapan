    "@LawType": "CabinetOrder",
    "@Num": "158",
    "@Year": "34",
    "LawNum": "昭和三十四年政令第百五十八号",
    "LawBody": {
      "LawTitle": "図書館法施行令",
      "EnactStatement": "内閣は、図書館法（昭和二十五年法律第百十八号）第二十条第二項の規定に基き、図書館法施行令（昭和二十五年政令第二百九十三号）の全部を改正するこの政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "図書館法第二十条第一項に規定する図書館の施設、設備に要する経費の範囲は、次に掲げるものとする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Column": [
                  {
                    "@LineBreak": "false",
                    "@Num": "1",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "施設費"
                    }
                  },
                  {
                    "@LineBreak": "false",
                    "@Num": "2",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "施設の建築に要する本工事費、附帯工事費及び事務費"
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Column": [
                  {
                    "@LineBreak": "false",
                    "@Num": "1",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "設備費"
                    }
                  },
                  {
                    "@LineBreak": "false",
                    "@Num": "2",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "図書館に備え付ける図書館資料及びその利用のための器材器具の購入に要する経費"
                    }
                  }
                ]
              }
            }
          ]
        }
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、公布の日から施行する。"
            }
          }
        }
      }
    }
  }
}
