{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "23",
    "@Year": "34",
    "LawNum": "昭和三十四年文部省令第二十三号",
    "LawBody": {
      "LawTitle": "青少年の読書指導のための資料の作成等に関する規程",
      "EnactStatement": "青少年の読書指導のための資料の作成等に関する規程（昭和三十四年文部省令第十号）の全部を改正する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "文部大臣は、社会教育として行われる青少年の読書指導に資するため、この規程の定めるところにより、おおむね十八歳までの者を対象として発行された図書（教科用図書、学習参考書その他学校で用いられることを目的としたもの、辞書及び事典を除く。）につき、社会教育審議会の議を経て健全な青少年の育成上有益な図書の目録を作成し、配付するものとする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "目録の作成は、発行者が当該目録に登載されることを申し出た図書について行うものとする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、申し出のない図書であつても、社会教育審議会が適当と認めるものについては、目録を作成することができる。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の申し出の時期、申出書の様式、読者対象の別その他申し出に関し必要な事項は告示する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "目録には当該図書につき、その内容のあらまし、読者対象、利用上の注意その他読書指導上必要な事項を記載するものとする。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "目録に登載された図書の発行者は、当該図書について、その内容の改訂その他重要な変更を行つた場合は、目録の記載事項の変更を申し出るものとする。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "目録に登載された図書について、その登載を不適当と社会教育審議会が認めたときは、当該図書を目録からまつ消するものとする。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": "１",
          "ParagraphSentence": {
            "Sentence": "この省令は、公布の日から施行する。"
          }
        }
      }
    }
  }
}
