{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "021",
    "@Year": "43",
    "LawNum": "昭和四十三年自治省令第二十一号",
    "LawBody": {
      "LawTitle": "小笠原総合事務所組織規則",
      "EnactStatement": "小笠原諸島の復帰に伴う法令の適用の暫定措置等に関する法律（昭和四十三年法律第八十三号）第二十六条第三項の規定に基づき、小笠原総合事務所組織規程を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（小笠原総合事務所の分課）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "小笠原総合事務所（以下「総合事務所」という。）に、次の三課を置く。"
              },
              "List": [
                {
                  "ListSentence": {
                    "Sentence": "総務課"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "業務課"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "国有林課"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（総務課の事務）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務課においては、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "職員の人事及び福利厚生に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "経費及び収入の予算、決算及び会計に関すること（国有林課の所管に属するものを除く。次号において同じ。）。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "行政財産及び物品の管理及び営繕に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "所長の官印及び総合事務所印の保管に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "公文書類の接受、発送、編集及び保存に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "小笠原諸島の振興に関する調査及び企画に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、総合事務所の所掌事務のうち他の所掌に属しないものに関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（業務課の事務）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "業務課においては、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "土地（国有地を除く。）の権利関係の調整に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "出入国管理及び外国人の在留に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "港及び飛行場における検疫及び防疫に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "緊急防除についての指導に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "労働条件及び労働者の保護並びに労働者災害補償保険事業に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "職業の安定及び雇用保険事業に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "土地（国有地を除く。）の使用に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "小笠原諸島の地域における旧島民の帰島の援護に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（国有林課の事務）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国有林課においては、国有林野事業に関する事務をつかさどる。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（専門調査官）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総合事務所に、所管行政の企画、調査及び連絡調整に関する事務を行なわせるため、専門調査官二人を置くことができる。"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（所長への委任）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この規則に定めるもののほか、総合事務所の組織については、所長が別に定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
