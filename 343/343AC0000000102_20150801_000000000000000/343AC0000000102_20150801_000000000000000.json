{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "102",
    "@Year": "43",
    "LawNum": "昭和四十三年法律第百二号",
    "LawBody": {
      "LawTitle": "公海に関する条約の実施に伴う海底電線等の損壊行為の処罰に関する法律",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "公海に関する条約第二十七条に規定する海底電線（海底電信線保護万国連合条約第一条に規定する海底電信線を除く。）を損壊して電気通信を妨害した者は、五年以下の懲役又は五十万円以下の罰金に処する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "過失により前項の罪を犯した者は、五十万円以下の罰金に処する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "公海に関する条約第二十七条に規定する海底パイプライン又は海底高圧電線を損壊して石油若しくは可燃性天然ガスの輸送又は送電を妨害した者は、五年以下の懲役又は十万円以下の罰金に処する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "過失により前項の罪を犯した者は、十万円以下の罰金に処する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "第一条第一項及び前条第一項の未遂罪は、罰する。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": "１",
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この法律は、公海に関する条約が日本国について効力を生ずる日から施行する。"
            }
          }
        }
      }
    }
  }
}
