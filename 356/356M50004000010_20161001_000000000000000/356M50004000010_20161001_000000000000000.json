            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成三年九月六日建設省令第一六号",
          "@Extract": "true",
