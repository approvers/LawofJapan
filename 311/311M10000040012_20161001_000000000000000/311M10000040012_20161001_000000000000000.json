{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "012",
    "@Year": "11",
    "LawNum": "昭和十一年大蔵省令第十二号",
    "LawBody": {
      "LawTitle": "供託又ハ寄託セル国債ノ償還金ヲ以テ為ス代リ国債ノ買入ニ関スル特別取扱規程",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "供託又ハ寄託セル国債ノ償還金ヲ以テ為ス代リ国債ノ買入ニ関スル特別取扱規程左ノ通定ム"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法令ノ規定ニ依リ供託シ又ハ政府ニ対スル保証若ハ担保トシテ寄託セル国債ノ償還セラルル場合差替ノ為其ノ償還金ヲ以テ該国債ノ借換ノ為発行セラルル国債ヲ日本銀行ヨリ買入レムトスル者ハ本令ノ定ムル所ニ依リ其ノ請求ヲ為スコトヲ得但シ政府保管有価証券取扱規程第二条但書ノ規定ニ依リ保管スルモノニ付テハ此ノ限ニ在ラス"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "前条ノ請求ヲ為サムトスル者ハ附録様式ノ特別取扱請求書二通ヲ供託局（供託事務ノ取扱ヲ為ス銀行ヲ含ム以下同シ）又ハ取扱官庁ニ提出スヘシ但シ左記書類ヲ添附スルコトヲ要ス"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "日本銀行所定ノ国債買受申込書"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "国債ノ変更ニ付主務官庁ノ認可又ハ権利者ノ承諾ヲ必要トスル場合ニ於テハ其ノ認可書又ハ承諾書但シ第六条ニ依ル剰余金ノ処置ヲ認可又ハ承諾スル旨ノ記載アルコトヲ要ス"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "供託又ハ寄託ニ代用セル甲種登録国債ニ付前条ノ請求ヲ為ス場合ニ在リテハ国債ノ変更ヲ認可スル官庁ヲ取扱官庁トス"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "供託局又ハ取扱官庁前条ノ特別取扱請求書ヲ調査シ其ノ請求ニ応スヘキモノト認メタルトキハ其ノ一通ニ承認ノ旨並ニ供託有価証券受託証書（供託事務ノ取扱ヲ為ス銀行ニ在リテハ供託書）、政府保管有価証券受託証書又ハ政府保管有価証券振込済通知書ノ番号ヲ記載シ他ノ一通及添附書類ト共ニ直ニ其ノ償還国債ヲ保管スル日本銀行本店支店又ハ代理店ニ送付スヘシ但シ其ノ償還国債カ甲種登録国債ナル場合ハ日本銀行本店ニ送付スヘシ"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "日本銀行国債売却ノ決定ヲ為シタルトキハ償還国債ノ償還ヲ受ケ其ノ償還金ヲ以テ国債売却代金ニ充当シ国債変更ニ必要ナル手続ヲ為スヘシ但シ左記各号ノ規定ニ準拠スルコトヲ要ス"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "特別取扱請求ニ係ル国債証券ヲ保管スル日本銀行代理店ハ請求書記載ノ事項ト自店保管ニ係ル当該国債証券ト照合ノ上承認ノ旨記載シアル請求書及添附書類ヲ所轄日本銀行本店又ハ支店ニ送付シ其ノ決定ヲ請フモノトス但シ台北、京城、大連代理店及其ノ管下代理店ニ在リテハ日本銀行本店ニ送付シ其ノ決定ヲ請フモノトス"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "前号ノ書類ノ送付ヲ受ケタル日本銀行本店又ハ支店カ国債売却ノ決定ヲ為シタルトキハ代リ国債証券ヲ当該店ニ送付スヘシ"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "日本銀行ニ於テ国債変更ノ手続ヲ了シタルトキハ受託有価証券変更証書又ハ登録国債変更証書ヲ作製シ之ヲ供託局又ハ取扱官庁ニ送付スヘシ"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "供託局又ハ取扱官庁前項変更証書ノ送付ヲ受ケタルトキハ国債変更ノ手続ヲ了シタル旨ヲ請求者ニ通知スヘシ"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": "償還国債ニ付事故アルニ因リ日本銀行ニ於テ国債ノ売却ヲ為シ得サリシトキハ其ノ旨ヲ供託局又ハ取扱官庁ニ通知シ供託局又ハ取扱官庁ハ更ニ其ノ旨ヲ請求者ニ通知スヘシ"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "日本銀行償還金ヲ売却代金ニ充当シ剰余金ヲ生シタルトキハ之ヲ請求者ニ返還スルモノトス"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "本令ニ依ル特別取扱ノ請求ハ之ヲ取消スコトヲ得ス"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "本令ニ依ル特別取扱請求書ノ供託局又ハ取扱官庁ニ於ケル受付期間ハ其ノ都度財務大臣之ヲ告示ス"
              }
            }
          },
          {
            "@Num": "9",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "本令ノ施行ニ必要ナル事項ニシテ財務大臣ノ定ムルモノヲ除クノ外ハ日本銀行之ヲ定メ財務大臣ニ報告スヘシ"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "本令ハ公布ノ日ヨリ之ヲ施行ス"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年八月二一日大蔵省令第六九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十三年一月六日から施行する。"
            }
          }
        }
      ],
