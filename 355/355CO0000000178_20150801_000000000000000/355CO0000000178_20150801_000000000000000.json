                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "農住組合法による不動産登記に関する政令（昭和五十六年政令第百七十一号）"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
