              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十条第一項第五号の政令で定める行為は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
