    "@LawType": "Rule",
    "@Num": "7",
    "@Year": "55",
    "LawNum": "昭和五十五年国家公安委員会規則第七号",
    "LawBody": {
      "LawTitle": "警察法第十二条の四第一項に規定する専門委員に関する規則",
      "EnactStatement": "警察法施行令（昭和二十九年政令第百五十一号）第一条第五項の規定に基づき、警察法第十二条の二第一項に規定する専門委員に関する規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（専門委員の調査審議）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国家公安委員会は、都道府県公安委員会が行つた次に掲げるものの支給に係る裁定についての審査請求があつた場合には、当該審査請求に係る専門の事項について専門委員に調査審議させるものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "犯罪被害者等給付金"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "オウム真理教犯罪被害者等を救済するための給付金の支給に関する法律（平成二十年法律第八十号）第三条第一項に規定する給付金"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "国外犯罪被害弔慰金等の支給に関する法律（平成二十八年法律第七十三号）第三条に規定する国外犯罪被害弔慰金等"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の調査審議に関し必要な事項は、専門委員が協議して定める。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（専門委員に関する庶務）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "専門委員に関する庶務は、警察庁長官官房給与厚生課において処理する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、昭和五十六年一月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成六年六月二四日国家公安委員会規則第一三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、平成六年七月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一三年二月一九日国家公安委員会規則第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、警察法の一部を改正する法律の施行の日（平成十三年三月一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年一〇月七日国家公安委員会規則第一九号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、オウム真理教犯罪被害者等を救済するための給付金の支給に関する法律（平成二十年法律第八十号）の施行の日（平成二十年十二月十八日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年三月三一日国家公安委員会規則第八号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、平成二十八年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年八月一二日国家公安委員会規則第二〇号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、国外犯罪被害弔慰金等の支給に関する法律（平成二十八年法律第七十三号）の施行の日（平成二十八年十一月三十日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
