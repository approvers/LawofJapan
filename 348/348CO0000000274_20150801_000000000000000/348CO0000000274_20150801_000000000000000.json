                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "水産業協同組合等が水産業協同組合に事業の経営に必要な資金として貸し付ける場合"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一千万円"
                            }
                          }
                        ]
