            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、平成十二年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年一一月一六日政令第三四〇号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
