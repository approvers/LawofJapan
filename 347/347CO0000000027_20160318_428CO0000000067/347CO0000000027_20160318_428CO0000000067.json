                      "@BorderRight": "solid",
                      "@BorderTop": "solid",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "東京都と大阪市とを連絡する新幹線鉄道のうち高崎市と長野市とを連絡する区間"
                      }
                    },
                    {
                      "@BorderBottom": "solid",
                      "@BorderLeft": "solid",
                      "@BorderRight": "solid",
                      "@BorderTop": "solid",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "平成九年十月一日"
                      }
