            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、平成十三年一月六日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年三月三一日国土交通省令第三七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
