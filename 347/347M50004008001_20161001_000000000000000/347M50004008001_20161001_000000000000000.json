{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "1",
    "@Year": "47",
    "LawNum": "昭和四十七年建設省・自治省令第一号",
    "LawBody": {
      "LawTitle": "公有地の拡大の推進に関する法律施行規則",
      "EnactStatement": "公有地の拡大の推進に関する法律（昭和四十七年法律第六十六号）第四条第一項、第五条第一項及び第十八条第七項並びに公有地の拡大の推進に関する法律施行令（昭和四十七年政令第二百八十四号）第二条第一項、第三条第一項及び第八条第三項の規定に基づき、公有地の拡大の推進に関する法律施行規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（有償譲渡の届出事項等）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "公有地の拡大の推進に関する法律（以下「法」という。）第四条第一項に規定する主務省令で定める事項は、次に掲げるものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "当該土地の地目"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "当該土地に所有権以外の権利があるときは、当該権利の種類及び内容並びに当該権利を有する者の氏名及び住所"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "当該土地に建築物その他の工作物があるときは、当該工作物並びに当該工作物につき所有権を有する者の氏名及び住所"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "前号の工作物に所有権以外の権利があるときは、当該権利の種類及び内容並びに当該権利を有する者の氏名及び住所"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "法第四条第一項の届出は、別記様式第一の土地有償譲渡届出書の正本一部及び写し一部を提出してしなければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "前項の土地有償譲渡届出書には、当該土地の位置及び形状を明らかにした図面を添付しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（史跡等に係る指定の公告）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "公有地の拡大の推進に関する法律施行令（以下「令」という。）第二条第一項第一号の規定による公告は、次に掲げる事項について都道府県知事（市の区域内にあつては、当該市の長。以下この条において同じ。）の定める方法で行うものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "史跡、名勝又は天然記念物の別及び名称"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "令第二条第一項第一号の規定による都道府県知事の指定に係る土地の区域"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（土地区画整理事業に係る指定の公告）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第四条第一項第三号の規定による公告は、土地区画整理事業の名称及び施行地区について都府県知事の定める方法で行なうものとする。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（令第三条第一項の総務省令・国土交通省令で定める法人）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "令第三条第一項の総務省令・国土交通省令で定める法人は、次に掲げる法人とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "削除"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "削除"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "日本勤労者住宅協会"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "市街地再開発組合"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "港湾法（昭和二十五年法律第二百十八号）第五十五条の七第一項の特定用途港湾施設の建設を主たる目的とし、かつ、基本財産の全額が地方公共団体の出資に係る法人で、主務大臣の指定するもの"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（買取り希望の申出事項等）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第五条第一項の申出は、次に掲げる事項を記載した別記様式第二の土地買取希望申出書の正本一部及び写し一部を提出してしなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "当該土地の所在、地目及び面積"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "当該土地の買取り希望価額"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "当該土地に所有権以外の権利があるときは、当該権利の種類及び内容並びに当該権利を有する者の氏名及び住所"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "当該土地に建築物その他の工作物があるときは、当該工作物並びに当該工作物につき所有権を有する者の氏名及び住所"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "前号の工作物に所有権以外の権利があるときは、当該権利の種類及び内容並びに当該権利を有する者の氏名及び住所"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の土地買取希望申出書には、当該土地の位置及び形状を明らかにした図面を添付しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（経理原則）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "土地開発公社は、その財政状態及び経営成績を明らかにするため、財産の増減及び異動並びに収益及び費用をその発生の事実に基づいて経理しなければならない。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（勘定区分）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "土地開発公社の会計においては、貸借対照表勘定及び損益勘定を設け、貸借対照表勘定においては資産、負債及び資本を計算し、損益勘定においては収益及び費用を計算する。"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleCaption": "（不動産登記規則の準用）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "不動産登記規則（平成十七年法務省令第十八号）第四十三条第一項第四号（第五十一条第八項、第六十五条第九項、第六十八条第十項及び第七十条第七項において準用する場合を含む。）、第六十三条第三項、第六十四条第一項第一号及び第四号、第百八十二条第二項並びに附則第十五条第四項第一号及び第三号の規定については、土地開発公社を地方公共団体とみなして、これらの規定を準用する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "#text": "この省令は、昭和四十七年九月一日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "#text": "ただし、第一条から第六条までの規定は、同年十二月一日から施行する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "昭和四九年八月一九日　自治省・建設省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、生産緑地法（昭和四十九年法律第六十八号）の施行の日（昭和四十九年八月三十一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和六〇年五月一日　自治省・建設省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年二月七日　建設省・自治省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十二年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年一〇月二〇日　建設省・自治省令第二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年三月三一日総務省・国土交通省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十六年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年三月七日総務省・国土交通省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一八年八月二五日　国土交通省令第八三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、都市の秩序ある整備を図るための都市計画法等の一部を改正する法律附則第一条第二号に掲げる規定の施行の日（平成十八年八月三十日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二三年一二月二二日総務省・国土交通省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、地域の自主性及び自立性を高めるための改革の推進を図るための関係法律の整備に関する法律附則第一条第二号に掲げる規定の施行の日（平成二十四年四月一日）から施行する。"
            }
          }
        }
      ],
