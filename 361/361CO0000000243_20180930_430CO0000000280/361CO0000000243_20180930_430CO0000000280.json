            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、都市再生特別措置法等の一部を改正する法律の施行の日（平成十九年九月二十八日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年一月二三日政令第二一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
