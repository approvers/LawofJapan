{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Rule",
    "@Num": "1",
    "@Year": "61",
    "LawNum": "昭和六十一年日本学術会議規則第一号",
    "LawBody": {
      "LawTitle": "日本学術会議傍聴規則",
      "EnactStatement": "日本学術会議法（昭和二十三年法律第百二十一号）第二十八条の規定に基づき、日本学術会議傍聴規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "日本学術会議の総会の傍聴に関しては、この規則の定めるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "傍聴しようとする者（以下「傍聴人」という。）は、その住所、氏名、年齢及び職業を備付けの帳簿に記入しなければならない。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "凶器その他危険な物を持つている者、酒気を帯びている者その他議事の運営に支障を及ぼすと認められる者は、傍聴することができない。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "傍聴人は、議場に入ることができない。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "傍聴人が傍聴席にあるときは、次の事項を守らなければならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "飲食又は喫煙をしないこと。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "みだりに傍聴席を離れないこと。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "議場における言論に対して賛否を表明し、又は拍手をしないこと。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "静粛を旨とし、議事の妨害になるような行為をしないこと。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "傍聴人は、公開しないこととする議決があつたときは、速やかに退席しなければならない。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "傍聴人は、事務局係員の指示に従わなければならない。"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "傍聴人がこの規則に違反したときは、退席させられることがある。"
              }
            }
          },
          {
            "@Num": "9",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "議長は、議事の運営上必要があると認めるときは、傍聴、撮影又は録音を制限することができる。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一三年五月一五日日本学術会議規則第二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、公布の日から施行する。"
            }
          }
        }
      ]
    }
  }
}
