                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "共済法附則第十四条の十第一項第一号"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "組合員"
