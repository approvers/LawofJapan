                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@Num": "1",
                        "@WritingMode": "vertical",
                        "#text": "試験区分ごとの一部の科目に合格した者の氏名、生年月日、受験番号、合格した科目及び合格した年"
                      }
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
