                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "法第三条第一項の貸付けに係る収入及び支出の経理"
                        }
                      }
                    ]
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
