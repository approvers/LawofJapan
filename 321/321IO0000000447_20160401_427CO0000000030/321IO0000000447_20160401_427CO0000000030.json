            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和四十三年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四六年八月二八日政令第二七六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
