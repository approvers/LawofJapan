{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "ImperialOrder",
    "@Num": "309",
    "@Year": "08",
    "LawNum": "昭和八年勅令第三百九号",
    "LawBody": {
      "LawTitle": "農業用動産抵当権実行令",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "農業用動産ノ抵当権実行ノ手続ハ本令ニ規定スルモノノ外動産ヲ目的トスル担保権ノ実行トシテノ競売（以下競売ト称ス）ニ関スル民事執行法（昭和五十四年法律第四号）其ノ他ノ法令ノ規定ニ従フ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "農業用動産ノ競売ノ申立ハ申立書ニ抵当権ニ関スル登記簿ノ謄本及債権証書ヲ添附シテ之ヲ為ス債権証書ヲ添附スルコト能ハザルトキハ申立書ニ其ノ事由ヲ記載スルコトヲ要ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "抵当権ニ付其ノ登記ナキ場合ニ於テハ裁判所ノ許可ヲ得テ競売ノ申立ヲ為スコトヲ要ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前条第二項ノ許可ノ申請ニ関スル裁判ハ農業用動産ノ所在ノ場所ヲ管轄スル地方裁判所ニ於テ非訟事件手続法（平成二十三年法律第五十一号）ニ依リ之ヲ為ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "許可ノ裁判ハ債務者並ニ農業用動産ノ所有者及占有者ニ対シテモ之ヲ告知スルコトヲ要ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "所有者又ハ競売ノ申立ニ係ル抵当権ニ優先スル権利ヲ有セザル者ノ占有スル農業用動産ノ競売ノ申立アリタルトキハ占有者ガ其ノ提出ヲ拒ミタルトキト雖モ執行官差押ヲ為スコトヲ得"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "執行官前項ノ規定ニ依リ差押ヲ為スニハ登記簿ノ謄本又ハ第二条第二項ノ許可ノ裁判ノ正本ヲ提示スルコトヲ要ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "民事執行法第百二十三条第二項ノ規定ハ執行官ガ第一項ノ規定ニ依リ差押ヲ為ス場合ニ之ヲ準用ス"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本令ハ農業動産信用法施行ノ日ヨリ之ヲ施行ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四一年一二月二〇日政令第三八一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、執行官法（昭和四十一年法律第百十一号）の施行の日（昭和四十一年十二月三十一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五五年八月三〇日政令第二三一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、民事執行法の施行の日（昭和五十五年十月一日）から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphCaption": "（経過措置）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令の施行前に申し立てられた農業用動産の抵当権実行の事件及び商品券取締法（昭和七年法律第二十八号）第二条第一項に規定する権利の実行の事件については、なお従前の例による。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成二四年七月一九日政令第一九七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、新非訟事件手続法の施行の日（平成二十五年一月一日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
