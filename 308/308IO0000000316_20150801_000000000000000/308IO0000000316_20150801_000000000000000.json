{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "ImperialOrder",
    "@Num": "316",
    "@Year": "08",
    "LawNum": "昭和八年勅令第三百十六号",
    "LawBody": {
      "LawTitle": "拒絶証書令",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "手形（為替手形、約束手形）及小切手ノ拒絶証書ハ公証人又ハ執行官之ヲ作ル"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "拒絶証書ニハ左ノ事項ヲ記載シ公証人又ハ執行官之ニ署名捺印スルコトヲ要ス"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "拒絶者及被拒絶者ノ名称"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "拒絶者ニ対スル請求ノ趣旨及拒絶者ガ其ノ請求ニ応ゼザリシコト、拒絶者ニ面会スルコト能ハザリシコト又ハ請求ヲ為スベキ場所ガ知レザリシコト"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "請求ヲ為シ又ハ之ヲ為スコト能ハザリシ地及年月日"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "拒絶証書作成ノ場所及年月日"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "法定ノ場所外ニ於テ拒絶証書ヲ作ルトキハ拒絶者ガ之ヲ承諾シタルコト"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "支払人ガ手形法第二十四条第一項ノ規定ニ依リ第二ノ呈示ヲ為スベキコトヲ請求シタルトキハ拒絶証書ニ其ノ旨ヲ記載スルコトヲ要ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "拒絶証書ノ作成ハ手形若ハ小切手又ハ附箋ニ依リテ之ヲ為ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "拒絶証書ハ手形又ハ小切手ノ裏面ニ記載シタル事項ニ接続シテ之ヲ作リ附箋ニ依ル場合ニ於テハ公証人又ハ執行官其ノ接目ニ契印ヲ為スコトヲ要ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "手形又ハ小切手ノ数通ノ複本又ハ原本及謄本ヲ呈示シタル場合ニ於テ拒絶証書ヲ作ルトキハ其ノ作成ハ一通ノ複本若ハ原本又ハ附箋ニ依リ之ヲ為スヲ以テ足ル"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ規定ニ依リテ拒絶証書ヲ作リタルトキハ他ノ複本又ハ謄本ニ其ノ旨ヲ記載シ公証人又ハ執行官之ニ署名捺印スルコトヲ要ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前条ノ規定ハ前項ノ場合ニ之ヲ準用ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "手形法第六十八条第二項（同法第七十七条第一項ニ於テ準用スル場合ヲ含ム）ノ規定ニ依ル拒絶証書ノ作成ハ手形ノ謄本又ハ附箋ニ依リテ之ヲ為ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "引受ノ一部ノ拒絶ニ因ル拒絶証書ノ作成ハ公証人又ハ執行官ニ於テ手形ノ謄本ヲ作リ其ノ謄本又ハ附箋ニ依リテ之ヲ為ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "第三条第二項ノ規定ハ前二項ノ場合ニ之ヲ準用ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "数人ニ対スル請求又ハ同一人ニ対スル数回ノ請求ニ付テハ一通ノ拒絶証書ヲ作ラシムルヲ以テ足ル"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "拒絶証書ハ請求ヲ為シタル場所ニ於テ之ヲ作ルコトヲ要ス但シ拒絶者ノ承諾アルトキハ他ノ場所ニ於テ之ヲ作ルコトヲ妨ゲズ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "請求ヲ為スベキ場所ガ知レザルトキハ拒絶証書ヲ作ルベキ公証人又ハ執行官ハ其ノ地ノ官署又ハ公署ニ問合ヲ為スコトヲ要ス若シ問合ヲ為スモ知レザルトキハ其ノ官署若ハ公署又ハ自己ノ役場若ハ勤務スル裁判所ニ於テ拒絶証書ヲ作ルコトヲ得"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "公証人又ハ執行官ガ拒絶証書ヲ作リタルトキハ其ノ謄本ニ左ノ事項ヲ記載シ之ヲ其ノ役場又ハ勤務スル裁判所ニ備フルコトヲ要ス"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "為替手形、約束手形又ハ小切手ノ別及番号アルトキハ其ノ番号"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "金額"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "振出人、支払人及支払ヲ受ケ又ハ之ヲ受クル者ヲ指図スル者ノ名称"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "振出ノ年月日及振出地"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "満期及支払地"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "支払ノ為指定セラレタル第三者、予備支払人又ハ参加引受人アルトキハ其ノ名称"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "拒絶証書ガ滅失シタル場合ニ於テ利害関係人ノ請求アリタルトキハ前項ノ記載ヲ為シタル謄本ニ依リテ謄本ヲ作リ之ヲ利害関係人ニ交付スルコトヲ要ス此ノ謄本ハ原本ト同一ノ効力ヲ有ス"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本令ハ昭和九年一月一日ヨリ之ヲ施行ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四一年一二月二〇日政令第三八一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、執行官法（昭和四十一年法律第百十一号）の施行の日（昭和四十一年十二月三十一日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
