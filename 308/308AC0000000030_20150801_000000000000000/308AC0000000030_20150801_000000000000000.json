            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本法施行ノ期日ハ勅令ヲ以テ之ヲ定ム"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和一八年三月一一日法律第四六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "76",
            "ArticleTitle": "第七十六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本法施行ノ期日ハ各規定ニ付勅令ヲ以テ之ヲ定ム"
