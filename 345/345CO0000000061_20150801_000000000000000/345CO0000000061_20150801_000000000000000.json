{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "61",
    "@Year": "45",
    "LawNum": "昭和四十五年政令第六十一号",
    "LawBody": {
      "LawTitle": "経済及び技術協力のため必要な物品等の外国政府等に対する譲与等に関する法律施行令",
      "EnactStatement": "内閣は、経済及び技術協力のため必要な物品等の外国政府等に対する譲与等に関する法律（昭和三十五年法律第二十三号）の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（譲与等ができる財産）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "経済及び技術協力のため必要な物品等の外国政府等に対する譲与等に関する法律（以下「法」という。）に規定する政令で定める財産は、船舶又は建物の従物とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（譲与等の相手方となる国際機関）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法に規定する政令で定めるその他の国際機関は、日本国が加盟している国際機関及び東南アジア文部大臣機構とする。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、公布の日から施行する。"
            }
          }
        }
      }
    }
  }
}
