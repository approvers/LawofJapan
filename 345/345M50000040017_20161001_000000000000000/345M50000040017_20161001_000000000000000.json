    "LawBody": {
      "LawTitle": "国税不服審判所組織規則",
      "EnactStatement": "国税通則法（昭和三十七年法律第六十六号）第七十八条第五項並びに国税不服審判所組織令（昭和四十五年政令第五十号）第四条第一項及び第五条の規定に基づき、国税不服審判所組織規程を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（支部の名称、位置及び管轄区域）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国税不服審判所の支部（以下「支部」という。）の名称、位置及び管轄区域は、別表のとおりとする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（国税審判官等の定数）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国税審判官及び国税副審判官の定数は、次のとおりとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "国税審判官"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "百八十一人"
                      }
                    ]
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "国税副審判官"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "八十七人"
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（国税審査官）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国税不服審判所に、国税審査官百八十一人以内を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "国税審査官は、国税審判官の命を受けて、その事務を整理する。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（管理室）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "国税不服審判所に、管理室を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "管理室においては、次の事務をつかさどる。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "国税不服審判所長の官印及び庁印を保管すること。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "人事及び機密に関すること。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "公文書類の審査及び進達を行うこと。"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "文書の接受、発送、編集及び保存を行うこと。"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "経費、会計事務、物品の管理及び庁内の取締りに関すること。"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "国税不服審判所の事務の運営に関し必要な事項の企画及び立案をし、並びにその実施に係る指導監督に関する事務を行うこと。"
                    }
                  },
                  {
                    "@Num": "7",
                    "ItemTitle": "七",
                    "ItemSentence": {
                      "Sentence": "国税不服審判所の事務の処理に必要な一般資料の収集整理を行うこと。"
                    }
                  },
                  {
                    "@Num": "8",
                    "ItemTitle": "八",
                    "ItemSentence": {
                      "Sentence": "前各号に掲げるもののほか、国税不服審判所の事務で他の所掌に属しないものを行うこと。"
                    }
                  }
                ]
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "管理室に、室長を置く。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": "（支部の内部組織）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "支部に、国税審判官、国税副審判官及び国税審査官を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項に掲げるもののほか、国税不服審判所沖縄事務所以外の各支部に、管理課を置く。"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（支部の管理課の事務）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "支部の管理課においては、次の事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "首席国税審判官の官印を保管すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "人事及び機密に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "公文書類の審査及び進達を行うこと。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "文書の接受、発送、編集及び保存を行うこと。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "経費、会計事務、物品の管理及び庁内の取締りに関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "支部の事務の運営に関し必要な事項の企画及び立案をすること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、支部の事務で他の所掌に属しないものを行うこと。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（次席国税審判官を置く支部）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国税不服審判所組織令第二条第一項で規定する財務省令で定める支部は、東京国税不服審判所、名古屋国税不服審判所及び大阪国税不服審判所とする。"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令で定めるもののほか、事務分掌その他国税不服審判所の組織の細目は、国税庁長官が定める。"
              }
            }
          }
        ]
