                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "地すべり防止施設"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
