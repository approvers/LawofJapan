{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "030",
    "@Year": "04",
    "LawNum": "平成四年自治省令第三十号",
    "LawBody": {
      "LawTitle": "地方拠点都市地域の整備及び産業業務施設の再配置の促進に関する法律第十六条第一項の事業者及び公共施設に準ずる施設を定める省令",
      "EnactStatement": "地方拠点都市地域の整備及び産業業務施設の再配置の促進に関する法律（平成四年法律第七十六号）第十六条第一項の規定に基づき、地方拠点都市地域の整備及び産業業務施設の再配置の促進に関する法律第十六条第一項の事業者及び公共施設に準ずる施設を定める省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（事業者の範囲）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "地方拠点都市地域の整備及び産業業務施設の再配置の促進に関する法律（平成四年法律第七十六号。以下「法」という。）第十六条第一項に規定する総務省令で定める事業者は、次に掲げる事業者とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "地方公共団体が、資本金、基本金その他これらに準ずるものの二分の一以上を出資している一般社団法人、一般財団法人、株式会社又は有限会社"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "前号に掲げるもののほか、人材育成、地域間交流、教養文化活動等の活動を行う法人格を有する公共的団体のうち、その活動が地方拠点都市地域の振興に寄与するものとして総務大臣が指定するもの"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（公共施設に準ずる施設の範囲）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第十六条第一項に規定する総務省令で定める公共施設に準ずる施設は、次に掲げる施設とする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "教養文化施設"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "スポーツ又はレクリエーション施設"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "教育施設"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "展示施設又は見本市場施設"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "研修施設又は会議場施設"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "休養施設"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、スポーツ、音楽、展示等の用に供するための多様な機能を有する施設"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年九月一四日自治省令第四四号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年一二月一日総務省令第一二九号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成二十年十二月一日から施行する。"
            }
          }
        }
      ]
    }
  }
}
