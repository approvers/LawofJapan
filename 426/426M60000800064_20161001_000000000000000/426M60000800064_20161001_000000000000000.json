            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二条第二項第一号イの国土交通省令で定める公共の用に供する施設は、次に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "道路"
                  }
                },
                {
                  "@Num": "2",
