{
  "Law": {
    "@DataInfo": "190521e03k03m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "40",
    "@Year": "26",
    "LawNum": "平成二十六年国土交通省令第四十号",
    "LawBody": {
      "LawTitle": "国土交通省関係奄美群島振興開発特別措置法施行規則",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（法第十一条第四項第一号の国土交通省令で定める旅館業）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "奄美群島振興開発特別措置法（以下「法」という。）第十一条第四項第一号の国土交通省令で定める旅館業は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旅館業法（昭和二十三年法律第百三十八号）第二条第二項に規定する旅館・ホテル営業又は同条第三項に規定する簡易宿所営業であって、風俗営業等の規制及び業務の適正化等に関する法律（昭和二十三年法律第百二十二号）第二条第五項に規定する性風俗関連特殊営業に該当するもの"
                    }
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旅館業法第二条第四項に規定する下宿営業"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（観光旅客滞在促進事業を定めた産業振興促進計画の認定の申請の際に添付すべき書類）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十八条第一項の国土交通省令で定める書類は、次の表の上欄に掲げる規定の適用を受けようとするときは、同表の下欄に掲げる書類とする。"
                }
              },
              "TableStruct": {
                "Table": {
                  "@WritingMode": "vertical",
                  "TableRow": [
                    {
                      "TableColumn": [
                        {
                          "@colspan": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "規定"
                          }
                        },
                        {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "書類"
                          }
                        }
                      ]
                    },
                    {
                      "TableColumn": [
                        {
                          "@rowspan": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "法第十八条第一項"
                          }
                        },
                        {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "旅行業法第三条の旅行業者代理業の登録に係る部分"
                          }
                        },
                        {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "実施主体の商号、主たる営業所の名称及び所在地、代理する旅行業者の氏名又は名称並びに住所を記載した書類並びに旅行業法施行規則（昭和四十六年運輸省令第六十一号）第一条の四第一項第一号及び第二号に掲げる書類"
                          }
                        }
                      ]
                    },
                    {
                      "TableColumn": [
                        {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "旅行業法第六条の四第三項の規定による届出に係る部分"
                          }
                        },
                        {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "変更事項を記載した書類並びに旅行業法施行規則第五条第二項第一号及び第二号に掲げる書類"
                          }
                        }
                      ]
                    }
                  ]
                }
              }
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（標識の様式）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十八条第二項の国土交通省令で定める様式は、別記第一号様式とする。"
                }
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（法第十八条第四項第二号の国土交通省令で定める研修）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十八条第四項第二号の国土交通省令で定める研修は、次に掲げる基準に適合するものとする。"
                }
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旅行業法施行規則第十二条第一項第一号から第三号までに掲げる科目について行うものであること。"
                    }
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旅行業法第十一条の三第三項に規定する研修の講師又はこれと同等以上の知識及び経験を有する者として国土交通大臣が告示で定める者を講師とするものであること。"
                    }
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前二号に掲げるもののほか、国土交通大臣が告示で定める方法により行うものであること。"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（奄美群島内限定旅行業務取扱管理者の要件）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十八条第四項第二号の国土交通省令で定める要件は、前条の研修の課程を修了した者であることとする。"
                }
              }
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（奄美群島内限定旅行業務取扱管理者の証明書の様式）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十八条第四項の規定により奄美群島内限定旅行業務取扱管理者を旅行業法第十一条の二第一項に規定する旅行業務取扱管理者とみなして、同法の規定を適用する場合において、旅行業法施行規則第二十七条の七中「第十号様式」とあるのは、「奄美群島振興開発特別措置法施行規則別記第二号様式」とする。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、平成二十六年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年七月一五日国土交通省令第五三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、独立行政法人に係る改革を推進するための国土交通省関係法律の整備に関する法律附則第一条第二号に掲げる規定の施行の日（平成二十七年七月十六日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年九月七日国土交通省令第六八号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、平成二十七年十月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年一二月九日国土交通省令第八二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この省令は、公布の日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、第三条、第八条、第十七条、第二十四条及び第二十五条の規定は、行政手続における特定の個人を識別するための番号の利用等に関する法律（平成二十五年法律第二十七号。以下「番号利用法」という。）附則第一条第四号に掲げる規定の施行の日（平成二十八年一月一日）から施行する。"
                  }
                ]
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二九年一〇月三一日国土交通省令第六六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、平成三十年一月四日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三〇年一月四日国土交通省令第一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この省令は、通訳案内士法及び旅行業法の一部を改正する法律（以下「改正法」という。）の施行の日（平成三十年一月四日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三〇年六月一四日国土交通省令第四七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、旅館業法の一部を改正する法律の施行の日（平成三十年六月十五日）から施行する。"
              }
            }
          }
        }
      ],
