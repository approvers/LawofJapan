{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "205",
    "@Year": "26",
    "LawNum": "平成二十六年政令第二百五号",
    "LawBody": {
      "LawTitle": "健康・医療戦略推進本部令",
      "EnactStatement": "内閣は、健康・医療戦略推進法（平成二十六年法律第四十八号）第二十九条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（専門調査会）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "健康・医療戦略推進本部（次条において「本部」という。）は、専門の事項を調査させるため必要があるときは、その議決により、専門調査会を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門調査会の委員は、当該専門の事項に関し学識経験を有する者のうちから、内閣総理大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門調査会の委員は、非常勤とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門調査会は、その設置に係る調査が終了したときは、廃止されるものとする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（本部の運営）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、本部の運営に関し必要な事項は、健康・医療戦略推進本部長が本部に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、健康・医療戦略推進法附則第一条ただし書に規定する規定の施行の日（平成二十六年六月十日）から施行する。"
            }
          }
        }
      }
    }
  }
}
