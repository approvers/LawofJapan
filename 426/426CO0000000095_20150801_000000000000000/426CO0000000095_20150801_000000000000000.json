            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、改正法の施行の日（平成二十六年四月一日）から施行する。"
              }
            }
          }
        }
      }
