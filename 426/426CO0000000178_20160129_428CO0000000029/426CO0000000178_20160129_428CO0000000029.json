    "LawBody": {
      "LawTitle": "国家戦略特別区域を定める政令",
      "EnactStatement": "内閣は、国家戦略特別区域法（平成二十五年法律第百七号）第二条第一項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "国家戦略特別区域法第二条第一項の政令で定める区域は、次に掲げる区域とする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "宮城県仙台市の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "秋田県仙北市の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "千葉県千葉市及び成田市、東京都並びに神奈川県の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "4",
              "ItemTitle": "四",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "新潟県新潟市の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "5",
              "ItemTitle": "五",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "愛知県の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "6",
              "ItemTitle": "六",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "京都府、大阪府及び兵庫県の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "7",
              "ItemTitle": "七",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "兵庫県養父市の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "8",
              "ItemTitle": "八",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "広島県及び愛媛県今治市の区域"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
