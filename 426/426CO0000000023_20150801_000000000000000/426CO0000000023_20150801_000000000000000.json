            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、廃止法の施行の日（平成二十六年四月一日）から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、第九条の規定は、公布の日から施行する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "平成二六年三月二八日政令第八四号",
          "SupplProvisionLabel": "附　則",
