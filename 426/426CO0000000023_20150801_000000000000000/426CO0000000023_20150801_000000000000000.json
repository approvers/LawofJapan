            "Article": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ArticleCaption": "（独立行政法人日本万国博覧会記念機構法施行令の廃止）",
                "ArticleTitle": "第一条",
                "Paragraph": {
