{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "ImperialOrder",
    "@Num": "263",
    "@Year": "02",
    "LawNum": "昭和二年勅令第二百六十三号",
    "LawBody": {
      "LawTitle": "北海道国有未開地処分法施行規則",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "売払ヒ又ハ貸付スベキ土地ハ之ヲ公示ス但シ第二条第一号及第三号ノ場合並ニ一区域五町歩（四百九十五アール百二十一分ノ百五）未満ノモノハ此ノ限ニ在ラズ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "土地ノ売払又ハ貸付ハ競争ニ付シテ之ヲ行フ但シ左ノ場合ニ於テハ競争ニ付セザルコトヲ得"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "公用又ハ公共ノ利益ト為ルベキ事業ニ供セントスル土地ヲ売払ヒ又ハ貸付スルトキ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "二十町歩（千九百八十三アール百二十一分ノ五十七）以内ノ土地ヲ売払ヒ又ハ貸付スルトキ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "拓殖上特ニ必要ナル事業ニ供セントスル土地ヲ売払ヒ又ハ貸付スルトキ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "二回以上競争ニ付シテ競落者ナキ土地ヲ予定価格以上ニテ売払ヒ又ハ貸付スルトキ"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "土地ノ売払又ハ貸付面積ハ一人ニ付左ノ制限ヲ超ユルコトヲ得ズ但シ前ニ売払ヲ受ケタル土地ノ事業ヲ成功シタル者ニ対シテハ其ノ面積ヲ通算セズ"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "耕作ニ供スル土地"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "二百町歩（一万九千八百三十四アール百二十一分ノ八十六）"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "牧畜ニ供スル土地"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "五百町歩（四万九千五百八十六アール百二十一分ノ九十四）"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "植樹ニ供スル土地"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "五百町歩（四万九千五百八十六アール百二十一分ノ九十四）"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "特定地"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "十町歩（九百九十一アール百二十一分ノ八十九）"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "其ノ他ノ目的ニ供スル土地"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "十町歩（九百九十一アール百二十一分ノ八十九）"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会社、組合其ノ他共同シテ事業ヲ経営セントスル者ニ対シテハ其ノ資産及人員ニ応ジ前項面積ノ五倍迄累加スルコトヲ得"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特定地ハ其ノ状況ニ依リ特殊ノ経営ヲ必要トスル場合ニ於テハ釧路国支庁管轄区域及根室支庁管轄区域ニ在リテハ第一項面積ノ二倍迄、其ノ他ノ支庁管轄区域ニ在リテハ第一項面積ノ一倍半迄累加スルコトヲ得"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "売払ヒ又ハ貸付シタル土地ノ事業成功期間ハ左ノ制限ヲ超ユルコトヲ得ズ"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "十町歩（九百九十一アール百二十一分ノ八十九）未満"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "五年"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "三十町歩（二千九百七十五アール百二十一分ノ二十五）未満"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "八年"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "三十町歩（二千九百七十五アール百二十一分ノ二十五）以上"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "十年"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "植樹又ハ泥炭地ノ使用ニ付テハ前項期間ノ二倍トス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特定地ヲ設定シタルトキハ之ヲ公示ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特定地ハ戸主又ハ成年者ニ限リ之ヲ貸付シ貸付許可ノ翌月ヨリ六月以内ニ其ノ土地又ハ其ノ附近ニ移住シ事業成功ニ至ル迄引続キ居住シタルトキハ之ヲ付与ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "売払フベキ土地ニ立木アルトキハ土地ト共ニ之ヲ売払フモノトス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "耕作ヲ目的トスル土地ニ在リテハ命令ノ定ムル所ニ依リ其ノ十分ノ四以内ヲ防風林、風致林又ハ薪炭用材林ト為スコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "土地ノ売払又ハ貸付ヲ受クベキ者ニ関シ必要ナル事項ハ命令ヲ以テ之ヲ定ム"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "○１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本令ハ公布ノ日ヨリ之ヲ施行ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和八年六月一七日勅令第一五八号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本令ハ公布ノ日ヨリ之ヲ施行ス"
              }
            }
          }
        }
      ]
    }
  }
}
