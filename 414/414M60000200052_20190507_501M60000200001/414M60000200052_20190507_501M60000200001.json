                        "@Num": "1",
                        "Subitem1Title": "イ",
                        "Subitem1Sentence": {
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "成年被後見人若しくは被保佐人又は外国の法令上これらと同様に取り扱われている者"
                          }
                        }
                      },
                      {
                        "@Delete": "false",
                        "@Hide": "false",
