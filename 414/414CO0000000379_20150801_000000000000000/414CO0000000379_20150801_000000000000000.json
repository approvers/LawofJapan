                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "不動産の表題登記"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "所有者"
                        }
                      }
