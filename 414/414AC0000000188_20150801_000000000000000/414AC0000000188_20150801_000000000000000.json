            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、会社法の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二六年六月二七日法律第九一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、会社法の一部を改正する法律の施行の日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
