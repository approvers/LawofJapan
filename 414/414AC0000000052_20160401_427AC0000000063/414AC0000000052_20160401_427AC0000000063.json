              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、公布の日から起算して三月を超えない範囲内において政令で定める日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年七月二六日法律第八七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
