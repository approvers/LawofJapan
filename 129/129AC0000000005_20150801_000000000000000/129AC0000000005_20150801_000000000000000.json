    "LawBody": {
      "LawTitle": "国債証券買入銷却法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "政府ハ毎年度国債費予算定額以内ニ於テ国債証券ヲ買入レ之カ銷却ヲ為スコトヲ得"
                }
              },
              "List": {
                "ListSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項買入ハ計算上利益アリト認ムルトキ其ノ他国債ノ整理ノ円滑ナル実施ノタメ必要アリト認ムルトキニ限リ之ヲ為スコトヲ得"
                  }
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "国債証券ノ買入銷却ヲ為シタルトキハ財務大臣ハ其ノ証券ノ種類、番号、総額及其ノ買入価格ヲ告示スヘシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "銷却ノ為ニスル国債証券ノ買入ハ随意契約ヲ以テ之ヲ為スコトヲ得"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@AmendLawNum": "平成一一年一二月二二日法律第一六〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この法律（第二条及び第三条を除く。）は、平成十三年一月六日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                  }
                ]
              },
              "Item": {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第九百九十五条（核原料物質、核燃料物質及び原子炉の規制に関する法律の一部を改正する法律附則の改正規定に係る部分に限る。）、第千三百五条、第千三百六条、第千三百二十四条第二項、第千三百二十六条第二項及び第千三百四十四条の規定"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "公布の日"
                      }
                    }
                  ]
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一四年六月一二日法律第六五号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
