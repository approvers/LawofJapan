    "LawNum": "明治二十三年法律第一号",
    "LawBody": {
      "LawTitle": "保管金規則",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法律勅令又ハ従来ノ規則ニ依リ政府ニ於テ保管スル公有金私有金ハ左ノ計算法ニ従ヒ満五年ヲ過キテ払戻ノ請求ナキトキハ政府ノ所得トス但別ニ法律ヲ以テ失権ノ期限ヲ定メタルモノハ各其定ムル所ニ依ル"
                }
              },
              "List": [
                {
                  "ListSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "第一"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "保管義務解除ノ期アルモノハ其義務ヲ解除シタル翌日ヨリ起算ス"
                        }
                      }
                    ]
                  }
                },
                {
                  "ListSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "第二"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "保管義務解除ノ期ナキモノハ保管ノ翌日ヨリ起算ス"
                        }
                      }
                    ]
                  }
                },
                {
                  "ListSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "第三"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "訴訟事件ノ為ニ払戻ヲ請求スル能ハサル場合ニ於テハ裁判確定ノ翌日ヨリ起算ス"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "保管金ハ法律勅令又ハ従来ノ規則若クハ契約ニ依ルノ外利子ヲ付セス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "保管金ノ証書ハ売買譲与又ハ書入質入スルコトヲ得ス"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@AmendLawNum": "明治三三年二月二六日法律第一八号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本法ノ期間ハ本法施行前ノ保管金ニ関シテハ本法施行ノ日ヨリ起算ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四二年五月三一日法律第二三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
