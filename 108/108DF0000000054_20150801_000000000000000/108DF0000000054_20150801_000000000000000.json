{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "54",
    "@Year": "08",
    "LawNum": "明治八年太政官布告第五十四号",
    "LawBody": {
      "LawTitle": "明治八年太政官布告第五十四号（勲章制定ノ件）",
      "Preamble": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "今般賞牌別冊ノ通被定候条此旨布告候事"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "（別冊）"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "朕惟フニ凡ソ国家ニ功ヲ立テ績ヲ顕ス者宜ク之ヲ褒賞シ以テ之ニ酬ユヘシ仍テ勲等賞牌ノ典ヲ定メ人々ヲシテ寵異表彰スル所アルヲ知ラシメントス汝有司其斯旨ヲ体セヨ"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "4",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "明治八年二月"
              }
            }
          }
        ]
      },
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "勲等ハ勲績及功労アル者ヲ賞スル為メニ設クル所ノ階級ニシテ位階ト異ナル故ニ各種ノ勲章ヲ佩用セシム"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "勲等ヲ分ツテ次ノ六級ト為ス"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旭日大綬章、瑞宝大綬章又ハ桐花大綬章ヲ賜フベキモノ"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旭日重光章又ハ瑞宝重光章ヲ賜フベキモノ"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旭日中綬章又ハ瑞宝中綬章ヲ賜フベキモノ"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旭日小綬章又ハ瑞宝小綬章ヲ賜フベキモノ"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旭日双光章又ハ瑞宝双光章ヲ賜フベキモノ"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旭日単光章又ハ瑞宝単光章ヲ賜フベキモノ"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "旭日大綬章、旭日重光章、旭日中綬章、旭日小綬章、旭日双光章及旭日単光章ハ国家又ハ公共ニ対シ勲績アル者ニ之ヲ賜フ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "右各勲章ニ在テハ章ハ旭日ノ形ヲ以テ飾リ綬ハ地白色双線紅色トス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "瑞宝大綬章、瑞宝重光章、瑞宝中綬章、瑞宝小綬章、瑞宝双光章及瑞宝単光章ハ国家又ハ公共ニ対シ積年ノ功労アル者ニ之ヲ賜フ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "右各勲章ニ在テハ章ハ鏡珠ノ形ヲ以テ飾リ綬ハ地藍色双線橙黄色トス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "桐花大綬章ハ旭日大綬章又ハ瑞宝大綬章ヲ賜フベキ者ノ中其勲績又ハ功労特ニ優レタルモノニ之ヲ賜フ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "桐花大綬章ノ章ハ旭日ト桐花ノ形ヲ以テ飾リ綬ハ地紅色双線白色トス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "勲章ハ佩用本人ニ止リ子孫之ヲ用ユルコトヲ得ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "勲章ノ製式其他ノ細目ハ内閣府令ヲ以テ之ヲ定ム"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@AmendLawNum": "平成一四年八月一二日政令第二七七号",
        "@Extract": "false",
        "@Type": "Amend",
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十五年五月一日から施行し、改正後の規定は、平成十五年十一月三日以後の日付をもって授与される勲章から適用する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphCaption": "（経過措置）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令による改正前の規定により授与された勲章及び平成十五年十一月二日以前の日付をもって授与される勲章については、改正前の規定は、なおその効力を有する。"
              }
            }
          }
        ]
      }
    }
  }
}
