    "LawBody": {
      "LawTitle": "不動産登記の嘱託職員を指定する省令",
      "EnactStatement": "不動産登記法（明治三十二年法律第二十四号）第三十五条第三項の規定に基づき、不動産登記の嘱託職員を指定する省令を次のように定める。",
      "MainProvision": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "不動産登記令（平成十六年政令第三百七十九号）第七条第二項の規定に基づき、厚生労働省の所管に属する不動産に関する権利の登記の嘱託については、次の職員を指定する。"
          },
          "List": [
            {
              "ListSentence": {
                "Sentence": "大臣官房会計課長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "医政局長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "労働基準局長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "職業安定局長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "年金局長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "検疫所長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "国立ハンセン病療養所長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "国立医薬品食品衛生研究所長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "国立保健医療科学院長"
              }
            },
            {
              "ListSentence": {
                "Sentence": "国立社会保障・人口問題研究所長"
              }
            },
            {
