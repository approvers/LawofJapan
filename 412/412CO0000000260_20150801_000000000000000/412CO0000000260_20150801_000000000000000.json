{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "260",
    "@Year": "12",
    "LawNum": "平成十二年政令第二百六十号",
    "LawBody": {
      "LawTitle": "金融危機対応会議令",
      "EnactStatement": "内閣は、内閣府設置法（平成十一年法律第八十九号）第四十二条第七項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（議長）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "金融危機対応会議（以下「会議」という。）の議長は、会務を総理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "議長に事故があるときは、内閣府設置法第十一条の特命担当大臣が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（資料提出の要求等）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "会議は、その所掌事務を遂行するため必要があると認めるときは、関係行政機関の長に対し、資料の提出、意見の開陳、説明その他必要な協力を求めることができる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "会議の庶務は、金融庁監督局総務課において財務省大臣官房信用機構課の協力を得て処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、議事の手続その他会議の運営に関し必要な事項は、議長が会議に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          }
        }
      }
    }
  }
}
