{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "282",
    "@Year": "12",
    "LawNum": "平成十二年政令第二百八十二号",
    "LawBody": {
      "LawTitle": "社会保障審議会令",
      "EnactStatement": "内閣は、厚生労働省設置法（平成十一年法律第九十七号）第七条第二項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "社会保障審議会（以下「審議会」という。）は、委員三十人以内で組織する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に、特別の事項を調査審議させるため必要があるときは、臨時委員を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に、専門の事項を調査させるため必要があるときは、専門委員を置くことができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（委員等の任命）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員及び臨時委員は、学識経験のある者のうちから、厚生労働大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、当該専門の事項に関し学識経験のある者のうちから、厚生労働大臣が任命する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（委員の任期等）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、再任されることができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "臨時委員は、その者の任命に係る当該特別の事項に関する調査審議が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、その者の任命に係る当該専門の事項に関する調査が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員、臨時委員及び専門委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に会長を置き、委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長は、会務を総理し、審議会を代表する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長に事故があるときは、あらかじめその指名する委員が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（分科会）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に、次の表の上欄に掲げる分科会を置き、これらの分科会の所掌事務は、審議会の所掌事務のうち、それぞれ同表の下欄に掲げるとおりとする。"
                  }
                },
                "TableStruct": {
                  "Table": {
                    "@WritingMode": "vertical",
                    "TableRow": [
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "名称"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "所掌事務"
                            }
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "統計分科会"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "統計の総合的企画、調査及び研究、統計の改善及び整備並びに統計の知識の普及及び指導に関する事項を調査審議すること。"
                            }
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "医療分科会"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "医療法（昭和二十三年法律第二百五号）の規定によりその権限に属させられた事項を処理すること。"
                            }
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "福祉文化分科会"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "児童福祉法（昭和二十二年法律第百六十四号）、身体障害者福祉法（昭和二十四年法律第二百八十三号）及び社会福祉法（昭和二十六年法律第四十五号）第百二十五条の規定によりその権限に属させられた事項を処理すること。"
                            }
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "介護給付費分科会"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "介護保険法（平成九年法律第百二十三号）及び介護保険法施行法（平成九年法律第百二十四号）の規定によりその権限に属させられた事項を処理すること。"
                            }
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "医療保険保険料率分科会"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "健康保険法（大正十一年法律第七十号）、船員保険法（昭和十四年法律第七十三号）及び健康保険法等の一部を改正する法律（昭和五十九年法律第七十七号）の規定によりその権限に属させられた事項を処理すること。"
                            }
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "年金記録訂正分科会"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "厚生年金保険法（昭和二十九年法律第百十五号）、国民年金法（昭和三十四年法律第百四十一号）第十四条の三第二項及び第十四条の四第三項並びに厚生年金保険の保険給付及び保険料の納付の特例等に関する法律（平成十九年法律第百三十一号）の規定によりその権限に属させられた事項を処理すること。"
                            }
                          }
                        ]
                      }
                    ]
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項の表の上欄に掲げる分科会に属すべき委員、臨時委員及び専門委員は、厚生労働大臣が指名する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "分科会に分科会長を置き、当該分科会に属する委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "分科会長は、当該分科会の事務を掌理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "分科会長に事故があるときは、当該分科会に属する委員又は臨時委員のうちから分科会長があらかじめ指名する者が、その職務を代理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、その定めるところにより、分科会の議決をもって審議会の議決とすることができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（部会）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会及び分科会は、その定めるところにより、部会を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に属すべき委員、臨時委員及び専門委員は、会長（分科会に置かれる部会にあっては、分科会長）が指名する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に部会長を置き、当該部会に属する委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長は、当該部会の事務を掌理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長に事故があるときは、当該部会に属する委員又は臨時委員のうちから部会長があらかじめ指名する者が、その職務を代理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会（分科会に置かれる部会にあっては、分科会。以下この項において同じ。）は、その定めるところにより、部会の議決をもって審議会の議決とすることができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（幹事）",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会に、幹事を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、関係行政機関の職員のうちから、厚生労働大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、審議会の所掌事務について、委員を補佐する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、委員及び議事に関係のある臨時委員の三分の一以上が出席しなければ、会議を開き、議決することができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会の議事は、委員及び議事に関係のある臨時委員で会議に出席したものの過半数で決し、可否同数のときは、会長の決するところによる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前二項の規定は、分科会及び部会の議事に準用する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "9",
            "ArticleCaption": "（資料の提出等の要求）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会は、その所掌事務を遂行するため必要があると認めるときは、関係行政機関の長に対し、資料の提出、意見の表明、説明その他必要な協力を求めることができる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "10",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第十条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "審議会の庶務は、厚生労働省政策統括官において総括し、及び処理する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、次の各号に掲げる分科会に係るものについては、それぞれ当該各号に定める課において処理する。"
                  }
                ]
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "医療分科会"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "厚生労働省医政局総務課"
                        }
                      }
                    ]
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
