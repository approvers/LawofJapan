{
  "Law": {
    "@DataInfo": "190415e04k03m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "302",
    "@Year": "12",
    "LawNum": "平成十二年政令第三百二号",
    "LawBody": {
      "LawTitle": "臨時水俣病認定審査会令",
      "EnactStatement": "内閣は、国家行政組織法（昭和二十三年法律第百二十号）第八条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "臨時水俣病認定審査会（以下「審査会」という。）は、委員十人以内で組織する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会に、水俣病に係る医学に関する専門の事項を調査させるため必要があるときは、専門委員を置くことができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（委員等の任命）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、水俣病に係る医学に関し高度の学識と豊富な経験を有する者のうちから、環境大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、当該専門の事項に関し高度の学識と豊富な経験を有する者のうちから、環境大臣が任命する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（委員の任期等）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、再任されることができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、その者の任命に係る当該専門の事項に関する調査が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員及び専門委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（会長及び副会長）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会に、会長及び副会長一人を置き、委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長は、会務を総理し、審査会を代表する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "副会長は、会長を補佐し、会長に事故があるときは、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会は、委員の半数以上が出席しなければ、会議を開き、議決することができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審査会の議事は、会議に出席した委員の過半数で決し、可否同数のときは、会長の決するところによる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審査会の庶務は、環境省大臣官房環境保健部環境保健企画管理課において処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、議事の手続その他審査会の運営に関し必要な事項は、会長が審査会に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年三月三一日政令第一二二号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成二十八年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二九年六月三〇日政令第一六九号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成二十九年七月十四日から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
