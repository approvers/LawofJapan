                      },
                      {
                        "TableColumn": [
                          {
                            "@BorderBottom": "solid",
                            "@BorderLeft": "solid",
                            "@BorderRight": "solid",
                            "@BorderTop": "solid",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "労働条件分科会"
                            }
                          },
                          {
                            "@BorderBottom": "solid",
