{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "177",
    "@Year": "12",
    "LawNum": "平成十二年政令第百七十七号",
    "LawBody": {
      "LawTitle": "電気通信回線による登記情報の提供に関する法律施行令",
      "EnactStatement": "内閣は、電気通信回線による登記情報の提供に関する法律（平成十一年法律第二百二十六号）第二条第一項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "電気通信回線による登記情報の提供に関する法律第二条第一項の政令で定める帳簿は、次に掲げる帳簿とする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法人（株式会社、合名会社、合資会社、合同会社及び外国会社を除く。）の登記簿"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "投資事業有限責任組合契約に関する法律（平成十年法律第九十号）による投資事業有限責任組合契約登記簿"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "有限責任事業組合契約に関する法律（平成十七年法律第四十号）による有限責任事業組合契約登記簿"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "4",
              "ItemTitle": "四",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "動産及び債権の譲渡の対抗要件に関する民法の特例等に関する法律（平成十年法律第百四号）による動産譲渡登記事項概要ファイル及び債権譲渡登記事項概要ファイル"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "5",
              "ItemTitle": "五",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "信託法（平成十八年法律第百八号）による限定責任信託登記簿"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "6",
              "ItemTitle": "六",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "地図、建物所在図、地図に準ずる図面及び不動産登記令（平成十六年政令第三百七十九号）第二十一条第一項に規定する図面が記録されたファイル"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "7",
              "ItemTitle": "七",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "船舶登記令（平成十七年政令第十一号）による船舶の登記簿及び製造中の船舶の登記簿"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "8",
              "ItemTitle": "八",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "農業用動産抵当登記令（平成十七年政令第二十五号）による農業用動産の登記簿"
                }
              }
            }
          ]
        }
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、電気通信回線による登記情報の提供に関する法律の施行の日（平成十二年四月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年四月二八日政令第一七三号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、中小企業等投資事業有限責任組合契約に関する法律の一部を改正する法律（平成十六年法律第三十四号）の施行の日（平成十六年四月三十日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年七月二九日政令第二六九号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法の施行の日（平成十七年八月一日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年九月九日政令第二九四号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、債権譲渡の対抗要件に関する民法の特例等に関する法律の一部を改正する法律（以下「改正法」という。）の施行の日（平成十七年十月三日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年一二月一四日政令第三六六号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、会社法の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年三月七日政令第四〇号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十九年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年八月八日政令第二四九号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成二十年十一月三十日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、第五条（電気通信回線による登記情報の提供に関する法律施行令第五号を同令第六号とし、同令第四号の次に一号を加える改正規定に限る。）の規定は、公布の日から施行する。"
                }
              ]
            }
          }
        }
      ]
    }
  }
}
