{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "507",
    "@Year": "12",
    "LawNum": "平成十二年政令第五百七号",
    "LawBody": {
      "LawTitle": "独立行政法人教員研修センター法の施行に伴う関係政令の整備及び経過措置に関する政令　抄",
      "EnactStatement": "内閣は、独立行政法人教員研修センター法（平成十二年法律第八十八号）第十一条第四項、附則第二条並びに附則第七条第一項、第二項及び第四項並びに関係法律の規定に基づき、この政令を制定する。",
      "TOC": {
        "TOCLabel": "目次",
        "TOCChapter": [
          {
            "@Delete": "false",
            "@Num": "1",
            "ChapterTitle": "第一章　関係政令の整備",
            "ArticleRange": "（第一条―第十一条）"
          },
          {
            "@Delete": "false",
            "@Num": "2",
            "ChapterTitle": "第二章　経過措置",
            "ArticleRange": "（第十二条―第十五条）"
          }
        ],
        "TOCSupplProvision": {
          "SupplProvisionLabel": "附則"
        }
      },
      "MainProvision": {
        "Chapter": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "2",
          "ChapterTitle": "第二章　経過措置",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "12",
              "ArticleCaption": "（職員の引継ぎに係る政令で定める部局）",
              "ArticleTitle": "第十二条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "独立行政法人教員研修センター法（以下「法」という。）附則第二条の政令で定める文部科学省の部局は、次に掲げる部局とする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "大臣官房人事課、総務課及び会計課"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "生涯学習政策局調査企画課"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "初等中等教育局初等中等教育企画課及び教職員課"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "スポーツ・青少年局学校健康教育課"
                      }
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "13",
              "ArticleCaption": "（独立行政法人教員研修センターの成立の時において承継される権利及び義務）",
              "ArticleTitle": "第十三条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第七条第一項の政令で定める権利及び義務は、次に掲げる権利及び義務とする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "文部科学大臣の所管に属する土地、建物及び工作物（その土地に定着する物及びその建物に附属する工作物を含む。次条第一号において「土地等」という。）のうち文部科学大臣が財務大臣に協議して指定するものに関する権利及び義務"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "文部科学大臣の所管に属する物品のうち文部科学大臣が指定するものに関する権利及び義務"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "法第十条に規定する業務に関し国が有する権利及び義務のうち前二号に掲げるもの以外のものであって、文部科学大臣が指定するもの"
                      }
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "14",
              "ArticleCaption": "（権利及び義務の承継の際出資があったものとされる財産）",
              "ArticleTitle": "第十四条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第七条第二項の政令で定める財産は、次に掲げる財産とする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "前条第一号の規定により指定された土地等"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "前条第三号の規定により指定された権利に係る財産のうち文部科学大臣が指定するもの"
                      }
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "15",
              "ArticleCaption": "（出資があったものとされる財産に係る評価委員の任命等）",
              "ArticleTitle": "第十五条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第七条第三項の評価委員は、次に掲げる者につき文部科学大臣が任命する。"
                    }
                  },
                  "Item": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "ItemTitle": "一",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "財務省の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "文部科学省の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "ItemTitle": "三",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "独立行政法人教員研修センターの役員（独立行政法人教員研修センターが成立するまでの間は、独立行政法人教員研修センターに係る独立行政法人通則法（平成十一年法律第百三号）第十五条第一項の設立委員）"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "ItemTitle": "四",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "学識経験のある者"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "二人"
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第七条第三項の規定による評価は、同項の評価委員の過半数の一致によるものとする。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "3",
                  "ParagraphNum": "３",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第七条第三項の規定による評価に関する庶務は、文部科学省初等中等教育局教職員課において処理する。"
                    }
                  }
                }
              ]
            }
          ]
        }
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、平成十三年一月六日から施行する。"
            }
          }
        }
      }
    }
  }
}
