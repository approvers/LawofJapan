    "@Year": "12",
    "LawNum": "平成十二年八月二十一日内閣総理大臣決定",
    "LawBody": {
      "LawTitle": "内閣広報室に内閣副広報官等を置く規則",
      "EnactStatement": "内閣官房組織令（昭和三十二年政令第二百十九号）第十二条の規定に基づき、内閣広報官室に内閣副広報官を置く規則（平成十年七月三十日内閣総理大臣決定）の全部を次のように改正する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（内閣副広報官）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣広報室に、内閣副広報官一人を置き、内閣参事官のうちから命ずる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "内閣副広報官は、内閣広報官を助け、命を受けて内閣官房組織令第三条第二項に規定する事務のうち、主として海外に対する広報に関する専門的事項に関するものに従事する。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（企画官）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣広報室に、併任の者を除き、企画官一人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "企画官は、命を受けて内閣広報室の事務のうち特定事項の企画及び立案に関する事務に従事する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（調査官）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣広報室に、併任の者を除き、調査官一人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "調査官は、命を受けて内閣広報室の事務のうち専門的事項の調査、研究及び企画に関する事務に従事する。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年四月一日",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二三年三月三一日",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、平成二十三年四月一日から実施する。"
            }
          }
        }
      ]
    }
  }
}
