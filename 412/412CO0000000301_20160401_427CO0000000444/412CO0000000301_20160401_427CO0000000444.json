{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "301",
    "@Year": "12",
    "LawNum": "平成十二年政令第三百一号",
    "LawBody": {
      "LawTitle": "運輸審議会令",
      "EnactStatement": "内閣は、国土交通省設置法（平成十一年法律第百号）第二十六条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（専門委員）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "運輸審議会（以下「審議会」という。）に、専門の事項を調査させるため必要があるときは、専門委員を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、当該専門の事項に関し学識経験のある者のうちから、国土交通大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、その者の任命に係る当該専門の事項に関する調査が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（部会）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、その定めるところにより、部会を置くことができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に属すべき委員及び専門委員は、会長が指名する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会に、部会長を置き、当該部会に属する委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長は、当該部会の事務を掌理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会長に事故があるときは、当該部会に属する委員のうちから部会長があらかじめ指名する者が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（議決方法）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、委員の過半数が出席しなければ、会議を開き、議決することができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会の議事は、出席委員の過半数で決し、可否同数のときは、会長の決するところによる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "特定の事案につき特別の利害関係を有する委員は、審議会の決議があったときは、当該事案に係る議決に参加することができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会は、国の関係行政機関の職員をその会議に出席させて必要な説明を求めることができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "国の関係行政機関の長は、その職員を審議会に出席させて意見を述べさせ、又は説明をさせることができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前各項の規定は、部会の議事について準用する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会の庶務は、国土交通省総合政策局総務課において処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（公聴会の主宰）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "国土交通省設置法第二十三条の公聴会は、審議会が事案を指定して指名する国土交通省の職員が主宰する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、事案が特に重要である場合において、審議会が公聴会を自ら主宰し、又は委員を指名して公聴会を主宰させることを妨げない。"
                  }
                ]
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（報告書の作成）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前条の規定により指名された委員又は国土交通省の職員は、公聴会の審理によって知ることができた事実を報告書として作成し、これを審議会に提出しなければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（報告書の提示）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "審議会は、前条の報告書を国土交通省設置法第二十三条の利害関係人であって公聴会において意見を述べた者（以下この条及び次条において単に「利害関係人」という。）に提示しなければならない。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、公聴会において、報告書の提示を必要としない旨の利害関係人の合意があったときは、この限りでない。"
                  }
                ]
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleCaption": "（申立て）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前条の報告書の提示を受けた利害関係人は、報告書に誤りがあると認めるときは、その提示を受けた日から十五日以内にその旨の申立てをすることができる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "9",
            "ArticleCaption": "（再審理）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会は、前条の申立てを審査して、報告書に誤りがあって審議会の決定に影響を及ぼすおそれがあると認めるときは、再び公聴会を開かなければならない。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "10",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第十条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議会の決定及び第六条の報告書は、国土交通省令の定めるところにより、公表しなければならない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令に定めるもののほか、議事の手続その他審議会の運営に関し必要な事項は、国土交通省令で定める。"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十三年一月六日から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "審議会が国土交通省設置法附則第八条第一項の規定に基づき旅客鉄道株式会社及び日本貨物鉄道株式会社に関する法律の一部を改正する法律（平成十三年法律第六十一号）及び旅客鉄道株式会社及び日本貨物鉄道株式会社に関する法律の一部を改正する法律（平成二十七年法律第三十六号）の規定によりその権限に属させられた事項を処理する場合には、第五条及び第七条中「第二十三条」とあるのは、「第二十三条（同法附則第八条第二項において準用する場合を含む。）」とする。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一三年一一月七日政令第三四六号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、旅客鉄道株式会社及び日本貨物鉄道株式会社に関する法律の一部を改正する法律の施行の日（平成十三年十二月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一八年三月三一日政令第一五二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十八年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年七月一八日政令第二三一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "この政令は、平成二十年十月一日から施行する。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ArticleCaption": "（処分等に関する経過措置）",
              "ArticleTitle": "第二条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "国土交通省設置法等の一部を改正する法律（以下この条において「改正法」という。）による改正前の法律（これに基づく命令を含む。以下この条において「旧法令」という。）の規定により次の表の中欄に掲げる従前の国の機関（以下この条において「旧機関」という。）がした認可、指定その他の処分又は通知その他の行為は、改正法の施行後は、改正法による改正後の法律（これに基づく命令を含む。以下この条において「新法令」という。）の相当規定に基づいて、同表の下欄に掲げる相当の国等の機関（以下この条において「新機関」という。）がした認可、指定その他の処分又は通知その他の行為とみなす。"
                    }
                  },
                  "TableStruct": {
                    "Table": {
