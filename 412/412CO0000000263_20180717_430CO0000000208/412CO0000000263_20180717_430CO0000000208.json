{
  "Law": {
    "@DataInfo": "180827e01k01m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "263",
    "@Year": "12",
    "LawNum": "平成十二年政令第二百六十三号",
    "LawBody": {
      "LawTitle": "金融審議会令",
      "EnactStatement": "内閣は、金融庁設置法（平成十年法律第百三十号）第七条第二項及び第三項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "金融審議会（以下「審議会」という。）は、委員三十人以内で組織する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "審議会に、特別の事項を調査審議させるため必要があるときは、臨時委員を置くことができる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "審議会に、専門の事項を調査させるため必要があるときは、専門委員を置くことができる。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（委員等の任命）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "委員及び臨時委員は、学識経験のある者のうちから、内閣総理大臣が任命する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "専門委員は、当該専門の事項に関し学識経験のある者のうちから、内閣総理大臣が任命する。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（委員の任期等）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "委員は、再任されることができる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "臨時委員は、その者の任命に係る当該特別の事項に関する調査審議が終了したときは、解任されるものとする。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "専門委員は、その者の任命に係る当該専門の事項に関する調査が終了したときは、解任されるものとする。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": "委員、臨時委員及び専門委員は、非常勤とする。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会に、会長を置き、委員の互選により選任する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "会長は、会務を総理し、審議会を代表する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "会長に事故があるときは、あらかじめその指名する委員が、その職務を代理する。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": "（分科会）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会に、次の表の上欄に掲げる分科会を置き、これらの分科会の所掌事務は、審議会の所掌事務のうち、それぞれ同表の下欄に掲げるとおりとする。"
                },
                "TableStruct": {
                  "Table": {
                    "TableRow": [
                      {
                        "TableColumn": [
                          {
                            "Sentence": "名称"
                          },
                          {
                            "Sentence": "所掌事務"
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "Sentence": "金融分科会"
                          },
                          {
                            "Sentence": "国内金融に関する制度等の改善に関する事項その他の国内金融等に関する重要事項を調査審議すること。"
                          }
                        ]
                      },
                      {
                        "TableColumn": [
                          {
                            "Sentence": "金利調整分科会"
                          },
                          {
                            "Sentence": [
                              "一　金融機関の金利に関する事項を調査審議すること。",
                              "二　臨時金利調整法（昭和二十二年法律第百八十一号）第二条第三項及び第六条の規定により審議会の権限に属させられた事項を処理すること。"
                            ]
                          }
                        ]
                      }
                    ]
                  }
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の表の上欄に掲げる分科会に属すべき委員、臨時委員及び専門委員は、内閣総理大臣が指名する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "分科会に、分科会長を置き、当該分科会に属する委員の互選により選任する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "分科会長は、当該分科会の事務を掌理する。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": "分科会長に事故があるときは、当該分科会に属する委員のうちから分科会長があらかじめ指名する者が、その職務を代理する。"
                }
              },
              {
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": "審議会は、その定めるところにより、分科会の議決をもって審議会の議決とすることができる。"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（部会）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会又は分科会は、その定めるところにより、部会を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "部会に属すべき委員、臨時委員及び専門委員は、会長（分科会に置かれる部会にあっては、分科会長。次項において同じ。）が指名する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "部会に、部会長を置き、当該部会に属する委員のうちから会長が指名する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "部会長は、当該部会の事務を掌理する。"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": "部会長に事故があるときは、当該部会に属する委員のうちから部会長があらかじめ指名する者が、その職務を代理する。"
                }
              },
              {
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": "審議会（分科会に置かれる部会にあっては、分科会。以下この項において同じ。）は、その定めるところにより、部会の議決をもって審議会の議決とすることができる。"
                }
              }
            ]
          },
          {
            "@Num": "7",
            "ArticleCaption": "（幹事）",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会に、幹事を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "幹事は、関係行政機関の職員及び学識経験のある者のうちから、内閣総理大臣が任命する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "幹事は、審議会の所掌事務について、委員、臨時委員及び専門委員を補佐する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "幹事は、非常勤とする。"
                }
              }
            ]
          },
          {
            "@Num": "8",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会は、委員及び議事に関係のある臨時委員の過半数が出席しなければ、会議を開き、議決することができない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "審議会の議事は、委員及び議事に関係のある臨時委員で会議に出席したものの過半数で決し、可否同数のときは、会長の決するところによる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "前二項の規定は、分科会及び部会の議事について準用する。"
                }
              }
            ]
          },
          {
            "@Num": "9",
            "ArticleCaption": "（資料の提出等の要求）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "審議会は、その所掌事務を遂行するため必要があると認めるときは、関係行政機関の長に対し、資料の提出、意見の開陳、説明その他必要な協力を求めることができる。"
              }
            }
          },
          {
            "@Num": "10",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第十条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "#text": "審議会の庶務は、金融庁企画市場局総務課において財務省大臣官房信用機構課の協力を得て総括し、及び処理する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "#text": "ただし、金利調整分科会に係るものについては、金融庁企画市場局市場課において財務省大臣官房総合政策課の協力を得て処理する。"
                  }
                ]
              }
            }
          },
          {
            "@Num": "11",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第十一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令に定めるもののほか、議事の手続その他審議会の運営に関し必要な事項は、会長が審議会に諮って定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成三〇年七月一三日政令第二〇八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、平成三十年七月十七日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
