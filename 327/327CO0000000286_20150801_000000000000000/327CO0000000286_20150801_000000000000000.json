            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法施行の日（昭和二十七年七月三十一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二九年一一月一七日政令第二九五号",
          "@Extract": "false",
          "@Type": "Amend",
