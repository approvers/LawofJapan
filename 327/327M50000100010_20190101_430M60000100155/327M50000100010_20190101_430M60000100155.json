          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この省令は、公布の日から施行し、昭和二十九年四月一日から適用する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "昭和二十九年三月分の調査表の提出については、なお従前の例による。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和三〇年五月三一日厚生省令第七号",
