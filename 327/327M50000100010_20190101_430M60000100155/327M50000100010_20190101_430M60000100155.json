            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三三年一二月二四日厚生省令第五一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
