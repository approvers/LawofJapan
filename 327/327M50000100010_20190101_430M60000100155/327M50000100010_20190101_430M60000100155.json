            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、公布の日から施行し、昭和二十八年四月一日から適用する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二九年四月二〇日厚生省令第九号",
          "@Extract": "true",
