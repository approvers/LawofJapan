{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "024",
    "@Year": "27",
    "LawNum": "昭和二十七年郵政省令第二十四号",
    "LawBody": {
      "LawTitle": "電波監理審議会議事規則",
      "EnactStatement": "電波法（昭和二十五年法律第百三十一号）第九十九条の十第三項の規定に基き、電波監理審議会議事規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（目的）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "電波監理審議会の会議（以下「会議」という。）の議事に関する手続については、電波法（昭和二十五年法律第百三十一号。以下「法」という。）に規定するものの外、この省令の定めるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（会議の招集等）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "会議は、会長が招集する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "委員は、会長に会議の招集を求めることができる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "会議は、東京都内の総務省の庁舎において開くことを常例とする。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "会長は、会議を招集しようとするときは、委員に対しあらかじめ議題、日時及び場所を通知しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（議長）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "会長は、会議の議長となり、議事を整理する。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（職員の出席）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "会長は、必要があると認めるときは、関係の職員を会議に出席させて、議題に関し説明をさせることができる。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（議事録）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "会議の議事は、議事録に記録しなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "議事録には少くとも左に掲げる事項を記載するものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "開催月日及び場所"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "開会及び閉会の時刻"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "出席した委員、審理官及び関係の職員の氏名"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "議題"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "審議の経過の概要"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "議決事項"
                    }
                  }
                ]
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "議事録は、会議に出席した委員の承認を得て確定する。"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（諮問）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務大臣は、電波監理審議会に諮問する場合は、文書により行い、かつ、必要な資料を添付するものとする。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（裁決案等の記載事項）",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "電波監理審議会が総務大臣に提出する法第九十三条の四（法第百四条の三第二項及び第百四条の四第二項において準用する場合を含む。）の規定による裁決案、法第九十九条の十二第七項（放送法（昭和二十五年法律第百三十二号）第百七十八条第三項において準用する場合を含む。）の規定による答申の文書（以下「答申書」という。）又は法第九十九条の十三第一項若しくは放送法第百七十九条第一項の規定による勧告の文書（以下「勧告書」という。）には、次に掲げる事項を記載するものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "主文"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "事実及び理由"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の裁決案、答申書又は勧告書には、少数の委員の意見その他必要と認める事項を付記することができる。"
                }
              }
            ]
          },
          {
            "@Num": "8",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "電波監理審議会の庶務は、総務省総合通信基盤局総務課において処理する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "総務省総合通信基盤局総務課長が指名する者は、会議の幹事となり、議長の命を受け、会議の事務を行う。"
                }
              }
            ]
          },
          {
            "@Num": "9",
            "ArticleCaption": "（細目）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令に定めるもののほか、会議の議事に関する手続の細目については、会長が電波監理審議会に諮って定める。"
              }
            }
          }
        ]
      },
