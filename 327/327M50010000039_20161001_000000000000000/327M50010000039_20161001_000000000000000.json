                        {
                          "Sentence": "区分"
                        },
                        {
                          "Sentence": "期日"
                        }
                      ]
                    },
                    {
                      "TableColumn": [
                        {
                          "Sentence": [
                            "稲",
                            "春まきの大麦、はだか麦及び小麦",
                            "大豆"
                          ]
                        },
                        {
                          "Sentence": "毎年二月末日"
                        }
                      ]
                    },
                    {
                      "TableColumn": [
                        {
                          "Sentence": "大麦、はだか麦及び小麦（これらのうち春まきのものを除く。）"
                        },
                        {
                          "Sentence": "毎年八月三十一日"
                        }
                      ]
                    }
                  ]
                }
              }
            }
