            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から起算して三月を経過した日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成八年五月九日法律第三五号",
