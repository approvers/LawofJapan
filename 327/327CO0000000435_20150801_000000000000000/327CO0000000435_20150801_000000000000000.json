            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和三十四年一月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三六年六月一日政令第一五七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
