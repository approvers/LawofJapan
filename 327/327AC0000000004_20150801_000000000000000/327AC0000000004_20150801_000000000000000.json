    "LawBody": {
      "LawTitle": "財政法、会計法等の財政関係法律の一部を改正する等の法律　抄",
      "MainProvision": {
        "Article": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "6",
          "ArticleCaption": "（各特別会計の歳入歳出予算の歳出予算の区分の特例）",
          "ArticleTitle": "第六条",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "政府の各特別会計の歳入歳出予算の歳出予算の区分については、各特別会計法の規定にかかわらず、歳出の目的に従つて項に区分するものとする。"
              }
            }
          }
        }
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": "１",
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この法律中継続費、歳出予算及び支出予算の区分並びに繰越に係る部分は、公布の日から、その他の部分は、昭和二十七年四月一日から施行する。"
            }
          }
        }
      }
    }
  }
