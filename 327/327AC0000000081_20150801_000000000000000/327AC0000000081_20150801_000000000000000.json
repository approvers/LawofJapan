{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "81",
    "@Year": "27",
    "LawNum": "昭和二十七年法律第八十一号",
    "LawBody": {
      "LawTitle": "ポツダム宣言の受諾に伴い発する命令に関する件の廃止に関する法律",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "ポツダム宣言の受諾に伴い発する命令に関する件（昭和二十年勅令第五百四十二号。以下「勅令第五百四十二号」という。）は、廃止する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "勅令第五百四十二号に基く命令は、別に法律で廃止又は存続に関する措置がなされない場合においては、この法律施行の日から起算して百八十日間に限り、法律としての効力を有するものとする。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphNum": "３",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、勅令第五百四十二号に基く命令により法律若しくは命令を廃止し、又はこれらの一部を改正した効果に影響を及ぼすものではない。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、日本国との平和条約の最初の効力発生の日から施行する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律施行のための経過的規定その他この法律の施行に関し必要な事項は、政令で定める。"
              }
            }
          }
        ]
      }
    }
  }
}
