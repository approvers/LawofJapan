{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "252",
    "@Year": "27",
    "LawNum": "昭和二十七年法律第二百五十二号",
    "LawBody": {
      "LawTitle": "内閣法制局設置法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（設置）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "内閣に内閣法制局を置く。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（法制局長官）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "内閣法制局の長は、内閣法制局長官とし、内閣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "長官は、内閣法制局の事務を統括し、部内の職員の任免、進退を行い、且つ、その服務につき、これを統督する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（所掌事務）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "内閣法制局は、左に掲げる事務をつかさどる。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "閣議に附される法律案、政令案及び条約案を審査し、これに意見を附し、及び所要の修正を加えて、内閣に上申すること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法律案及び政令案を立案し、内閣に上申すること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法律問題に関し内閣並びに内閣総理大臣及び各省大臣に対し意見を述べること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "内外及び国際法制並びにその運用に関する調査研究を行うこと。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "その他法制一般に関すること。"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（内部部局）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "内閣法制局の事務を分掌させるため、内閣法制局に左の四部及び長官総務室を置く。"
                  }
                },
                "List": [
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第一部"
                      }
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第二部"
                      }
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第三部"
                      }
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第四部"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部及び長官総務室の所掌事務及び内部組織は、政令で定める。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（職員）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "内閣法制局に内閣法制次長一人及び内閣法制局参事官、内閣法制局事務官その他所要の職員を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "次長は、長官を助け、局務を整理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "参事官は、命を受け、第三条各号に掲げる事務をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "事務官は、命を受け、事務を整理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部の長は部長とし、参事官をもつて充てる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "削除"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（主任の大臣）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "内閣法制局に係る事項については、内閣法（昭和二十二年法律第五号）にいう主任の大臣は、内閣総理大臣とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleCaption": "（実施細則）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律の施行に関し必要な細則は、政令で定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、昭和二十七年八月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三七年四月一六日法律第七七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この法律は、公布の日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、第六条及び附則第五項から第十一項までの規定は、昭和三十七年七月一日から施行する。"
                  }
                ]
              }
            },
            {
              "@Hide": "false",
              "@Num": "5",
              "ParagraphNum": "５",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "第六条の規定の施行前における法制局及びその職員は、改正後の内閣法制局設置法の規定に基づく相当の機関及びその職員となり、同一性をもつて存続するものとする。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和四四年五月一六日法律第三三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行し、昭和四十四年四月一日から適用する。"
              }
            }
          }
        }
      ]
    }
  }
}
