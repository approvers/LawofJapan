    "LawBody": {
      "LawTitle": "昭和七年法律第十六号（国債ノ価額計算ニ関スル法律）",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "○１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "国債ノ価額ヲ会計帳簿又ハ財産目録ニ記載又ハ記録スルニハ会社法（平成十七年法律第八十六号）第四百三十二条第一項其ノ他ノ法令ノ規定ニ拘ラズ財務大臣ノ告示スル標準発行価格ニ依ルコトヲ得但シ其ノ取得ノ際ニ於ケル時価ヲ超ユルコトヲ得ズ"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "○２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "前項ノ規定ハ外国ニ於テ発行シタル国債ニハ之ヲ適用セズ"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "○１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本法ハ公布ノ日ヨリ之ヲ施行ス"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "○２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "明治三十八年法律第二十号ハ之ヲ廃止ス"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "3",
              "ParagraphNum": "○３",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本法施行ノ際所有スル国債ニシテ最終ノ財産目録調製前ニ取得シタルモノハ第一項但書ノ規定ノ適用ニ付テハ之ヲ最終ノ財産目録調製ノ時ニ於テ取得シタルモノト看做ス"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和一四年四月五日法律第六八号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "○１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本法施行ノ期日ハ勅令ヲ以テ之ヲ定ム"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四九年四月二日法律第二三号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から起算して六月をこえない範囲内において政令で定める日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一一年一二月二二日法律第一六〇号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
