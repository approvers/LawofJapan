            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、港湾労働法の一部を改正する法律の施行の日（平成十二年十月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一三年八月一〇日政令第二七二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十三年九月十日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一三年一一月七日政令第三四六号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
