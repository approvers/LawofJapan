            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第六条第一項の規定による届出は、別記様式第一による届出書を提出して行うものとする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の届出書には、次に掲げる図書を添付しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "土地の区画形質の変更にあつては、次に掲げる図面"
                    },
                    "Subitem1": [
                      {
                        "@Num": "1",
                        "Subitem1Title": "イ",
                        "Subitem1Sentence": {
                          "Sentence": "当該行為を行う土地の区域並びに当該区域内及び当該区域の周辺の公共施設を表示する図面で縮尺千分の一以上のもの"
                        }
                      },
                      {
                        "@Num": "2",
                        "Subitem1Title": "ロ",
                        "Subitem1Sentence": {
                          "Sentence": "設計図で縮尺百分の一以上のもの"
                        }
                      }
                    ]
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "建築物その他の工作物（以下「建築物等」という。）の新築、改築若しくは増築又は用途の変更にあつては、次に掲げる図面"
                    },
                    "Subitem1": [
                      {
                        "@Num": "1",
                        "Subitem1Title": "イ",
                        "Subitem1Sentence": {
                          "Sentence": "敷地内における建築物等の位置を表示する図面で縮尺百分の一以上のもの"
                        }
                      },
                      {
                        "@Num": "2",
                        "Subitem1Title": "ロ",
                        "Subitem1Sentence": {
                          "Sentence": "二面以上の建築物等の立面図及び各階平面図（建築物である場合に限る。）で縮尺五十分の一以上のもの"
                        }
                      }
                    ]
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "建築物等の形態又は意匠の変更にあつては、前号イに掲げる図面及び二面以上の立面図で縮尺五十分の一以上のもの"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "木竹の伐採にあつては、次に掲げる図面"
                    },
                    "Subitem1": [
                      {
                        "@Num": "1",
                        "Subitem1Title": "イ",
                        "Subitem1Sentence": {
                          "Sentence": "当該行為を行う土地の区域を表示する図面で縮尺千分の一以上のもの"
                        }
                      },
                      {
                        "@Num": "2",
                        "Subitem1Title": "ロ",
                        "Subitem1Sentence": {
                          "Sentence": "当該行為の施行方法を明らかにする図面で縮尺百分の一以上のもの"
                        }
                      }
                    ]
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "その他参考となるべき事項を記載した図書"
                    }
                  }
                ]
              }
            ]
