{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "105",
    "@Year": "63",
    "LawNum": "昭和六十三年法律第百五号",
    "LawBody": {
      "LawTitle": "国会に置かれる機関の休日に関する法律",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（国会に置かれる機関の休日）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "次の各号に掲げる日は、国会に置かれる機関の休日とし、当該機関の執務は、原則として行わないものとする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "日曜日及び土曜日"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国民の祝日に関する法律（昭和二十三年法律第百七十八号）に規定する休日"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "十二月二十九日から翌年の一月三日までの日（前号に掲げる日を除く。）"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項の「国会に置かれる機関」とは、裁判官弾劾裁判所、裁判官訴追委員会、国立国会図書館並びに各議院に置かれる事務局及び法制局その他法令に基づき各議院に置かれる機関で両議院の議長が協議して定めるものをいう。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "第一項の規定は、国会に置かれる機関の休日に当該各機関がその権限を行使し、又はその所掌事務を遂行することを妨げるものではない。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（期限の特例）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "国会に置かれる機関に対する申立てその他の行為の期限で法令で規定する期間をもつて定めるものが国会に置かれる機関の休日に当たるときは、国会に置かれる機関の休日の翌日をもつてその期限とみなす。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、法令に別段の定めがある場合は、この限りでない。"
                  }
                ]
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、行政機関の休日に関する法律（昭和六十三年法律第九十一号）の施行の日から施行する。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "東京電力福島原子力発電所事故調査委員会法（平成二十三年法律第百十二号）がその効力を有する間における第一条第二項の規定の適用については、同項中「定めるもの」とあるのは、「定めるもの並びに東京電力福島原子力発電所事故調査委員会」とする。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成四年四月二日法律第二七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、一般職の職員の給与等に関する法律及び行政機関の休日に関する法律の一部を改正する法律（平成四年法律第二十八号）の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二三年一〇月七日法律第一一二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、国会法の一部を改正する法律（平成二十三年法律第百十一号）の施行の日から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
