{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "007",
    "@Year": "63",
    "LawNum": "昭和六十三年大蔵省令第七号",
    "LawBody": {
      "LawTitle": "通貨の単位及び貨幣の発行等に関する法律施行規則",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "通貨の単位及び貨幣の発行等に関する法律（昭和六十二年法律第四十二号）第八条、附則第六条及び附則第七条の規定に基づき、並びに同法を実施するため、通貨の単位及び貨幣の発行等に関する法律施行規則を次のように定める"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（貨幣の引換事務の取扱機関等）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "通貨の単位及び貨幣の発行等に関する法律（以下「法」という。）第八条の規定による貨幣の引換えに関する事務は、日本銀行がその本店及び支店において行い、その事務に要する経費は日本銀行が負担する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "前条の貨幣の引換えは、模様の認識ができ、かつ、量目が通貨の単位及び貨幣の発行等に関する法律施行令（昭和六十三年政令第五十号）（以下この条において「令」という。）第一条又は第二条に定める量目の二分の一を超えるものについて行うものとする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、金を素材とする貨幣の引換えは、模様の認識ができ、かつ、量目が令第一条又は第二条に定める量目の百分の九十八以上のものについて行うものとする。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前条の貨幣の引換えは、災害その他やむを得ない事由により量目が減少した貨幣にあつては、模様の認識ができるものについて行うものとする。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "削除"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（旧金貨幣の引換事務の取扱店）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法附則第三条に規定する旧金貨幣（以下「旧金貨幣」という。）の引換えは、日本銀行本店及び支店（以下「引換事務取扱店」という。）において行うものとする。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（旧金貨幣の引換手続）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法附則第四条の規定により旧金貨幣の引換えを請求しようとする者は、当該旧金貨幣に引換えのための請求書を添えて引換事務取扱店に提出しなければならない。"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（引換事務取扱店の日計表）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "引換事務取扱店は、旧金貨幣の引換えに関する事項を記録するため、日計表を作成しなければならない。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（引換事務取扱店の備付帳簿）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "引換事務取扱店は、旧金貨幣の引換えに伴う旧金貨幣の受入れを記録するため、帳簿を備え付けなければならない。"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleCaption": "（引換事務の報告）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "日本銀行本店は、法附則第四条から第六条までに規定する旧金貨幣の引換えに関する報告書を、翌月末日までに財務大臣に提出しなければならない。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、昭和六十三年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年八月二一日大蔵省令第六九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十三年一月六日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一五年三月三一日財務省令第四七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、平成十五年四月一日から施行する。"
            }
          }
        }
      ]
    }
  }
}
