    "LawNum": "大正十二年内務省令第四十五号",
    "LawBody": {
      "LawTitle": "専用軌道規則",
      "EnactStatement": "軌道法第一条第二項ノ規定ニ依ル一般交通ノ用ニ供セサル軌道ニ関スル件左ノ通定ム",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "一般交通ノ用ニ供セサル軌道ヲ道路ニ敷設セムトスル者ハ都道府県知事ノ許可ヲ受クヘシ"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "明治四十三年内務省令第二十七号第一条乃至第五条ノ規定ハ前条ノ許可申請ニ之ヲ準用ス"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "許可申請書ニハ運転及信号ニ関スル方法ヲ記載スヘシ"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "都道府県知事第一条ノ許可ヲ為サムトスルトキハ軌道ノ敷設ニ関シ関係道路管理者ノ意見ヲ徴スヘシ"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "大正八年閣令第十九号専用鉄道規程第三条、第七条乃至第十条ノ規定ハ本令ニ規定スル軌道ニ之ヲ準用ス但シ陸運局長トアルハ都道府県知事トス"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "軌道法第十二条、第十八条、第十九条及第二十四条ノ規定ハ本令ニ規定スル軌道ニ之ヲ準用ス但シ主務大臣トアルハ都道府県知事トス"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "許可ヲ受ケタル者カ法令若ハ法令ニ基キテ為ス命令又ハ許可若ハ認可ニ附シタル条件ニ違反シ其ノ他公益ヲ害スル行為ヲ為シタルトキハ都道府県知事ハ許可ノ全部又ハ一部ヲ取消スコトヲ得"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "許可ヲ得スシテ本令ニ規定スル軌道ヲ敷設シタル者ハ百円以下ノ罰金ニ処ス"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "○１",
              "ParagraphSentence": {
                "Sentence": "本令ハ大正十三年一月一日ヨリ之ヲ施行ス"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "○２",
              "ParagraphSentence": {
                "Sentence": "本令ニ規定スル軌道ニシテ本令施行ノ際現ニ存スルモノハ本令ニ依リ許可ヲ受ケタルモノト看做ス"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和一五年四月一日内務省令第一〇号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "本令ハ公布ノ日ヨリ之ヲ施行ス"
            }
          }
        },
        {
          "@AmendLawNum": "昭和四六年一月一三日建設省令第一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        }
      ]
    }
  }
