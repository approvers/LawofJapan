{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "067",
    "@Year": "28",
    "LawNum": "平成二十八年法律第六十七号",
    "LawBody": {
      "LawTitle": "平成二十八年熊本地震災害関連義援金に係る差押禁止等に関する法律",
      "MainProvision": {
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "平成二十八年熊本地震災害関連義援金の交付を受けることとなった者の当該交付を受ける権利は、譲り渡し、担保に供し、又は差し押さえることができない。"
            }
          },
          {
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": "平成二十八年熊本地震災害関連義援金として交付を受けた金銭は、差し押さえることができない。"
            }
          },
          {
            "@Num": "3",
            "ParagraphNum": "３",
            "ParagraphSentence": {
              "Sentence": {
                "Ruby": {
                  "Rt": "しや",
                  "#text": "慰藉"
                },
                "#text": "この法律において「平成二十八年熊本地震災害関連義援金」とは、平成二十八年熊本地震による災害の被災者又はその遺族（以下この項において「被災者等」という。）の生活を支援し、被災者等をする等のため自発的に拠出された金銭を原資として、都道府県又は市町村（特別区を含む。）が一定の配分の基準に従い被災者等に交付する金銭をいう。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この法律は、公布の日から施行する。"
            }
          },
          {
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "#text": "この法律は、この法律の施行前に交付を受け、又は交付を受けることとなった平成二十八年熊本地震災害関連義援金についても適用する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "#text": "ただし、この法律の施行前に生じた効力を妨げない。"
                }
              ]
            }
          }
        ]
      }
    }
  }
}
