{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "48",
    "@Year": "28",
    "LawNum": "平成二十八年厚生労働省令第四十八号",
    "LawBody": {
      "LawTitle": "自殺対策基本法第十四条に規定する交付金に関する省令",
      "EnactStatement": "自殺対策基本法（平成十八年法律第八十五号）第十四条の規定に基づき、自殺対策基本法第十四条に規定する交付金に関する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（交付金の交付）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "自殺対策基本法（平成十八年法律第八十五号）第十四条に規定する交付金（以下「交付金」という。）は、地方公共団体が実施する地域の状況に応じた自殺対策のために必要な事業、その総合的かつ効果的な取組等のうち、厚生労働大臣が適当と認めるものの実施に要する経費に充てるために交付するものとする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（委任規定）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "前条に定めるもののほか、交付金の額を算出するために必要な事項、交付金の交付の手続、交付金の経理その他の必要な事項については、厚生労働大臣の定めるところによる。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この省令は、平成二十八年四月一日から施行する。"
          }
        }
      }
    }
  }
}
