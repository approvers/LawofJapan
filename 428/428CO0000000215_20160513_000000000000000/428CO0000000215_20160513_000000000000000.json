{
  "Law": {
    "@DataInfo": "190507e04k03m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "215",
    "@Year": "28",
    "LawNum": "平成二十八年政令第二百十五号",
    "LawBody": {
      "LawTitle": "成年後見制度利用促進会議令",
      "EnactStatement": "内閣は、成年後見制度の利用の促進に関する法律（平成二十八年法律第二十九号）第十四条第四項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長は、会務を総理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長に事故があるときは、会長があらかじめ指名する委員が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "成年後見制度利用促進会議（次条において「会議」という。）の庶務は、内閣府大臣官房企画調整課において処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（会議の運営）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前二条に定めるもののほか、議事の手続その他会議の運営に関し必要な事項は、会長が会議に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphCaption": "（施行期日）",
          "ParagraphNum": "１",
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、成年後見制度の利用の促進に関する法律の施行の日（平成二十八年五月十三日）から施行する。"
            }
          }
        }
      }
    }
  }
}
