                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "整備法附則第六条第一項の政令で指定する日は、平成四十四年三月三十一日とする。"
                    }
