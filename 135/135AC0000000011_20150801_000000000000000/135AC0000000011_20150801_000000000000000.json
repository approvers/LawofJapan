    "LawBody": {
      "LawTitle": "明治三十五年法律第十一号（警察署内ノ留置場ニ拘禁又ハ留置セラルル者ノ費用ニ関スル法律）",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "監獄則第一条ニ依リ警察署内ノ留置場ニ拘禁又ハ留置セラルル者ニ関スル費用ハ総テ警察費ヲ以テ之ヲ支弁ス但シ其ノ費額ニシテ北海道地方費及府県ノ負担ニ属スル部分ハ法務省令ノ定ムル所ニ依リ監獄費ヨリ之ヲ償還スヘシ"
            }
          }
        }
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本法ハ明治三十五年四月一日ヨリ之ヲ施行ス"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "従来監獄所属ノ物品ニシテ警察署内ノ留置場ニ設備セルモノハ本法施行ノ際之ヲ北海道地方費及府県ノ所属トス但シ警察費ノ国庫支弁ニ属スル地方ハ此ノ限ニ在ラス"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一一年一二月二二日法律第一六〇号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
