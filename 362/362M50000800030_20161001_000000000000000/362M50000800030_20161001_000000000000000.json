{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "30",
    "@Year": "62",
    "LawNum": "昭和六十二年運輸省令第三十号",
    "LawBody": {
      "LawTitle": "軌道運賃料金割引等規則",
      "EnactStatement": "軌道法（大正十年法律第七十六号）を実施するため、軌道運賃割引規程（昭和十年鉄道省令第二号）の全部を改正する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（趣旨）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "軌道法第十一条第一項の認可に係る運賃又は料金の割引又は割増しについては、この省令の定めるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（運賃及び料金の割引又は割増し）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Num": "1",
                    "#text": "軌道経営者は、軌道法第十一条第一項の認可を受けた運賃又は料金について、次に掲げる割引又は割増しを行うことができる。"
                  },
                  {
                    "@Num": "2",
                    "#text": "この場合には、当該軌道経営者は、あらかじめ、その旨を地方運輸局長に届け出なければならない。"
                  }
                ]
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "次に掲げる証票その他の物（以下「証票等」という。）に係る割引"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "回数乗車券"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "運賃及び料金の支払のために使用することができるものとして電磁的方法により記録されている金額（金額を度その他の単位により換算して表示していると認められる場合の当該単位数を含む。以下同じ。）に応ずる対価を得て発行する証票等（当該方法により証票等に記録される金額に応ずる対価を得て当該金額の記録の加算が行われるものを含む。）であつて未使用残高が当該方法により記録されるもの"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "運賃及び料金の支払のために使用することができるものとして記載されている金額に応ずる対価を得て発行する証票等であつて未使用残高が記載されるもの"
                      }
                    }
                  ]
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "他の運送事業者の運送との間の乗継ぎを行う旅客の運送に係る割引"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "学校教育法（昭和二十二年法律第二十六号）第一条に規定する学校その他の教育施設に在籍する幼児、児童、生徒又は学生の運送に係る割引"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "障害者の運送に係る割引その他の社会福祉の増進を目的として行う割引"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "危険品割増し、貴重品割増し、特殊な貨車又はコンテナを使用して行う荷物の運送に係る割増しその他の特殊な取扱い又は設備を必要とする運送に係る割増し"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "軽量品割引、荷主から貨車又はコンテナの提供を受けて行う荷物の運送に係る割引その他の通常の取扱い又は設備を必要としない運送に係る割引"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "第一号から第四号まで又は前号に掲げるもののほか、当該軌道事業に係る総収入を減少させないと見込まれる範囲内で、適用する期間又は区間その他の条件を定めて行う割引"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（届出書）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "前条第一号から第四号までの割引の届出をしようとする者は、次に掲げる事項を記載した運賃料金割引届出書を提出しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "氏名又は名称及び住所"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "割引を行おうとする運賃又は料金の種類"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "割引の方法"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "割引率"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前条第五号の割増し又は前条第六号の割引の届出をしようとする者は、次に掲げる事項を記載した運賃割増（割引）届出書を提出しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "氏名又は名称及び住所"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "割増し又は割引を行おうとする運賃の種類"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "割増し又は割引を適用する範囲"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "割増率又は割引率"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "割増し又は割引を必要とする理由"
                    }
                  }
                ]
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "前条第七号の割引の届出をしようとする者は、次に掲げる事項を記載した運賃料金割引届出書を提出しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "氏名又は名称及び住所"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "割引を行おうとする運賃又は料金の種類"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "割引率"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "割引を適用する期間又は区間その他の条件"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "割引を必要とする理由"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（届出書の提出）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令の規定により地方運輸局長に提出すべき届出書は、当該事案の関する土地を管轄する地方運輸局長（当該事案が二以上の地方運輸局長の管轄区域にわたるときは、当該事案の主として関する土地を管轄する地方運輸局長）に提出しなければならない。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、昭和六十二年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成七年三月二三日運輸省令第一四号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、許可、認可等の整理及び合理化に関する法律第二十七条から第三十条まで、第三十二条、第三十三条及び第三十五条の規定の施行の日（平成七年四月一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成八年一二月一八日運輸省令第六五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成九年一月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年三月三〇日国土交通省令第二三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、公布の日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（経過措置）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この省令の施行前にこの省令による改正前の軌道運賃料金割引等規則第三条第一項の規定により提出された回数乗車券（証票等）割引届出書は、この省令による改正後の軌道運賃料金割引等規則第三条第一項の規定により提出された運賃料金割引届出書とみなす。"
              }
            }
          ]
        }
      ]
    }
  }
}
