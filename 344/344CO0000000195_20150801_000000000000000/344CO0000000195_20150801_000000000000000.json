                          "@BorderLeft": "solid",
                          "@BorderRight": "solid",
                          "@BorderTop": "solid",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "資本に年一パーセントを乗じて算出した金額までの部分"
                          }
                        },
                        {
                          "@BorderBottom": "solid",
                          "@BorderLeft": "solid",
                          "@BorderRight": "solid",
