    "@Num": "33",
    "@Year": "44",
    "LawNum": "昭和四十四年法律第三十三号",
    "LawBody": {
      "LawTitle": "行政機関の職員の定員に関する法律",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（定員の総数の最高限度）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣の機関（内閣官房及び内閣法制局をいう。以下同じ。）、内閣府及び各省の所掌事務を遂行するために恒常的に置く必要がある職に充てるべき常勤の職員の定員の総数の最高限度は、三十三万千九百八十四人とする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "次に掲げる職員は、前項の職員に含まないものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "国家公務員法（昭和二十二年法律第百二十号）第二条第三項第一号、第二号及び第四号から第七号の三までに掲げる職員並びに同項第九号に掲げる職員のうち常勤の職員"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "宮内庁長官、侍従長、東宮大夫、式部官長及び侍従次長"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "自衛官"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "国際平和協力隊の隊員"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（内閣府、各省等の定員）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "内閣の機関、内閣府及び各省の前条第一項の定員は、それぞれ政令で定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この法律は、公布の日から施行し、昭和四十四年四月一日から適用する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和五二年五月二日法律第二九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日等）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この法律は、公布の日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この法律による改正後の国立学校設置法附則第三項及び行政機関の職員の定員に関する法律（昭和四十四年法律第三十三号）附則第二項の規定並びに附則第五項の規定は、昭和五十二年四月一日から適用する。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "昭和五九年八月一〇日法律第六七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この法律は、公布の日から起算して一年を超えない範囲内において政令で定める日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和六一年五月二七日法律第七一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この法律は、昭和六十一年七月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成元年一月一一日法律第一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この法律は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成四年六月一九日法律第七九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この法律は、公布の日から起算して三月を超えない範囲内において政令で定める日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一一年七月一六日法律第八七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この法律は、平成十二年四月一日から施行する。"
                }
              }
            },
            {
              "@Num": "250",
              "ArticleCaption": "（検討）",
              "ArticleTitle": "第二百五十条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "新地方自治法第二条第九項第一号に規定する第一号法定受託事務については、できる限り新たに設けることのないようにするとともに、新地方自治法別表第一に掲げるもの及び新地方自治法に基づく政令に示すものについては、地方分権を推進する観点から検討を加え、適宜、適切な見直しを行うものとする。"
                }
              }
            },
            {
              "@Num": "251",
              "ArticleTitle": "第二百五十一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "政府は、地方公共団体が事務及び事業を自主的かつ自立的に執行できるよう、国と地方公共団体との役割分担に応じた地方税財源の充実確保の方途について、経済情勢の推移等を勘案しつつ検討し、その結果に基づいて必要な措置を講ずるものとする。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一一年一二月二二日法律第一六〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "#text": "この法律（第二条及び第三条を除く。）は、平成十三年一月六日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                  }
                ]
              },
              "Item": {
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Column": [
                    {
                      "@Num": "1",
                      "Sentence": "第九百九十五条（核原料物質、核燃料物質及び原子炉の規制に関する法律の一部を改正する法律附則の改正規定に係る部分に限る。）、第千三百五条、第千三百六条、第千三百二十四条第二項、第千三百二十六条第二項及び第千三百四十四条の規定"
                    },
                    {
                      "@Num": "2",
                      "Sentence": "公布の日"
                    }
                  ]
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年五月一九日法律第七〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この法律は、平成十三年一月六日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一四年七月三一日法律第九八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この法律は、公社法の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年六月九日法律第八二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この法律は、公布の日から施行し、改正後の行政機関の職員の定員に関する法律の規定は、平成十六年四月一日から適用する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二四年六月二七日法律第四二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この法律は、平成二十五年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二六年四月一八日法律第二二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この法律は、公布の日から起算して六月を超えない範囲内において、政令で定める日から施行する。"
              }
            }
          }
        }
      ]
    }
