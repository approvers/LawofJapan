            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法の施行の日（昭和四十五年三月一日）から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、次項から附則第六項までの規定は、昭和四十七年四月一日から施行する。"
                }
              ]
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphCaption": "（開拓者資金融通特別会計法施行令等の廃止）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "次に掲げる命令は、廃止する。"
              }
            },
