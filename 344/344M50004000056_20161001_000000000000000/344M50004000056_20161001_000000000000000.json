{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "56",
    "@Year": "44",
    "LawNum": "昭和四十四年建設省令第五十六号",
    "LawBody": {
      "LawTitle": "標準地の鑑定評価の基準に関する省令",
      "EnactStatement": "地価公示法（昭和四十四年法律第四十九号）第四条の規定に基づき、標準地の鑑定評価の基準に関する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（目的）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、地価公示法第四条の規定に基づき、同法第二条第一項の規定により不動産鑑定士が行う標準地の鑑定評価の基準を定めることを目的とする。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（用語の定義）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令において、次の各号に掲げる用語の意義は、それぞれ当該各号に定めるところによる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "価格形成要因"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "土地の客観的価値に作用する諸要因をいう。"
                      }
                    ]
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "地域要因"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "土地の用途が同質と認められるまとまりのある地域内の土地の価格の水準に作用する価格形成要因をいう。"
                      }
                    ]
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "個別的要因"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "土地の価格について、当該土地を含む地域で土地の用途が同質と認められるまとまりのあるものにおける土地の価格の水準に比し、個別的な差異を生じさせる価格形成要因をいう。"
                      }
                    ]
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "近隣地域"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "標準地を含む地域で、当該標準地の用途と土地の用途が同質と認められるまとまりのあるものをいう。"
                      }
                    ]
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "類似地域"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "土地の用途が同質と認められるまとまりのある地域で、当該地域内の土地の用途が近隣地域内の土地の用途と同質又は類似のものをいう。"
                      }
                    ]
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "同一需給圏"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "標準地と一般的に代替関係が成立して、その価格の形成について相互に影響を及ぼす関係にある他の土地の存する圏域をいう。"
                      }
                    ]
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@Num": "1",
                        "Sentence": "事例地"
                      },
                      {
                        "@Num": "2",
                        "Sentence": "取引、賃貸借等の事例で標準地の鑑定評価に用いられるものに係る土地をいう。"
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（鑑定評価の原則）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "Ruby": {
                    "Rt": "は",
                    "#text": "把"
                  },
                  "#text": "標準地の鑑定評価は、当該標準地及び事例地に係る価格形成要因を明確に握するとともに、土地の価格形成の過程において認められる経済上の諸原則の認識のもとに価格形成要因の作用する程度を適切に判定することにより行なわなければならない。"
                }
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "標準地の鑑定評価は、事例地の取引価格から算定される推定の価格、事例地の地代等から算定される推定の価格及び同等の効用を有する土地の造成に要する推定の費用の額のうち当該標準地の鑑定評価に適切と認められるものを勘案して行なわなければならない。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（資料の収集等）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "標準地の鑑定評価にあたつては、これに必要と認められる次の各号に掲げる資料を適切かつ十分に収集しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "価格形成要因に関する資料"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "取引、賃貸借等の事例に係る土地の取引価格、地代等に関する資料"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "標準地の鑑定評価に当たつては、前項の規定により収集した資料を適正に選択し、用いなければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "前項の場合において、第一項第二号に掲げる資料の選択に当たつては、近隣地域及び同一需給圏内の類似地域（以下この項において「近隣地域等」という。）に存する土地に係るものを選択しなければならない。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、近隣地域等に存する土地に係る資料の大部分が特殊な事情による影響を著しく受けていることその他の特別な事情により、当該資料のみによつては標準地の鑑定評価を適切に行うことができないと認められるときには、当該資料に加えて、同一需給圏内の近隣地域の周辺の地域（以下「周辺地域」という。）に存する土地に係るものを選択することができる。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（地域分析及び個別分析）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "Ruby": {
                      "Rt": "は",
                      "#text": "把"
                    },
                    "#text": "標準地の鑑定評価に当たつては、近隣地域及び事例地に係る類似地域（前条第三項ただし書の規定により周辺地域に存する土地に係る資料を選択する場合にあつては、近隣地域、事例地に係る類似地域及び周辺地域に存する事例地を含む地域で、土地の用途が同質と認められるまとまりのあるもの（以下「周辺地域に係る用途的地域」という。））の地域要因に関する資料を分析し、これに基づいて、それぞれの地域要因を明確に握することにより、それぞれの地域要因がそれぞれの地域における土地の価格の水準に作用する程度を判定しなければならない。"
                  }
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "Ruby": {
                      "Rt": "は",
                      "#text": "把"
                    },
                    "#text": "標準地の鑑定評価にあたつては、当該標準地及び事例地の個別的要因に関する資料を分析し、これに基づいて、それぞれの個別的要因を明確に握することにより、それぞれの個別的要因がそれぞれの土地の価格の形成に作用する程度を判定しなければならない。"
                  }
                }
              }
            ]
          },
          {
            "@Num": "7",
            "ArticleCaption": "（取引事例比較法）",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "第四条に規定する事例地の取引価格から算定される推定の価格は、前条の手続の結果に基づき、事例地が近隣地域にあるときは当該近隣地域の地域要因を考慮した上、事例地が同一需給圏内の類似地域にあるときは当該類似地域及び近隣地域のそれぞれの地域要因を考慮し、かつ、相互に比較を行つた上、事例地が周辺地域にあるときは周辺地域に係る用途的地域及び近隣地域のそれぞれの地域要因を考慮し、かつ、相互に比較を行つた上、標準地及び各事例地のそれぞれの個別的要因についての比較を行い、その比較の結果に従い、各事例地の取引価格から求められた各価格を相互に比較考量することにより求めなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の場合において、事例地の取引価格が特殊な事情による影響を受けていると認められるときは、適正な補正を行なわなければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "第一項の場合において、事例地の取引価格の時点が価格判定の基準日と異なり、その間に土地の価格の変動があると認められるときは、当該事例地の取引価格を価格判定の基準日における価格に修正しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "8",
            "ArticleCaption": "（収益還元法）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "第四条に規定する事例地の地代等から算定される推定の価格は、標準地に係る総収益及び総費用から求められた標準地の純収益を還元利回りで元本に還元することにより求めなければならない。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、当該標準地に係る総収益及び総費用を適切に求めることが困難である場合その他やむを得ない理由がある場合にあつては、第六条の手続の結果に基づき、事例地が近隣地域にあるときは当該近隣地域の地域要因を考慮した上、事例地が同一需給圏内の類似地域にあるときは当該類似地域及び近隣地域のそれぞれの地域要因を考慮し、かつ、相互に比較を行つた上、事例地が周辺地域にあるときは周辺地域に係る用途的地域及び近隣地域のそれぞれの地域要因を考慮し、かつ、相互に比較を行つた上、標準地及び各事例地のそれぞれの個別的要因の比較を行い、その比較の結果に従い、各事例地の純収益から求められた各純収益を相互に比較考量して得た標準地の純収益を還元利回りで元本に還元することにより求めることができる。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項に規定する純収益は、原則として一年を単位とし、総収益から総費用を控除して得た額とする。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "前条第二項及び第三項の規定は、第一項に規定する事例地の純収益について準用する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "第一項の場合において、純収益を元本に還元する場合における還元利回りは、最も一般的と思われる投資の利回りを標準とし、その投資の対象及び標準地の投資対象としての流動性、管理の難易、資産としての安全度等を相互に比較考量して決定しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "9",
            "ArticleCaption": "（原価法）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Num": "1",
                    "#text": "第四条に規定する同等の効用を有する土地の造成に要する推定の費用の額は、標準地を価格判定の基準日において造成すると仮定したならばその造成に要すると認められる適正な費用（以下「造成原価」という。）の額とする。"
                  },
                  {
                    "@Num": "2",
                    "#text": "この場合において、当該標準地が当該仮定に係る造成が行なわれた土地と比較して減価していると認められるときは、当該造成原価の額から当該減価の額に相当する額を控除した額としなければならない。"
                  }
                ]
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二年一二月二八日総理府令第六五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一八年一月二七日国土交通省令第三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令は、平成十八年二月一日から施行する。"
                }
              }
            },
            {
              "@Num": "5",
              "ArticleCaption": "（地価公示法施行規則等の一部改正に伴う経過措置）",
              "ArticleTitle": "第五条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令の施行の際現に不動産鑑定士補である者及び改正法附則第六条第一項の規定によりなおその効力を有することとされる同法第四条の規定による改正前の不動産の鑑定評価に関する法律第十五条第一項の規定によりこの省令の施行の日以後に不動産鑑定士補となった者については、第二条の規定による改正前の地価公示法施行規則第四条第一項第四号及び第八条並びに第三条の規定による改正前の標準地の鑑定評価の基準に関する省令第一条の規定は、なおその効力を有する。"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
