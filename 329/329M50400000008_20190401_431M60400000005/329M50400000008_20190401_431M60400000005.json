              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "機動通信課においては、次の事務をつかさどる。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
