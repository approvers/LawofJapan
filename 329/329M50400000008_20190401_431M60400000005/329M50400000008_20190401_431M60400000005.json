            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "府県情報通信部及び方面情報通信部に、次の四課を置く。"
                }
              },
              "List": [
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "通信庶務課"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "機動通信課"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "通信施設課"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "情報技術解析課"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "通信庶務課においては、次の事務をつかさどる。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "通信関係業務の企画及び調整に関すること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "通信用機材の整備に関すること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前二号に掲げるもののほか、他課の所掌に属しない事務に関すること。"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
