{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "258",
    "@Year": "29",
    "LawNum": "昭和二十九年政令第二百五十八号",
    "LawBody": {
      "LawTitle": "日本中央競馬会法施行令",
      "EnactStatement": "内閣は、日本中央競馬会法（昭和二十九年法律第二百五号）第四条、第二十七条及び第二十八条第一項の規定に基き、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（出資財産とならない動産の範囲）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "日本中央競馬会法（以下「法」という。）第四条第一項の政令で定める動産は、左に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "日本中央競馬会（以下「競馬会」という。）の成立の際現にもつぱら地方競馬（競馬法（昭和二十三年法律第百五十八号）第一条第二項の地方競馬をいう。）の指導監督の事務の用に供している動産"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "前号に掲げるものの外、中央競馬（競馬法第一条第二項の中央競馬をいう。）の指導監督の事務の用に供する必要があると認めて農林水産大臣が指定した動産"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（出資財産の評価の方法等）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第四条第一項の財産は、農林水産大臣が、大蔵大臣と協議して、競馬会の成立の時における時価により評価するものとする。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "農林水産大臣は、前項の規定による評価をする場合には、評価に関して学識経験を有する者のうちから農林水産大臣が評価人として委嘱した者及び競馬会の理事長の意見を聞かなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（学識経験者の意見を聴いて行う処分）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十条第三号の政令で定める処分は、競馬法施行令（昭和二十三年政令第二百四十二号）第十条第一項第一号から第四号までに掲げる処分とする。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（国庫納付金の納付）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "競馬会は、法第二十七条第一項の規定による納付金を、競馬の開催ごと又は海外競馬（競馬法第三条の二第一項に規定する海外競馬をいう。）の競走についての勝馬投票券の発売について農林水産省令で定める期間ごとに、その終了した日から三十日以内に、国庫に納付しなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "競馬会は、法第二十七条第二項の規定による納付金を、当該事業年度の終了後三月以内に国庫に納付しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": {
              "Ruby": [
                {
                  "Rt": "ヽ",
                  "#text": "て"
                },
                {
                  "Rt": "ヽ",
                  "#text": "ん"
                }
              ],
              "#text": "（損失\n補準備金の必要積立額）"
            },
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十八条第一項の政令で定める額は、二億円とする。"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（特別振興資金の運用又は使用の基準）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法第二十九条の二第五項の規定により特別振興資金（同条第一項の特別振興資金をいう。以下この条において同じ。）を運用し、又は使用する場合には、一事業年度における法第十九条第三項及び第四項に規定する業務に必要な経費に充てることができる額は、当該事業年度の前事業年度末における特別振興資金の額に法第二十九条の二第三項及び第四項の規定により当該事業年度に特別振興資金に充てられた額を加えて得た額のおおむね十分の九に相当する金額を超えてはならない。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この政令は、法の施行の日（昭和二十九年九月十六日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和五三年七月五日政令第二八二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三年九月三日政令第二七八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、競馬法及び日本中央競馬会法の一部を改正する法律（以下「改正法」という。）の施行の日（平成三年九月十六日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三一〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年一一月二五日政令第三六一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この政令は、平成十七年一月一日から施行する。"
                }
              }
            },
            {
              "@Num": "5",
              "ArticleCaption": "（競馬法及び日本中央競馬会法の一部を改正する法律の施行に伴う関係政令の整備に関する政令の一部改正に伴う経過措置）",
              "ArticleTitle": "第五条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この政令の施行前に実施された競走については、前条の規定による改正前の競馬法及び日本中央競馬会法の一部を改正する法律の施行に伴う関係政令の整備に関する政令附則第二条、第三条及び第五条の規定は、平成十七年三月三十一日までの間、なおその効力を有する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一九年八月一〇日政令第二五五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この政令は、競馬法及び日本中央競馬会法の一部を改正する法律附則第一条第二号に掲げる規定の施行の日（平成十九年九月一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年九月九日政令第三二二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この政令は、競馬法の一部を改正する法律の施行の日（平成二十七年十一月一日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
