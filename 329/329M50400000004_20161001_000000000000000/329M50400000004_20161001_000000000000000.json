            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一四年七月五日国家公安委員会規則第一八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この規則は、平成十四年十月一日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "警察手帳規則を皇宮護衛官に準用する規則（昭和二十九年国家公安委員会規則第十号）は、廃止する。"
              }
            }
          ]
        }
      ],
