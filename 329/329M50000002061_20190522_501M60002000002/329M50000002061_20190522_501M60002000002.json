                "Paragraph": {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "防衛大臣は、前条の報告書等を受けたときは、これを派遣国当局に交付する手続をとらなければならない。"
                    }
                  }
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
