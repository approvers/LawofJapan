{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "151",
    "@Year": "29",
    "LawNum": "昭和二十九年法律第百五十一号",
    "LawBody": {
      "LawTitle": "日本国における国際連合の軍隊の地位に関する協定の実施に伴う刑事特別法",
      "TOC": {
        "TOCLabel": "目次",
        "TOCChapter": [
          {
            "@Delete": "false",
            "@Num": "1",
            "ChapterTitle": "第一章　総則",
            "ArticleRange": "（第一条）"
          },
          {
            "@Delete": "false",
