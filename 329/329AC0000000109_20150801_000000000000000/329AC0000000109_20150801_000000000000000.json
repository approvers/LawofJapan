                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第五条から第十一条まで並びに附則第四項及び第二十三項"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
