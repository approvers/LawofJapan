{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "025",
    "@Year": "17",
    "LawNum": "平成十七年法務省令第二十五号",
    "LawBody": {
      "LawTitle": "漁業財団抵当登記規則",
      "EnactStatement": "不動産登記法（平成十六年法律第百二十三号）及び不動産登記法の施行に伴う関係法律の整備等に関する法律（平成十六年法律第百二十四号）の施行に伴い、並びに漁業財団抵当法（大正十四年法律第九号）の規定を実施するため、漁業財団抵当登記取扱手続（大正十四年司法省令第十六号）の全部を改正する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（工場抵当登記規則の準用）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "漁業財団抵当法による漁業財団の登記については、この省令に別段の定めがあるもののほか、工場抵当登記規則（平成十七年法務省令第二十三号）中工場財団に関する規定を準用する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（表題部の登記事項）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "主として漁業権をもって組成する漁業財団の登記記録の表題部の登記事項は、次のとおりとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "漁業の種類及び名称"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "漁業権の免許番号"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "漁場の位置"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "主たる営業所"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "主として船舶（船舶登記令（平成十七年政令第十一号）の規定により登記した船舶をいう。）をもって組成する漁業財団の登記記録の表題部の登記事項は、次のとおりとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "船舶登記令第十一条第一号から第三号までに掲げる事項"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "漁業の種類"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "主たる営業所"
                    }
                  }
                ]
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "主として水産物の養殖場をもって組成する漁業財団の登記記録の表題部の登記事項は、次のとおりとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "養殖場の名称及び位置"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "漁獲物の種類"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "主たる営業所"
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（漁業権の記録）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "漁業財団目録に漁業権を記録するときは、次に掲げる事項を記録するものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "存続期間"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "漁場の位置"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "漁業の種類及び名称"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "漁獲物の種類"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "漁業の時期"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "免許に付した条件又は制限"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "免許の年月日及び免許番号"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（土地の使用権の記録）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "漁業財団目録に土地の使用権を記録するときは、次に掲げる事項を記録するものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "土地の所在する市、区、郡、町、村及び字並びに当該土地の地番"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "使用の目的"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "使用の範囲"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "使用の時期及び期間"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "許可の年月日"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "使用料又は補償金及びその支払時期"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（水面の使用権の記録）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "漁業財団目録に水面の使用権を記録するときは、次に掲げる事項を記録するものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "水面の位置及び面積"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "使用の目的"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "使用の時期及び期間"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "許可の年月日"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "使用料及びその支払時期"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（引水又は排水に関する権利の記録）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "漁業財団目録に引水又は排水に関する権利を記録するときは、次に掲げる事項を記録するものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "存続期間"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "水路の位置"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "水量（水量を記録することができないときは、引水権にあっては水路の取入口、排水権にあってはその排出口における水路の断面積及び流速）"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "許可の年月日"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "使用料及びその支払時期"
                  }
                }
              ]
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（漁網の記録）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "漁業財団目録に漁網を記録するときは、その大きさ及び個数を記録するものとする。"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleCaption": "（登記及び登録がされていない船舶の記録）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "#text": "漁業財団目録に第二条第二項に規定する船舶及び工場抵当登記規則第九条第二項に規定する小型船舶以外の船舶を記録するときは、次に掲げる事項を記録するものとする。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "#text": "ただし、端舟その他の舟であって次の各号に掲げる事項を記録することができないものであるときは、当該船舶の長さ、幅及び隻数を記録すれば足りる。"
                  }
                ]
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "船名"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "船舶の種類（帆船（主として帆をもって運航する装置を有する船舶をいう。以下この号において同じ。）又は汽船（機械力をもって運航する装置を有する船舶であって、帆船でないものをいう。）の別をいう。）"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "総トン数"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "進水年月"
                  }
                }
              ]
            }
          },
          {
            "@Num": "9",
            "ArticleCaption": {
              "Ruby": {
                "Rt": "ひび",
                "#text": "篊"
              },
              "#text": "（の記録）"
            },
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "Ruby": {
                    "Rt": "ひび",
                    "#text": "篊"
                  },
                  "#text": "漁業財団目録にを記録するときは、柵の数及び一柵当たりの株数を記録するものとする。"
                }
              }
            }
          },
          {
            "@Num": "10",
            "ArticleCaption": "（所有権の保存の登記の添付情報）",
            "ArticleTitle": "第十条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "漁業財団について所有権の保存の登記の申請をする場合には、次に掲げる情報をその申請情報と併せて登記所に提供しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "漁業財団に属する漁業権があるときは、漁場の図面"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "漁業財団に属する工作物であって漁場の外に建設したものがあるときは、その配置を記録した図面"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項各号に掲げる図面は、漁場ごとに作成しなければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "不動産登記規則（平成十七年法務省令第十八号）第七十三条及び第七十四条第二項の規定は、第一項各号の図面について準用する。"
                }
              }
            ]
          },
          {
            "@Num": "11",
            "ArticleCaption": "（保存期間）",
            "ArticleTitle": "第十一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "漁業財団目録及び前条第一項各号の図面は、漁業財団の登記記録を閉鎖した日から二十年間保存しなければならない。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、不動産登記法の施行の日（平成十七年三月七日）から施行する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（未指定事務に係る旧登記簿）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "不動産登記法附則第三条第一項による指定（同条第三項の規定により指定を受けたものとみなされるものを含む。以下「第三条指定」という。）がされるまでの間における第三条指定を受けていない事務についての新令（この省令による改正後の漁業財団抵当登記規則をいう。）の適用については、新令第二条中「登記記録」とあるのは「登記簿」と、同令第十一条中「登記記録」とあるのは「登記用紙」とする。"
              }
            }
          }
        ]
      }
    }
  }
}
