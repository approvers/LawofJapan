{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "027",
    "@Year": "17",
    "LawNum": "平成十七年内閣府令第二十七号",
    "LawBody": {
      "LawTitle": "情報公開・個人情報保護審査会事務局組織規則",
      "EnactStatement": "情報公開・個人情報保護審査会設置法施行令（平成十五年政令第五百五十号）第五条第三項の規定に基づき、情報公開・個人情報保護審査会事務局組織規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（事務局に置く課等）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "情報公開・個人情報保護審査会事務局に、総務課及び審査官五人（うち一人は、関係のある他の職を占める者をもって充てられるものとする。）を置く。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（総務課の所掌事務）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務課は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "会長の官印及び情報公開・個人情報保護審査会印の保管に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "局務の総合調整に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "情報公開・個人情報保護審査会の人事に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "情報公開・個人情報保護審査会の所掌に係る会計及び会計の監査に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "情報公開・個人情報保護審査会所属の物品の管理に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "公文書類の接受、発送、編集及び保存に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "情報公開・個人情報保護審査会の保有する情報の公開に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "情報公開・個人情報保護審査会の保有する個人情報の保護に関すること。"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "広報に関すること。"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "開示決定等、訂正決定等、利用停止決定等又は開示請求、訂正請求若しくは利用停止請求に係る不作為に係る審査請求についての調査審議に関すること（審査官の所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、局務で審査官の所掌に属しないものに関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（審査官の職務）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "審査官は、命を受けて、開示決定等、訂正決定等、利用停止決定等又は開示請求、訂正請求若しくは利用停止請求に係る不作為に係る審査請求についての調査審議に関する事務を分掌する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この府令は、行政機関の保有する個人情報の保護に関する法律（平成十五年法律第五十八号）の施行の日（平成十七年四月一日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（情報公開審査会事務局組織規則の廃止）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "情報公開審査会事務局組織規則（平成十三年内閣府令第四十八号）は、廃止する。"
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一八年三月三一日内閣府令第三一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、平成十八年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年三月三一日内閣府令第二三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、平成二十八年四月一日から施行する。"
            }
          }
        }
      ]
    }
  }
}
