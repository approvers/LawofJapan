            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、最低賃金法の一部を改正する法律の施行の日（平成二十年七月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二四年八月一〇日政令第二一一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
