                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "激甚災害"
                          }
                        },
                        {
                          "@Align": "center",
                          "@BorderBottom": "solid",
                          "@BorderLeft": "solid",
                          "@BorderRight": "solid",
                          "@BorderTop": "solid",
                          "@Valign": "middle",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "適用すべき措置"
