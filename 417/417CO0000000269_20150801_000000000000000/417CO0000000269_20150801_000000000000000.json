{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "269",
    "@Year": "17",
    "LawNum": "平成十七年政令第二百六十九号",
    "LawBody": {
      "LawTitle": "有限責任事業組合契約に関する法律施行令",
      "EnactStatement": "内閣は、有限責任事業組合契約に関する法律（平成十七年法律第四十号）第七条第一項第一号及び第二号の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（その性質上組合員の責任の限度を出資の価額とすることが適当でない業務）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "有限責任事業組合契約に関する法律（以下「法」という。）第七条第一項第一号に規定するその性質上組合員の責任の限度を出資の価額とすることが適当でない業務として政令で定めるものは、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "公認会計士法（昭和二十三年法律第百三号）第二条第一項に規定する業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "弁護士法（昭和二十四年法律第二百五号）第七十二条本文の規定により弁護士又は弁護士法人でない者が行うことができない業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "司法書士法（昭和二十五年法律第百九十七号）第三条第一項に規定する業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "土地家屋調査士法（昭和二十五年法律第二百二十八号）第三条第一項に規定する業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "行政書士法（昭和二十六年法律第四号）第一条の二に規定する業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "海事代理士法（昭和二十六年法律第三十二号）第一条に規定する業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "税理士法（昭和二十六年法律第二百三十七号）第二条第一項及び第二条の二第一項に規定する業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "社会保険労務士法（昭和四十三年法律第八十九号）第二条第一項第一号から第二号までに掲げる業務"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "弁理士法（平成十二年法律第四十九号）第四条第二項、第五条第一項、第六条及び第六条の二第一項に規定する業務並びに同法第七十五条の規定により弁理士又は特許業務法人でない者が行うことができない業務"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（組合の債権者に不当な損害を与えるおそれがある業務）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第七条第一項第二号に規定する組合の債権者に不当な損害を与えるおそれがある業務として政令で定めるものは、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "当せん金付証票法（昭和二十三年法律第百四十四号）第二条第一項に規定する当せん金付証票の購入"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "競馬法（昭和二十三年法律第百五十八号）第六条第一項及び第二項（同法第二十二条において準用する場合を含む。）の勝馬投票券の購入"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "自転車競技法（昭和二十三年法律第二百九号）第八条の車券の購入"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "小型自動車競走法（昭和二十五年法律第二百八号）第十二条の勝車投票券の購入"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "モーターボート競走法（昭和二十六年法律第二百四十二号）第十条第一項及び第二項の舟券の購入"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "スポーツ振興投票の実施等に関する法律（平成十年法律第六十三号）第八条第一項及び第二項のスポーツ振興投票券の購入"
                    }
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法の施行の日（平成十七年八月一日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年一一月七日政令第三三七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、不動産登記法等の一部を改正する法律の施行の日（平成十八年一月二十日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年三月三一日政令第一一八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十九年四月一日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                }
              ]
            },
            "Item": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "略"
                  }
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "2",
                "ItemTitle": "二",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第一条中地方財政法施行令附則第二条第一項第四号の改正規定（「第十条第一項」を「第十五条第一項」に改める部分に限る。）、第二条から第四条まで、第七条及び第十条の規定"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "平成二十年四月一日"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "@AmendLawNum": "平成一九年九月一四日政令第二八七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法附則第一条第二号に掲げる規定の施行の日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                }
              ]
            },
            "Item": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "略"
                  }
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "2",
                "ItemTitle": "二",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "第二条、第四条、第六条、第八条、第十条、第十二条、第十四条、第十六条、第十八条、第二十条、第二十二条、第二十四条、第二十六条、第二十八条及び第三十条の規定"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "法附則第一条第一号に掲げる規定の施行の日"
                      }
                    }
                  ]
                }
              }
            ]
          }
        },
        {
          "@AmendLawNum": "平成二五年七月二六日政令第二二二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、改正法の施行の日（平成二十六年四月一日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
