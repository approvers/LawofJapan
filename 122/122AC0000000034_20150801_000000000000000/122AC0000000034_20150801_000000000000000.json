{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "34",
    "@Year": "22",
    "LawNum": "明治二十二年法律第三十四号",
    "LawBody": {
      "LawTitle": "明治二十二年法律第三十四号（決闘罪ニ関スル件）",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "@OldStyle": "false",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "決闘ヲ挑ミタル者又ハ其挑ニ応シタル者ハ六月以上二年以下ノ重禁錮ニ処シ十円以上百円以下ノ罰金ヲ附加ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "@OldStyle": "false",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "決闘ヲ行ヒタル者ハ二年以上五年以下ノ重禁錮ニ処シ二十円以上二百円以下ノ罰金ヲ附加ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "@OldStyle": "false",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "決闘ニ依テ人ヲ殺傷シタル者ハ刑法ノ各本条ニ照シテ処断ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "@OldStyle": "false",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "決闘ノ立会ヲ為シ又ハ立会ヲ為スコトヲ約シタル者ハ証人介添人等何等ノ名義ヲ以テスルニ拘ラス一月以上一年以下ノ重禁錮ニ処シ五円以上五十円以下ノ罰金ヲ附加ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "@OldStyle": "false",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "情ヲ知テ決闘ノ場所ヲ貸与シ又ハ供用セシメタル者ハ罰前項ニ同シ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "@OldStyle": "false",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "決闘ノ挑ニ応セサルノ故ヲ以テ人ヲ誹毀シタル者ハ刑法ニ照シ誹毀ノ罪ヲ以テ論ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "@OldStyle": "false",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前数条ニ記載シタル犯罪刑法ニ照シ其重キモノハ重キニ従テ処断ス"
                }
              }
            }
          }
        ]
      }
    }
  }
}
