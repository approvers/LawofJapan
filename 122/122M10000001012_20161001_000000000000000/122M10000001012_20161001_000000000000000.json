{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "012",
    "@Year": "22",
    "LawNum": "明治二十二年閣令第十二号",
    "LawBody": {
      "LawTitle": "歳入歳出予算概定順序",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "歳入ノ事務管理庁ハ毎年度歳入概算書ヲ調製シ前年度五月三十一日マテニ之ヲ大蔵大臣ニ送付スヘシ"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "歳入概算書ハ経常ト臨時トニ大別シ更ニ之ヲ款項目ニ区分シ前年度ノ予算ニ比シ増減ノ理由ヲ説明スヘシ"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各省大臣ハ毎年度歳出概算書ヲ調製シ前年度五月三十一日マテニ之ヲ大蔵大臣ニ送付スヘシ"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "歳出概算書ハ各省ノ所管経費ヲ経常ト臨時トニ大別シ更ニ之ヲ款項ニ区分シ前年度ノ予算ニ比シ増減ノ理由ヲ説明スヘシ"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "大蔵大臣ハ各庁ノ歳入概算書及歳出概算書ヲ検案シ歳入出ヲ対照調理シ歳入出総概算書ヲ調製シ前年度六月三十日マテニ之ヲ閣議ニ提出スヘシ"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "歳入出総概算書ハ歳入出共ニ経常ト臨時トニ大別シ更ニ之ヲ款項ニ区分シ前年度ニ比シ増減ノ理由ヲ説明スヘシ"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "内閣ニ於テハ前年度七月十五日マテニ歳入出総概算書ヲ決定スヘシ"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "各省大臣ハ内閣ニ於テ決定シタル各省所管経費毎項ノ概算額以内ニ於テ節約ヲ旨トシ毎年度ノ各省予定経費要求書ヲ調製シ前年度八月三十一日マテニ之ヲ大蔵大臣ニ送付スヘシ"
              }
            }
          },
          {
            "@Num": "9",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "歳入概算書及歳出概算書ノ様式ハ大蔵大臣之ヲ定ムヘシ"
              }
            }
          },
          {
            "@Num": "10",
            "ArticleTitle": "第十条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "明治二十三年度予算ニ限リ前各条ノ期限ヲ一箇月間延スコトヲ得"
              }
            }
          }
        ]
      }
    }
  }
}
