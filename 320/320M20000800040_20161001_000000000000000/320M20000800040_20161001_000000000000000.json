    "@Year": "20",
    "LawNum": "昭和二十年運輸省令第四十号",
    "LawBody": {
      "LawTitle": "昭和二十年運輸省令第四十号（航海ノ制限等ニ関スル件）",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "昭和二十年勅令第五百四十二号ポツダム宣言ノ受諾ニ伴ヒ発スル命令ニ関スル件ニ基ク航海ノ制限等ニ関スル件左ノ通定ム"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国土交通大臣ハ国際間ノ紛争ニ際シ日本船舶ノ安全ヲ保持スルタメ其ノ他緊急ノ必要アリト認ムルトキハ航路又ハ区域ヲ指定シテ日本ト日本以外ノ地域トノ間又ハ日本以外ノ地域相互間ニ於ケル航海ヲ制限シ又ハ禁止スルコトヲ得"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "削除"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国土交通大臣ハ船舶ノ安全ヲ保持スルタメ船長又ハ船舶所有者若ハ運航業者ニ対シ船舶ノ出入港ノ手続又ハ旗旒ノ掲揚ニ関シ必要ナル命令ヲ為スコトヲ得"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "削除"
              }
            }
          },
          {
            "@Num": "4_2",
            "ArticleTitle": "第四条ノ二",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "削除"
              }
            }
          },
          {
            "@Num": "4_3",
            "ArticleTitle": "第四条ノ三",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "海域ニアル爆薬兵器若ハ弾薬又ハ其ノ部分品（以下爆発物件等ト謂フ）ハ海上保安庁長官ノ許可ヲ受クルニ非ザレバ之ヲ引揚ゲ又ハ解撤スルコトヲ得ズ"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "海上保安庁長官ハ前項ノ許可ノ申請ガ当該爆発物件等ヲ産業ノ用途ニ供セントシテ為サレタル場合ノ外之ヲ許可スルコトヲ得ズ"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": "第一項ノ許可ヲ受ケタル者ガ当該爆発物件等ノ引揚又ハ解撤ヲ為サントスルトキハ海上保安庁長官ノ定ムル方法ニ依ルコトヲ要ス"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "本令ニ依ル制限若ハ禁止又ハ命令ニ違反シタル者ハ三年以下ノ懲役若ハ禁錮又ハ五千円以下ノ罰金ニ処ス"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法人ノ代表者又ハ法人若ハ人ノ代理人、使用人其ノ他ノ従業者ガ其ノ法人又ハ人ノ業務ニ関シ本令又ハ本令ニ基ク命令ニ違反シタルトキハ行為者ヲ罰スルノ外其ノ法人又ハ人ニ対シ前条ノ罰金刑ヲ科ス"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "本令及本令ニ基ク命令中船舶所有者ニ関スル規定ハ船舶共有ノ場合ニ在リテハ船舶管理人ニ之ヲ適用ス"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "本令ハ公布ノ日ヨリ之ヲ施行ス"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二五年八月二一日運輸省令第六三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二七年四月五日法律第七二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この法律は、日本国との平和条約の最初の効力発生の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一一年一二月二二日法律第一六〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "#text": "この法律（第二条及び第三条を除く。）は、平成十三年一月六日から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                  }
                ]
              },
              "Item": [
