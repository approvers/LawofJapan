{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "086",
    "@Year": "20",
    "LawNum": "平成二十年内閣府令第八十六号",
    "LawBody": {
      "LawTitle": "官民人材交流センター組織規則",
      "EnactStatement": "官民人材交流センター令（平成二十年政令第三百九十一号）第二条の規定に基づき、官民人材交流センター組織規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（官民人材交流センターに置かれる課等）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "官民人材交流センター（以下「センター」という。）に、総務課、法令等遵守担当室及び主任調整官二人を置く。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（総務課の所掌事務）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務課は、次の事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "センターの官印及びセンター印の保管に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "センターの所掌事務に関する総合調整に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "センターの職員の人事に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "センターの所掌に係る会計及び会計の監査に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "センター所属の物品の管理に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "公文書類の接受、発送、編集及び保存に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "センターの保有する情報の公開に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "センターの保有する個人情報の保護に関すること。"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "広報に関すること。"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "国家公務員法（昭和二十二年法律第百二十号）第十八条の五第一項（自衛隊法（昭和二十九年法律第百六十五号）第六十五条の十第二項の規定により準用する場合を含む。）に定める職員の離職に際しての離職後の就職の援助（以下「再就職支援」という。）に関する事務のうち、再就職支援の依頼の受付けに関すること。"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "国家公務員法第十八条の五第二項に定める官民の人材交流の円滑な実施のための支援に関すること。"
                  }
                },
                {
                  "@Num": "12",
                  "ItemTitle": "十二",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、センターの所掌事務で他の所掌に属しないものに関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（法令等遵守担当室の所掌事務）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法令等遵守担当室は、次の事務をつかさどる。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "再就職支援に関する事務に係る法令等の遵守（以下「法令等遵守」という。）に関すること。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "再就職支援に関する事務に係る法令等遵守についての情報の収集及び調査に関すること。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "再就職等監視委員会その他の行政機関に対し、再就職支援に関する事務の公正性及び透明性を確保するため、必要な情報の提供を行うこと。"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "再就職支援に関する事務に係る法令等遵守について必要な措置を検討すること。"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "法令等遵守担当室に、室長を置く。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "室長は、非常勤とする。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（主任調整官の職務）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "主任調整官は、命を受けて、再就職支援に関する事務を行う（総務課の所掌に属するものを除く。）。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、国家公務員法等の一部を改正する法律（平成十九年法律第百八号）の施行の日（平成二十年十二月三十一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二六年五月二九日内閣府令第四三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、国家公務員法等の一部を改正する法律（平成二十六年法律第二十二号）の施行の日（平成二十六年五月三十日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年一〇月一日内閣府令第五七号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、国家公務員法等の一部を改正する法律（平成二十六年法律第二十二号）附則第一条第三号に掲げる規定の施行の日（平成二十七年十月一日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
