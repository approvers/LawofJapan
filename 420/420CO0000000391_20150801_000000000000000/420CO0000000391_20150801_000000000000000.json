{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "391",
    "@Year": "20",
    "LawNum": "平成二十年政令第三百九十一号",
    "LawBody": {
      "LawTitle": "官民人材交流センター令",
      "EnactStatement": "内閣は、国家公務員法（昭和二十二年法律第百二十号）第十八条の七第十項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（審議官）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "官民人材交流センター（以下「センター」という。）に、審議官一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "審議官は、命を受けて、センターの所掌事務に関する重要事項についての企画及び立案に参画し、関係事務を総括整理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（内閣府令への委任）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前条に定めるもののほか、センターの内部組織は、内閣府令で定める。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "センターの支所の名称、位置、所掌事務及び内部組織は、内閣府令で定める。"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、国家公務員法等の一部を改正する法律（平成十九年法律第百八号）の施行の日（平成二十年十二月三十一日）から施行する。"
            }
          }
        }
      }
    }
  }
}
