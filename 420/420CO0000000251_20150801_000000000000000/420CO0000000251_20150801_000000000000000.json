    "LawNum": "平成二十年政令第二百五十一号",
    "LawBody": {
      "LawTitle": "宇宙開発戦略本部令",
      "EnactStatement": "内閣は、宇宙基本法（平成二十年法律第四十三号）第三十四条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "宇宙開発戦略本部の運営に関し必要な事項は、宇宙開発戦略本部長が宇宙開発戦略本部に諮って定める。"
            }
          }
        }
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、宇宙基本法の施行の日（平成二十年八月二十七日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二四年七月一一日政令第一八七号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、内閣府設置法等の一部を改正する法律の施行の日（平成二十四年七月十二日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
