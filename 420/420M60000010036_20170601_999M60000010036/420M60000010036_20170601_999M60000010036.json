{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "36",
    "@Year": "20",
    "LawNum": "平成二十年法務省令第三十六号",
    "LawBody": {
      "LawTitle": "地方更生保護委員会事務局組織規則",
      "EnactStatement": "更生保護法（平成十九年法律第八十八号）第二十条第二項の規定に基づき、地方更生保護委員会事務局組織規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（事務局次長）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "関東地方更生保護委員会事務局に、事務局次長一人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "事務局次長は、事務局長を助け、事務局の事務を整理する。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（地方更生保護委員会事務局に置く課等）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "地方更生保護委員会事務局に、次に掲げる課を置く。"
                },
                "List": [
                  {
                    "ListSentence": {
                      "Sentence": "総務課"
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": "会計課（関東地方更生保護委員会及び近畿地方更生保護委員会に限る。）"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項に掲げる課のほか、関東地方更生保護委員会事務局に更生保護管理官一人、調整指導官一人及び首席審査官一人を、その他の地方更生保護委員会事務局にそれぞれ更生保護管理官一人及び調整指導官一人を置く。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（総務課の所掌事務）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務課は、次に掲げる事務（関東地方更生保護委員会事務局及び近畿地方更生保護委員会事務局においては、第五号に掲げるものを除く。）をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "事務局の所掌事務に関する総合調整に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "公印の保管に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "公文書類の接受、発送、編集及び保存に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "人事に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "会計に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "保護観察所の事務（関東地方更生保護委員会事務局及び近畿地方更生保護委員会事務局の総務課においては、保護観察所組織規則（平成十九年法務省令第二十二号）第三条第一号から第四号まで、第七号から第九号まで及び第十二号に掲げるものに、その他の地方更生保護委員会事務局の総務課においては、保護観察所組織規則第三条第一号から第五号まで、第七号から第九号まで及び第十二号に掲げるものに限る。）の監督に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "地方更生保護委員会の議事に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "保護司の設置区域及び組織に関すること。"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "保護司の委嘱、解嘱及び監督の事務に関すること。"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "保護司、保護司会、保護司会連合会並びに更生保護事業法（平成七年法律第八十六号）に定める認可事業者及び届出事業者並びにその役職員の表彰に関すること。"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "更生保護についての広報に関すること。"
                  }
                },
                {
                  "@Num": "12",
                  "ItemTitle": "十二",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、地方更生保護委員会事務局の所掌事務で他の所掌に属しないものに関すること（関東地方更生保護委員会事務局以外の地方更生保護委員会事務局の総務課においては、第五条各号に掲げる事務を除く。）。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3_2",
            "ArticleCaption": "（会計課の所掌事務）",
            "ArticleTitle": "第三条の二",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "会計課は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "前条第五号に掲げる事務に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "保護観察所の事務（保護観察所組織規則第三条第五号に掲げるものに限る。）の監督に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（更生保護管理官の職務）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "更生保護管理官は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "保護観察所の事務の監督に関すること（総務課、会計課及び調整指導官の所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "保護司の研修に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "更生保護事業の助長及び監督に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "更生保護に関する各種団体との連絡調整に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "更生保護に関する重要な調査、研究及び企画を行うこと。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "更生保護に関する資料の収集及び統計に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4_2",
            "ArticleCaption": "（調整指導官の職務）",
            "ArticleTitle": "第四条の二",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "調整指導官は、刑事施設、少年院又は婦人補導院に収容されている者に対する生活環境の調整に関する事務をつかさどる。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（首席審査官の職務）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "首席審査官は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "仮釈放を許し、又はその処分を取り消す処分に関すること（調整指導官の所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "仮出場を許す処分に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "少年院からの仮退院を許し、又は退院を許す処分に関すること（調整指導官の所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "少年院から仮退院中の者を少年院に戻して収容する旨の決定の申請に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "仮釈放中の者の保護観察の停止に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "少年法（昭和二十三年法律第百六十八号）第五十二条第一項及び第二項の規定により言い渡された刑の執行を受け終わったものとする処分に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "保護観察を仮に解除し、又はその処分を取り消す処分に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "婦人補導院からの仮退院を許し、又はその処分を取り消す処分に関すること（調整指導官の所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "刑法（明治四十年法律第四十五号）第二十七条の三第一項又は薬物使用等の罪を犯した者に対する刑の一部の執行猶予に関する法律（平成二十五年法律第五十号）第四条第一項の規定により保護観察に付する旨の言渡しを受けた者（以下「保護観察付一部猶予者」という。）の住居を特定し、又はその処分を取り消す処分に関すること（調整指導官の所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "少年院からの仮退院を許された者、仮釈放を許された者、保護観察付一部猶予者又は婦人補導院からの仮退院を許された者についてする特別遵守事項の設定、変更及び取消しに関すること。"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "前各号の処分の執行に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（統括審査官）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "地方更生保護委員会事務局を通じて統括審査官十七人以内を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "統括審査官の配置は、法務大臣が定める。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "統括審査官は、命を受けて、前条各号に掲げる事務のうち事務局長の指定する事務を統括する。"
                }
              }
            ]
          },
          {
            "@Num": "7",
            "ArticleCaption": "（九州地方更生保護委員会事務局の特例）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "九州地方更生保護委員会事務局の所掌事務の一部を分掌させるため、分室を那覇市に置く。"
              }
            }
          },
          {
            "@Num": "8",
            "ArticleCaption": "（他の課等の所掌事務の処理）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "事務局長は、特に必要があるときは、一の課等に属する事務を他の課等において処理させることができる。"
              }
            }
          },
          {
            "@Num": "9",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令に定めるもののほか、事務分掌その他組織の細目は、事務局長が法務大臣の承認を受けて定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令は、更生保護法の施行の日（平成二十年六月一日）から施行する。"
                }
              }
            },
            {
              "@Num": "2",
              "ArticleCaption": "（職員の身分引継ぎ）",
              "ArticleTitle": "第二条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "この省令の施行の際現に従前の地方更生保護委員会事務局の職員である者は、別に辞令を発せられない限り、同一の勤務条件をもって、この省令の施行後の地方更生保護委員会事務局の相当の職員となるものとする。"
                }
              }
            },
            {
              "@Num": "3",
              "ArticleCaption": "（地方更生保護委員会事務局組織規則の廃止）",
              "ArticleTitle": "第三条",
              "Paragraph": {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "地方更生保護委員会事務局組織規則（平成十九年法務省令第二十一号）は、廃止する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成二二年三月三一日法務省令第一二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成二十二年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年三月三一日法務省令第二五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "#text": "この省令は、平成二十八年四月一日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "#text": "ただし、第四条第一号の改正規定、第五条中第十号を第十一号とする改正規定、同条第九号の改正規定及び同号を同条第十号とし、同条第八号の次に一号を加える改正規定は、刑法等の一部を改正する法律（平成二十五年法律第四十九号）の施行の日から施行する。"
                }
              ]
            }
          }
        },
        {
          "@AmendLawNum": "平成二九年三月三一日法務省令第一三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成二十九年四月一日から施行する。"
            }
          }
        }
      ]
    }
  }
}
