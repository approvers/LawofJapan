{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "Rule",
    "@Num": "",
    "@Year": "20",
    "LawNum": "平成二十年三月三十一日内閣総理大臣決定",
    "LawBody": {
      "LawTitle": "内閣情報調査室に内閣情報分析官等を置く規則",
      "EnactStatement": "内閣官房組織令（昭和三十二年政令第二百十九号）第十二条の規定に基づき、内閣情報調査室に内閣情報分析官等を置く規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（内閣情報分析官）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣情報調査室に、内閣情報分析官を置き、内閣審議官又は内閣参事官のうちから命ずる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "内閣情報分析官は、命を受けて、内閣情報調査室の事務のうち特定の地域又は分野に関する特に高度な分析に従事する。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（内閣情報分析専門官）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "内閣情報調査室に、内閣情報分析専門官を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "内閣情報分析専門官は、高度の専門的な知識経験に基づき内閣の重要政策に関する情報の分析その他の調査を行うことにより、内閣情報分析官の従事する特に高度な分析の支援を行う。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この規則は、平成二十年四月一日から実施する。"
          }
        }
      }
    }
  }
}
