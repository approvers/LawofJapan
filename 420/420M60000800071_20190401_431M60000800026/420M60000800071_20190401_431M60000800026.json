{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "71",
    "@Year": "20",
    "LawNum": "平成二十年国土交通省令第七十一号",
    "LawBody": {
      "LawTitle": "観光庁組織規則",
      "EnactStatement": "国土交通省設置法（平成十一年法律第百号）及び国土交通省組織令（平成十二年政令第二百五十五号）を実施するため、観光庁組織規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（調整室並びに企画官、広報広聴官及び危機管理・安全対策官）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "総務課に、調整室並びに企画官、広報広聴官及び危機管理・安全対策官それぞれ一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "調整室は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光庁の職員の任免、給与、懲戒、服務その他の人事並びに教養及び訓練に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "表彰及び儀式に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光庁の所掌に係る経費及び収入の予算、決算及び会計並びに会計の監査に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光庁所属の行政財産及び物品の管理に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光庁の職員の衛生、医療その他の福利厚生に関すること。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "調整室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "企画官は、命を受けて、総務課の所掌事務に関する重要事項についての企画及び立案並びに調整に関する事務に参画する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "広報広聴官は、命を受けて、広報及び広聴に関する重要事項についての企画及び立案並びに調整に関する事務をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "危機管理・安全対策官は、命を受けて、観光庁の所掌事務に関する危機管理及び安全の確保に関する重要事項に関する事務をつかさどる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（観光経済調査室並びに国際観光振興政策企画官及び統計分析官）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "観光戦略課に、観光経済調査室並びに国際観光振興政策企画官及び統計分析官それぞれ一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "観光経済調査室は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光に関連する経済事情に関する情報の収集、整理及び分析に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光に関する調査及び研究に関すること（統計分析官の所掌に属するものを除く。）。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光に関する統計に関すること（統計分析官の所掌に属するものを除く。）。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "観光立国推進基本法（平成十八年法律第百十七号）第八条の規定による観光の状況及び施策に関する年次報告等に関すること。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "観光経済調査室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "国際観光振興政策企画官は、命を受けて、次に掲げる事務のうち重要事項に係るものに関する事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国際観光の振興に資する施策に関する基本的な政策の企画及び立案並びに推進に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国際観光の振興に資する施策に関する関係行政機関の事務の調整に関すること。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "統計分析官は、観光に関する統計その他の情報の総合的な分析に関する事務をつかさどる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（旅行業務適正化指導室及び宿泊業活性化調整室並びに新事業推進官及び民泊業務適正化指導官）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "観光産業課に、旅行業務適正化指導室及び宿泊業活性化調整室並びに新事業推進官及び民泊業務適正化指導官それぞれ一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "旅行業務適正化指導室は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旅行業務及び旅行サービス手配業務の運営の適正化に関する企画及び立案並びに指導に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "旅行業、旅行業者代理業及び旅行サービス手配業の登録に係る安全上の審査に関すること。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "旅行業務適正化指導室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "宿泊業活性化調整室は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "宿泊業の活性化に関する企画及び立案並びに調整に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "ホテル及び旅館の登録に関すること。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "宿泊業活性化調整室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "新事業推進官は、新たな観光産業の分野の振興に関する企画及び立案並びに調整に関する事務をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "7",
                "ParagraphNum": "７",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "民泊業務適正化指導官は、住宅宿泊事業及び住宅宿泊仲介業の運営の適正化に関する企画及び立案並びに指導に関する事務をつかさどる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（アジア市場推進室、欧米豪市場推進室及びＭＩＣＥ推進室並びに外客受入推進官、外客誘致推進官、ＭＩＣＥ連携推進官及び相互交流促進官）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "国際観光課に、アジア市場推進室、欧米豪市場推進室及びＭＩＣＥ推進室並びに外客受入推進官一人、外客誘致推進官二人、ＭＩＣＥ連携推進官及び相互交流促進官それぞれ一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "アジア市場推進室は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "アジアからの外国人観光旅客の来訪の促進に関する企画及び立案並びに調整に関すること（観光戦略課並びに外客受入推進官及び相互交流促進官の所掌に属するものを除く。）。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "アジアからの外国人観光旅客の来訪の促進による国際観光の振興に関すること（観光地域振興部及び相互交流促進官の所掌に属するものを除く。）。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "アジア市場推進室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "欧米豪市場推進室は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "欧州、北米及びオーストラリアからの外国人観光旅客の来訪の促進に関する企画及び立案並びに調整に関すること（観光戦略課並びに外客受入推進官及び相互交流促進官の所掌に属するものを除く。）。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "欧州、北米及びオーストラリアからの外国人観光旅客の来訪の促進による国際観光の振興に関すること（観光地域振興部及び相互交流促進官の所掌に属するものを除く。）。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "欧米豪市場推進室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "ＭＩＣＥ推進室は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "会議、討論会、講習会、展覧会その他これらに類する集会であって海外からの相当数の外国人の参加が見込まれるもの並びにこれらに併せて行われる観光旅行その他の外国人のための観光及び交流を目的とする催し（次号及び第十項において「ＭＩＣＥ」という。）の誘致に関する企画及び立案並びに調整に関すること（ＭＩＣＥ連携推進官の所掌に属するものを除く。）。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "ＭＩＣＥの誘致の促進による国際観光の振興に関すること。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "7",
                "ParagraphNum": "７",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "ＭＩＣＥ推進室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "8",
                "ParagraphNum": "８",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "外客受入推進官は、外国人観光旅客の受入環境の整備に関する企画及び立案並びに関係行政機関その他の関係者との連絡調整に関する事務をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "9",
                "ParagraphNum": "９",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "外客誘致推進官は、命を受けて、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "外国人観光旅客の来訪の促進に関する特定事項についての企画及び立案並びに調整に関すること（観光戦略課並びにアジア市場推進室及び欧米豪市場推進室並びに外客受入推進官及び相互交流促進官の所掌に属するものを除く。）。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "外国人観光旅客の来訪の促進による国際観光の振興に関する特定事項に関すること（観光地域振興部並びにアジア市場推進室及び欧米豪市場推進室並びに相互交流促進官の所掌に属するものを除く。）。"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "10",
                "ParagraphNum": "１０",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "ＭＩＣＥ連携推進官は、地方公共団体その他の関係者との連携によるＭＩＣＥの誘致に関する企画及び立案並びに地方公共団体その他の関係者との連絡調整に関する事務をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "11",
                "ParagraphNum": "１１",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "相互交流促進官は、次に掲げる事務をつかさどる。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "海外旅行の促進に関する企画及び立案に関すること。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "外国人観光旅客の来訪の促進に資する情報の外国人観光旅客に対する提供に関すること（観光地域振興部の所掌に属するものを除く。）。"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国際観光課の所掌事務に係る国際機関及び外国の行政機関その他の者との連絡調整に関すること。"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（広域連携推進室並びに観光地経営推進官、地域競争力強化推進官及び先進技術活用推進官）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "観光地域振興課に、広域連携推進室並びに地域競争力強化推進官、観光地経営推進官及び先進技術活用推進官それぞれ一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "広域連携推進室は、地域間の広域的な連携による観光地及び観光施設の改善並びに地域の振興に資する観光の振興に関する企画及び立案並びに調整に関する事務（観光地経営推進官、地域競争力強化推進官及び先進技術活用推進官の所掌に属するものを除く。）をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "広域連携推進室に、室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "観光地経営推進官は、観光地及び観光施設の経営に関する企画及び立案並びに関係行政機関その他の関係者との連絡調整に関する事務（先進技術活用推進官の所掌に属するものを除く。）をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "地域競争力強化推進官は、観光地及び観光施設の改善並びに地域の振興に資する観光の振興に関する事務のうち、地域の競争力を強化するための国際競争力の高い魅力ある観光地の形成に関する企画及び立案並びに調整に関する事務（観光地経営推進官及び先進技術活用推進官の所掌に属するものを除く。）をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "先進技術活用推進官は、観光地及び観光施設の改善並びに地域の振興に資する観光の振興に関する事務のうち、先進技術の活用に関する企画及び立案並びに調整に関する事務をつかさどる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（地域資源活用推進室及び新コンテンツ開発企画官）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "観光資源課に、地域資源活用推進室及び新コンテンツ開発企画官一人を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "地域資源活用推進室は、地域の観光資源の活用の推進に関する企画及び立案並びに関係行政機関その他の関係者との連絡調整に関する事務をつかさどる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "地域資源活用推進室に室長を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "新コンテンツ開発企画官は、命を受けて、新たな観光資源の開発及び活用に関する重要事項についての企画及び立案並びに調整に関する事務をつかさどる。"
                  }
                }
              }
            ]
          }
        ]
      },
