    "LawBody": {
      "LawTitle": "発電水力調査図表類交付規則",
      "Preamble": {
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "発電水力調査図表類交付規則左ノ通定ム"
            }
          },
          {
            "@Num": "2",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "本令ハ公布ノ日ヨリ之ヲ施行ス"
            }
          }
        ]
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "経済産業省ニ於テ調査シタル発電水力調査図表類ハ其ノ副本ノ交付ヲ申請スルコトヲ得"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "調査図表類ノ交付ヲ申請セムトスル者ハ第一号書式ノ申請書ヲ差出スヘシ"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "前項ノ申請書ニハ第二号書式ノ企業概要書ヲ添附スヘシ"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "調査図表類ノ交付ヲ申請セムトスル者ハ左ノ手数料ヲ納付スベシ"
                },
                "List": [
                  {
                    "ListSentence": {
                      "Sentence": "流量ニ関スル調査図表類"
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": "一流量測定地点（又ハ一測水所）"
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "一暦年分ニ付"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "五千百円"
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "一暦年ニ於ケル調査期間一年未満ノモノニ付"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "二千五百五十円"
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "行政手続等における情報通信の技術の利用に関する法律（平成十四年法律第百五十一号）第三条第一項ノ規定ニ依リ同項ニ規定スル電子情報処理組織ヲ使用シテ前条ノ申請ヲ為ス場合ニ於ケル第一項ノ規定ノ適用ニ付テハ同項中「五千百円」トアルハ「二千六百円」ト、「二千五百五十円」トアルハ「千三百円」トス"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "○３",
                "ParagraphSentence": {
                  "Sentence": "前二項ノ場合ニ於テ毎日ノ流量ニ関スル図表ノ交付ヲ受ケサルモノノ手数料ハ各其ノ半額トス"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "○４",
                "ParagraphSentence": {
                  "Sentence": "手数料ハ其ノ金額ニ相当スル収入印紙ヲ申請書ニ貼付シテ納付スヘシ"
                }
              },
              {
                "@Num": "5",
                "ParagraphNum": "○５",
                "ParagraphSentence": {
                  "Sentence": "前項ノ収入印紙ハ経済産業省ニ於テ之ヲ消印ス"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "経済産業省ニ於テ調査シタル流量測定地点（又ハ測水所）ハ都道府県又ハ経済産業局若ハ経済産業局支局ニ就キ承合スヘシ"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "削除"
              }
            }
          }
        ]
      },
