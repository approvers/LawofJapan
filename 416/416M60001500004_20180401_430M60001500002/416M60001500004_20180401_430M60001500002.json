            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、平成十六年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二二年三月三一日厚生労働省・経済産業省・環境省令第二号",
