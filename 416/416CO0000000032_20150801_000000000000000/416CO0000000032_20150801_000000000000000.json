{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "32",
    "@Year": "16",
    "LawNum": "平成十六年政令第三十二号",
    "LawBody": {
      "LawTitle": "国立研究開発法人海洋研究開発機構法施行令",
      "EnactStatement": "内閣は、独立行政法人海洋研究開発機構法（平成十五年法律第九十五号）第六条第六項、第十八条第四項及び第二十二条並びに附則第二条第一号、第八条、第九条、第十条第三項及び第六項、第十一条第一項及び第六項並びに第十八条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "国立研究開発法人海洋研究開発機構法（平成十五年法律第九十五号。以下「法」という。）第六条第五項の評価委員は、必要の都度、次に掲げる者につき文部科学大臣が任命する。"
              }
            },
            "Item": [
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "財務省の職員"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "一人"
                      }
                    }
                  ]
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "2",
                "ItemTitle": "二",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "文部科学省の職員"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "一人"
                      }
                    }
                  ]
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "3",
                "ItemTitle": "三",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "国立研究開発法人海洋研究開発機構（以下「機構」という。）の役員"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "一人"
                      }
                    }
                  ]
                }
              },
              {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "4",
                "ItemTitle": "四",
                "ItemSentence": {
                  "Column": [
                    {
                      "@LineBreak": "false",
                      "@Num": "1",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "学識経験のある者"
                      }
                    },
                    {
                      "@LineBreak": "false",
                      "@Num": "2",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "二人"
                      }
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "法第六条第五項の規定による評価は、同項の評価委員の過半数の一致によるものとする。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphNum": "３",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "法第六条第五項の規定による評価に関する庶務は、文部科学省研究開発局海洋地球課において処理する。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ArticleCaption": "（施行期日）",
              "ArticleTitle": "第一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "この政令は、公布の日から施行する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、附則第十三条から第二十四条までの規定は、平成十六年四月一日から施行する。"
                    }
                  ]
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ArticleCaption": "（研究所）",
              "ArticleTitle": "第二条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第二条第一号に規定する政令で定める研究所は、東京大学附置の海洋研究所とする。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "3",
              "ArticleCaption": "（機構の成立の時において承継される国の権利及び義務）",
              "ArticleTitle": "第三条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第八条に規定する政令で定める権利及び義務は、次に掲げる権利及び義務とする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "東京大学に所属する土地、建物、工作物及び船舶（その土地に定着する物及びその建物に附属する工作物を含む。附則第七条第一号において「土地等」という。）のうち文部科学大臣が財務大臣に協議して指定するものに関する権利及び義務"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "機構の成立の際現に東京大学附置の海洋研究所に使用されている物品のうち文部科学大臣が指定するものに関する権利及び義務"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "機構の業務に関し国が有する権利及び義務のうち前二号に掲げるもの以外のものであって、文部科学大臣が指定するもの"
                      }
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "4",
              "ArticleCaption": "（国有財産の無償使用）",
              "ArticleTitle": "第四条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第九条の規定により国が機構に無償で使用させることができる国有財産及び当該国有財産の使用に関し必要な手続は、文部科学大臣が財務大臣に協議して定める。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "5",
              "ArticleCaption": "（機構の成立の時において国が承継する資産の範囲等）",
              "ArticleTitle": "第五条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第十条第二項の規定により国が承継する資産は、海洋科学技術センターが有する資産のうち文部科学大臣が財務大臣に協議して指定するものとする。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "前項の規定により国が承継する資産は、一般会計に帰属する。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "6",
              "ArticleCaption": "（海洋科学技術センターの解散の登記の嘱託等）",
              "ArticleTitle": "第六条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "法附則第十条第一項の規定により海洋科学技術センターが解散したときは、文部科学大臣は、遅滞なく、その解散の登記を登記所に嘱託しなければならない。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "登記官は、前項の規定による嘱託に係る解散の登記をしたときは、その登記用紙を閉鎖しなければならない。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "7",
              "ArticleCaption": "（国の有する権利及び義務の承継の際出資があったものとされる財産）",
              "ArticleTitle": "第七条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "法附則第十一条第一項に規定する政令で定める財産は、次に掲げるものとする。"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "附則第三条第一号の規定により指定された土地等"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "附則第三条第三号の規定により指定された権利に係る財産のうち文部科学大臣が指定するもの"
                      }
                    }
                  }
                ]
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "8",
              "ArticleCaption": "（評価に関する規定の準用）",
              "ArticleTitle": "第八条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "第一条の規定は、法附則第十一条第五項の評価委員その他評価について準用する。"
                    },
                    {
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "この場合において、第一条第一項中「必要の都度、次に掲げる者」とあるのは「次に掲げる者」と、同項第三号中「役員」とあるのは「役員（機構が成立するまでの間は、機構に係る独立行政法人通則法（平成十一年法律第百三号）第十五条第一項の設立委員）」と読み替えるものとする。"
                    }
                  ]
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "9",
              "ArticleCaption": "（電波法の適用に関する経過措置）",
              "ArticleTitle": "第九条",
              "Paragraph": [
                {
                  "@Hide": "false",
                  "@Num": "1",
                  "ParagraphNum": null,
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "機構の成立前に電波法（昭和二十五年法律第百三十一号）の規定により東京大学について国に対しされた承認その他の処分又は通知その他の行為であって、法附則第八条の規定により機構が承継することとなる権利及び義務に係るものは、機構の成立後は、電波法の規定により機構に対しされた免許、許可その他の処分又は通知その他の行為とみなす。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "機構の成立前に電波法の規定により東京大学について国がしている届出その他の行為であって、法附則第八条の規定により機構が承継することとなる権利及び義務に係るものは、機構の成立後は、電波法の規定により機構がした届出その他の行為とみなす。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "3",
                  "ParagraphNum": "３",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "機構は、機構の成立前に東京大学について国が承認の申請をした無線局であって機構の業務に係るものに限り、電波法第百四条第一項の政令で定める独立行政法人とみなす。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "10",
              "ArticleCaption": "（港湾法の適用に関する経過措置）",
              "ArticleTitle": "第十条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "機構の成立前に東京大学について国が港湾法（昭和二十五年法律第二百十八号）第三十七条第三項の規定により読み替えて適用される同条第一項の規定により港湾管理者とした協議に基づく占用であって、機構の業務に係るものは、機構の成立後は、同項の規定により港湾管理者がした許可に基づく占用とみなす。"
                    },
                    {
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "この場合において、同条第四項本文の規定は、適用しない。"
                    }
                  ]
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "11",
              "ArticleCaption": "（都市公園法の適用に関する経過措置）",
              "ArticleTitle": "第十一条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "機構の成立前に東京大学について国が都市公園法（昭和三十一年法律第七十九号）の規定により公園管理者とした協議に基づく占用であって、機構の業務に係るものは、機構の成立後は、同法の規定により公園管理者がした許可に基づく占用とみなす。"
                  }
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "13",
              "ArticleCaption": "（海洋科学技術センター法施行令の廃止）",
              "ArticleTitle": "第十三条",
              "Paragraph": {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "海洋科学技術センター法施行令（昭和四十六年政令第二百三十九号）は、廃止する。"
                  }
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一七年四月一日政令第一一八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、公布の日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二一年一二月二四日政令第二九六号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成二十二年一月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年三月一八日政令第七四号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成二十七年四月一日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
