{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "020",
    "@Year": "16",
    "LawNum": "平成十六年国土交通省令第二十号",
    "LawBody": {
      "LawTitle": "独立行政法人環境再生保全機構法附則第七条第七項の軽微な変更を定める省令",
      "EnactStatement": "独立行政法人環境再生保全機構法（平成十五年法律第四十三号）附則第七条第七項の規定に基づき、独立行政法人環境再生保全機構法附則第七条第七項の軽微な変更を定める省令を次のように定める。",
      "MainProvision": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "独立行政法人環境再生保全機構法附則第七条第七項の主務省令で定める軽微な変更は、次のとおりとする。"
          },
          "Item": [
            {
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": "事業区域の変更で、十パーセント以内を減ずるもの"
              }
            },
            {
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": "事業の着手又は完了の予定時期の六月以内の変更"
              }
            },
            {
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": "事業に要する費用の変更で、二十パーセント以内を減ずるもの"
              }
            },
            {
              "@Num": "4",
              "ItemTitle": "四",
              "ItemSentence": {
                "Sentence": "年度別執行計画額の変更"
              }
            },
            {
              "@Num": "5",
              "ItemTitle": "五",
              "ItemSentence": {
                "Sentence": "事業に要する資金の内訳の変更で、第三号の変更に伴うもの"
              }
            }
          ]
        }
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、平成十六年四月一日から施行する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（環境事業団法第十八条第一項第三号及び第四号の業務並びに同項第五号の業務のうち都市公園となるべき緑地を設置し、及び譲渡する業務に係る事業実施計画に関する省令の廃止）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "環境事業団法第十八条第一項第三号及び第四号の業務並びに同項第五号の業務のうち都市公園となるべき緑地を設置し、及び譲渡する業務に係る事業実施計画に関する省令（昭和六十二年建設省令第二十号）は、廃止する。"
              }
            }
          }
        ]
      }
    }
  }
}
