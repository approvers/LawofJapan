{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "ImperialOrder",
    "@Num": "76",
    "@Year": "21",
    "LawNum": "明治二十一年勅令第七十六号",
    "LawBody": {
      "LawTitle": "勲章佩用式",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（大勲位菊花章）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "大勲位菊花章ハ頸飾ヲ以テ喉下ニ佩ヒ其副章ヲ左肋ニ佩フ大綬ヲ以テ佩フル時ハ右肩ヨリ左脇ヘ垂レ其副章ハ左肋ニ佩フ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（宝冠章）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "宝冠章ノ佩用ハ次ニ定ムル所ニ依ル"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "宝冠大綬章ハ大綬ヲ以テ右肩ヨリ左脇ヘ垂レ其副章ヲ左肋ニ佩フ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "宝冠牡丹章宝冠白蝶章宝冠藤花章宝冠杏葉章及宝冠波光章ハ結蝶状ノ綬ヲ以テ左肋ニ佩フ"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（桐花大綬章）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "桐花大綬章ハ大綬ヲ以テ右肩ヨリ左脇ヘ垂レ其副章ヲ左肋ニ佩ブ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（旭日章）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "旭日章ノ佩用ハ次ニ定ムル所ニ依ル"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旭日大綬章ハ大綬ヲ以テ右肩ヨリ左脇ヘ垂レ其副章ヲ左肋ニ佩フ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旭日重光章ハ右肋ニ佩ヒ其副章ヲ中綬ヲ以テ喉下ニ佩フ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旭日中綬章ハ中綬ヲ以テ喉下ニ佩フ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "旭日小綬章旭日双光章及旭日単光章ハ小綬ヲ以テ左肋ニ佩フ"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（瑞宝章）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "瑞宝章ノ佩用ハ次ニ定ムル所ニ依ル"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "瑞宝大綬章ハ大綬ヲ以テ右肩ヨリ左脇ヘ垂レ其副章ヲ左肋ニ佩フ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "瑞宝重光章ハ右肋ニ佩ビ其副章ヲ中綬ヲ以テ喉下ニ佩ブ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "瑞宝中綬章ハ中綬ヲ以テ喉下ニ佩フ"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "瑞宝小綬章瑞宝双光章及瑞宝単光章ハ小綬ヲ以テ左肋ニ佩フ"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（勲章ノ併佩）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "別種ノ勲章ハ之ヲ併佩スルコトヲ得其大綬章ハ之ヲ併佩セス"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@AmendLawNum": "平成一四年八月一二日政令第二七七号",
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十五年五月一日から施行し、改正後の規定は、平成十五年十一月三日以後の日付をもって授与される勲章から適用する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphCaption": "（経過措置）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令による改正前の規定により授与された勲章及び平成十五年十一月二日以前の日付をもって授与される勲章については、改正前の規定は、なおその効力を有する。"
              }
            }
          }
        ]
      }
    }
  }
}
