{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "ImperialOrder",
    "@Num": "1",
    "@Year": "21",
    "LawNum": "明治二十一年勅令第一号",
    "LawBody": {
      "LawTitle": "明治二十一年勅令第一号（宝冠章及大勲位菊花章頸飾ニ関スル件）",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "明治八年太政官布告第五十四号ニ定ムルノ外特別ノ場合婦人ノ勲労アル者ニ対シ同布告第一条ニ掲グル勲章ニ代ヘテ次ノ勲章ヲ賜フ"
                  }
                },
                "Item": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "宝冠大綬章"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "宝冠牡丹章"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "宝冠白蝶章"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "宝冠藤花章"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "宝冠杏葉章"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "宝冠波光章"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "宝冠大綬章、宝冠牡丹章、宝冠白蝶章、宝冠藤花章、宝冠杏葉章及宝冠波光章ノ章ハ宝冠ト竹桜ノ形ヲ以テ飾リ綬ハ地黄色双線紅色トス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "大勲位菊花章頸飾ハ大勲位ニ叙セシ者ニ特別之ヲ賜フ"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "菊花菊葉ノ形ト明治二字古篆文ヲ以テ飾ル"
                  }
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "@AmendLawNum": "平成一四年八月一二日政令第二七七号",
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成十五年五月一日から施行し、改正後の規定は、平成十五年十一月三日以後の日付をもって授与される勲章から適用する。"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphCaption": "（経過措置）",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令による改正前の規定により授与された勲章及び平成十五年十一月二日以前の日付をもって授与される勲章については、改正前の規定は、なおその効力を有する。"
              }
            }
          }
        ]
      }
    }
  }
}
