                              "@BorderTop": "solid",
                              "Sentence": {
                                "@WritingMode": "vertical",
                                "#text": "一年以上"
                              }
                            },
                            {
                              "@BorderBottom": "solid",
                              "@BorderLeft": "solid",
                              "@BorderRight": "solid",
                              "@BorderTop": "solid",
