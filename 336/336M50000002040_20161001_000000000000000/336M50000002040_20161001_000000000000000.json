    "@Num": "40",
    "@Year": "36",
    "LawNum": "昭和三十六年総理府令第四十号",
    "LawBody": {
      "LawTitle": "統合幕僚学校組織規則",
      "EnactStatement": "防衛庁設置法（昭和二十九年法律第百六十四号）第二十八条の二第七項の規定に基づき、統合幕僚学校組織規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（位置）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "統合幕僚学校（以下「学校」という。）は、東京都に置く。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（副校長）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "学校に、副校長一人を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "副校長は、自衛官をもつて充てる。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "副校長は、校長を助け、校務を整理する。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "副校長は、校長に事故があるとき、又は校長が欠けたときは、その職務を行う。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（内部組織）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "学校に、次の二課及び一室並びに国際平和協力センターを置く。"
              },
              "List": [
                {
                  "ListSentence": {
                    "Sentence": "企画室"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "総務課"
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": "教育課"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleCaption": "（企画室）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "企画室は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "教育訓練及び調査研究の総合的な企画及び調整に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "学校の組織及び定員に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "業務の能率的運営の調査及び業務の運営の改善に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（総務課）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "総務課は、次に掲げる事務をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "機密に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "学校の公印の保管に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "公文書の接受、発送、編集及び保存に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "職員及び学生（学校において教育訓練を受ける者をいう。以下同じ。）の人事及び給与に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "職員及び学生の福利厚生及び保健衛生に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "儀式及び広報に関すること（国際平和協力センターの所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "経費及び収入の予算、決算及び会計に関すること（国際平和協力センターの所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "行政財産及び物品の取得及び管理に関すること（国際平和協力センターの所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "9",
                  "ItemTitle": "九",
                  "ItemSentence": {
                    "Sentence": "教育訓練及び調査研究に関する資料の収集、整理及び保管に関すること（国際平和協力センターの所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "10",
                  "ItemTitle": "十",
                  "ItemSentence": {
                    "Sentence": "記録及び統計に関すること（教育課及び国際平和協力センターの所掌に属するものを除く。）。"
                  }
                },
                {
                  "@Num": "11",
                  "ItemTitle": "十一",
                  "ItemSentence": {
                    "Sentence": "前各号に掲げるもののほか、学校の所掌事務で他の所掌に属しない事項に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（教育課）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "教育課は、次に掲げる事務（国際平和協力センターの所掌に属するものを除く。）をつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "学生の教育訓練及び調査研究の計画に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "学生の教育訓練及び調査研究の実施に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "学生の教育訓練及び調査研究に必要な記録及び統計に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "前三号に掲げるもののほか、教育訓練及び調査研究に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（国際平和協力センター）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国際平和協力センターは、次に掲げる事務のうち自衛隊法（昭和二十九年法律第百六十五号）第三条第二項第二号の自衛隊の活動に関するものをつかさどる。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "広報に関すること。"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "経費及び収入の予算、決算及び会計に関すること。"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "物品の管理に関すること。"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "教育訓練及び調査研究に関する資料の収集、整理及び保管に関すること。"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "学生の教育訓練及び調査研究の計画に関すること。"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "学生の教育訓練及び調査研究の実施に関すること。"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "学生の教育訓練及び調査研究に必要な記録及び統計に関すること。"
                  }
                },
                {
                  "@Num": "8",
                  "ItemTitle": "八",
                  "ItemSentence": {
                    "Sentence": "前三号に掲げるもののほか、教育訓練及び調査研究に関すること。"
                  }
                }
              ]
            }
          },
          {
            "@Num": "8",
            "ArticleCaption": "（課長及び室長並びにセンター長）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "課に課長を、室に室長を、国際平和協力センターにセンター長を置く。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "課長若しくは室長又はセンター長は、校長の命を受け、課務若しくは室務又は国際平和協力センターの事務を掌理する。"
                }
              }
            ]
          },
          {
            "@Num": "9",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令に定めるもののほか、学校の内部組織に関し必要な事項は、防衛大臣が定める。"
              }
            }
          }
        ]
      },
