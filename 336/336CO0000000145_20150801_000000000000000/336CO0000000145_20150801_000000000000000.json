            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和六十年七月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成七年一〇月三一日政令第三六五号",
          "@Extract": "true",
          "@Type": "Amend",
