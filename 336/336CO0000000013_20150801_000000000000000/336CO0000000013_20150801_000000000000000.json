                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "厚生労働大臣が定めた業務の停止の期間を経過していない薬剤師に係る処分"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
