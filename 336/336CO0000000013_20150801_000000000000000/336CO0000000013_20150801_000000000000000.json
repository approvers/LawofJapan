              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十六条第一項に規定する政令で定める手数料の額は、六千八百円とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
