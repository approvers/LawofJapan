    "LawNum": "大正四年法律第十八号",
    "LawBody": {
      "LawTitle": "大正四年法律第十八号（法人ノ役員処罰ニ関スル法律）",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "法人ノ業務ヲ執行スル社員、取締役、会計参与、執行役、理事、監査役又ハ監事ニシテ刑事訴追又ハ刑ノ執行ヲ免レシムル為合併其ノ他ノ方法ニ依リ法人ヲ消滅セシメタル者ハ五年以下ノ懲役ニ処ス"
            }
          }
        }
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本法ハ大正四年七月一日ヨリ之ヲ施行ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一四年五月二九日法律第四五号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から起算して一年を超えない範囲内において政令で定める日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一七年七月二六日法律第八七号",
          "@Extract": "true",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、会社法の施行の日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
