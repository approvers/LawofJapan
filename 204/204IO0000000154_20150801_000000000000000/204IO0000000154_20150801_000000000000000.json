    "LawNum": "大正四年勅令第百五十四号",
    "LawBody": {
      "LawTitle": "大正四年勅令第百五十四号（大礼記念章制定ノ件）",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "大礼記念ノ表章トシテ記念章ヲ設ク"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "記念章ノ図式左ノ如シ"
                },
                "List": [
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "章"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "銀円形径一寸輪廓内表面上部ニ金菊御紋、両側縁ニ桜橘枝ト万歳旛交架ノ図、裏面ニ大礼記念章大正四年十一月ノ文字ヲ識ス"
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "環"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "銀彎形"
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@Num": "1",
                          "Sentence": "綬"
                        },
                        {
                          "@Num": "2",
                          "Sentence": "織地幅一寸二分、中央紅色、両縁白色紅線"
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "記念章ハ綬ヲ以テ左肋ニ佩フ"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "記念章ハ左ニ掲クル者ニ之ヲ授与ス"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "践祚ノ式ニ召サレタル者"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "即位礼及大嘗祭ノ式ニ召サレタル者"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "各所在地ニ於テ饗饌ヲ賜リタル者"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "大礼ノ事務及大礼ニ伴フ要務ニ関与シタル者"
                  }
                }
              ]
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "記念章ハ本人ニ限リ終身之ヲ佩用シ子孫ヲシテ之ヲ保存セシム"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "記念章ヲ授与セラルヘキ者其ノ授与前死亡シタルトキハ之ヲ其ノ家督相続人又ハ戸主ニ交付シテ保存セシム"
              }
            }
          }
        ]
      },
      "AppdxStyle": {
        "AppdxStyleTitle": "大礼記念章ノ図",
