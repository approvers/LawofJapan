    "LawNum": "昭和五十九年政令第百七十九号",
    "LawBody": {
      "LawTitle": "最低賃金法第三十五条第二項の地方運輸局を定める政令",
      "EnactStatement": "内閣は、最低賃金法（昭和三十四年法律第百三十七号）第四十条の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "最低賃金法第三十五条第二項の政令で定める地方運輸局は、近畿運輸局とする。"
            }
          }
        }
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和五十九年七月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年四月二五日政令第一五一号",
          "@Extract": "false",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、最低賃金法の一部を改正する法律の施行の日（平成二十年七月一日）から施行する。"
              }
            }
          }
        }
      ]
    }
