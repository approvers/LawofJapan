{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "222",
    "@Year": "59",
    "LawNum": "昭和五十九年政令第二百二十二号",
    "LawBody": {
      "LawTitle": "公証人法第十三条ノ二の審議会等を定める政令",
      "EnactStatement": "内閣は、公証人法（明治四十一年法律第五十三号）第十三条ノ二の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "公証人法第十三条ノ二の政令で定める審議会等は、検察官・公証人特別任用等審査会とする。"
            }
          }
        }
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和五十九年七月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三〇五号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、附則第三項の規定は、公布の日から施行する。"
                  }
                ]
              }
            },
            {
              "@Hide": "false",
              "@Num": "3",
              "ParagraphCaption": "（委員等の任期に関する経過措置）",
              "ParagraphNum": "３",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令の施行の日の前日において従前の総理府の検察官適格審査会の委員及び予備委員である者の任期、従前の法務省の法制審議会の委員、部会に置かれた委員及び幹事である者の任期並びに従前の法務省の公証人審査会の委員及び予備委員である者の任期は、当該委員、部会に置かれた委員、予備委員及び幹事の任期を定めたそれぞれの政令の規定にかかわらず、その日に満了する。"
                }
              }
            }
          ]
        },
        {
          "@AmendLawNum": "平成一五年一二月三日政令第四七七号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十六年一月一日から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
