          "ArticleCaption": "（施行期日）",
          "ArticleTitle": "第一条",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@Function": "main",
                "@Num": "1",
                "@WritingMode": "vertical",
                "#text": "この政令は、改正法の施行の日（平成二十二年十月一日）から施行する。"
              }
            }
          }
        }
      }
    }
  }
