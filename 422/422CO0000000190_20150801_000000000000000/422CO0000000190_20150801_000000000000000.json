                      }
                    },
                    {
                      "@BorderBottom": "solid",
                      "@BorderLeft": "solid",
                      "@BorderRight": "solid",
                      "@BorderTop": "solid",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "適用すべき措置"
                      }
                    }
                  ]
                },
                {
                  "TableColumn": [
                    {
