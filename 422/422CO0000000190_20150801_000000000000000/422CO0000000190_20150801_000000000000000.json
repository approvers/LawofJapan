                      "@BorderLeft": "solid",
                      "@BorderRight": "solid",
                      "@BorderTop": "solid",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "平成二十二年六月十一日から七月十九日までの間の豪雨による災害"
                      }
                    },
