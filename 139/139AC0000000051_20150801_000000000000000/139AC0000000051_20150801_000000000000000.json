    "LawNum": "明治三十九年法律第五十一号",
    "LawBody": {
      "LawTitle": "紙幣類似証券取締法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "一様ノ形式ヲ具ヘ箇々ノ取引ニ基カスシテ金額ヲ定メ多数ニ発行シタル証券ニシテ紙幣類似ノ作用ヲ為スモノト認ムルトキハ財務大臣ニ於テ其ノ発行及流通ヲ禁止スルコトヲ得"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ規定ハ一様ノ価格ヲ表示シテ物品ノ給付ヲ約束スル証券ニ付之ヲ準用ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前条ニ依リ証券ノ発行及流通ヲ禁止シタルトキハ財務大臣ハ直ニ其ノ旨ヲ公告ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "禁止ノ公告後ニ発行シ又ハ流通セシムルノ目的ヲ以テ授受シタル証券ハ無効トス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "禁止ニ違反シテ証券ヲ発行シ又ハ其ノ証券ヲ授受シタル者ハ一年以下ノ重禁錮又ハ千円以下ノ罰金ニ処シ其ノ証券ヲ没収ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "禁止ニ違反シテ証券ヲ流通セシムルノ目的ヲ以テ授受シタル者ノ罰亦前項ニ同シ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "禁止ノ公告後ニ発行シ又ハ流通セシムルノ目的ヲ以テ授受シタル証券ハ裁判ニ依リ没収スル場合ヲ除クノ外何人ノ所有ヲ問ハス行政処分ヲ以テ之ヲ官没ス"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@AmendLawNum": "平成一一年一二月二二日法律第一六〇号",
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Article": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "1",
          "ArticleCaption": "（施行期日）",
          "ArticleTitle": "第一条",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この法律（第二条及び第三条を除く。）は、平成十三年一月六日から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、次の各号に掲げる規定は、当該各号に定める日から施行する。"
                }
              ]
            },
            "Item": {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Column": [
                  {
                    "@LineBreak": "false",
                    "@Num": "1",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "第九百九十五条（核原料物質、核燃料物質及び原子炉の規制に関する法律の一部を改正する法律附則の改正規定に係る部分に限る。）、第千三百五条、第千三百六条、第千三百二十四条第二項、第千三百二十六条第二項及び第千三百四十四条の規定"
                    }
                  },
                  {
                    "@LineBreak": "false",
                    "@Num": "2",
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "公布の日"
                    }
                  }
                ]
              }
            }
          }
        }
      }
    }
