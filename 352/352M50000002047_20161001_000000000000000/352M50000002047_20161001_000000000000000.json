{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "047",
    "@Year": "52",
    "LawNum": "昭和五十二年総理府令第四十七号",
    "LawBody": {
      "LawTitle": "沖縄位置境界明確化審議会規則",
      "EnactStatement": "沖縄開発庁設置法（昭和四十七年法律第二十九号）第十二条第三項の規定に基づき、沖縄位置境界明確化審議会規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "沖縄位置境界明確化審議会（以下「審議会」という。）は、委員十人以内で組織する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "委員は、学識経験のある者のうちから、内閣総理大臣が任命する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "委員は、非常勤とする。"
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ArticleCaption": "（委員の任期）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "委員は、再任されることができる。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会に会長を置き、委員の互選によつてこれを定める。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "会長は、会務を総理する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "会長に事故があるときは、会長があらかじめ指名する委員が、その職務を代理する。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（専門委員）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "専門の事項を調査審議するため必要があるときは、審議会に、専門委員を置くことができる。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "専門委員は、学識経験のある者のうちから、内閣総理大臣が任命する。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "専門委員は、当該専門の事項に関する調査審議が終了したときは、解任されるものとする。"
                }
              },
              {
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": "専門委員は、非常勤とする。"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleCaption": "（会議）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "審議会の会議は、会長が招集する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "審議会は、委員の過半数が出席しなければ、会議を開き、議決をすることができない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "審議会の議事は、出席委員の過半数で決し、可否同数のときは、会長の決するところによる。"
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "審議会の庶務は、沖縄総合事務局総務部跡地利用対策課において処理する。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この府令に定めるもののほか、議事の手続その他審議会の運営に関し必要な事項は、会長が審議会に諮つて定める。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成九年四月一日総理府令第一三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この府令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年八月一四日　平成一三年内閣府令第六号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphCaption": "（施行期日）",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この中央省庁等改革推進本部令（次項において「本部令」という。）は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphCaption": "（この本部令の効力）",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "この本部令は、その施行の日に、中央省庁等改革のための内閣府組織関係命令の整備に関する命令（平成十三年内閣府令第六号）となるものとする。"
              }
            }
          ]
        }
      ]
    }
  }
}
