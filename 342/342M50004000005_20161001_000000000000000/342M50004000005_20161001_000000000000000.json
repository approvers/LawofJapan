                        "ItemSentence": {
                          "Sentence": "請求に係るダム使用権の設定番号"
                        }
                      },
                      {
                        "@Num": "3",
                        "ItemTitle": "三",
                        "ItemSentence": {
                          "Sentence": "ダム使用権登録簿の謄本又は抄本の交付を請求するときは、謄本又は抄本の数"
                        }
                      },
                      {
                        "@Num": "4",
                        "ItemTitle": "四",
                        "ItemSentence": {
                          "Sentence": "ダム使用権登録簿の抄本の交付を請求するときは、その請求する部分"
                        }
                      },
                      {
                        "@Num": "5",
                        "ItemTitle": "五",
                        "ItemSentence": {
                          "Sentence": "手数料の額"
                        }
                      },
                      {
                        "@Num": "6",
                        "ItemTitle": "六",
                        "ItemSentence": {
                          "Sentence": "請求する者の氏名及び住所（法人にあつては、その名称及び住所）"
                        }
                      },
                      {
                        "@Num": "7",
                        "ItemTitle": "七",
                        "ItemSentence": {
                          "Sentence": "請求の年月日"
                        }
                      }
