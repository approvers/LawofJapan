        },
        {
          "@AmendLawNum": "昭和六二年三月二〇日政令第五四号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
