              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、法の施行の日（昭和四十四年六月十四日）から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四五年一二月二日政令第三三三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
