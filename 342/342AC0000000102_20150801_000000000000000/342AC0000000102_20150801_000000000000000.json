          "@AmendLawNum": "昭和四三年六月一五日法律第一〇一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律（第一条を除く。）は、新法の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和五三年五月二三日法律第五五号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日等）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この法律は、公布の日から施行する。"
