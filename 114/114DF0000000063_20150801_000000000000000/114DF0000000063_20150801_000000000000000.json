    "LawBody": {
      "LawTitle": "明治十四年太政官布告第六十三号（褒章条例）",
      "Preamble": {
        "Paragraph": [
          {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "褒章条例別紙ノ通相定来明治十五年一月一日ヨリ之ヲ施行ス"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "2",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "右奉　勅旨布告候事"
              }
            }
          },
          {
            "@Hide": "false",
            "@Num": "3",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "（別紙）"
              }
            }
          }
        ]
      },
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "凡ソ自己ノ危難ヲ顧ミス人命ノ救助ニ尽力シタル者又ハ自ラ進デ社会ニ奉仕スル活動ニ従事シ徳行顕著ナル者又ハ業務ニ精励シ衆民ノ模範タルヘキ者又ハ学術芸術上ノ発明改良創作ニ関シ事績著明ナル者又ハ教育衛生慈善防疫ノ事業、学校病院ノ建設、道路河渠堤防橋梁ノ修築、田野ノ墾闢、森林ノ栽培、水産ノ繁殖、農商工業ノ発達ニ関シ公衆ノ利益ヲ興シ成績著明ナル者又ハ公同ノ事務ニ勤勉シ労効顕著ナル者又ハ公益ノ為私財ヲ寄附シ功績顕著ナル者ヲ表彰スル為左ノ六種ノ褒章ヲ定ム"
                }
              },
              "List": [
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "紅綬褒章"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "右自己ノ危難ヲ顧ミス人命ノ救助ニ尽力シタル者ニ賜フモノトス"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "緑綬褒章"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "右自ラ進デ社会ニ奉仕スル活動ニ従事シ徳行顕著ナル者ニ賜フモノトス"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "黄綬褒章"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "右業務ニ精励シ衆民ノ模範タルベキ者ニ賜フモノトス"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "紫綬褒章"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "右学術芸術上ノ発明改良創作ニ関シ事績著明ナル者ニ賜フモノトス"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "藍綬褒章"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "右教育衛生慈善防疫ノ事業、学校病院ノ建設、道路河渠堤防橋梁ノ修築、田野ノ墾闢、森林ノ栽培、水産ノ繁殖、農商工業ノ発達ニ関シ公衆ノ利益ヲ興シ成績著明ナル者又ハ公同ノ事務ニ勤勉シ労効顕著ナル者ニ賜フモノトス"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "紺綬褒章"
                    }
                  }
                },
                {
                  "ListSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "右公益ノ為私財ヲ寄附シ功績顕著ナル者ニ賜フモノトス"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本条例ニ依リ表彰セラルヘキ者団体ナルトキハ褒状ヲ賜フ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "已ニ褒章ヲ賜ハリタルモノ再度以上同様ノ実行アリテ褒章ヲ賜フヘキトキハ其都度飾版一箇ヲ賜与シ其章ノ綬ニ附加セシメ以テ標識トス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項ノ飾版五箇以上ニ達シタルトキハ五箇毎ニ別種ノ飾版一箇ヲ引替ヘ賜与ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "褒章ハ本人ニ限リ終身之ヲ佩用スルコトヲ得"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "第一条ノ規定ニ依リ褒章ヲ賜フヘキ者ニハ褒章ト金銀木杯トヲ併セ賜フコトアルヘシ"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "本条例ニ依リ表彰セラルヘキ者死亡シタルトキハ金銀木杯又ハ褒状ヲ其遺族ニ賜ヒ之ヲ追賞ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "褒章ノ製式ハ次ノ通トス"
                }
              },
              "TableStruct": {
                "Table": {
