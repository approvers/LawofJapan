            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、廃止法の施行の日（平成十九年八月十日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年九月一四日政令第二八七号",
