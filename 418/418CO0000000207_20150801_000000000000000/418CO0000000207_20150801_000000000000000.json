            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、平成十九年四月一日から施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年三月三一日政令第一一八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
