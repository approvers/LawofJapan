                "Paragraph": [
                  {
                    "@Num": "1",
                    "ParagraphNum": null,
                    "ParagraphSentence": {
                      "Sentence": "多数の者が共同して審査要求をしようとするときは、三人を超えない総代を互選することができる。"
                    }
                  },
                  {
                    "@Num": "2",
                    "ParagraphNum": "２",
                    "ParagraphSentence": {
                      "Sentence": "会計検査院は、共同審査要求人が総代を互選しない場合において、必要があると認めるときは、総代の互選を求めることができる。"
                    }
                  },
                  {
                    "@Num": "3",
                    "ParagraphNum": "３",
                    "ParagraphSentence": {
                      "Sentence": "総代は、各自、他の共同審査要求人のために、審査要求の取下げを除き、当該審査要求に関する一切の行為をすることができる。"
                    }
                  },
                  {
                    "@Num": "4",
                    "ParagraphNum": "４",
                    "ParagraphSentence": {
                      "Sentence": "共同審査要求人は、総代が選任されている場合は、総代を通じてのみ前項の行為をすることができる。"
                    }
                  },
                  {
                    "@Num": "5",
                    "ParagraphNum": "５",
                    "ParagraphSentence": {
                      "Sentence": "共同審査要求人に対する会計検査院の通知その他の行為は、二人以上の総代が選任されている場合においても、一人の総代に対してすれば足りる。"
                    }
                  },
                  {
                    "@Num": "6",
                    "ParagraphNum": "６",
                    "ParagraphSentence": {
                      "Sentence": "共同審査要求人は、必要があると認めるときは、総代を解任することができる。"
                    }
                  }
                ]
              },
