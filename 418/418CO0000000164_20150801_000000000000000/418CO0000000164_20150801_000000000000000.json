                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "独立行政法人労働安全衛生総合研究所の役員（平成十八年三月三十一日までの間は、産業医学総合研究所の役員）"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "ItemTitle": "四",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "学識経験のある者"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "二人"
                            }
                          }
                        ]
                      }
                    }
                  ]
                },
                {
                  "@Hide": "false",
                  "@Num": "2",
                  "ParagraphNum": "２",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "整備法附則第九条第二項の規定による評価は、同項の評価委員の過半数の一致によるものとする。"
                    }
                  }
                },
                {
                  "@Hide": "false",
                  "@Num": "3",
                  "ParagraphNum": "３",
                  "ParagraphSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "整備法附則第九条第二項の規定による評価に関する庶務は、厚生労働省労働基準局安全衛生部計画課において処理する。"
                    }
                  }
                }
              ]
            }
          ]
        }
      },
      "SupplProvision": {
