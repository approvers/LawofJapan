                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
                          }
                        ]
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "ItemTitle": "二",
                      "ItemSentence": {
                        "Column": [
                          {
                            "@LineBreak": "false",
                            "@Num": "1",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "厚生労働省の職員"
                            }
                          },
                          {
                            "@LineBreak": "false",
                            "@Num": "2",
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "一人"
                            }
