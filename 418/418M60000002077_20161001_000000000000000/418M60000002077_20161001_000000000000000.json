{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "77",
    "@Year": "18",
    "LawNum": "平成十八年内閣府令第七十七号",
    "LawBody": {
      "LawTitle": "中心市街地の活性化に関する法律施行規則",
      "EnactStatement": "中心市街地の活性化に関する法律（平成十年法律第九十二号）第十一条第一項の規定に基づき、中心市街地の活性化に関する法律施行規則を次のように定める。",
      "MainProvision": {
        "Article": {
          "@Num": "1",
          "ArticleTitle": "第一条",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "法第十一条第一項の内閣府令で定める軽微な変更は、次に掲げるものとする。"
            },
            "Item": [
              {
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Sentence": "地域の名称の変更又は地番の変更に伴う範囲の変更"
                }
              },
              {
                "@Num": "2",
                "ItemTitle": "二",
                "ItemSentence": {
                  "Sentence": "基本計画に定められた事業及び措置の実施期間に影響を与えない場合における計画期間の六月以内の変更"
                }
              },
              {
                "@Num": "3",
                "ItemTitle": "三",
                "ItemSentence": {
                  "Sentence": "前二号に掲げるもののほか、基本計画の実施に支障がないと内閣総理大臣が認める変更"
                }
              }
            ]
          }
        }
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この府令は、法の施行の日（平成十八年八月二十二日）から施行する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（沖縄総合事務局組織規則の一部改正）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "沖縄総合事務局組織規則（平成十三年内閣府令第四号）の一部を次のように改正する。"
              },
              "AmendProvision": {
                "AmendProvisionSentence": {
                  "Sentence": "第八十四条第四号中「中心市街地における市街地の整備改善及び商業等の活性化の一体的推進に関する法律」を「中心市街地の活性化に関する法律」に、「第四条第四項第五号」を「第七条第九項第四号」に改める。"
                }
              }
            }
          }
        ]
      }
    }
  }
}
