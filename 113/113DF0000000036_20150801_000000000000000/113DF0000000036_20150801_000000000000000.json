              "ChapterTitle": "第二章　刑例",
              "Section": {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "3",
                "SectionTitle": "第三節　附加刑処分",
                "Article": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "31",
                    "ArticleTitle": "第三十一条",
                    "Paragraph": {
                      "@Hide": "false",
                      "@Num": "1",
                      "@OldStyle": "false",
                      "ParagraphNum": null,
                      "ParagraphSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "剥奪公権ハ左ノ権ヲ剥奪ス"
                        }
                      },
                      "Item": [
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "1",
                          "ItemTitle": "一",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "国民ノ特権"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "2",
                          "ItemTitle": "二",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "官吏ト為ルノ権"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "3",
                          "ItemTitle": "三",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "勲章年金位記貴号恩給ヲ有スルノ権"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "4",
                          "ItemTitle": "四",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "外国ノ勲章ヲ佩用スルノ権"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "5",
                          "ItemTitle": "五",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "兵籍ニ入ルノ権"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "6",
                          "ItemTitle": "六",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "裁判所ニ於テ証人ト為ルノ権但単ニ事実ヲ陳述スルハ此限ニ在ラス"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "7",
                          "ItemTitle": "七",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "後見人ト為ルノ権但親属ノ許可ヲ得テ子孫ノ為メニスルハ此限ニ在ラス"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "8",
                          "ItemTitle": "八",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "分散者ノ管財人ト為リ又ハ会社及ヒ共有財産ヲ管理スルノ権"
                            }
                          }
                        },
                        {
                          "@Delete": "false",
                          "@Hide": "false",
                          "@Num": "9",
                          "ItemTitle": "九",
                          "ItemSentence": {
                            "Sentence": {
                              "@WritingMode": "vertical",
                              "#text": "学校長及ヒ教師学監ト為ルノ権"
                            }
                          }
                        }
                      ]
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "33",
                    "ArticleTitle": "第三十三条",
                    "Paragraph": {
                      "@Hide": "false",
                      "@Num": "1",
                      "@OldStyle": "false",
                      "ParagraphNum": null,
                      "ParagraphSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "禁錮ニ処セラレタル者ハ別ニ宣告ヲ用ヒス現任ノ官職ヲ失ヒ及ヒ其刑期間公権ヲ行フコトヲ停止ス"
                        }
                      }
                    }
                  }
                ]
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "PartTitle": "第二編　公益ニ関スル重罪軽罪",
            "Chapter": {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "4",
              "ChapterTitle": "第四章　信用ヲ害スル罪",
              "Section": {
                "@Delete": "false",
                "@Hide": "false",
                "@Num": "9",
                "SectionTitle": "第九節　公選ノ投票ヲ偽造スル罪",
                "Article": [
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "233",
                    "ArticleTitle": "第二百三十三条",
                    "Paragraph": {
                      "@Hide": "false",
                      "@Num": "1",
                      "@OldStyle": "false",
                      "ParagraphNum": null,
                      "ParagraphSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "公選ノ投票ヲ偽造シ又ハ其数ヲ増減シタル者ハ一月以上一年以下ノ軽禁錮ニ処シ二円以上二十円以下ノ罰金ヲ附加ス"
                        }
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "234",
                    "ArticleTitle": "第二百三十四条",
                    "Paragraph": {
                      "@Hide": "false",
                      "@Num": "1",
                      "@OldStyle": "false",
                      "ParagraphNum": null,
                      "ParagraphSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "賄賂ヲ以テ投票ヲ為サシメ又ハ賄賂ヲ受ケテ投票ヲ為シタル者ハ二月以上二年以下ノ軽禁錮ニ処シ三円以上三十円以下ノ罰金ヲ附加ス"
                        }
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "235",
                    "ArticleTitle": "第二百三十五条",
                    "Paragraph": {
                      "@Hide": "false",
                      "@Num": "1",
                      "@OldStyle": "false",
                      "ParagraphNum": null,
                      "ParagraphSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "投票ヲ検査シ及ヒ其数ヲ計算スル者其投票ヲ偽造シ又ハ増減シタル時ハ六月以上三年以下ノ軽禁錮ニ処シ四円以上四十円以下ノ罰金ヲ附加ス"
                        }
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "236",
                    "ArticleTitle": "第二百三十六条",
                    "Paragraph": {
                      "@Hide": "false",
                      "@Num": "1",
                      "@OldStyle": "false",
                      "ParagraphNum": null,
                      "ParagraphSentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "調書ヲ造リ投票ノ結局ヲ報告スル者其数ヲ増減シ其他詐偽ノ所為アル時ハ一年以上五年以下ノ軽禁錮ニ処シ五円以上五十円以下ノ罰金ヲ附加ス"
                        }
                      }
                    }
                  }
                ]
              }
            }
          }
        ]
      }
