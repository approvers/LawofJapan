          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "国家公務員法第二十五条第一項の政令で指定する機関は、次のとおりとする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "会計検査院"
                }
              }
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
