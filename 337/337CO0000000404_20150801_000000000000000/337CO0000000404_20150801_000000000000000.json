{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "404",
    "@Year": "37",
    "LawNum": "昭和三十七年政令第四百四号",
    "LawBody": {
      "LawTitle": "都市の美観風致を維持するための樹木の保存に関する法律施行令",
      "EnactStatement": "内閣は、都市の美観風致を維持するための樹木の保存に関する法律（昭和三十七年法律第百四十二号）第二条第一項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "都市の美観風致を維持するための樹木の保存に関する法律第二条第一項の政令で定める基準は、次のとおりとする。"
            }
          },
          "Item": [
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "樹木については、次のいずれかに該当し、健全で、かつ、樹容が美観上特にすぐれていること。"
                }
              },
              "Subitem1": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "Subitem1Title": "イ",
                  "Subitem1Sentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "一・五メートルの高さにおける幹の周囲が一・五メートル以上であること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "Subitem1Title": "ロ",
                  "Subitem1Sentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "高さが十五メートル以上であること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "Subitem1Title": "ハ",
                  "Subitem1Sentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "株立ちした樹木で、高さが三メートル以上であること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "Subitem1Title": "ニ",
                  "Subitem1Sentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "Ruby": {
                        "Rt": "はん",
                        "#text": "攀"
                      },
                      "#text": "登性樹木で、枝葉の面積が三十平方メートル以上であること。"
                    }
                  }
                }
              ]
            },
            {
              "@Delete": "false",
              "@Hide": "false",
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "樹木の集団については、次のいずれかに該当し、その集団に属する樹木が健全で、かつ、その集団の樹容が美観上特にすぐれていること。"
                }
              },
              "Subitem1": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "Subitem1Title": "イ",
                  "Subitem1Sentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "その集団の存する土地の面積が五百平方メートル以上であること。"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "Subitem1Title": "ロ",
                  "Subitem1Sentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "いけがきをなす樹木の集団で、そのいけがきの長さが三十メートル以上であること。"
                    }
                  }
                }
              ]
            }
          ]
        }
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、公布の日から施行する。"
            }
          }
        }
      }
    }
  }
}
