            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、昭和五十五年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成元年一二月二二日法律第八四号",
