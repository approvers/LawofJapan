            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、平成十三年一月六日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三〇年三月三一日財務省令第二一号",
          "@Type": "Amend",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
