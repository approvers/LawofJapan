            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四三年六月一五日法律第一〇一号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
