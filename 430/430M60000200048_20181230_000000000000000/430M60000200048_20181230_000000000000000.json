                    },
                    {
                      "@Align": "center",
                      "@Valign": "middle",
                      "Sentence": "全重量に占める添加物の割合"
                    }
                  ]
                },
                {
                  "TableColumn": [
                    {
                      "@Valign": "middle",
                      "Sentence": "食用青色一号（別名ブリリアントブル―ＦＣＦ）"
                    },
                    {
                      "@Valign": "middle",
                      "@rowspan": "2",
                      "Sentence": "〇・〇〇一二パーセント"
                    }
                  ]
                },
                {
                  "TableColumn": {
                    "@Valign": "middle",
                    "Sentence": "食用青色二号（別名インジゴカルミン）"
                  }
                }
              ]
            }
          }
        }
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この省令は、環太平洋パートナーシップ協定の締結及び環太平洋パートナーシップに関する包括的及び先進的な協定の締結に伴う関係法律の整備に関する法律（平成二十八年法律第百八号）の施行の日から施行する。"
          }
        }
