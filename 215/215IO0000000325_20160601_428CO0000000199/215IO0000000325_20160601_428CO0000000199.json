            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "叙位条例ハ之ヲ廃止ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二二年五月三日政令第四号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から、これを施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年四月一五日政令第一九九号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、刑法等の一部を改正する法律の施行の日（平成二十八年六月一日）から施行する。"
              }
            }
          }
        }
      ]
    }
