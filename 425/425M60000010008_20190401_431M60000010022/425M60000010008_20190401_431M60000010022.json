            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "矯正管区に、それぞれ首席管区監査官一人及び管区監査官一人（東京矯正管区にあっては、二人）を置く。"
                  }
                }
