                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "東京矯正管区及び大阪矯正管区に、それぞれ矯正就労支援情報センター室を置く。"
                  }
                }
              },
