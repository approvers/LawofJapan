      "MainProvision": {
        "Chapter": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "2",
          "ChapterTitle": "第二章　経過措置",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "11",
            "ArticleCaption": "（財団法人地方自治情報センターの解散の登記の嘱託等）",
            "ArticleTitle": "第十一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "地方公共団体情報システム機構法附則第五条第一項の規定により昭和四十五年五月一日に設立された財団法人地方自治情報センターが解散したときは、総務大臣は、遅滞なく、その解散の登記を登記所に嘱託しなければならない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "登記官は、前項の規定による嘱託に係る解散の登記をしたときは、その登記記録を閉鎖しなければならない。"
                  }
                }
              }
            ]
          }
        }
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、平成二十六年四月一日から施行する。"
            }
          }
        }
      }
    }
  }
}
