{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "5",
    "@Year": "25",
    "LawNum": "平成二十五年環境省令第五号",
    "LawBody": {
      "LawTitle": "使用済小型電子機器等の再資源化の促進に関する法律施行令第四条に規定する委託の基準に関する省令",
      "EnactStatement": "使用済小型電子機器等の再資源化の促進に関する法律施行令（平成二十五年政令第四十五号）第四条の規定に基づき、使用済小型電子機器等の再資源化の促進に関する法律施行令第四条に規定する委託の基準に関する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（承諾に係る書面の記載事項）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "使用済小型電子機器等の再資源化の促進に関する法律施行令（以下「令」という。）第四条第一号の環境省令で定める事項は、次のとおりとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "委託に係る使用済小型電子機器等（廃棄物の処理及び清掃に関する法律（昭和四十五年法律第百三十七号）第二条第四項に規定する産業廃棄物であるものに限る。以下同じ。）の数量"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "委託を行う認定事業者（以下単に「認定事業者」という。）の氏名又は名称、住所及び認定番号"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "承諾の年月日"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "使用済小型電子機器等の再資源化の促進に関する法律（平成二十四年法律第五十七号）第十三条第二項に規定する行為を委託しようとする者（以下「委託先」という。）の氏名又は名称及び住所"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（委託契約に含まれるべき事項）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "令第四条第二号ニの環境省令で定める事項は、次のとおりとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "委託契約の有効期間"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "認定事業者が委託先に支払う料金"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "使用済小型電子機器等の運搬に係る委託契約にあっては、委託先が当該委託契約に係る使用済小型電子機器等の積替え又は保管を行う場合には、当該積替え又は保管を行う場所の所在地及び当該場所に係る積替えのための保管上限"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "認定事業者の有する委託に係る使用済小型電子機器等の適正な処理のために必要な次に掲げる事項に関する情報"
                  },
                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "当該使用済小型電子機器等の荷姿に関する事項"
                      }
                    },
                    {
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": "他の廃棄物（廃棄物の処理及び清掃に関する法律第二条第一項に規定する廃棄物をいう。）との混合等により生ずる支障に関する事項"
                      }
                    },
                    {
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": "当該使用済小型電子機器等が廃パーソナルコンピュータ又は廃電子レンジであって、日本工業規格Ｃ〇九五〇号に規定する含有マークが付されたものである場合には、当該含有マークの表示に関する事項"
                      }
                    },
                    {
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": "その他当該使用済小型電子機器等を取り扱う際に注意すべき事項"
                      }
                    }
                  ]
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "委託契約の有効期間中に当該使用済小型電子機器等に係る前号の情報に変更があった場合の当該情報の伝達方法に関する事項"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "委託契約に係る業務終了時の委託先の認定事業者への報告に関する事項"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "委託契約を解除した場合の処理されない使用済小型電子機器等の取扱いに関する事項"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（委託契約書の保存期間）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "令第四条第三号の環境省令で定める期間は、五年とする。"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphCaption": "（施行期日）",
          "ParagraphNum": "１",
          "ParagraphSentence": {
            "Sentence": "この省令は、使用済小型電子機器等の再資源化の促進に関する法律施行令（平成二十五年政令第四十五号）の施行の日（平成二十五年四月一日）から施行する。"
          }
        }
      }
    }
  }
}
