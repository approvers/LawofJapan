{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "14",
    "@Year": "25",
    "LawNum": "平成二十五年法律第十四号",
    "LawBody": {
      "LawTitle": "外国政府に対して有する米穀の売渡しに係る債権の免除に関する特別措置法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（趣旨）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、国際的協調の下で対外債務の負担の軽減を図ることとされている国について、その負担の軽減を図るため、これらの国の政府に対して我が国が有する米穀の売渡しに係る債権であって当該政府が弁済することができる見込みがないと認められるものについての特別の措置を定めるものとする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（米穀の売渡しに係る債権の免除）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "政府は、この法律の施行前に米穀の売渡しにより取得した債権であって、マダガスカル、マリ、モザンビーク、シエラレオネ又はタンザニアの政府に対して有するものについては、当該政府からの要請があったときは、当該債権の全部を免除することができる。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この法律は、公布の日から施行する。"
            }
          }
        }
      }
    }
  }
}
