    "@Num": "011",
    "@Year": "24",
    "LawNum": "昭和二十四年法務府令第十一号",
    "LawBody": {
      "LawTitle": "確定日附簿及び日附印章調製規則",
      "EnactStatement": "確定日附簿及び日附印章調製規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "登記所及び公証人役場に備えるべき確定日附簿及び日附のある印章は、附録様式により調製しなければならない。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "登記所に備えるべき確定日附簿には、法務局又は地方法務局の長において、その枚数を表紙の裏面に記載し、職氏名を署し、職印を押し、且つ、毎葉のつづり目に職印で契印をしなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の規定は、公証人役場に備えるべき確定日附簿につき、当該公証人に準用する。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この府令は、公布の日から施行する。"
            }
          },
          {
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": "確定日附簿及び日附印章調製方（明治三十一年司法省令第七号）は、廃止する。"
            }
          },
          {
            "@Num": "3",
            "ParagraphNum": "３",
            "ParagraphSentence": {
              "Sentence": "この府令施行の際現に存する帳簿又は印章に限り、この府令の規定にかかわらず、当分の間使用することを妨げない。"
            }
          }
        ]
      },
      "AppdxStyle": {
        "AppdxStyleTitle": "附録",
