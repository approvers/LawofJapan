    "@LawType": "Act",
    "@Num": "83",
    "@Year": "24",
    "LawNum": "昭和二十四年法律第八十三号",
    "LawBody": {
      "LawTitle": "公共企業体労働関係法の施行に関する法律　抄",
      "Preamble": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "公共企業体労働関係法の施行に関する法律"
            }
          }
        }
      },
      "MainProvision": {
        "Article": {
          "@Delete": "false",
          "@Hide": "false",
          "@Num": "1",
          "ArticleCaption": "（職員の団体の経過措置）",
          "ArticleTitle": "第一条",
          "Paragraph": [
            {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "公共企業体労働関係法（昭和二十三年法律第二百五十七号）（以下「法」という。）施行の際法第二条第二項の職員（以下「職員」という。）となるべき者を主たる構成員とする団体であつて現に存し、且つ、労働組合法（昭和二十四年法律第百七十四号）第二条に規定する要件を備えるものは、法施行の際法の適用を受ける労働組合（以下「組合」という。）となり、引き続き存続するものとする。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前項の規定により法人として存続する団体及び同項の規定に該当しない団体であつて法人であつたものの登記その他必要な事項は、政令で定める。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "3",
              "ParagraphNum": "３",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "第一項の団体の構成員であつて法施行の際職員とならない者は、法施行の際その団体を脱退したものとする。"
                }
              }
            },
            {
              "@Hide": "false",
              "@Num": "4",
              "ParagraphNum": "４",
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "第一項の規定によつて組合となつたものの代表者は、昭和二十四年六月三十日までに労働大臣に対しその規約並びに役員の住所及び氏名を届け出なければならない。"
                }
              }
            }
          ]
        }
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、昭和二十四年六月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二四年六月一日法律第一七四号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律施行の期日は、公布の日から起算して三十日を越えない期間内において、政令で定める。"
              }
            }
          }
        }
      ]
    }
  }
}
