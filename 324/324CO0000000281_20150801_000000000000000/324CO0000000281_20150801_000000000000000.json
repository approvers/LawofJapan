{
  "Law": {
    "@DataInfo": "190313e04k03m01",
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "281",
    "@Year": "24",
    "LawNum": "昭和二十四年政令第二百八十一号",
    "LawBody": {
      "LawTitle": "日本芸術院令",
      "EnactStatement": "内閣は、文部省設置法（昭和二十四年法律第百四十六号）第二十三条第三項の規定に基き、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（日本芸術院の性格）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "日本芸術院は、芸術上の功績顕著な芸術家を優遇するための栄誉機関とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "日本芸術院は、院長一人及び会員百二十人以内で組織する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "日本芸術院に次の三部を置く。"
                  }
                },
                "List": [
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "第一部"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "美術"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "第二部"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "文芸"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "第三部"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "音楽、演劇、舞踊"
                          }
                        }
                      ]
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会員は、いずれかの部に分属する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会員は、部会が推薦し、総会の承認を経た候補者につき、院長の申出により、文部科学大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項の部会の推薦する者は、部会において芸術上の功績顕著な芸術家につき選挙を行い、部会員の過半数の投票を得た者とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前項の投票において、病気その他の事故のため出席できない者は、郵便その他の方法により投票することができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Function": "main",
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "会員は、終身とする。"
                  },
                  {
                    "@Function": "proviso",
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "ただし、会員が退任を申し出た場合には、総会の承認を経て、これを認めることができる。"
                  }
                ]
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "院長は、芸術に関し卓越した識見を有する者のうち、会員の選挙により過半数の投票を得た者につき、文部科学大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "前項の場合において、過半数の得票者のないときは、投票の最多数を得た者二人につき、更に会員が投票を行い、多数の得票を得た者をもつて当選者とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、得票数が同数のときは、年長者をもつて当選者とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "第三条第三項の規定は、前二項の選挙に準用する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "院長の任期は、三年とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "院長は、非常勤とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "院長は、院務を総理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "7",
                "ParagraphNum": "７",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "院長に事故があるときは、部長のうち最年長者が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "各部に属する会員により部長として互選された者は、各部の部務を掌理する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部長は、三年ごとに改選する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（会議）",
            "ArticleTitle": "第七条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "日本芸術院の会議は、総会、部会及び連合部会とする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "総会は、年二回、院長が招集する。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、必要があるときは、臨時にこれを招集することができる。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "部会は、部長が招集する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "連合部会は、関係する部の部長の申出により、院長が招集する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "5",
                "ParagraphNum": "５",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "総会は、会員の過半数が出席しなければ、議決をすることができない。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、あらかじめ通知した議題について、書面をもつて意思を表示した者は、その議題に限り、出席したものと認めることができる。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "6",
                "ParagraphNum": "６",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "総会の議決は、出席した会員の多数決による。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "7",
                "ParagraphNum": "７",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "前二項の規定は、部会及び連合部会の会議に準用する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleCaption": "（職員）",
            "ArticleTitle": "第八条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "日本芸術院に事務長その他所要の職員を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "事務長は、院長の指揮をうけ、日本芸術院に関する庶務を整理し、その他の職員は、上司の指揮をうけ、庶務に従事する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "9",
            "ArticleCaption": "（雑則）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、日本芸術院の運営に関し必要な事項は、総会の議を経て、院長が定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行し、昭和二十四年六月一日から適用する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三六年六月二日政令第一七一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行し、この政令による改正後の日本芸術院令第八条第一項の規定は、昭和三十六年四月一日から適用する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和四三年六月一五日政令第一七〇号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年六月七日政令第三〇八号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
