            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この規則は、平成十九年十月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二七年三月一八日人事院規則一―六三",
