                      }
                    },
                    {
                      "@Num": "12",
                      "Subitem1Title": "ル",
                      "Subitem1Sentence": {
                        "Sentence": "新会社の設立を必要とする事由"
                      }
                    }
                  ]
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
