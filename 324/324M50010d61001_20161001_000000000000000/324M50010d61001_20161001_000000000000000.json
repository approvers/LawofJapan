                  "Subitem1": [
                    {
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": "資産の全部又は相当部分を包括して処分する場合には、左に掲げる事項"
                      },
