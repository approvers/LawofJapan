{
  "Law": {
    "@Era": "Meiji",
    "@Lang": "ja",
    "@LawType": "Act",
    "@Num": "33",
    "@Year": "33",
    "LawNum": "明治三十三年法律第三十三号",
    "LawBody": {
      "LawTitle": "未成年者喫煙禁止法",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "満二十年ニ至ラサル者ハ煙草ヲ喫スルコトヲ得ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "前条ニ違反シタル者アルトキハ行政ノ処分ヲ以テ喫煙ノ為ニ所持スル煙草及器具ヲ没収ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "未成年者ニ対シテ親権ヲ行フ者情ヲ知リテ其ノ喫煙ヲ制止セサルトキハ科料ニ処ス"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "親権ヲ行フ者ニ代リテ未成年者ヲ監督スル者亦前項ニ依リテ処断ス"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "煙草又ハ器具ヲ販売スル者ハ満二十年ニ至ラザル者ノ喫煙ノ防止ニ資スル為年齢ノ確認其ノ他ノ必要ナル措置ヲ講ズルモノトス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "満二十年ニ至ラサル者ニ其ノ自用ニ供スルモノナルコトヲ知リテ煙草又ハ器具ヲ販売シタル者ハ五十万円以下ノ罰金ニ処ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法人ノ代表者又ハ法人若ハ人ノ代理人、使用人其ノ他ノ従業者ガ其ノ法人又ハ人ノ業務ニ関シ前条ノ違反行為ヲ為シタルトキハ行為者ヲ罰スルノ外其ノ法人又ハ人ニ対シ同条ノ刑ヲ科ス"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "本法ハ明治三十三年四月一日ヨリ之ヲ施行ス"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二二年一二月二二日法律第二二三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "29",
            "ArticleTitle": "第二十九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この法律は、昭和二十三年一月一日から、これを施行する。"
                }
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年一二月一日法律第一三四号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から起算して三十日を経過した日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成一三年一二月一二日法律第一五二号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この法律は、公布の日から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
