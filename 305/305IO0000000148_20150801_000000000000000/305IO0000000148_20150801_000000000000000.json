    "LawNum": "昭和五年勅令第百四十八号",
    "LawBody": {
      "LawTitle": "帝都復興記念章令",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "帝都復興記念ノ表章トシテ特ニ記念章ヲ設ク"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "記念章ノ図式左ノ如シ"
                  }
                },
                "List": [
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "章"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "銀円形、径三糎トス"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "表面ニハ輪廓内上部ニ菊御紋、中央ニ市街ト旭光ノ図、下部ニ桜ノ模様ヲ表シ裏面ニハ輪廓内中央ニ帝都復興記念章ノ文字、下部ニ昭和五年三月ノ文字ヲ識ス"
                      }
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "環"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "銀円形トス"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Column": [
                        {
                          "@LineBreak": "false",
                          "@Num": "1",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "綬"
                          }
                        },
                        {
                          "@LineBreak": "false",
                          "@Num": "2",
                          "Sentence": {
                            "@WritingMode": "vertical",
                            "#text": "織地、幅三糎六粍トス"
                          }
                        }
                      ]
                    }
                  },
                  {
                    "ListSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "中央萌黄色、左右白色、両縁萌黄色トス"
                      }
                    }
                  }
                ]
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "記念章ハ綬ヲ用ヒテ左肋ニ佩ブ"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "記念章ハ左ノ各号ノ一ニ該当スル者ニ之ヲ授与ス"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "帝都復興ノ事業ニ直接関与シタル者"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "帝都復興ノ事業ニ伴フ要務ニ関与シタル者"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "左ノ各号ノ一ニ該当スル者ニハ記念章ヲ授与セズ但シ処刑、免官又ハ免職ノ後前条ニ該当スル者ニ付テハ此ノ限ニ在ラズ"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "禁錮以上ノ刑ニ処セラレタル者"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "懲戒ノ裁判又ハ処分ニ依リ免官又ハ免職セラレタル者"
                    }
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "記念章ヲ授与セラルベキ者ニ対シテハ其ノ授与前死亡シタルトキト雖モ仍之ヲ授与ス"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "記念章ハ本人ニ限リ終身之ヲ佩用シ遺族之ヲ保存スルコトヲ許ス"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "本令ハ公布ノ日ヨリ之ヲ施行ス"
            }
          },
          "List": {
            "ListSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "帝都復興記念章ノ図"
              }
            }
          }
        }
      }
    }
  }
}
