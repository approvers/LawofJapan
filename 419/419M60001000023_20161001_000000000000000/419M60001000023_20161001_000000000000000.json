            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "法第十九条の二第二項の規定による届出は、次に掲げる事項を記載した様式第六号による届出書を提出して行うものとする。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "氏名又は名称及び住所並びに法人にあってはその代表者の氏名及び住所"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "海底及びその下の形質の変更を行っている指定海域の名称"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "海底及びその下の形質の変更の種類、場所及び施行方法"
                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "海底及びその下の形質の変更の内容"
                    }
                  },
                  {
                    "@Num": "5",
                    "ItemTitle": "五",
                    "ItemSentence": {
                      "Sentence": "海底及びその下の形質の変更の着手日"
                    }
                  },
                  {
                    "@Num": "6",
                    "ItemTitle": "六",
                    "ItemSentence": {
                      "Sentence": "海底及びその下の形質の変更の完了日又は完了予定日"
                    }
