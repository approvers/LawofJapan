{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "003",
    "@Year": "19",
    "LawNum": "平成十九年内閣府令第三号",
    "LawBody": {
      "LawTitle": "防衛省の所管に属する補助金等の事務委任の範囲及びその委任を受ける者を定める省令",
      "Preamble": {
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "補助金等に係る予算の執行の適正化に関する法律施行令（昭和三十年政令第二百五十五号）第十六条の規定に基づき、防衛省の所管に属する補助金等の事務委任の範囲及びその委任を受ける者を定める省令を次のとおり定める。"
            }
          },
          {
            "@Num": "2",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "補助金等に係る予算の執行の適正化に関する法律施行令（以下「令」という。）第十六条の規定に基づき、防衛省の所管に属する補助金等の事務を委任する範囲及びその委任を受ける者は、次の各号に定めるところによるものとする。"
            }
          }
        ]
      },
      "MainProvision": {
        "Paragraph": [
          {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "事務委任の範囲"
            },
            "Item": [
              {
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Sentence": "補助金等の交付の申請の受理に関すること。"
                }
              },
              {
                "@Num": "2",
                "ItemTitle": "二",
                "ItemSentence": {
                  "Sentence": "補助金等の交付の決定及びその取消に関すること。"
                }
              },
              {
                "@Num": "3",
                "ItemTitle": "三",
                "ItemSentence": {
                  "Sentence": "補助事業等の実績報告の受理に関すること。"
                }
              },
              {
                "@Num": "4",
                "ItemTitle": "四",
                "ItemSentence": {
                  "Sentence": "補助金等の額の確定に関すること。"
                }
              },
              {
                "@Num": "5",
                "ItemTitle": "五",
                "ItemSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "補助金等の返還に関する処分に関すること。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、令第九条第三項及び第十二条の規定により財務大臣に協議する場合を除く。"
                    }
                  ]
                }
              },
              {
                "@Num": "6",
                "ItemTitle": "六",
                "ItemSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "#text": "補助事業等の監督に関すること。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "#text": "ただし、令第十四条第二項の規定による財務大臣への協議及び防衛大臣が特に必要と認める場合における補助金等に係る予算の執行の適正化に関する法律（昭和三十年法律第百七十九号）第二十三条の規定による立入検査等を除く。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "2",
            "ParagraphNum": "２",
            "ParagraphSentence": {
              "Sentence": "委任を受ける者"
            },
            "Item": [
              {
                "@Num": "1",
                "ItemTitle": "一",
                "ItemSentence": {
                  "Column": [
                    {
                      "@Num": "1",
                      "Sentence": "地方防衛局の所掌に属する補助金等のうち防衛大臣の指定する補助金等の事務（次号に掲げるものを除く。）"
                    },
                    {
                      "@Num": "2",
                      "Sentence": "地方防衛局長"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ItemTitle": "二",
                "ItemSentence": {
                  "Column": [
                    {
                      "@Num": "1",
                      "Sentence": "近畿中部防衛局の所掌に属する補助金等のうち東海防衛支局の所掌に属するものであって防衛大臣の指定する補助金等の事務"
                    },
                    {
                      "@Num": "2",
                      "Sentence": "東海防衛支局長"
                    }
                  ]
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この府令は、防衛庁設置法等の一部を改正する法律（平成十八年法律第百十八号）の施行の日（平成十九年一月九日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一九年八月二〇日防衛省令第九号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、防衛省設置法及び自衛隊法の一部を改正する法律（平成十九年法律第八十号）の施行の日（平成十九年九月一日）から施行する。"
            }
          }
        }
      ]
    }
  }
}
