{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "033",
    "@Year": "19",
    "LawNum": "平成十九年経済産業省令第三十三号",
    "LawBody": {
      "LawTitle": "特別会計に関する法律施行令第五十一条第四項第十二号に規定する経済産業省令で定める要件を定める省令",
      "EnactStatement": "特別会計に関する法律施行令（平成十九年政令第百二十四号）第五十一条第四項第十二号の規定に基づき、特別会計に関する法律施行令第五十一条第四項第十二号に規定する経済産業省令で定める要件を定める省令を次のように制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "特別会計に関する法律施行令第五十一条第四項第十二号に規定する経済産業省令で定める要件は、次のとおりとする。"
          },
          "Item": [
            {
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": "実用化が我が国の電気の安定供給の確保に資する見込みがあると認められること。"
              }
            },
            {
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": "新規性があると認められること。"
              }
            },
            {
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": "実用化のための開発に相当期間を要すること。"
              }
            }
          ]
        }
      },
      "SupplProvision": {
        "@Extract": "true",
        "SupplProvisionLabel": "附　則",
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、特別会計に関する法律（平成十九年法律第二十三号）の施行の日（平成十九年四月一日）から施行する。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（電源開発促進対策特別会計法施行規則の廃止）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "電源開発促進対策特別会計法施行規則（昭和五十六年通商産業省令第四十四号）は、廃止する。"
              }
            }
          }
        ]
      }
    }
  }
}
