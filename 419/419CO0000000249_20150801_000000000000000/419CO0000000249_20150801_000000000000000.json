{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "249",
    "@Year": "19",
    "LawNum": "平成十九年政令第二百四十九号",
    "LawBody": {
      "LawTitle": "広域的地域活性化のための基盤整備に関する法律施行令",
      "EnactStatement": "内閣は、広域的地域活性化のための基盤整備に関する法律（平成十九年法律第五十二号）第二条第四項、第七条第一項及び第十六条第一項第四号の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（公共施設）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "広域的地域活性化のための基盤整備に関する法律（以下「法」という。）第二条第四項の政令で定める公共の用に供する施設は、下水道、緑地、河川、運河及び水路並びに防水、防砂又は防潮の施設並びに港湾における水域施設、外郭施設及び係留施設とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（民間事業者が計画の認定を申請することができる拠点施設の整備に関する事業の規模）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第七条第一項の政令で定める規模は、次の各号に掲げる拠点施設の整備に関する事業の区分に応じ、それぞれ当該各号に定める面積とする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "次に掲げる区域における拠点施設の整備に関する事業（次号に掲げる拠点施設の整備に関する事業を除く。）"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "〇・五ヘクタール"
                        }
                      }
                    ]
                  },
                  "Subitem1": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "首都圏整備法（昭和三十一年法律第八十三号）第二条第三項に規定する既成市街地又は同条第四項に規定する近郊整備地帯"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "近畿圏整備法（昭和三十八年法律第百二十九号）第二条第三項に規定する既成都市区域又は同条第四項に規定する近郊整備区域"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "中部圏開発整備法（昭和四十一年法律第百二号）第二条第三項に規定する都市整備区域"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "4",
                      "Subitem1Title": "ニ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "地方自治法（昭和二十二年法律第六十七号）第二百五十二条の十九第一項に規定する指定都市の区域"
                        }
                      }
                    }
                  ]
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "前号イからニまでに掲げる区域における拠点施設の整備に関する事業であって、当該拠点施設の整備に関する事業の事業区域に隣接し、又は近接してこれと一体的に他の拠点施設の整備に関する事業で次のイからハまでのいずれにも該当するものが施行され、又は施行されることが確実であると見込まれ、かつ、これらの拠点施設の整備に関する事業の事業区域の面積の合計が〇・五ヘクタール以上となる場合における当該拠点施設の整備に関する事業"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "〇・二五ヘクタール"
                        }
                      }
                    ]
                  },
                  "Subitem1": [
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "1",
                      "Subitem1Title": "イ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "広域的地域活性化基盤整備計画に記載された重点地区の区域における建築物及びその敷地の整備に関する事業（これに附帯する事業を含む。）で公共施設の整備を伴うものであること。"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "2",
                      "Subitem1Title": "ロ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "基本方針のうち法第四条第二項第二号に掲げる事項及び広域的地域活性化基盤整備計画のうち当該重点地区の区域に係る法第五条第二項第一号に掲げる事項に照らして適切なものであること。"
                        }
                      }
                    },
                    {
                      "@Delete": "false",
                      "@Hide": "false",
                      "@Num": "3",
                      "Subitem1Title": "ハ",
                      "Subitem1Sentence": {
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "都市における土地の合理的かつ健全な利用及び都市機能の増進に寄与するものであること。"
                        }
                      }
                    }
                  ]
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Column": [
                      {
                        "@LineBreak": "false",
                        "@Num": "1",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "第一号イからニまでに掲げる区域以外の区域における拠点施設の整備に関する事業"
                        }
                      },
                      {
                        "@LineBreak": "false",
                        "@Num": "2",
                        "Sentence": {
                          "@WritingMode": "vertical",
                          "#text": "〇・二ヘクタール"
                        }
                      }
                    ]
                  }
                }
              ]
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（認定事業者が都市計画の決定等を提案することができる都市施設）",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "法第十六条第一項第四号の政令で定める都市施設は、次に掲げるものとする。"
                }
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "道路、都市高速鉄道、駐車場、自動車ターミナルその他の交通施設"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "公園、緑地、広場その他の公共空地"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "水道、電気供給施設、ガス供給施設、下水道、ごみ焼却場その他の供給施設又は処理施設"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "河川、運河その他の水路"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "学校、図書館、研究施設その他の教育文化施設"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "病院、保育所その他の医療施設又は社会福祉施設"
                    }
                  }
                },
                {
                  "@Delete": "false",
                  "@Hide": "false",
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": {
                      "@WritingMode": "vertical",
                      "#text": "防水、防砂又は防潮の施設"
                    }
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、法の施行の日（平成十九年八月六日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二〇年三月三一日政令第一〇五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、平成二十年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二三年一一月二八日政令第三六三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令は、地域の自主性及び自立性を高めるための改革の推進を図るための関係法律の整備に関する法律附則第一条第一号に掲げる規定の施行の日（平成二十三年十一月三十日）から施行する。"
                }
              }
            }
          }
        }
      ]
    }
  }
}
