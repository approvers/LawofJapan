{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "009",
    "@Year": "19",
    "LawNum": "昭和十九年閣令第九号",
    "LawBody": {
      "LawTitle": "官吏功労表彰令施行規則",
      "Preamble": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "官吏功労表彰令施行規則左ノ通定ム"
          }
        }
      },
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "顕功章ハ式典其ノ他ノ廉アル場合之ヲ佩用スベキモノトシ尚平素勤務ノ場合ニ於テモ之ヲ佩用スルヲ例トス"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "顕功章ハ本人ニ限リ終身之ヲ佩用シ遺族之ヲ保存スルコトヲ得"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "顕功状ヲ授与セラレタル庁又ハ部局ニ付其ノ体面ヲ汚辱スベキ事実アルニ至リタルトキハ内閣総理大臣ハ所管大臣ヲ経テ当該庁又ハ部局ニ対シ其ノ受有スル顕功状ノ処置ニ関シ所要ノ指示ヲ為スコトアルベシ"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "官制ノ廃止又ハ改正等ニ因リ顕功状ヲ授与セラレタル庁又ハ部局存在セザルニ至リタル場合ニ於テハ之ニ相当スル庁又ハ部局ニ於テ顕功状ヲ保存スルモノトス"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "前項ノ場合ニ於テ従前ノ庁又ハ部局ニ相当スル庁又ハ部局ナキトキ又ハ不明ナルトキハ内閣総理大臣ノ指定スル庁又ハ部局ヲシテ之ヲ保存セシムルモノトス"
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "顕功章ヲ授与セラルベキ者ニ対シテハ其ノ授与前死亡シタルトキト雖モ危篤ニ陥リタル時ニ遡リテ之ヲ授与ス"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "顕功章又ハ顕功状ノ亡失又ハ毀損ノ場合ニ於テハ申出ニ依リ之ヲ再交付スルコトアルベシ"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "○２",
                "ParagraphSentence": {
                  "Sentence": "前項ノ規定ニ依ル再交付アリタル後亡失ノ顕功章又ハ顕功状ヲ発見シタルトキハ速ニ之ヲ返納スベシ"
                }
              }
            ]
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "顕功章若ハ顕功状ヲ授与シタルトキ又ハ官吏功労表彰令第六条ノ規定ニ依リ之ヲ返納セシメタルトキハ官報ヲ以テ其ノ旨ヲ公示ス"
              }
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "本令ハ公布ノ日ヨリ之ヲ施行ス"
          }
        }
      }
    }
  }
}
