                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "経営改善措置の実施に伴い必要となる資金の額及びその調達方法"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
