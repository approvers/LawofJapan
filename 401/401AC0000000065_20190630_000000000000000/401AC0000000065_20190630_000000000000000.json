                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "経営改善措置の内容及び実施時期"
                      }
                    }
                  },
                  {
                    "@Delete": "false",
                    "@Hide": "false",
