            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この省令は、松くい虫等その他の森林病害虫の駆除予防に関する法律の施行の日から、施行する。"
              }
            }
          }
        },
