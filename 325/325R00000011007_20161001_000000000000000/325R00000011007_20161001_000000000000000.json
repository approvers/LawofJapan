{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "Rule",
    "@Num": "007",
    "@Year": "25",
    "LawNum": "昭和二十五年文化財保護委員会規則第七号",
    "LawBody": {
      "LawTitle": "国宝又は重要文化財指定書規則",
      "EnactStatement": "文化財保護法（昭和二十五年法律第二百十四号）第二十八条第二項の規定に基き、国宝又は重要文化財指定書規則を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（この規則の趣旨）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "文化財保護法（昭和二十五年法律第二百十四号。以下「法」という。）第二十八条第三項の規定により国宝又は重要文化財の所有者に交付する指定書に記載すべき事項及び指定書の附書、形式、再交付、原簿等については、この規則の定めるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（記載事項）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "指定書には、左に掲げる事項を記載するものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "当該国宝又は重要文化財の名称及び員数"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "法第二十七条の規定により国宝又は重要文化財に指定された年月日"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "当該国宝又は重要文化財が建造物であるときは、その構造及び形式"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "当該国宝又は重要文化財が絵画、彫刻、工芸品その他建造物以外のものであるときは、その寸法、重量又は材質その他の特徴"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "指定書の記号番号"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "当該国宝又は重要文化財の所在の場所"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "当該国宝又は重要文化財の所有者の氏名又は名称及び住所"
                  }
                }
              ]
            }
          },
          {
            "@Num": "3",
            "ArticleCaption": "（附書）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "#text": "前条第一号の員数に細目がある場合には、その細目及び同条第三号又は第四号に掲げる事項は、指定書の附書に記載するものとする。"
                    },
                    {
                      "@Num": "2",
                      "#text": "この場合において、附書は、当該指定書の一部分として取り扱うものとする。"
                    }
                  ]
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "前項の附書には、当該指定書の裏面に掛けて割印を押すものとする。"
                }
              }
            ]
          },
          {
            "@Num": "4",
            "ArticleCaption": "（形式及び記載上の注意）",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "国宝の指定書及びその附書並びに重要文化財の指定書及びその附書の形式及び記載上の注意は、それぞれ別表第一から別表第四までの通りとする。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleCaption": "（再交付）",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Num": "1",
                    "#text": "指定書を亡失し、若しくは盗み取られ、又はこれが滅失し、若しくは破損した場合には、その再交付を申請することができる。"
                  },
                  {
                    "@Num": "2",
                    "#text": "この場合においては、これらの事実を証明するに足りる書類又は破損した指定書を添えなければならない。"
                  }
                ]
              }
            }
          },
          {
            "@Num": "6",
            "ArticleCaption": "（原簿）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "文化庁に指定書の原簿を備え、第二条各号に掲げる事項を記載する。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "指定書の交付又は再交付をしようとする場合には、前項の原簿に交付又は再交付の年月日及び再交付のときは、その理由を記載し、且つ、この原簿に掛けて当該指定書に割印を押すものとする。"
                }
              }
            ]
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この規則は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和四三年一二月二六日文部省令第三一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": [
            {
              "@Num": "1",
              "ParagraphNum": "１",
              "ParagraphSentence": {
                "Sentence": "この省令は、公布の日から施行する。"
              }
            },
            {
              "@Num": "2",
              "ParagraphNum": "２",
              "ParagraphSentence": {
                "Sentence": "次に掲げる命令は、廃止する。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "重要美術品等ノ保存ニ関スル法律施行規則（昭和八年文部省令第十号）"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "史跡名勝天然記念物を管理すべき地方公共団体を指定する規則（昭和二十五年文化財保護委員会規則第三号）"
                  }
                },
                {
                  "@Num": "3",
                  "ItemTitle": "三",
                  "ItemSentence": {
                    "Sentence": "国宝又は重要文化財台帳規則（昭和二十五年文化財保護委員会規則第八号）"
                  }
                },
                {
                  "@Num": "4",
                  "ItemTitle": "四",
                  "ItemSentence": {
                    "Sentence": "史跡名勝天然記念物台帳規則（昭和二十八年文化財保護委員会規則第一号）"
                  }
                },
                {
                  "@Num": "5",
                  "ItemTitle": "五",
                  "ItemSentence": {
                    "Sentence": "重要民俗資料台帳規則（昭和三十一年文化財保護委員会規則第二号）"
                  }
                },
                {
                  "@Num": "6",
                  "ItemTitle": "六",
                  "ItemSentence": {
                    "Sentence": "重要無形文化財等台帳規則（昭和三十一年文化財保護委員会規則第四号）"
                  }
                },
                {
                  "@Num": "7",
                  "ItemTitle": "七",
                  "ItemSentence": {
                    "Sentence": "文化財保護委員会補助金交付規則（昭和三十九年文化財保護委員会規則第二号）"
                  }
                }
              ]
            }
          ]
        },
        {
          "@AmendLawNum": "昭和五〇年九月三〇日文部省令第三三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この省令は、文化財保護法の一部を改正する法律の施行の日（昭和五十年十月一日）から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一二年一〇月三一日文部省令第五三号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Article": {
            "@Num": "1",
            "ArticleCaption": "（施行期日）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "この省令は、内閣法の一部を改正する法律（平成十一年法律第八十八号）の施行の日（平成十三年一月六日）から施行する。"
              }
            }
          }
        }
      ],
