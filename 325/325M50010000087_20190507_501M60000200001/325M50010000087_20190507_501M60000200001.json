              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Num": "1",
                    "@WritingMode": "vertical",
                    "#text": "法第十三条第一項の届出は、書類でしなければならない。"
                  },
                  {
                    "@Num": "2",
                    "@WritingMode": "vertical",
                    "#text": "この場合には左に掲げる書面を添附しなければならない。"
                  }
                ]
              },
              "Item": [
                {
                  "@Delete": "false",
                  "@Hide": "false",
