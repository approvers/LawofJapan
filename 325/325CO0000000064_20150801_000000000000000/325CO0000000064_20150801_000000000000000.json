            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、昭和二十五年四月一日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二七年七月三一日政令第二八八号",
