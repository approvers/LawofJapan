    "LawBody": {
      "LawTitle": "国際観光ホテル整備法施行令",
      "EnactStatement": "内閣は、国際観光ホテル整備法（昭和二十四年法律第二百七十九号）を実施するため並びに同法第十四条第三項及び第二十九条の規定に基き、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（登録実施機関の登録の有効期間）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "国際観光ホテル整備法（以下「法」という。）第二十一条第一項の政令で定める期間は、五年とする。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（関係大臣との協議）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "観光庁長官は、法第三十三条第一項の規定による勧告をしようとする場合において、その勧告が、国立公園の区域内のホテル又は旅館に対して行われるときは環境大臣に、公衆衛生の改善を図る事項を内容とするときは厚生労働大臣に、それぞれ協議するものとする。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、公布の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和二六年七月二八日政令第二七五号",
          "SupplProvisionLabel": "附　則",
