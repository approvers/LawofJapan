                    }
                  },
                  {
                    "@Num": "4",
                    "ItemTitle": "四",
                    "ItemSentence": {
                      "Sentence": "法第三条の二第一項第二号及び第三号に掲げる基準に適合することを説明する書類"
                    }
                  }
                ]
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
