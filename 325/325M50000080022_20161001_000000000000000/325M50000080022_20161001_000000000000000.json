{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "022",
    "@Year": "25",
    "LawNum": "昭和二十五年文部省令第二十二号",
    "LawBody": {
      "LawTitle": "文部省映画及び幻灯画頒布規程",
      "EnactStatement": "文部省設置法（昭和二十四年法律第百四十六号）の規定を実施するため、この省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "文部省が製作した映画又は幻灯画（以下「映画又は幻灯画」という。）の有償頒布については、この省令の定めるとこによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "映画又は幻灯画の頒布を受けようとする者は、別記様式による申請書を文部大臣に提出するものとする。"
              }
            }
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "映画又は幻灯画の頒布を受けようとする者は、その料金を前納するものとする。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "頒布する映画又は幻灯画の題名、内容及び料金については、官報その他に公告するものとする。"
              }
            }
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "第三条の頒布料金は、納入告知書により日本銀行（支店、代理店及び歳入代理店を含む。）に納入するものとする。"
              }
            }
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "映画又は幻灯画の頒布を受けた者（頒布を受けた映画又は幻灯画の譲受人を含む。）は、その映画又は幻灯画について左に掲げる行為をしてはならない。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "内容を変えること"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "全部又は一部を複製すること"
                  }
                }
              ]
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "特別の事由により、この規程によりがたい場合は、文部大臣は適宜その取扱について定めるものとする。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、昭和二十五年三月一日から適用する。"
            }
          }
        },
        {
          "@AmendLawNum": "昭和二七年一〇月二七日文部省令第二五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        }
      ],
      "AppdxStyle": {
        "AppdxStyleTitle": "別記様式",
        "StyleStruct": {
          "Style": {
            "Fig": {
              "@src": ""
            }
          }
        }
      }
    }
  }
}
