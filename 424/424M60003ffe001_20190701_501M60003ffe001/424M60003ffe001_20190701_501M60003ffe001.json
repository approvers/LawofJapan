                  "TableColumn": [
                    {
                      "@BorderBottom": "solid",
                      "@BorderLeft": "solid",
                      "@BorderRight": "solid",
                      "@BorderTop": "solid",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "四　予決令第十七条に規定する移用又は流用を必要とする理由、科目及び金額を明らかにした書類"
                      }
                    },
                    {
                      "@BorderBottom": "solid",
                      "@BorderLeft": "solid",
                      "@BorderRight": "solid",
                      "@BorderTop": "solid",
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "移用又は流用をする必要があることについて所管大臣の決定があった日の翌々日"
