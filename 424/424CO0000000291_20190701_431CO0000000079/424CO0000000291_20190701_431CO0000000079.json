{
  "Law": {
    "@DataInfo": "190528e01k01m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "291",
    "@Year": "24",
    "LawNum": "平成二十四年政令第二百九十一号",
    "LawBody": {
      "LawTitle": "消費者教育推進会議令",
      "EnactStatement": "内閣は、消費者教育の推進に関する法律（平成二十四年法律第六十一号）第十九条第四項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Article": [
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "1",
            "ArticleCaption": "（組織）",
            "ArticleTitle": "第一条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "消費者教育推進会議（以下「会議」という。）は、委員二十人以内で組織する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会議に、専門の事項を調査させるため必要があるときは、専門委員を置くことができる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "2",
            "ArticleCaption": "（専門委員の任命）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "専門委員は、当該専門の事項に関し学識経験のある者のうちから、内閣総理大臣が任命する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "3",
            "ArticleCaption": "（委員の任期等）",
            "ArticleTitle": "第三条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "委員の任期は、二年とする。"
                    },
                    {
                      "@Function": "proviso",
                      "@Num": "2",
                      "@WritingMode": "vertical",
                      "#text": "ただし、補欠の委員の任期は、前任者の残任期間とする。"
                    }
                  ]
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員は、再任されることができる。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "専門委員は、その者の任命に係る当該専門の事項に関する調査が終了したときは、解任されるものとする。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "委員及び専門委員は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "4",
            "ArticleCaption": "（会長）",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会議に、会長を置き、委員の互選により選任する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長は、会務を総理し、会議を代表する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会長に事故があるときは、あらかじめその指名する委員が、その職務を代理する。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleCaption": "（幹事）",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会議に、幹事を置く。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、関係行政機関の職員のうちから、内閣総理大臣が任命する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、会議の所掌事務について、委員及び専門委員を補佐する。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "4",
                "ParagraphNum": "４",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "幹事は、非常勤とする。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "6",
            "ArticleCaption": "（議事）",
            "ArticleTitle": "第六条",
            "Paragraph": [
              {
                "@Hide": "false",
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会議は、委員の過半数が出席しなければ、会議を開き、議決することができない。"
                  }
                }
              },
              {
                "@Hide": "false",
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": {
                    "@WritingMode": "vertical",
                    "#text": "会議の議事は、出席した委員の過半数で決し、可否同数のときは、会長の決するところによる。"
                  }
                }
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "7",
            "ArticleCaption": "（資料の提出等の要求）",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "会議は、その所掌事務を遂行するため必要があると認めるときは、関係行政機関の長に対し、資料の提出、意見の表明、説明その他必要な協力を求めることができる。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "8",
            "ArticleCaption": "（庶務）",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "会議の庶務は、消費者庁消費者教育推進課において処理する。"
                }
              }
            }
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "9",
            "ArticleCaption": "（会議の運営）",
            "ArticleTitle": "第九条",
            "Paragraph": {
              "@Hide": "false",
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": {
                  "@WritingMode": "vertical",
                  "#text": "この政令に定めるもののほか、議事の手続その他会議の運営に関し必要な事項は、会長が会議に諮って定める。"
                }
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "@Extract": "false",
          "@Type": "New",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Hide": "false",
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、消費者教育の推進に関する法律の施行の日（平成二十四年十二月十三日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三〇年三月三〇日政令第八一号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この政令は、平成三十年四月一日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成三一年三月二九日政令第七九号",
          "@Extract": "true",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphCaption": "（施行期日）",
            "ParagraphNum": "１",
            "ParagraphSentence": {
              "Sentence": "この政令は、平成三十一年七月一日から施行する。"
            }
          }
        }
      ]
    }
  }
}
