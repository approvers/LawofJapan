                  {
                    "@Delete": "false",
                    "@Hide": "false",
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": {
                        "@WritingMode": "vertical",
                        "#text": "平成二十二年度の実績医療費拠出金の額とその額に係る調整金額との合計額から当該合計額に退職被保険者等所属割合を乗じて得た額を控除した額の百分の七に相当する額"
                      }
                    }
                  }
                ]
              }
            ]
          },
          {
            "@Delete": "false",
            "@Hide": "false",
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
