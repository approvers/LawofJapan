{
  "Law": {
    "@DataInfo": "190316e81k80m01",
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "Misc",
    "@Num": "2",
    "@Year": "24",
    "LawNum": "平成二十四年財務省・経済産業省令第二号",
    "LawBody": {
      "LawTitle": "中小企業等経営強化法第二十条第一項第一号の経済産業省令・財務省令で定める金融機関を定める省令",
      "MainProvision": {
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@Num": "1",
              "@WritingMode": "vertical",
              "#text": "中小企業等経営強化法第二十条第一項第一号の経済産業省令・財務省令で定めるものは、次に掲げるものとする。"
            }
          },
          "Item": [
            {
              "@Num": "1",
              "ItemTitle": "一",
              "ItemSentence": {
                "Sentence": {
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "銀行法（昭和五十六年法律第五十九号）第二条第一項に規定する銀行（外国において支店その他の営業所を設置しているものに限る。）"
                }
              }
            },
            {
              "@Num": "2",
              "ItemTitle": "二",
              "ItemSentence": {
                "Sentence": {
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "外国の法令に準拠して外国において銀行法第二条第二項に規定する銀行業を営む者（同法第四条第五項に規定する銀行等を除く。）"
                }
              }
            },
            {
              "@Num": "3",
              "ItemTitle": "三",
              "ItemSentence": {
                "Sentence": {
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "外国の政府、政府機関又は地方公共団体が主たる出資者となっている金融機関（前号に掲げるものを除く。）"
                }
              }
            },
            {
              "@Num": "4",
              "ItemTitle": "四",
              "ItemSentence": {
                "Sentence": {
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "農林中央金庫"
                }
              }
            },
            {
              "@Num": "5",
              "ItemTitle": "五",
              "ItemSentence": {
                "Sentence": {
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "株式会社商工組合中央金庫"
                }
              }
            }
          ]
        }
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@Num": "1",
                "@WritingMode": "vertical",
                "#text": "この省令は、中小企業の海外における商品の需要の開拓の促進等のための中小企業の新たな事業活動の促進に関する法律等の一部を改正する法律（平成二十四年法律第四十四号）の施行の日（平成二十四年八月三十日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成二八年六月三〇日財務省・経済産業省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@Num": "1",
                "@WritingMode": "vertical",
                "#text": "この省令は、中小企業の新たな事業活動の促進に関する法律の一部を改正する法律の施行の日から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "平成三〇年七月六日財務省・経済産業省令第三号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": {
                "@Num": "1",
                "@WritingMode": "vertical",
                "#text": "この省令は、産業競争力強化法等の一部を改正する法律の施行の日（平成三十年七月九日）から施行する。"
              }
            }
          }
        }
      ]
    }
  }
}
