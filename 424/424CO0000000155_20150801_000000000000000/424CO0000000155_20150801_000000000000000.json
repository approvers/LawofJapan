{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "CabinetOrder",
    "@Num": "155",
    "@Year": "24",
    "LawNum": "平成二十四年政令第百五十五号",
    "LawBody": {
      "LawTitle": "刑事事件における第三者所有物の没収手続に関する応急措置法第二条第二項の規定による公告の方法を定める政令",
      "EnactStatement": "内閣は、刑事事件における第三者所有物の没収手続に関する応急措置法（昭和三十八年法律第百三十八号）第二条第二項の規定に基づき、この政令を制定する。",
      "MainProvision": {
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": [
              {
                "@Function": "main",
                "@Num": "1",
                "@WritingMode": "vertical",
                "#text": "刑事事件における第三者所有物の没収手続に関する応急措置法第二条第二項の規定による公告は、検察庁の掲示場に十四日間掲示する方法によって行う。"
              },
              {
                "@Function": "proviso",
                "@Num": "2",
                "@WritingMode": "vertical",
                "#text": "ただし、必要があるときは、官報又は新聞紙に掲載する方法を併せて行うことができる。"
              }
            ]
          }
        }
      },
      "SupplProvision": {
        "@Extract": "false",
        "@Type": "New",
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Hide": "false",
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": {
              "@WritingMode": "vertical",
              "#text": "この政令は、情報処理の高度化等に対処するための刑法等の一部を改正する法律（平成二十三年法律第七十四号）附則第一条第一号に掲げる規定の施行の日（平成二十四年六月二十二日）から施行する。"
            }
          }
        }
      }
    }
  }
}
