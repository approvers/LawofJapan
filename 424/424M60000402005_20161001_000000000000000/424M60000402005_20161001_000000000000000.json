{
  "Law": {
    "@Era": "Heisei",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "005",
    "@Year": "24",
    "LawNum": "平成二十四年内閣府・経済産業省令第五号",
    "LawBody": {
      "LawTitle": "産業高度化・事業革新措置実施計画の認定申請等に関する命令",
      "EnactStatement": "沖縄振興特別措置法（平成十四年法律第十四号）第三十五条の三第三項及び沖縄振興特別措置法施行令（平成十四年政令第百二号）第四条第八号の規定に基づき、産業高度化・事業革新措置実施計画の認定申請等に関する命令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleCaption": "（申請書の添付書類）",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "沖縄振興特別措置法第三十五条の三第三項の主務省令で定める書類は、次に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "定款又は寄附行為及び登記事項証明書"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "認定の申請の日の属する事業年度の前事業年度における貸借対照表及び損益計算書（認定の申請の日の属する事業年度に設立された法人にあっては、その設立時における財産目録）"
                  }
                }
              ]
            }
          },
          {
            "@Num": "2",
            "ArticleCaption": "（施設又は設備）",
            "ArticleTitle": "第二条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "沖縄振興特別措置法施行令第四条第八号に規定する主務省令で定める施設又は設備は、次の各号に掲げるものとする。"
              },
              "Item": [
                {
                  "@Num": "1",
                  "ItemTitle": "一",
                  "ItemSentence": {
                    "Sentence": "電気事業会計規則（昭和四十年通商産業省令第五十七号）別表第１に定める固定資産のうち、水力発電設備、汽力発電設備、内燃力発電設備、新エネルギー等発電設備、送電設備、変電設備又は配電設備"
                  }
                },
                {
                  "@Num": "2",
                  "ItemTitle": "二",
                  "ItemSentence": {
                    "Sentence": "海水温度差発電施設又は設備"
                  }
                }
              ]
            }
          }
        ]
      },
      "SupplProvision": {
        "SupplProvisionLabel": "附　則",
        "Paragraph": {
          "@Num": "1",
          "ParagraphNum": null,
          "ParagraphSentence": {
            "Sentence": "この命令は、平成二十四年四月一日から施行する。"
          }
        }
      }
    }
  }
}
