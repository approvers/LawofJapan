                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Function": "main",
                      "@Num": "1",
                      "@WritingMode": "vertical",
                      "#text": "令第十八条に規定する立札による掲示は、別記様式第五により行うことを例とする。"
                    },
                    {
                      "@Function": "proviso",
