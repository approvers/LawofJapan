{
  "Law": {
    "@Era": "Showa",
    "@Lang": "ja",
    "@LawType": "MinisterialOrdinance",
    "@Num": "027",
    "@Year": "32",
    "LawNum": "昭和三十二年法務省令第二十七号",
    "LawBody": {
      "LawTitle": "戸籍法附則第三条第一項の戸籍の改製に関する省令",
      "EnactStatement": "戸籍法（昭和二十二年法律第二百二十四号）第百二十八条第一項ただし書の規定に基き、戸籍法第百二十八条第一項の戸籍の改製に関する省令を次のように定める。",
      "MainProvision": {
        "Article": [
          {
            "@Num": "1",
            "ArticleTitle": "第一条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "戸籍法（昭和二十二年法律第二百二十四号）附則第三条第一項の戸籍（以下「旧法戸籍」という。）の同項ただし書の規定による改製については、この省令の定めるところによる。"
              }
            }
          },
          {
            "@Num": "2",
            "ArticleTitle": "第二条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "市町村長は、昭和三十三年四月一日に改製の事務に着手し、すみやかにこれを完了しなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": "市町村長は、昭和三十二年十一月三十日までに、昭和三十三会計年度内において改製しようとする旧法戸籍について、監督法務局又は地方法務局の長に改製の計画を報告しなければならない。"
                }
              },
              {
                "@Num": "3",
                "ParagraphNum": "３",
                "ParagraphSentence": {
                  "Sentence": "市町村長は、改製の事務が昭和三十四会計年度以降にわたる場合には、前項の例により、当該年度の前年の十一月三十日までに改製の計画を報告しなければならない。"
                }
              }
            ]
          },
          {
            "@Num": "3",
            "ArticleTitle": "第三条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "法務局又は地方法務局の長は、前条第二項又は第三項の規定による報告を受けたときは、必要な事項を調査した後、意見を付してこれを法務大臣に進達しなければならない。"
              }
            }
          },
          {
            "@Num": "4",
            "ArticleTitle": "第四条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "旧法戸籍のうちその筆頭に記載した者及びその戸籍に在るその他の者の記載が戸籍法第六条及び第十四条の規定に適合するものについては、その戸籍の筆頭に記載した者の事項欄に改製の事由を記載しなければならない。"
                }
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "#text": "前項の戸籍については、同項の手続をした後、その戸籍の筆頭に記載した者及びその戸籍に在る者についてあらたに戸籍を編製することができる。"
                    },
                    {
                      "@Num": "2",
                      "#text": "この場合には、戸籍法施行規則（昭和二十二年司法省令第九十四号）第三十七条ただし書及び第三十九条第一項の規定を準用する。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "5",
            "ArticleTitle": "第五条",
            "Paragraph": [
              {
                "@Num": "1",
                "ParagraphNum": null,
                "ParagraphSentence": {
                  "Sentence": "旧法戸籍に在る者で、次に掲げる者以外のものについては、戸籍法第六条及び第十四条の定めるところにより新戸籍を編製し、その戸籍事項欄に改製の事由を記載しなければならない。"
                },
                "Item": [
                  {
                    "@Num": "1",
                    "ItemTitle": "一",
                    "ItemSentence": {
                      "Sentence": "戸籍の筆頭に記載した者（その戸籍に在る者の子で、これと氏を同じくし、かつ、配偶者又は氏を同じくする子を有しない者を除く。）"
                    }
                  },
                  {
                    "@Num": "2",
                    "ItemTitle": "二",
                    "ItemSentence": {
                      "Sentence": "戸籍の筆頭に記載した者の配偶者"
                    }
                  },
                  {
                    "@Num": "3",
                    "ItemTitle": "三",
                    "ItemSentence": {
                      "Sentence": "戸籍の筆頭に記載した者又はその配偶者の子で、これと氏を同じくする者（配偶者又は氏を同じくする子を有する者を除く。）"
                    }
                  }
                ]
              },
              {
                "@Num": "2",
                "ParagraphNum": "２",
                "ParagraphSentence": {
                  "Sentence": [
                    {
                      "@Num": "1",
                      "#text": "旧法戸籍に在る者について前項の規定により新戸籍を編製したときは、従前の戸籍の筆頭に記載した者の事項欄に改製の事由を記載しなければならない。"
                    },
                    {
                      "@Num": "2",
                      "#text": "この場合には、前条第二項の規定を準用する。"
                    }
                  ]
                }
              }
            ]
          },
          {
            "@Num": "6",
            "ArticleTitle": "第六条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "市町村長は、第四条第一項の手続をしたときは、当該戸籍を表示して、一箇月ごとに遅滞なく管轄法務局若しくは地方法務局又はその支局にその旨を報告しなければならない。"
              }
            }
          },
          {
            "@Num": "7",
            "ArticleTitle": "第七条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": [
                  {
                    "@Num": "1",
                    "#text": "第五条第一項の規定により、新戸籍を編製した場合において従前の戸籍の全部を消除したときは、その除かれた戸籍及びその副本の保存期間は、当該年度の翌年から百五十年とする。"
                  },
                  {
                    "@Num": "2",
                    "#text": "第四条第二項（第五条第二項で準用する場合を含む。）の規定によりあらたに戸籍を編製した場合における従前の戸籍及びその副本の保存期間についても、同様とする。"
                  }
                ]
              }
            }
          },
          {
            "@Num": "8",
            "ArticleTitle": "第八条",
            "Paragraph": {
              "@Num": "1",
              "ParagraphNum": null,
              "ParagraphSentence": {
                "Sentence": "管轄法務局若しくは地方法務局又はその支局は、前条の除かれた戸籍の副本の送付を受けたときは、戸籍法施行規則第四十九条第二項の規定にかかわらず、当該戸籍に関する書類で、市町村長が受理し又は送付を受けた年度の翌年から五年を経過したものを廃棄することができる。"
              }
            }
          }
        ]
      },
      "SupplProvision": [
        {
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成一六年四月一日法務省令第二九号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二一年四月三〇日法務省令第二五号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、公布の日から施行する。"
            }
          }
        },
        {
          "@AmendLawNum": "平成二二年五月六日法務省令第二二号",
          "SupplProvisionLabel": "附　則",
          "Paragraph": {
            "@Num": "1",
            "ParagraphNum": null,
            "ParagraphSentence": {
              "Sentence": "この省令は、平成二十二年六月一日から施行する。"
            }
          }
        }
      ]
    }
  }
