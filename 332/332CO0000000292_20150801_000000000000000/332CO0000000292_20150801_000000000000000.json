            "ParagraphSentence": {
              "Sentence": {
                "@WritingMode": "vertical",
                "#text": "この政令は、小型船海運組合法の施行の日（昭和三十二年十月一日）から施行する。"
              }
            }
          }
        },
        {
          "@AmendLawNum": "昭和三九年七月一六日政令第二五四号",
          "@Extract": "false",
