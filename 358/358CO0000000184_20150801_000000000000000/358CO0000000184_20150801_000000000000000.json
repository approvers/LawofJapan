            "ParagraphSentence": {
              "Sentence": [
                {
                  "@Function": "main",
                  "@Num": "1",
                  "@WritingMode": "vertical",
                  "#text": "この政令は、改正法附則第一条第二号に定める日（昭和五十八年十月二日）から施行する。"
                },
                {
                  "@Function": "proviso",
                  "@Num": "2",
                  "@WritingMode": "vertical",
                  "#text": "ただし、第三条及び第八条の規定は、改正法附則第一条第一号に定める日（昭和五十八年八月二十五日）から施行する。"
                }
              ]
            }
          }
